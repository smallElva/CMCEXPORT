;AACD,OAJD,MAKK,IAAG,KAAK9I,IAAL,IAAa,CAAb,IAAkBoE,cAAc,CAACwL,OAAf,CAAuB,eAAvB,CAArB,EAA6D;AAChE,YAAIhH,UAAS,GAAItE,IAAI,CAACoE,KAAL,CAAWtE,cAAc,CAACwL,OAAf,CAAuB,eAAvB,CAAX,CAAjB;;AACAF,qBAAa,CAACG,YAAd,CAA2BjH,UAAS,CAAC,CAAD,CAAT,CAAakH,MAAxC,EAA+ClH,UAAS,CAAC,CAAD,CAAT,CAAamH,UAAb,CAAwB,CAAxB,EAA2BhN,IAA1E,EAA+E6F,UAAS,CAAC,CAAD,CAAT,CAAamH,UAAb,CAAwB,CAAxB,EAA2BC,IAA1G,EAA+GpH,UAAS,CAAC,CAAD,CAAT,CAAaqH,UAA5H;AACAP,qBAAa,CAAC5G,MAAd;AACD,OAJI,MAKA,IAAG,KAAK9I,IAAL,IAAa,CAAb,IAAkBoE,cAAc,CAACwL,OAAf,CAAuB,eAAvB,CAArB,EAA6D;AAChE,YAAIhH,WAAS,GAAItE,IAAI,CAACoE,KAAL,CAAWtE,cAAc,CAACwL,OAAf,CAAuB,eAAvB,CAAX,CAAjB;;AACAF,qBAAa,CAACG,YAAd,CAA2BjH,WAAS,CAAC,CAAD,CAAT,CAAakH,MAAxC,EAA+ClH,WAAS,CAAC,CAAD,CAAT,CAAamH,UAAb,CAAwB,CAAxB,EAA2BhN,IAA1E,EAA+E6F,WAAS,CAAC,CAAD,CAAT,CAAamH,UAAb,CAAwB,CAAxB,EAA2BC,IAA1G,EAA+GpH,WAAS,CAAC,CAAD,CAAT,CAAaqH,UAA5H;AACAP,qBAAa,CAAC5G,MAAd;AACD;AACF,KA7EK;AA8EN;AACAvH,WA/EM,qBA+EG;AACP,aAAO,KAAKpB,IAAZ;AACD,KAjFK;;AAkFN;AACAsC,UAnFM,oBAmFE;AACN,aAAO,KAAKoB,GAAZ;AACD;AArFK;AAnDc,CAAjB,C","file":"js/11-e499419b.js","sourcesContent":["\r\n\r\n/* 表头部分\r\n */\r\n\r\n//设置 列头(公共)\r\nimport {alarmIndex} from \"@/components/content/system/alarm/table/js/alarm_methods\";\r\n\r\nexport function setHeaders(type){\r\n    let header;\r\n    if(type==1){\r\n        header =[\r\n                '组织名称',\r\n                '机组名称',\r\n                '测点名称',\r\n                '测点类型',\r\n                '数据类型',\r\n                '工况#1',\r\n                '工况#1预警（高）',\r\n                '工况#1Ⅰ级报警（高）',\r\n                '工况#1Ⅱ级报警（高）',\r\n                '工况#2',\r\n                '工况#2预警（高）',\r\n                '工况#2Ⅰ级报警（高）',\r\n                '工况#2Ⅱ级报警（高）',\r\n                '工况#3',\r\n                '工况#3预警（高）',\r\n                '工况#3Ⅰ级报警（高）',\r\n                '工况#3Ⅱ级报警（高）',\r\n                '工况#4',\r\n                '工况#4预警（高）',\r\n                '工况#4Ⅰ级报警（高）',\r\n                '工况#4Ⅱ级报警（高）',\r\n                '其他工况',\r\n                '其他工况预警（高）',\r\n                '其他工况Ⅰ级报警（高）',\r\n                '其他工况Ⅱ级报警（高）',\r\n        ];\r\n    }\r\n    else if(type==2){\r\n        header = [\r\n\r\n                '组织名称',\r\n                '机组名称',\r\n                '测点名称',\r\n                '测点类型',\r\n                // 'Ⅱ级报警(高)',\r\n                '报警(高)',\r\n                '预警(高)',\r\n                '预警(低)',\r\n                '报警(低)',\r\n                // 'Ⅱ级报警(低)',\r\n        ]\r\n    }\r\n    else if(type==3){\r\n        header = [\r\n                '组织名称',\r\n                '机组名称',\r\n                '测点名称',\r\n                '测点类型',\r\n                '数据类型',\r\n                '报警门限',\r\n                '报警计数',\r\n        ]\r\n    }\r\n    return header\r\n\r\n}\r\n\r\n\r\n/* 表数据部分\r\n */\r\n\r\n//设置columns（公共）（组织名称、机组名称、测点名称、测点类型）\r\nexport function setColumns(type,data){\r\n    let alarmBaseInfo ='alarmBaseInfo';\r\n    let columns;\r\n    // let columnObj = data.alarmBaseInfo.code==2000?alarm_threshold_index_val:vib_k_hh;\r\n    columns=[\r\n        {data: `${alarmBaseInfo}.t_name`},\r\n        {data: `${alarmBaseInfo}.mac_me`},\r\n        {data: `${alarmBaseInfo}.pos_name`},\r\n        {data: `${alarmBaseInfo}.pos_type_name`},\r\n    ];\r\n    columns.push(...selectColumns(parseInt(type),alarmBaseInfo,data))\r\n    return columns\r\n}\r\n//列表渲染的选择\r\nfunction selectColumns(type,alarmBaseInfo,data){\r\n    let col;\r\n    switch(type){\r\n        case 1: //工况报警\r\n            col=[\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.name`,\r\n                    /*editor: 'select',\r\n                    selectOptions: function(row){\r\n                        return data[row].alarmBaseInfo.valueSources;\r\n                    }*/\r\n                    type: 'dropdown',\r\n                    source: [],\r\n                    strict: true\r\n                },\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.status_1`,\r\n                    type: 'checkbox',\r\n                    checkedTemplate: '1',\r\n                    uncheckedTemplate: '0',\r\n                },\r\n                {data: `${alarmBaseInfo}.alarmInfo.h_limit_1`},\r\n                {data: `${alarmBaseInfo}.alarmInfo.hh_limit_1`},\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.hhh_limit_1`,\r\n                    readOnly: true,\r\n                },\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.status_2`,\r\n                    type: 'checkbox',\r\n                    checkedTemplate: '1',\r\n                    uncheckedTemplate: '0',\r\n                },\r\n                {data: `${alarmBaseInfo}.alarmInfo.h_limit_2`},\r\n                {data: `${alarmBaseInfo}.alarmInfo.hh_limit_2`},\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.hhh_limit_2`,\r\n                    readOnly: true,\r\n                },\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.status_3`,\r\n                    type: 'checkbox',\r\n                    checkedTemplate: '1',\r\n                    uncheckedTemplate: '0',\r\n                },\r\n                {data: `${alarmBaseInfo}.alarmInfo.h_limit_3`},\r\n                {data: `${alarmBaseInfo}.alarmInfo.hh_limit_3`},\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.hhh_limit_3`,\r\n                    readOnly: true,\r\n                },\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.status_4`,\r\n                    type: 'checkbox',\r\n                    checkedTemplate: '1',\r\n                    uncheckedTemplate: '0',\r\n                },\r\n                {data: `${alarmBaseInfo}.alarmInfo.h_limit_4`},\r\n                {data: `${alarmBaseInfo}.alarmInfo.hh_limit_4`},\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.hhh_limit_4`,\r\n                    readOnly: true,\r\n                },\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.status_5`,\r\n                    type: 'checkbox',\r\n                    checkedTemplate: '1',\r\n                    uncheckedTemplate: '0',\r\n                },\r\n                {data: `${alarmBaseInfo}.alarmInfo.h_limit_5`},\r\n                {data: `${alarmBaseInfo}.alarmInfo.hh_limit_5`},\r\n                {\r\n                    data: `${alarmBaseInfo}.alarmInfo.hhh_limit_5`,\r\n                    readOnly: true,\r\n                },\r\n            ];\r\n            break;\r\n        case 2://常规报警\r\n            col =[\r\n                /*{\r\n                    data: `${alarmBaseInfo}.baseInfo.hhh_limit`,\r\n                    readOnly: true,\r\n                },//报警高*/\r\n                {data: `${alarmBaseInfo}.baseInfo.hh_limit`},//预警高\r\n                {data: `${alarmBaseInfo}.baseInfo.h_limit`},//报警低\r\n                {\r\n                    data: `${alarmBaseInfo}.baseInfo.l_limit`,\r\n\r\n                },//报警低\r\n                {data: `${alarmBaseInfo}.baseInfo.ll_limit`},//预警低\r\n                /*{\r\n                    data: `${alarmBaseInfo}.baseInfo.lll_limit`,\r\n                    readOnly: true,\r\n                },//报警高*/\r\n            ];\r\n            break;\r\n        case 3://MHD报警\r\n            col =[\r\n                {\r\n                    data: `${alarmBaseInfo}.code_val`,\r\n                    editor: 'select',\r\n                    selectOptions: function(row){\r\n                        return data[row].alarmBaseInfo.codeSources;\r\n                    }\r\n                },\r\n                {data: `${alarmBaseInfo}.columnObj`},\r\n                {data: `${alarmBaseInfo}.alarm_counter`},\r\n                ];\r\n            break;\r\n    }\r\n    return col\r\n}\r\n\r\n","import getApi from 'network/getApi'\r\nimport hdtable from 'common/hdtable.js'\r\nimport {changeAlarm} from './alarm_info.js'\r\nimport {setHeaders,setColumns} from './alarm_columns.js'\r\nimport {setChangeCellResult,addSessionStorage} from 'utils/utils'\r\nimport {arraymove} from \"./alarm_methods.js\";\r\n\r\n\r\nlet colWidth = '';\r\nexport default function alarmhdtable(el,property){\r\n  // console.log(new Handsontable());\r\n  let data = property.getData();\r\n\r\n  //创建表格属性的实例\r\n  let tableDataIns= new tableData(property)\r\n  // 组装振动表格数据\r\n  let vibTable ={\r\n\r\n    // 表格右键菜单\r\n    contextMenu:tableDataIns.contextMenu(),\r\n    // 表格显示的数据\r\n    data:data,\r\n    manualColumnFreeze: true,\r\n    // fixedColumnsLeft: 4,\r\n    //表格的列\r\n    colHeaders:tableDataIns.colHeaders(),\r\n    //表格的列头自定义\r\n    // nestedHeaders:tableDataIns.nestedHeaders(),\r\n    //数据渲染renderData\r\n    columns:tableDataIns.columns(),\r\n    //过滤之后\r\n    afterFilter:tableDataIns.afterFilter(),\r\n    //单元格属性设置\r\n    cells:tableDataIns.cells(),\r\n    //单击单元格或行/列标题后触发。如果单击行/列标题，则坐标索引为负。\r\n    afterOnCellMouseUp:tableDataIns.afterOnCellMouseUp(),\r\n    beforeChange: tableDataIns.beforeChange(),\r\n    afterChange: tableDataIns.afterChange(),\r\n    afterScrollHorizontally: tableDataIns.afterScrollHorizontally(),//横向滚动之后\r\n    //列宽改变\r\n    afterColumnResize:tableDataIns.afterColumnResize(),\r\n    //渲染后\r\n    afterLoadData:tableDataIns.afterLoadData(),\r\n  };\r\n  /**列宽设置**/\r\n  //默认情况下自适应\r\n  if(data.length===0){//如果表格没有数据，设置默认宽度，以免表头挤在一起\r\n    vibTable.colWidths = '170';\r\n  }\r\n  else if(data.length && colWidth!==''){//如果有数据，且拖动表格列宽的情况下，保存为拖动后的列宽\r\n    vibTable.colWidths = colWidth;\r\n  }\r\n  return hdtable(el,vibTable)\r\n}\r\n\r\n\r\n//表格的方法\r\nclass tableData{\r\n  constructor(property){\r\n    this.type= property.type\r\n    this.getData= property.getData\r\n    this.getHot= property.getHot\r\n    this.reRenderSelectOption = property.reRenderSelectOption\r\n    this.initList= property.initList\r\n    this.changeList= property.changeList\r\n    this.$this = property.$this\r\n  }\r\n  contextMenu(){\r\n    return {\r\n      items: {\r\n        'freeze_column':{\r\n          name: '固定列',\r\n        },\r\n        \"unfreeze_column\": {\r\n          name: '取消列固定',\r\n        },\r\n      }\r\n    }\r\n  }\r\n  /* 表格的数据渲染 */\r\n  colHeaders(){\r\n    return setHeaders(this.type)\r\n  }\r\n  nestedHeaders(){\r\n    return setHeaders(this.type)\r\n  }\r\n  columns(){\r\n    return setColumns(this.type,this.getData())\r\n  }\r\n  /* 表格的方法渲染 */\r\n  afterLoadData(){\r\n    let that = this\r\n    return function (isForced){\r\n      if(isForced){\r\n        that.$this.$emit(\"child-load\",false);\r\n      }\r\n    }\r\n  }\r\n  //振动的单元格渲染\r\n  cells(){\r\n    let that = this\r\n    return function (row, col, prop){\r\n      let data =that.getData();\r\n      if(data.length == 0) return\r\n      let type = that.type;\r\n      let cellProperties = this;\r\n      if(col == 0 || col == 1 || col == 2 || col == 3){\r\n        cellProperties.readOnly  = true;\r\n      }\r\n      if(type==1){\r\n          /* 工况报警特征值选择下拉选项 */\r\n        if(prop=='alarmBaseInfo.alarmInfo.name'){\r\n           let val = data[row].alarmBaseInfo.valueSources;\r\n           if (typeof val != 'undefined') {\r\n               cellProperties.source = val;\r\n           }\r\n        }\r\n        if(col>4){\r\n          if(data[row].alarmBaseInfo.alarmInfo.name==\"无\"){\r\n            cellProperties.readOnly  = true;\r\n          }\r\n          else{\r\n            cellProperties.readOnly  = false;\r\n          }\r\n        }\r\n        if(data[row].alarmBaseInfo.dgm_type==7 && data[row].alarmBaseInfo.alarmInfo.name!=\"无\" && (prop=='alarmBaseInfo.alarmInfo.hhh_limit_5' || prop=='alarmBaseInfo.alarmInfo.hhh_limit_1' || prop=='alarmBaseInfo.alarmInfo.hhh_limit_2' || prop=='alarmBaseInfo.alarmInfo.hhh_limit_3' || prop=='alarmBaseInfo.alarmInfo.hhh_limit_4')){\r\n          cellProperties.readOnly  = false;\r\n        }\r\n        else if(data[row].alarmBaseInfo.dgm_type!=7 && (prop=='alarmBaseInfo.alarmInfo.hhh_limit_5' || prop=='alarmBaseInfo.alarmInfo.hhh_limit_1' || prop=='alarmBaseInfo.alarmInfo.hhh_limit_2' || prop=='alarmBaseInfo.alarmInfo.hhh_limit_3' || prop=='alarmBaseInfo.alarmInfo.hhh_limit_4')){\r\n          cellProperties.readOnly  = true;\r\n        }\r\n        /* tmu的其他工况必须选中，不能不选*/\r\n        if(data[row].alarmBaseInfo.dgm_type==7 && prop==\"alarmBaseInfo.alarmInfo.status_5\"){\r\n          cellProperties.readOnly  = true;\r\n        }\r\n      }\r\n      if(type==3 && col==5){\r\n        if(data[row].alarmBaseInfo.code==2000){\r\n          cellProperties.prop=\"alarmBaseInfo.alarm_threshold_index_val\";\r\n          cellProperties.editor= 'select';\r\n          cellProperties.selectOptions=data[row].alarmBaseInfo.alarmIndexSources;\r\n        }\r\n        else{\r\n          cellProperties.prop=\"alarmBaseInfo.vib_k_hh\";\r\n          cellProperties.type= 'text';\r\n        }\r\n      }\r\n      if(type==2){\r\n        if((data[row].alarmBaseInfo.dgm_type==2 || data[row].alarmBaseInfo.dgm_type==1)&& data[row].alarmBaseInfo.pos_type==5){\r\n          cellProperties.readOnly  = true;\r\n        }\r\n        if(data[row].alarmBaseInfo.pos_type==5 && (prop=='alarmBaseInfo.baseInfo.ll_limit' || prop=='alarmBaseInfo.baseInfo.l_limit')){\r\n          cellProperties.readOnly  = true;\r\n        }\r\n        if(data[row].alarmBaseInfo.pos_type==7 && data[row].alarmBaseInfo.dgm_type==7 && (prop=='alarmBaseInfo.baseInfo.lll_limit' || prop=='alarmBaseInfo.baseInfo.hhh_limit')){\r\n          cellProperties.readOnly  = false;\r\n        }\r\n      }\r\n      return cellProperties;\r\n    }\r\n  }\r\n  // 列宽被改变后\r\n    afterColumnResize(){\r\n      let that = this;\r\n      return function(condition){\r\n        let hot = that.getHot();\r\n        colWidth = hot.getColWidth(condition);//修改拖动后的列宽\r\n      }\r\n    }\r\n  //点击单元格之后输出该行信息（共用）\r\n  afterOnCellMouseUp(){\r\n    let that = this\r\n    return function (e, coords, td){\r\n      if(coords.row>=0){\r\n        let row_info =that.getPositionByIndex(coords.row)\r\n\r\n        // console.log(row_info);\r\n      }\r\n    }\r\n  }\r\n// 过滤的方法(有待优化)（共用）\r\n  afterFilter(){\r\n    let that = this\r\n    return function(condition){\r\n      let hot = that.getHot();\r\n      switch(parseInt(that.type)){\r\n        case 1:\r\n          sessionStorage.setItem('filter_alarm1',JSON.stringify(condition));\r\n          break;\r\n        case 2:\r\n          sessionStorage.setItem('filter_alarm2',JSON.stringify(condition));\r\n          break;\r\n        case 3:\r\n          sessionStorage.setItem('filter_alarm3',JSON.stringify(condition));\r\n          break;\r\n      }\r\n      hot.view.wt.wtOverlays.adjustElementsSize(true);\r\n    }\r\n  }\r\n  //改变数据的时候拼装数据结构（共用）\r\n  beforeChange(){\r\n    let that =this\r\n    return function (changes,source){\r\n      let change_index\r\n      let change_prop\r\n      let change_oldVal\r\n      let change_newVal;\r\n      let flag;\r\n      let data = that.getData();\r\n      let hot = that.getHot();\r\n      // console.log('beforeChange');\r\n\r\n      //创建更改组织、机组、采集器、通道、任务的联动实例\r\n\r\n      for(let i= 0;i<changes.length;i++){\r\n        let physicalRow = hot.toPhysicalRow(changes[i][0]);\r\n        let columnVal = hot.propToCol(changes[i][1]);\r\n        change_index = changes[i][0]\r\n        change_prop = changes[i][1]\r\n        change_oldVal = changes[i][2]\r\n        change_newVal = changes[i][3]\r\n        if(change_oldVal == change_newVal ){\r\n          continue\r\n        }\r\n        let alarm=that.getPositionByIndex(change_index);\r\n        //工况报警\r\n        if(change_prop==\"alarmBaseInfo.alarmInfo.name\"){\r\n          flag = setChangeCellResult(alarm.alarmBaseInfo.valueSources,alarm.alarmBaseInfo.valueSources_key,change_newVal,change_index);\r\n          if(flag === false) { return false}\r\n          else{\r\n            for(let i=0;i<data.length;i++){\r\n              if(change_newVal!=\"无\" && alarm.alarmBaseInfo.mac_pos_id == data[i].alarmBaseInfo.mac_pos_id && change_newVal==data[i].alarmBaseInfo.alarmInfo.name){\r\n                swal({\r\n                  title: \"提示\",\r\n                  text: '该特征值已经存在，请另选其他特征值！',\r\n                  button: \"确认\",\r\n                });\r\n                return false;\r\n              }\r\n            }\r\n            alarm.alarmBaseInfo.alarmInfo.code = flag;\r\n            alarm.alarmBaseInfo.alarmInfo.status = \"1\";\r\n          }\r\n        }\r\n        else if(change_prop == \"alarmBaseInfo.alarmInfo.h_limit_1\"){\r\n          if(Number(change_newVal)>=alarm.alarmBaseInfo.alarmInfo.hh_limit_1){\r\n            swal({\r\n              title: \"提示\",\r\n              text: '预警值应该小于报警值！',\r\n              button: \"确认\",\r\n            });\r\n            return false;\r\n          }\r\n        }\r\n        else if(change_prop == \"alarmBaseInfo.alarmInfo.hh_limit_1\"){\r\n          if(Number(change_newVal)<=alarm.alarmBaseInfo.alarmInfo.h_limit_1){\r\n            swal({\r\n              title: \"提示\",\r\n              text: '报警值应该大于预警值！',\r\n              button: \"确认\",\r\n            });\r\n            return false;\r\n          }\r\n        }\r\n        else if(change_prop == \"alarmBaseInfo.alarmInfo.hhh_limit_1\"){\r\n          if(Number(change_newVal)<=alarm.alarmBaseInfo.alarmInfo.hh_limit_1){\r\n            swal({\r\n              title: \"提示\",\r\n              text: 'Ⅱ级报警值应该大于Ⅰ级报警值！',\r\n              button: \"确认\",\r\n            });\r\n            return false;\r\n          }\r\n        }\r\n        else if(change_prop == \"alarmBaseInfo.alarmInfo.h_limit_2\"){\r\n          if(Number(change_newVal)>=alarm.alarmBaseInfo.alarmInfo.hh_limit_2){\r\n            swal({\r\n              title: \"提示\",\r\n              text: '预警值应该小于报警值！',\r\n              button: \"确认\",\r\n            });\r\n            return false;\r\n          }\r\n        }\r\n        else if(change_prop == \"alarmBaseInfo.alarmInfo.hh_limit_2