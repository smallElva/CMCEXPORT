    if (origionData[i].baseInfo.length > 0) {\r\n                    for (let j = 0; j < origionData[i].baseInfo.length; j++) {\r\n                        if (origionData[i].baseInfo[j].status == 1) {\r\n                            origionData[i].statusInfo.push(origionData[i].baseInfo[j]);\r\n                        }\r\n                    }\r\n                }\r\n                origionData[i].codeArr = [];\r\n                let posType = parseInt(origionData[i].pos_type);\r\n                if (origionData[i].dgm_type == 1) {\r\n                    origionData[i].statusInfo.length = 1;\r\n                    origionData[i].codeArr = getCodeArr(getFlagType(posType), 0,origionData[i].tt_pos_loc,origionData[i].dgm_type);\r\n                }\r\n                else if(origionData[i].dgm_type == 7){\r\n                    origionData[i].statusInfo.length = 15;\r\n                    origionData[i].codeArr = getCodeArr(getFlagType(posType), 2,origionData[i].tt_pos_loc,origionData[i].dgm_type);\r\n                }\r\n                else {\r\n                    origionData[i].statusInfo.length = 5;\r\n                    origionData[i].codeArr = getCodeArr(getFlagType(posType), 0,origionData[i].tt_pos_loc,origionData[i].dgm_type);\r\n                }\r\n\r\n                for (let j = 0; j < origionData[i].codeArr.length; j++) {\r\n                    origionData[i].valueSources[\"无\"] = \"\";\r\n                    origionData[i].valueSources[origionData[i].codeArr[j].value] = origionData[i].codeArr[j].code;\r\n                }\r\n\r\n                for (let m = 0; m < origionData[i].statusInfo.length; m++) {\r\n\r\n                    if (origionData[i].statusInfo[m] == undefined) {\r\n                        origionData[i].statusInfo[m] = {\r\n                            code: \"\",\r\n                            h_limit_1: \"3\",\r\n                            h_limit_2: \"3\",\r\n                            h_limit_3: \"3\",\r\n                           