                    for(let i=0;i<data.length;i++){\r\n                                if(parseInt(i/5)==parseInt(physicalRow/5)){\r\n                                    changeConditionList.push(data[i]);\r\n                                }\r\n                            }\r\n                            for(let n=0;n<changeConditionList.length;n++){\r\n                                conditionObj[`capturenum_${n+1}`] = changeConditionList[n].condition['capturenum'];\r\n                                conditionObj[`wavenum_${n+1}`] = changeConditionList[n].condition['wavenum'];\r\n                                if(n==4){\r\n                                    continue;\r\n                                }\r\n                                if(changeConditionList[n].condition['condition_state']==undefined){\r\n                                    changeConditionList[n].condition['condition_state']= 0;\r\n                                }\r\n                                conditionObj[`conditionH_1_${n+1}`] = changeConditionList[n].condition['conditionH_1'];\r\n                                conditionObj[`conditionH_2_${n+1}`] = changeConditionList[n].condition['conditionH_2'];\r\n                                conditionObj[`conditionL_1_${n+1}`] = changeConditionList[n].condition['conditionL_1'];\r\n                                conditionObj[`conditionL_2_${n+1}`] = changeConditionList[n].condition['conditionL_2'];\r\n                                conditionObj[`condition_1_${n+1}`] = changeConditionList[n].condition['condition_state'];\r\n                                conditionObj[`condition_2_${n+1}`] = changeConditionList[n].condition['condition_state'];\r\n                            }\r\n                            conditionObj['dgm_type'] = condition.condition.dgm_type;\r\n                            conditionObj['pos_id1'] = condition.condition.pos_id1;\r\n                            conditionObj['pos_type1'] = condition.condition.pos_type1;\r\n       