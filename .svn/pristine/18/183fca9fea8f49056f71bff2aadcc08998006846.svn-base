alCircleX,finalCircleY;\r\n\r\n            for(let i=0;i<this.MoveMsg.length;i++){\r\n                let item = this.MoveMsg[i];\r\n                finalX = item.x + item.moveX;\r\n                finalY = item.y + item.moveY;\r\n                finalCircleX = item.oriCircleX + item.moveCircleX;\r\n                finalCircleY = item.oriCircleY + item.moveCircleY;\r\n                item.px = (finalX/this.origBlock.oriW).toFixed(2);\r\n                item.py = (finalY/this.origBlock.oriH).toFixed(2);\r\n                item.circleX = (finalCircleX/this.origBlock.oriW).toFixed(2);\r\n                item.circleY = (finalCircleY/this.origBlock.oriH).toFixed(2);\r\n                obj={\r\n                    id:i,//计数id\r\n                    type:type,//类型 1=组织 2=机组 3=机泵 4=测点\r\n                    itemType:this.MoveMsg[i].itemType,//设备类型 如1=破碎机\r\n                    uid:this.MoveMsg[i].uid,//节点uid，如\"3_05\"，表示id=05的振动测点\r\n                    px:(finalX/this.origBlock.oriW).toFixed(2),//相对于背景图的x位置\r\n                    py:(finalY/this.origBlock.oriH).toFixed(2),//相对于背景图的y位置\r\n                    width:this.MoveMsg[i].width,\r\n                    height:this.MoveMsg[i].height,\r\n                    itemName:this.MoveMsg[i].text,//编辑的部件名\r\n                    circleD:this.MoveMsg[i].circleD,//圆点的直径\r\n                    circleX: (finalCircleX/this.origBlock.oriW).toFixed(2),//小圆百分比的x位置\r\n                    circleY: (finalCircleY/this.origBlock.oriH).toFixed(2),//小圆百分比的y\r\n                };\r\n                overViewObj.itemList.push(obj);\r\n            }\r\n            this.$axios.post('updateTreeOverview',overViewObj)\r\n                .then(res=>{\r\n                    if(res.msg==0){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '保存成功',\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                    else if(res.msg==1