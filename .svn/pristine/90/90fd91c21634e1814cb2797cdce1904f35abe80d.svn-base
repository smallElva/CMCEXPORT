CA,GADA,GAEA,yBAFA;AAGA,KApGA;AAqGA;AACA,aAtGA,uBAsGA;AACA;;AACA;AACA;AACA,OAFA,MAGA;AACA;AACA;AACA,KA9GA;AA+GA;AACA,gBAhHA,0BAgHA;AACA;AACA,mCAFA,CAGA;;AACA,4GAJA,CAKA;;AACA;AACA;AACA;AACA;;AACA;AACA,OAXA,CAYA;;;AACA,+EAbA,CAcA;;AACA;AACA,sBAhBA,CAiBA;;AACA;AACA;AACA;AACA;AACA;AAtIA;AAlEA,G;;;;;;;;;;;;ACvDA,uC;;;;;;;;;;;;ACAA;AAAA;AAAA;AAAA;AACA;AACA;AACA;AACA,oBAAoB,SAAS,uBAAuB,EAAE;AACtD,kBAAkB,0BAA0B;AAC5C,iBAAiB,4BAA4B;AAC7C,mBAAmB,8BAA8B;AACjD,sBAAsB,2BAA2B;AACjD;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,sBAAsB,mBAAmB;AACzC;AACA;AACA;AACA;AACA;AACA;AACA,uBAAuB;AACvB;AACA;AACA;AACA,uBAAuB;AACvB;AACA;AACA;AACA,mBAAmB;AACnB;AACA;AACA;AACA,aAAa;AACb;AACA;AACA;AACA;AACA,0BAA0B,wBAAwB;AAClD,6BAA6B;AAC7B,iBAAiB;AACjB;AACA;AACA,aAAa;AACb;AACA;AACA;AACA;AACA,mBAAmB,6BAA6B;AAChD;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,WAAW;AACX;AACA;AACA,mBAAmB,6BAA6B;AAChD;AACA;AACA;AACA;AACA,oBAAoB;AACpB,WAAW;AACX;AACA;AACA;AACA,iBAAiB,4BAA4B;AAC7C;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,kBAAkB,mCAAmC;AACrD,qBAAqB,uBAAuB;AAC5C;AACA;AACA;AACA;AACA;AACA;AACA;AACA,eAAe;AACf;AACA;AACA;AACA,SAAS;AACT;AACA;AACA;AACA,eAAe;AACf,SAAS;AACT;AACA;AACA;AACA;AACA,SAAS,gCAAgC,0BAA0B,EAAE;AACrE;AACA;AACA;AACA;AACA,eAAe,yBAAyB;AACxC,iBAAiB,6BAA6B;AAC9C,qBAAqB,6BAA6B;AAClD;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,aAAa;AACb;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,WAAW;AACX;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;;;;;;;;;;;;AClLA;AAAA;AAAA;AAAA;AAAA;AAAyF;AAC3B;AACL;AACc;;;AAGvE;AACgG;AAChG,gBAAgB,2GAAU;AAC1B,EAAE,gFAAM;AACR,EAAE,qFAAM;AACR,EAAE,8FAAe;AACjB;AACA;AACA;AACA;;AAEA;;AAEA;AACA,IAAI,IAAU;AACd,YAAY,mBAAO,CAAC,wGAAsE;AAC1F,cAAc,mBAAO,CAAC,+CAAK;AAC3B;AACA;AACA;AACA;AACA,KAAK;AACL;AACA;AACA,sBAAsB,gJAAiD,EAAE;AAAA;AACzE;AACA,gBAAgB,qFAAM;AACtB,yBAAyB,8FAAe;AACxC,OAAO;AACP,KAAK;AACL;AACA;AACA;AACe,gF;;;;;;;;;;;;ACvCf;AAAA;AAAA,wCAAgM,CAAgB,sPAAG,EAAC,C;;;;;;;;;;;;ACApN;AAAA;AAAA;AAAA;AAA0X,CAAgB,2YAAG,EAAC,C;;;;;;;;;;;;ACA9Y;AAAA;AAAA;AAAA;AAAA;AAAA","file":"js/48-da5e3a28904592c66d8b.js","sourcesContent":["<template>\r\n    <div id=\"record-content\">\r\n        <header class=\"myHeader\">\r\n            <div class=\"form-group\">\r\n                <div class=\"select-group\">\r\n                    <span class=\"lableName\">账户：</span>\r\n                    <select id=\"select-mac\" class=\"select-header\" v-model=\"chooseUserId\" @change=\"selectUser\">\r\n                        <option v-for=\"item in userList\" :value=\"item.user_id\" :title=\"item.user_name\">{{item.user_name}}</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"header-time\">\r\n                    <span>开始时间：</span>\r\n                    <input type=\"text\" id=\"start_time\" class=\"am-form-field start_date flatpickr\" :placeholder=\"startTime\" :data-default-date=\"startTime\" />\r\n                </div>\r\n                <div class=\"header-time\">\r\n                    <span>结束时间：</span>\r\n                    <input type=\"text\" id=\"end_time\" class=\"am-form-field end_date flatpickr\" :placeholder=\"endTime\" />\r\n                </div>\r\n            </div>\r\n            <div class=\"search-div\">\r\n                <input type=\"text\" class=\"record-input\" placeholder=\"关键字\" v-model=\"recordVal\" @input=\"searchRecord\">\r\n                <i class=\"iconfont icon-zhongzi-sousuo search-icon\" @click=\"searchRecord\"></i>\r\n            </div>\r\n            <button class=\"headerBtn\" @click=\"tableToExcel\">导出数据</button>\r\n        </header>\r\n        <div class=\"content\">\r\n            <div class=\"table_block\">\r\n                <table class=\"index_table\">\r\n                    <thead>\r\n                    <tr>\r\n                        <th>序号</th>\r\n                        <th>操作时间</th>\r\n                        <th>操作账号</th>\r\n                        <th>操作描述</th>\r\n                    </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                    <tr v-for=\"item in lastData\">\r\n                        <td>{{item.index}}</td>\r\n                        <td>{{item.occur_time}}</td>\r\n                        <td>{{item.user_name}}</td>\r\n                        <td>{{item.detail}}</td>\r\n                    </tr>\r\n                    </tbody>\r\n                </table>\r\n                <div class=\"noData\" v-show=\"noLogData\">当前用户无操作日志</div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import flatpickr from 'flatpickr';\r\n    import 'flatpickr/dist/flatpickr.min.css'\r\n\r\n    export default {\r\n        name: \"OperateLog\",\r\n        components: {},\r\n        data() {\r\n            return {\r\n                chooseUserId:'',//所选的用户id\r\n                userList:[],//用户列表\r\n                lastData:[],//最后显示的数据列表\r\n                activeNum:5,//当前界面的index\r\n                recordVal:'',//搜索的内容\r\n                origRecordList:[],//获取到的所有的日志列表信息\r\n                startTime: new Date(new Date - 7*24*3600*1000),\r\n                endTime: new Date,\r\n                noLogData:false,//是否有日志数据\r\n            }\r\n        },\r\n        created: function(){\r\n            this.$emit('ievent', this.activeNum);\r\n            // window.document.title = '查看数据';\r\n            this.startTime = this.buildTime(this.startTime);\r\n            this.endTime = this.buildTime(this.endTime);\r\n            const zh = require(\"flatpickr/dist/l10n/zh.js\").default.zh;//时间选择器汉化\r\n            flatpickr.localize(zh);\r\n        },\r\n        destroyed:function () {\r\n            let obj = {};\r\n            let linkInfo = JSON.parse(sessionStorage.getItem('paramsConfig'));\r\n            if(!$.isEmptyObject(linkInfo)){\r\n                obj = linkInfo;\r\n            }\r\n            sessionStorage.setItem('paramsConfig',JSON.stringify(obj));\r\n        },\r\n        mounted(){\r\n            this.getHeadData();\r\n            let that = this;\r\n            flatpickr(\"#start_time\",{\r\n                onChange: function(selectedDates, dateStr, instance) {\r\n                    if (new Date(dateStr).getTime() > new Date(that.endTime).getTime()) {\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '开始日期应小于结束日期',\r\n                            button: \"确认\",\r\n                        });\r\n                        instance.setDate(that.startTime);\r\n                    } else {\r\n                        that.startTime = dateStr;\r\n                        that.getPosData();\r\n                    }\r\n                },\r\n            });\r\n            flatpickr(\"#end_time\",{\r\n                onChange: function(selectedDates, dateStr, instance) {\r\n                    if (new Date(dateStr).getTime() < new Date(that.startTime).getTime()) {\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '开始日期应小于结束日期',\r\n                            button: \"确认\",\r\n                        });\r\n                        instance.setDate(that.endTime);\r\n                    } else {\r\n                        that.endTime = dateStr;\r\n                        that.getPosData();\r\n                    }\r\n                },\r\n            });\r\n        },\r\n        methods: {\r\n            //获取组织、机组信息\r\n            getHeadData() {\r\n                let that = this;\r\n                that.userList.length = 0;\r\n                this.$axios.post('getAllUser')\r\n                    .then(res => {\r\n                        if(res && res.length>0){\r\n                            res.forEach(user =>{\r\n                                that.userList.push({\r\n                                    \"user_id\":user.id,\r\n                                    \"user_name\":user.username\r\n                                })\r\n                            });\r\n                            that.chooseUserId = that.userList[0].user_id;\r\n                            that.getPosData();\r\n                        }\r\n\r\n                    }).catch(err => {\r\n                    console.log(err)\r\n                })\r\n            },\r\n            //改变用户选择\r\n            selectUser(){\r\n                this.getPosData();\r\n            },\r\n            //获取日志信息\r\n            getPosData(){\r\n                let that = this;\r\n                if(that.chooseUserId!=''){\r\n                    let params=null;\r\n                    params = {\r\n                        user_id:that.chooseUserId,\r\n                        start_time:(new Date(this.startTime.replace(/-/ig,\"/\") + \" 00:00:00\")).getTime(),\r\n                        end_time:(new Date(this.endTime.replace(/-/ig,\"/\") + \" 23:59:59\")).getTime()\r\n                    };\r\n                    this.$axios.post('getClientLog',params)\r\n                        .then(res => {\r\n                            that.lastData = [];\r\n                            if(res.msg===0 && res.list.length>0){\r\n                                that.noLogData = false;\r\n                                for(let i=0;i<res.list.length; i++){\r\n                                    let obj = {};\r\n                                    obj.index = Number(i+1);\r\n                                    obj.occur_time = res.list[i].occur_time;\r\n                                    obj.user_name = res.list[i].user_name;\r\n                                    obj.detail = res.list[i].log_desc;\r\n                                    that.lastData.push(obj);\r\n                                }\r\n                                that.origRecordList = that.lastData.concat();\r\n                            }\r\n                            else{\r\n                                that.noLogData = true;\r\n                            }\r\n                        }).catch(err => {\r\n                        console.log(err)\r\n                    })\r\n                }\r\n            },\r\n            //搜索日志\r\n            searchRecord(){\r\n                var reg = RegExp(this.recordVal);\r\n                var that = this;\r\n                if (that.recordVal.length>0) {\r\n                    that.lastData = [];\r\n                    // 过滤需要的数据\r\n                    for(let i=0;i<that.origRecordList.length;i++){\r\n                        if (reg.test(that.origRecordList[i].detail)) {\r\n                            that.lastData.push(that.origRecordList[i]);\r\n                        }\r\n                    }\r\n                }\r\n                else{\r\n                    that.lastData=that.origRecordList.concat();\r\n                }\r\n\r\n            },\r\n            //转换日期方法\r\n            translateDate(value){\r\n                value = new Date(value);\r\n                value = value.getFullYear() + '-'\r\n                    + (value.getMonth() + 1) + '-'\r\n                    + this.fixTime(value.getDate()) + ' '\r\n                    + this.fixTime(value.getHours()) + ':'\r\n                    + this.fixTime(value.getMinutes()) + ':'\r\n                    + this.fixTime(value.getSeconds());\r\n                return value;\r\n            },\r\n            //补0\r\n            fixTime (str) {\r\n                if (Number(str) < 10) {\r\n                    str = \"0\" + str;\r\n                }\r\n                return str;\r\n            },\r\n            buildTime: function(date) {\r\n                var t = new Date(date);\r\n                return t.getFullYear() + '-' +\r\n                    (this.fixTime(t.getMonth() + 1)) + '-' +\r\n                    this.fixTime(t.getDate());\r\n            },\r\n            //切换按钮\r\n            toggleBtn(){\r\n                this.isReal = !this.isReal;\r\n                if(this.isReal){\r\n                    this.toggleText = '实时';\r\n                }\r\n                else{\r\n                    this.toggleText = '历史';\r\n                }\r\n            },\r\n            //导出数据\r\n            tableToExcel(){\r\n                //要导出的json数据\r\n                var jsonData = this.lastData;\r\n                //列标题，逗号隔开，每一个逗号就是隔开一个单元格\r\n                let str = `序号,操作时间,操作账号,操作描述\\n`;\r\n                //增加\\t为了不让表格显示科学计数法或者其他格式\r\n                for(let i = 0 ; i < jsonData.length ; i++ ){\r\n                    for(let item in jsonData[i]){\r\n                        str+=`${jsonData[i][item] + '\\t'},`;\r\n                    }\r\n                    str+='\\n';\r\n                }\r\n                //encodeURIComponent解决中文乱码\r\n                let uri = 'data:text/csv;charset=utf-8,\\ufeff' + encodeURIComponent(str);\r\n                //通过创建a标签实现\r\n                var link = document.createElement(\"a\");\r\n                link.href = uri;\r\n                //对下载的文件命名\r\n                link.download =  \"操作日志表.csv\";\r\n                document.body.appendChild(link);\r\n                link.click();\r\n                document.body.removeChild(link);\r\n            },\r\n        },\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n    #record-content{\r\n        .myHeader{\r\n            height: 80px;\r\n            line-height: 80px;\r\n            padding: 0 20px;\r\n            background: #fff;\r\n            .form-group{\r\n                float: left;\r\n                .select-group{\r\n                    display: inline-block;\r\n                    margin-right: 90px;\r\n                    .lableName{\r\n                        font-size: 20px;\r\n                        vertical-align: middle;\r\n                    }\r\n                    .select-header{\r\n                        font-size: 14px;\r\n                        min-width:100px;\r\n                        height:30px;\r\n                        border:1px solid #D9D9D9;\r\n                        border-radius:8px;\r\n                        cursor: pointer;\r\n                        vertical-align: middle;\r\n                        &:focus{\r\n                            outline: none;\r\n                            border:1px solid #C9DBEF;\r\n                        }\r\n                    }\r\n\r\n                }\r\n            }\r\n            .header-time{\r\n                display: inline-block;\r\n                span{\r\n                    font-size: 20px;\r\n                    vertical-align: middle;\r\n                }\r\n                .am-form-field{\r\n                    font-size: 14px;\r\n                    padding: 0 12px;\r\n                    max-width:100px;\r\n                    height:30px;\r\n                    border:1px solid #D9D9D9;\r\n                    border-radius:8px;\r\n                    cursor: pointer;\r\n                    vertical-align: middle;\r\n                    &:focus{\r\n                        outline: none;\r\n                        border:1px solid #C9DBEF;\r\n                    }\r\n                }\r\n                .start_date{\r\n                    margin-right: 90px;\r\n                }\r\n            }\r\n            .search-div{\r\n                position: absolute;\r\n                right: 150px;\r\n                top: 0;\r\n                .record-input{\r\n                    font-size: 14px;\r\n                    min-width:100px;\r\n                    height:30px;\r\n                    padding: 0 12px;\r\n                    border:1px solid #D9D9D9;\r\n                    border-radius:8px;\r\n                    cursor: pointer;\r\n                    vertical-align: middle;\r\n                    &:focus{\r\n                        outline: none;\r\n                        border:1px solid #C9DBEF;\r\n                    }\r\n                }\r\n                .search-icon{\r\n                    position: absolute;\r\n                    right: 12px;\r\n                    top: 0;\r\n                    cursor: pointer;\r\n                    color: #7D7D7D;\r\n                    &:hover{\r\n                        color: darken(#7D7D7D,20%);\r\n                    }\r\n                    &:active{\r\n                        color: darken(#7D7D7D,5%);\r\n                    }\r\n                }\r\n            }\r\n            .headerBtn{\r\n                position: absolute;\r\n                right: 20px;\r\n                top: 25px;\r\n                width:100px;\r\n                height:30px;\r\n                background:rgba(89,125,210,1);\r\n                border-radius:4px;\r\n                outline: none;\r\n                border: none;\r\n                color: #fff;\r\n            }\r\n\r\n        }\r\n        .content{\r\n            background: #e4e8f4;\r\n            padding: 20px;\r\n            flex: 1;\r\n            .table_block{\r\n                width: calc(100vw - 200px);\r\n                overflow: auto;\r\n                position: relative;\r\n                .noData{\r\n                    position: absolute;\r\n                    top: 50%;\r\n                    left: 45%;\r\n                    text-align: center;\r\n                    font-size: 18px;\r\n                }\r\n                .index_table{\r\n                    border-spacing: 0;\r\n                    width: calc(100vw - 200px);\r\n                }\r\n                .index_table th,.index_table td{\r\n                    height: 38px;\r\n                    /*line-height: 38px;*/\r\n                    text-align: center;\r\n                    padding: 0!important;\r\n                    width: 1%;\r\n                    vertical-align: middle !important;\r\n                }\r\n                .index_table td{\r\n                    font-size: 14px;\r\n                }\r\n                .index_table>thead>tr,.in