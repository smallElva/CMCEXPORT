AA;AAAA;AAAA;AAAA;AAAA","file":"js/50-ef441039.js","sourcesContent":["<template>\r\n    <div class=\"overview-content\" ref='overviewContent' @click=\"closeSearch\">\r\n        <my-content-title :title='title'></my-content-title>\r\n        <div class=\"title-control\">\r\n            <div class=\"setFont\">\r\n                <i class=\"iconfont icon-font iconFontSize\" title=\"设置标题大小\" @click=\"isShowFont = true\"></i>\r\n                <div class=\"choose-panel\">\r\n                    <div class=\"choosed\" @click=\"isShowFont = true\">\r\n                        <span class=\"choose-circle\"></span>\r\n                        <span class=\"choose-text\">{{overviewFont}}像素</span>\r\n                    </div>\r\n                    <div v-for=\"item in fontList\" class=\"fontDiv\" @click = \"pickFont(item)\" v-show=\"isShowFont\">{{item}}像素</div>\r\n                </div>\r\n            </div>\r\n            <div class=\"control-block\">\r\n                <div class=\"icon-block\">\r\n                    <i class=\"iconfont \" :class=\"item.icon\" :title=\"item.title\" v-for=\"(item,index) in iconList\" @click=\"calcPos(index)\"></i>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"overview-block\">\r\n            <div class=\"overview-search\">\r\n                <!--一级组织-->\r\n                <div class=\"tree-search\">\r\n                    <div class=\"search\" @click.stop=\"addClass($event)\">\r\n                        <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput1\" @input=\"searchFolder($event,1)\">\r\n                        <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(1)\"></i>\r\n                    </div>\r\n                    <div class=\"search-list-div\" v-show=\"searchFolderList1.length>0 && isShow1\" @click.stop>\r\n                        <ul class=\"search-result\">\r\n                            <li class=\"search-li\" v-for=\"(item,index) in sortFolderList1\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,1,1)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                        </ul>\r\n                    </div>\r\n                </div>\r\n                <!--二级组织-->\r\n                <div class=\"tree-search\">\r\n                    <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList2.length>0\">\r\n                        <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput2\" @input=\"searchFolder($event,2)\">\r\n                        <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(2)\"></i>\r\n                    </div>\r\n                    <div class=\"search-list-div\" v-show=\"searchFolderList2.length>0 && isShow2\" @click.stop>\r\n                        <ul class=\"search-result\">\r\n                            <li class=\"search-li\" v-for=\"(item,index) in sortFolderList2\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,2,1)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                        </ul>\r\n                    </div>\r\n                </div>\r\n                <!--三级组织-->\r\n                <div class=\"tree-search\">\r\n                    <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList3.length>0\">\r\n                        <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput3\" @input=\"searchFolder($event,3)\">\r\n                        <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(3)\"></i>\r\n                    </div>\r\n                    <div class=\"search-list-div\" v-show=\"searchFolderList3.length>0 && isShow3\" @click.stop>\r\n                        <ul class=\"search-result\">\r\n                            <li class=\"search-li\" v-for=\"(item,index) in sortFolderList3\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,3,1)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                        </ul>\r\n                    </div>\r\n                </div>\r\n                <!--四级组织-->\r\n                <div class=\"tree-search\">\r\n                    <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList4.length>0\">\r\n                        <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput4\" @input=\"searchFolder($event,4)\">\r\n                        <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(4)\"></i>\r\n                    </div>\r\n                    <div class=\"search-list-div\" v-show=\"searchFolderList4.length>0 && isShow4\" @click.stop>\r\n                        <ul class=\"search-result\">\r\n                            <li class=\"search-li\" v-for=\"(item,index) in sortFolderList4\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,4,1)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                        </ul>\r\n                    </div>\r\n                </div>\r\n                <div class=\"search-pump-div\" v-show=\"choosePumpList.length>0\">\r\n                    <ul class=\"pump-result\">\r\n                        <li class=\"pump-li\" v-for=\"(pump,index) in sortPumpList\" @click=\"clickPump(pump,index)\" draggable=\"true\" @dragstart=\"dragstart(pump, index, $event,1)\">\r\n                            <i class=\"iconfont icon-zhongzi-shebeimoxing\"></i>\r\n                            {{pump.pump_name}}\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n                <div id=\"imgForm\" class=\"upload\" v-show=\"chooseFolder.t_id!=''\">\r\n                    <button class=\"btn\" @click=\"monidianji\">上传图片</button>\r\n                    <input class=\"input-loc-img\"  name=\"imgL