D,IAAI;AAAA,aAAIyD,IAAI,GAAGD,QAAQ,CAACxD,IAAD,EAAOyD,IAAP,CAAX,GAA0BzD,IAAlC;AAAA,KAAZ,CAAP;AACD,GAFD,MAEO,IAAI,QAAOuD,GAAP,MAAe,QAAnB,EAA6B;AAClC,QAAMM,KAAK,GAAG,CAAC,OAAD,EAAU,MAAV,EAAkB,QAAlB,CAAd;AACA,QAAIC,IAAI,GAAGC,MAAM,CAACC,SAAP,CAAiBC,QAAjB,CAA0BC,IAA1B,CAA+BX,GAA/B,EAAoCY,KAApC,CAA0C,CAA1C,EAA6C,CAAC,CAA9C,CAAX;AACA,QAAIN,KAAK,CAACO,QAAN,CAAeN,IAAf,CAAJ,EAA0B,OAAO,IAAIO,MAAM,CAACP,IAAD,CAAV,CAAiBP,GAAjB,CAAP;AAC1B,QAAIe,GAAG,GAAG,EAAV;;AACA,SAAK,IAAInE,GAAT,IAAgBoD,GAAhB,EAAqB;AACnBe,SAAG,CAACnE,GAAD,CAAH,GAAWsD,IAAI,GAAGD,QAAQ,CAACD,GAAG,CAACpD,GAAD,CAAJ,EAAWsD,IAAX,CAAX,GAA8BF,GAAG,CAACpD,GAAD,CAAhD;AACD;;AACD,WAAOmE,GAAP;AACD;AACF,C,CACD;;AACO,SAASC,QAAT,CAAkBxD,CAAlB,EAAqBC,CAArB,EAAwB;AAC7B,MAAIwD,GAAG,GAAGzD,CAAC,CAAC0D,MAAF,CAASzD,CAAT,CAAV;AACA,MAAI0D,MAAM,GAAG,EAAb;AACA,MAAIC,GAAG,GAAG,EAAV;;AAH6B,6CAKfH,GALe;AAAA;;AAAA;AAK7B,wDAAmB;AAAA,UAAVI,CAAU;;AACjB,UAAI,CAACD,GAAG,CAACC,CAAD,CAAR,EAAa;AACXF,cAAM,CAAC5E,IAAP,CAAY8E,CAAZ;AACAD,WAAG,CAACC,CAAD,CAAH,GAAS,CAAT;AACD;AACF;AAV4B;AAAA;AAAA;AAAA;AAAA;;AAW7B,SAAOF,MAAP;AACD,C","file":"js/28-8aed2fe063bd16fc6d9b.js","sourcesContent":["<template>\r\n  <div class=\"my-content-header\" @dblclick=\"dblclick\">\r\n    <span class=\"page-title\">{{title}}</span>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"MyContentTitle\",\r\n  props: [\"title\"],\r\n  data() {\r\n    return {\r\n      flag: true\r\n    };\r\n  },\r\n  methods: {\r\n    dblclick() {\r\n      //放大\r\n      let homeHeader = document.getElementById(\"homeHeader\");\r\n      let aside = document.getElementById(\"aside\");\r\n      let content = document.getElementById(\"content\");\r\n      let wHeight = document.documentElement.clientHeight;\r\n      if (this.flag) {\r\n        homeHeader.style.display = \"none\";\r\n        aside.style.display = \"none\";\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 20px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 40px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.flag = !this.flag;\r\n      } else {\r\n        //缩小\r\n        homeHeader.style.display = \"block\";\r\n        aside.style.display = \"block\";\r\n        content.style.width = \"100px\"; //改变宽让宽自动渲染\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 90px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 120px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.flag = !this.flag;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-content-header {\r\n  /* width: 100%-15px;\r\n   */\r\n  box-sizing: border-box;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  background: #343c45;\r\n  font-size: 20px;\r\n  color: #fff;\r\n  padding-left: 15px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n","<template>\r\n  <div class=\"dgms-content\">\r\n    <my-content-title :title='title'></my-content-title>\r\n    <div id='myContentCenter'>\r\n      <div id=\"myTable\" class=\"myTable\">\r\n          <input class=\"input-loc-file\" name=\"fileLocal\" id=\"fileLocal\" type='file' multiple=\"multiple\" @change=\"selectFile($event)\"/>\r\n          <router-link  :to=\"{path:fitBtn.name}\" @click=\"getTable(fitBtn.name,fitBtn.isDgm)\" class=\"my-btn-a btn-active btn6\" v-show=\"fitBtnShow\">{{fitBtn.textBtn}}</router-link>\r\n          <router-link  :to=\"{path:btnData[14].name}\"  @click.native=\"getTable(btnData[14].name,btnData[14].isDgm)\" class=\"my-btn-a btn-active btn7\" v-show=\"fitShow\">{{btnData[14].textBtn}}</router-link>\r\n          <select class=\"my-btn btn-active btn8\" v-show=\"handIsShow && !isShow\" v-model=\"selectTmsId\">\r\n              <option v-for=\"(val,key) in tmsList\" :value=\"key\">{{val}}</option>\r\n          </select>\r\n          <button class=\"my-btn btn-active btn7\" v-show=\"nsShow\" @click=\"getNSnum()\">获取识别码</button>\r\n          <button class=\"my-btn btn-active btn6\" id='getFile' v-show=\"fitShow\" @click=\"chooseFile()\">选择文件</button>\r\n          <button class=\"my-btn btn-active btn7\" @click=\"getTmsAll\" v-show=\"handIsShow && !isShow\">获取前置</button>\r\n          <button class=\"my-btn btn-active btn6\" @click=\"add\" v-show=\"isAdd && !isShow\">新建设备</button>\r\n          <button class=\"my-btn btn-active btn3\" @click=\"save\">保存设置</button>\r\n          <div id=\"tableContent\">\r\n             <keep-alive>\r\n                <router-view ref='child' @child-msg='updateIsShow' @click.native=\"chooseBtn($event)\"></router-view>\r\n             </keep-alive>\r\n              <div class=\"noData\" id=\"noData\" v-if=\"isShow\">系统中没有该通道的设备!</div>\r\n          </div>\r\n          <div class=\"controls-btn\">\r\n              <div class=\"push-controls\">\r\n<!--                  <a class=\"top-btn\" @click=\"chooseBtn\"> ↑ </a>-->\r\n                  <a :class=\"leftClass\" @click=\"toLeft()\"> < </a>\r\n                  <a :class=\"rightClass\" @click=\"toRight()\"> > </a>\r\n              </div>\r\n              <div class=\"btn-content\" @mousedown=\"mouseMove($event)\">\r\n                  <router-link v-for=\"(btn,index) in btnData\" :key=\"btn.type\" :to=\"{path:btn.name}\"  @click.native=\"getTable(btn.name,btn.isDgm)\" class=\"con-dgm-btn\" v-show=\"btn.none\">{{btn.textBtn}}</router-link>\r\n              </div>\r\n          </div>\r\n          <!--&lt;!&ndash; 采集器Tab显示 &ndash;&gt;\r\n          <div class = \"choose-btn\" v-show=\"chooseShow\">\r\n              <div class =\"choose-content\">\r\n                  <div class = \"choose-item\"   v-for=\"(btn,index) in btnData\" >\r\n                      <label class = \"choose-name\" :for=\"btn.name\">{{btn.textBtn}}</label>\r\n                      <div class = \"check-box\">\r\n                          <input type=\"checkbox\" checked :id=\"btn.name\" v-model=\"btn.none\">\r\n                          <label class = \"choose-box\" :for=\"btn.name\"></label>\r\n                      </div>\r\n                  </div>\r\n              </div>\r\n          </div>-->\r\n      </div>\r\n  </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import MyContentTitle from 'components/common/MyContentTitle';\r\n  import getApi from 'network/getApi';\r\n  import {addRows,distinct} from 'utils/utils'\r\n  export default {\r\n    name: \"Dgm\",\r\n    inject: ['reload'],\r\n    data(){\r\n      return{\r\n        title:'采集器设置',\r\n        ins:0,//切换按钮active\r\n        name:'vib',\r\n        isAdd:false,\r\n        isShow:false,\r\n        chooseShow:false,//采集器tab\r\n        handIsShow:false,//手动获取按钮是否显示\r\n        fitBtn:{//拟合系数按钮\r\n            type:999,textBtn:\"拟合系数\",name:'fitinfo',isDgm: true, none:true\r\n        },\r\n        fitBtnShow: false,//拟合系数按钮是否显示\r\n        fittingInfo:[],//拟合系数选中的采集器信息\r\n        fitShow:false,//拟合系数界面的选择文件和采集器信息按钮是否显示\r\n        nsShow:false,//e821获取识别码按钮是否显现\r\n        btnData: [//按钮初始化\r\n              {type:1,textBtn:\"采集器信息\",name:'dgminfo',isDgm: true, none:true},\r\n              {type:10,textBtn:\"前置处理器\",name:'prech',isDgm: true, none:true},\r\n              {type:2,textBtn:\"振动设置\",name:'vibch',isDgm: true, none:true},\r\n              {type:3,textBtn:\"温度设置\",name:'tempch',isDgm: true, none:true},\r\n              {type:4,textBtn:\"过程量设置\",name:'stach',isDgm: true, none:true},\r\n              {type:5,textBtn:\"转速设置\",name:'speedch',isDgm: true, none:true},\r\n              {type:6,textBtn:\"数字量设置\",name:'digch',isDgm: true, none:true},\r\n              {type:7,textBtn:\"串口设置\",name:'modch',isDgm: true, none:true},\r\n              // {type:8,textBtn:\"工艺量设置\"},\r\n              {type:9,textBtn:\"长采样设置\",name:'capturech',isDgm: true, none:true},\r\n              {type:11,textBtn:\"MHD从设备\",name:'prinsub',isDgm: true, none:true},\r\n              {type:12,textBtn:\"连续采集\",name:'collectch',isDgm: true, none:true},\r\n              {type:13,textBtn:\"采集设置\",name:'gatherch',isDgm: true, none:true},\r\n              {type:14,textBtn:\"法兰设置\",name:'flangech',isDgm: true, none:true},\r\n              {type:15,textBtn:\"预处理器设置\",name:'prepsch',isDgm: true, none:true},\r\n              {type:16,textBtn:\"螺栓设置\",name:'boltch',isDgm: true, none:true},\r\n              {type:17,textBtn:\"E821从设备\",name:'prinsub821',isDgm: true, none:true},\r\n              {type:18,textBtn:\"外部集成设置\",name:'ExteInter',isDgm: true, none:true},\r\n          ],\r\n          leftClass: {\r\n              \"left-btn\": true,\r\n              \"active\": false,\r\n              // \"disable\" : true,\r\n          },\r\n          rightClass: {\r\n              \"right-btn\": true,\r\n              \"active\": false,\r\n          },\r\n          fileContent:[],//拟合系数上传的文件\r\n          tmsList:{},//tms的采集器列表\r\n          selectTmsId:'',//选中的前置处理器的id\r\n          select821Info:{\r\n            master_id: '',\r\n            slaver_id: '',\r\n            num:'',\r\n          },//e821从设备界面选中的采集器信息\r\n      }\r\n    },\r\n    mounted(){\r\n        // 在点击标题的时候放大并重新渲染表格\r\n      this.$bus.$on('reloadtable',()=>{\r\n        if(this.$refs.child){\r\n          this.$refs.child.hot.render()\r\n        }\r\n      });\r\n        this.name = this.$route.name //刷新的时候让button 的active 正确\r\n        this.statePage(this.name);\r\n        let chTypeBtn = sessionStorage.getItem('chTypeBtn');\r\n        if(chTypeBtn!==null){\r\n            this.btnData.forEach(btnInfo =>{\r\n                if(chTypeBtn.indexOf(btnInfo.type)===-1){\r\n                    btnInfo.none=false;\r\n                }\r\n                else{\r\n                    btnInfo.none=true;\r\n                }\r\n            })\r\n        }\r\n        else{\r\n            this.setBtnContentPosition();\r\n        }\r\n    },\r\n    watch: {\r\n        \"$route\":\"fetchData\"\r\n    },\r\n    methods:{\r\n        getTmsAll(){//手动获取前置处理器信息\r\n            this.$getApi.updateTMSPre(this.selectTmsId)\r\n                .then(res=>{\r\n                    if(res.msg==4){\r\n                        console.log('请求超时')\r\n                    }\r\n                })\r\n                .catch(function(err) {\r\n                    console.log('failed', err);\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: `请求超时`,\r\n                        button: \"确认\",\r\n                    });\r\n                });\r\n        },\r\n        getNSnum(){//获取识别码\r\n            let params = null;\r\n            if(this.select821Info.master_id==='' || this.select821Info.slaver_id===''){\r\n                swal({\r\n                    title: \"提示\",\r\n                    text: `请先选择设备`,\r\n                    button: \"确认\",\r\n                });\r\n                return\r\n            }\r\n            params = {\r\n                master_id: this.select821Info.master_id,\r\n                slaver_id: this.select821Info.slaver_id,\r\n            };\r\n            let index = this.select821Info.num;\r\n            this.$getApi.getNSDgmInfo(params)\r\n                .then(res=>{\r\n                    if(res.msg==0){\r\n                        if(res.SNA!=undefined && res.SNA!=''){\r\n                            this.$set(this.$refs.child.data[index].dgm,'SNA',res.SNA);\r\n                            this.$set(this.$refs.child.changeList.changeMhd,index,this.$refs.child.data[index].dgm);\r\n                            this.$refs.child.hot.render();\r\n                        }\r\n                        else{\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: `获取的识别码为空`,\r\n                                button: \"确认\",\r\n                            });\r\n                        }\r\n                    }\r\n                    else{\r\n                        swal({\r\n                           