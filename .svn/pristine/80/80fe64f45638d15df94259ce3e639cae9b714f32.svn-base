               }\r\n                let reader = new FileReader();//这是核心,读取操作就是由它完成.\r\n                reader.readAsText(selectedFile[i]);//读取文件的内容,也可以读取文件的URL\r\n                reader.onload = function (event) {\r\n                    //当读取完成后回调这个函数,然后此时文件的内容存储到了result中,直接操作即可\r\n                    // showFile();\r\n                    let datas = event.target.result;\r\n                    let rows = datas.split(\"\\n\");//所有行\r\n                    let len = rows.length;\r\n                    let row=[];//每一行\r\n                    let dgm= {};\r\n                    let obj = {};\r\n                    that.fittingInfo.forEach(fit=>{\r\n                        obj.dgm_id = fit.dgm_id;\r\n                        dgm= {\r\n                            dgm_id: fit.dgm_id,//采集器uid\r\n                            dgm_name: fit.dgm_name,//采集站名称\r\n                        };\r\n                        for(let i = 0; i<len; i++){\r\n                            row = [];\r\n                            row = rows[i].split(\",\");\r\n                            if(that.p_atob(row[0])=='standardLen') {\r\n                                dgm['fBoltLength'] = that.p_atob(row[1]);\r\n                            }\r\n                            else if(that.p_atob(row[0])=='diameter') {\r\n                                dgm['fDiameter'] = that.p_atob(row[1]);\r\n                            }\r\n                            else if(that.p_atob(row[0])=='fitting'){\r\n                                dgm[that.p_atob(row[0])] = '算法' + that.p_atob(row[1]);\r\n                            }\r\n                            else{\r\n                                dgm[that.p_atob(row[0])] = that.p_atob(row[1]);\r\n                            }\r\n                            obj[that.p_atob(row[0])] = that.p_atob(row[1]);\r\n                        }\r\n                    });\r\n\r\n                    that.fileContent.push(obj);\r\n                    let oriData = that.$refs.child.data;\r\n\r\n                    for(let j=0;j<oriData.length;j++){\r\n                        if(oriData[j].dgm.fBoltLength == dgm.fBoltLength && oriData[j].dgm.fDiameter == dgm.fDiameter){\r\n                            that.$refs.child.data.splice(j, 1)\r\n                        }\r\n                    }\r\n                    that.$refs.child.data.push({dgm});\r\n                    that.$refs.child.filter_by_dgmName();\r\n                    that.$refs.child.renderTable();\r\n                }\r\n            }\r\n        },\r\n        chooseFile(){\r\n            document.getElementById('fileLocal').click();//模拟点击\r\n        },\r\n        getTable(name,isDgm){ //点击下标签的时候改变active，改变路由，刷新之后路由不变，active不变\r\n            this.isShow  = !isDgm;\r\n            this.isAdd = false //工况设置不可以添加\r\n            this.statePage(name);\r\n        },\r\n        add(){\r\n            return addRows(this.$refs.child.hot,this.$refs.child.data,-1);\r\n        },\r\n        save(){\r\n            /* 保存按钮 */\r\n            let changeData = this.$refs.child.changeList.changeData;\r\n            let changeMhdData = this.$refs.child.changeList.changeMhd;\r\n            let changeTmsData = this.$refs.child.changeList.changeTms;\r\n            let addData = this.$refs.child.changeList.addMhd;\r\n            let addTmsData = this.$refs.child.changeList.addTms;\r\n            let addDefaultDgm = this.$refs.child.changeList.addDefaultDgm;\r\n            let type = this.$refs.child.type;\r\n            let flag = this.checkPosLoc();\r\n            if(flag && type!=11 && type!=17 && type!=10 && type!=13  && type!=14  && type!=15 && type!=999 && type!=18){\r\n                //修改采集器信息\r\n                if(!$.isEmptyObject(changeData)){\r\n                    getApi.updateDgm(changeData)\r\n                        .then(res=>{\r\n                            if(res.msg==0){\r\n                                swal({\r\n                                    title: \"提示\",\r\n                        