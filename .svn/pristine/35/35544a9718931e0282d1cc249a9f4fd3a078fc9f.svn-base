\n                        if_end: res[i].pump.baseInfo.if_end,\r\n                        if_end_val:if_headEnd_source[res[i].pump.baseInfo.if_end],//下行车头 0:否 1：是\r\n                        if_end_sources:Object.values(if_headEnd_source),\r\n                        if_end_sources_key:Object.keys(if_headEnd_source),\r\n                    }\r\n                };\r\n                data.push({\r\n                    pump\r\n                });\r\n            }\r\n        }\r\n        return[data];\r\n    }\r\n\r\n}\r\n\r\nexport function createPump(type,folderData,pump_macNameList,pump_macIdList,index){\r\n    let pump_type_source = PUMP_TYPE_SOURCE;\r\n    let pump_activity_source = {'0':'有效','1':'无效'};\r\n    let macSource = pump_macNameList[Object.values(folderData)[0]];\r\n    let macSource_key = pump_macIdList[Object.keys(folderData)[0]];\r\n    let t_root = Object.keys(folderData)[0].split('_')[1];\r\n    if(t_root==2){\r\n        pump_type_source = {'1':'TC','2':'T','3':'MP','4':'M','5':'MCP','6':'MC'};\r\n    }\r\n    let pump= {\r\n        operate: 1,\r\n        folder_id: Object.keys(folderData)[0],\r\n        folder_name: Object.values(folderData)[0],\r\n        folder_sources:Object.values(folderData),\r\n        folder_sources_key:Object.keys(folderData),\r\n\r\n        machine_id: macSource_key[0],\r\n        machine_name: macSource[0],\r\n        updateTime: 0,//更新时间\r\n        t_root: t_root,\r\n        mac_id: macSource_key[0],//机组uid\r\n        macName_sources:macSource,\r\n        macName_sources_key:macSource_key,\r\n        pump_name: \"\",//机泵名称\r\n        pump_id: index + \"A\",//机泵uid\r\n\r\n        pump_type_val:Object.values(pump_type_source)[0],\r\n        pump_type: Object.keys(pump_type_source)[0],//水泥：机泵类型 1=运行泵 2=备用泵；走行部：车厢类型  1=动车，2=拖车，3=MC-1、4=MC-2\r\n        pump_type_sources:Object.values(pump_type_source),\r\n        pump_type_sources_key:Object.keys(pump_type_source),\r\n\r\n        pump_activity_val: Object.values(pump_activity_source)[0],\r\n        pump_activity: Object.keys(pump_activity_source)[0],//机泵状态 0:正常 1：禁用\r\n        pump_activity_sources:Object.values(pump_activity_source),\r\n        pump_activity_sources_key:Object.keys(pump_activity_source),\r\n        ch_class: 0,\r\n        item: 0,\r\n        driveChain_name:\"\",//传动链名称\r\n        itemCount:0,//部件数目\r\n        baseInfo: {\r\n            brand: \"\",//品牌\r\n            pn: \"\",//PN号\r\n            sn: \"\",//SN号\r\n            power: \"\",//功率\r\n            rated_speed: 1,//额定转速\r\n            speed_rate: \"\",//转速比\r\n            mf_date: \"\",//生产日期\r\n            other: \"\",//其它\r\n\r\n\r\n            if_head: '0',\r\n            if_head_val:'否',//上行车头 0:否 1：是\r\n            if_head_sources:Object.values(if_headEnd_source),\r\n            if_head_sources_key:Object.keys(if_headEnd_source),\r\n\r\n            if_end: '0',\r\n            if_end_val:'否',//下行车头 0:否 1：是\r\n            if_end_sources:Object.values(if_headEnd_source),\r\n            if_end_sources_key:Object.keys(if_headEnd_source),\r\n        }\r\n    };\r\n    if(pump.t_root==2){\r\n        pump.baseInfo.car_id = '';//车厢序号\r\n        pump.baseInfo.rated_speed = '';\r\n        pump.itemCount = '';\r\n        pump.baseInfo.car_wheel_d ='840';//轮径\r\n    }\r\n    return pump\r\n}\r\n\r\nexport function changePump(type,oldPump,index) {\r\n    let pump={\r\n        folder_id: oldPump.folder_id.split('_')[0],\r\n        folder_name: oldPump.folder_name,\r\n        machine_id: oldPump.machine_id,\r\n        machine_name: oldPump.machine_name,\r\n        t_root: oldPump.t_root,\r\n        pump:{\r\n            ch_class: oldPump.ch_class,\r\n            mac_id: oldPump.mac_id,\r\n            pump_activity: oldPump.pump_activity,\r\n            pump_id: oldPump.pump_id,\r\n            pump_name: oldPump.pump_name,\r\n            pump_type: oldPump.pump_type,\r\n            updateTime: oldPump.updateTime,\r\n            driveChain_name:oldPump.driveChain_name,//传动链名称\r\n            itemCount:oldPump.itemCount,//部件数目\r\n            baseInfo:{\r\n                brand: oldPump.baseInfo.brand,\r\n                mf_date: oldPump.baseInfo.mf_date,\r\n                other: oldPump.baseInfo.other,\r\n                pn: oldPump.baseInfo.pn,\r\n                power: oldPump.baseInfo.power,\r\n                rated_speed: oldPump.baseInfo.rated_speed,\r\n                sn: oldPump.baseInfo.sn,\r\n                speed_rate: oldPump.baseInfo.speed_rate,\r\n\r\n            }\r\n        }\r\n    };\r\n    if(oldPump.t_root==2){\r\n        pump.pump.baseInfo.if_head=oldPump.baseInfo.if_head;//上行车头\r\n        pump.pump.baseInfo.if_end=oldPump.baseInfo.if_end;//下行车头\r\n        pump.pump.baseInfo.car_id=oldPump.baseInfo.car_id;\r\n        pump.pump.baseInfo.car_wheel_d= oldPump.baseInfo.car_wheel_d;\r\n    }\r\n    return pump;\r\n}\r\n\r\nexport function getTmsInfo(res){\r\n    let data = [];\r\n    if(res==undefined){\r\n        console.log('数据错误')\r\n    }\r\n    else{\r\n        var tms={};\r\n        if(res.length>0){\r\n            res.forEach(item =>{\r\n                tms={\r\n                    operate:0,\r\n                    line_id: item.line_id,\r\n                    line_name: item.line_name,\r\n                    stations: item.stations,//站点信息\r\n                };\r\n                data.push({\r\n                    tms\r\n                });\r\n            })\r\n        }\r\n        return[data];\r\n    }\r\n}\r\nexport function createTms(type,index){\r\n    let tms= {\r\n        index: index,\r\n        operate: 1,\r\n        line_id: -1,\r\n        line_name: '',\r\n        stations: [],\r\n    };\r\n    return tms\r\n}\r\nexport function changeTms(type,oldTms,index) {\r\n    let tms={\r\n        index: index,\r\n        line_id: oldTms.line_id,\r\n        line_name: oldTms.line_name,\r\n        stations: oldTms.stations,\r\n    };\r\n    return tms;\r\n}\r\n","\r\n//采集器类型名称转化\r\nexport function dgmType_name(type,t_root){\r\n    let dgmType_name = '';\r\n    switch(parseInt(type)){\r\n        case 1:\r\n            dgmType_name = \"DGU2000V1\";\r\n            break;\r\n        case 2:\r\n            dgmType_name =\"DGU2200\";\r\n            break;\r\n        case 3:\r\n            dgmType_name =\"DGU2000V2\";\r\n            break;\r\n        case 4:\r\n            if(parseInt(t_root)===1){\r\n                dgmType_name =\"DGU2100E\";\r\n            }\r\n            else{\r\n                dgmType_name =\"MHD\";\r\n            }\r\n            break;\r\n        case 5:\r\n            dgmType_name =\"DGU2600\";\r\n            break;\r\n        case 6:\r\n            dgmType_name =\"E821\";\r\n            break;\r\n        case 7:\r\n            dgmType_name =\"TMU2000\";\r\n            break;\r\n        case 9:\r\n            dgmType_name =\"DGU2000E\";\r\n            break;\r\n        case 10:\r\n            dgmType_name =\"WL9100\";\r\n            break;\r\n    }\r\n    return dgmType_name\r\n}\r\n\r\n\r\n/**\r\n * *频段类型转换\r\n * **/\r\nexport function optfreq_type(val) {\r\n    let optfreqTypeVal=\"\";\r\n    switch (val) {\r\n        case \"0\":\r\n            optfreqTypeVal = \"能量有效值\";\r\n            break;\r\n        case \"1\":\r\n            optfreqTypeVal = \"速度值\";\r\n            break;\r\n        case \"2\":\r\n            optfreqTypeVal = \"加速度值\";\r\n            break;\r\n    }\r\n    return optfreqTypeVal;\r\n}\r\n/**\r\n * *组织过滤\r\n * **/\r\nexport function filterFolder(folderData,type) {\r\n    let folderDataObj={};\r\n    let t_root;\r\n    for(let key in folderData){\r\n        t_root = Number(key.split('_')[1]);\r\n        switch (Number(type)) {\r\n            case 1:\r\n                if(t_root===0){\r\n                    folderDataObj[key] = folderData[key];\r\n                }\r\n                break;\r\n            case 5: case 3:\r\n                if(t_root===1){\r\n                    folderDataObj[key] = folderData[key];\r\n                }\r\n                break;\r\n            case 6: case 8:\r\n                if(t_root===2){\r\n                    folderDataObj[key] = folderData[key];\r\n                }\r\n                break;\r\n            case 7:\r\n                if(t_root===3){\r\n                    folderDataObj[key] = folderData[key];\r\n                }\r\n                break;\r\n            case 9: case 10:\r\n                if(t_root===-1){\r\n                    folderDataObj[key] = folderData[key];\r\n                }\r\n                break;\r\n\r\n        }\r\n    }\r\n    return folderDataObj;\r\n}\r\n/**\r\n * *不同组织类型的机组级别类型\r\n * **/\r\nexport const MacTypeBtn = [1,5,6,7,9];\r\n/**\r\n * *不同组织类型的机泵级别类型\r\n * **/\r\nexport const PumpTypeBtn = [3,8,10];\r\n","import machdtable from './mac_hdtable.js'\r\nimport {getMacInfo,getConInfo,getPumpInfo,getTmsInfo} from './mac_info.js'\r\nimport {MacTypeBtn,PumpTypeBtn} from './mac_methods.js'\r\n\r\nexport const macMixin={\r\n    data(){\r\n        return{\r\n            hot: {},//hottable对象\r\n            data: [],//hottable的data数据\r\n            initList:{\r\n                folderData:{},//组织信息下拉选项\r\n                pumpFolderData:{},//机泵组织列表\r\n                pumpMacData:{},//机泵机组列表\r\n                pump_macNameList:{},//机泵所有的组织对应的机组列表\r\n                pump_macIdList:{},//机泵所有的组织对应的机组列表\r\n                lineList:{},//线路列表\r\n            },\r\n            changeList:{\r\n                changeData:{},//修改的数据\r\n                changeConData:[],//修改的工况的信息\r\n                deleteData:[],//删除的数据\r\n                addData:{},//新增的数据\r\n                sortMacList:[],//过滤机组名列表\r\n                sortDgmList:[],//过滤采集器列表\r\n                sameName:{},//同一组织下机组同名列表\r\n                isValidResult: true,//表格中单元格验证是否正确\r\n            },\r\n\r\n            scroll:0,//滚动距离\r\n        }\r\n    },\r\n    mounted(){\r\n        if(PumpTypeBtn.includes(Number(this.type))){\r\n            this.getPumpFolderData();\r\n        }\r\n        else if(this.type==4){\r\n            this.getTmsFolderData();\r\n        }\r\n        else if(this.type==2){\r\n            this.getListData(this.type);\r\n        }\r\n        else{\r\n            this.getFolderData();\r\n        }\r\n    },\r\n    beforeRouteLeave (to, from, next) {\r\n        let changeData = this.changeList.changeData;\r\n        let changeConData = this.changeList.changeConData;\r\n        let addData = this.changeList.addData;\r\n        if(!$.isEmptyObject(changeData) || !$.isEmptyObject(addData) || changeConData.length!==0){\r\n            swal({\r\n                title: \"警告\",\r\n                text: \"正在离开本页面，本页面内所有未保存数据都会丢失。\",\r\n                buttons: {\r\n                    sure: \"确认\",\r\n                    cancel: \"取消\",\r\n                },\r\n                dangerMode: true,\r\n            })\r\n                .then((willDelete) => {\r\n                    if (willDelete) {\r\n                        next();\r\n                    } else {\r\n                        window.history.go(1);\r\n                    }\r\n                });\r\n        }\r\n        else{\r\n            next();\r\n        }\r\n    },\r\n    methods:{\r\n        getFolderData(){\r\n            this.$getApi.queryAllFolder(this.$store.state.tids)\r\n                .then(res=>{\r\n                    if(res !== undefined){\r\n                        if(res.length === 0){\r\n                            this.$emit(\"child-msg\",true)\r\n                        }else{\r\n                            //渲染表格\r\n                            if(res.length>0){\r\n                                for(let i=0;i<res.length;i++){\r\n                                    if(this.type==1 && res[i].t_root==0){\r\n                                        this.initList.folderData[res[i].t_id + '_' + res[i].t_root]=res[i].t_name;\r\n                                    }\r\n                                    if(this.type==5 && res[i].t_root==1){\r\n                                        this.initList.folderData[res[i].t_id + '_' + res[i].t_root]=res[i].t_name;\r\n                                    }\r\n                                    if(this.type==6 && res[i].t_root==2){\r\n                                        this.initList.folderData[res[i].t_id + '_' + res[i].t_root]=res[i].t_name;\r\n                                    }\r\n                                    if(this.type==7 && res[i].t_root==3){\r\n                                        this.initList.folderData[res[i].t_id + '_' + res[i].t_root]=res[i].t_name;\r\n                                    }\r\n                                    if(this.type==9 && res[i].t_root==-1){\r\n                                        this.initList.folderData[res[i].t_id + '_' + res[i].t_root]=res[i].t_name;\r\n                                    }\r\n                                }\r\n                                if($.isEmptyObject(this.initList.folderData)){\r\n                                    this.$emit(\"child-msg\",true)\r\n                                }\r\n                                else{\r\n                                    this.$emit(\"child-msg\",false);\r\n                                    this.getListData(this.type);\r\n                                }\r\n                            }\r\n                        }\r\n                    }\r\n                    else{\r\n                        console.log('组织数据错误')\r\n                    }\r\n\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求错误' ,err)\r\n                })\r\n        },\r\n        getPumpFolderData(){\r\n            let params = null;\r\n            params = {\r\n                 t_ids: this.$store.state.tids,\r\n                 t_root: 1\r\n            }\r\n            this.$getApi.queryTreeIdAndMacId(params)\r\n                .then(res=>{\r\n                    if(res !== undefined){\r\n                        if(res.length === 0){\r\n                            this.$emit(\"child-msg\",true)\r\n                        }\r\n                        else{\r\n                            //渲染表格\r\n                            res.forEach(folder =>{\r\n                                if(this.type == 3 && folder.t_root==1){\r\n                                    this.initList.pumpFolderData[folder.t_id + '_' + folder.t_root]=folder.t_name;\r\n                                    this.initList.pump_macNameList[folder.t_name]=[];\r\n                                    this.initList.pump_macIdList[folder.t_id + '_' + folder.t_root]=[];\r\n                                    if(folder.machine_list !== undefined && folder.machine_list.length>0){\r\n                                        folder.machine_list.forEach(machine =>{\r\n                                            this.initList.pumpMacData[machine.mac_id + '_' + machine.m_type]=machine.mac_me;\r\n                                            this.initList.pump_macNameList[folder.t_name].push(machine.mac_me);\r\n                                            this.initList.pump_macIdList[folder.t_id + '_' + folder.t_root].push(machine.mac_id);\r\n                                        })\r\n                                    }\r\n                                }\r\n                                if(this.type == 8 && folder.t_root==2){\r\n                                    this.initList.pumpFolderData[folder.t_id + '_' + folder.t_root]=folder.t_name;\r\n                                    this.initList.pump_macNameList[folder.t_name]=[];\r\n                                    this.initList.pump_macIdList[folder.t_id + '_' + folder.t_root]=[];\r\n                                    if(folder.machine_list !== undefined && folder.machine_list.length>0){\r\n                                        folder.machine_list.forEach(machine =>{\r\n                                            this.initList.pumpMacData[machine.mac_id + '_' + machine.m_type]=machine.mac_me;\r\n                                            this.initList.pump_macNameList[folder.t_name].push(machine.mac_me);\r\n                                            this.initList.pump_macIdList[folder.t_id + '_' + folder.t_root].push(machine.mac_id);\r\n                                        })\r\n                                    }\r\n                                }\r\n                                if(this.type == 10 && folder.t_root==-1){\r\n                                    this.initList.pumpFolderData[folder.t_id + '_' + folder.t_root]=folder.t_name;\r\n                                    this.initList.pump_macNameList[folder.t_name]=[];\r\n                                    this.initList.pump_macIdList[folder.t_id + '_' + folder.t_root]=[];\r\n                                    if(folder.machine_list !== undefined && folder.machine_list.length>0){\r\n                                        folder.machine_list.forEach(machine =>{\r\n                                            this.initList.pumpMacData[machine.mac_id + '_' + machine.m_type]=machine.mac_me;\r\n                                            this.initList.pump_macNameList[folder.t_name].push(machine.mac_me);\r\n                                            this.initList.pump_macIdList[folder.t_id + '_' + folder.t_root].push(machine.mac_id);\r\n                                        })\r\n                                    }\r\n                                }\r\n                            });\r\n                            if($.isEmptyObject(this.initList.pumpFolderData)){\r\n                                this.$emit(\"child-msg\",true)\r\n                            }\r\n                            else{\r\n                                this.$emit(\"child-msg\",false);\r\n                                this.getListData(this.type);\r\n                            }\r\n                        }\r\n                    }\r\n                    else{\r\n                        console.log('组织数据错误')\r\n                    }\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求错误' ,err)\r\n                })\r\n        },\r\n        getTmsFolderData(){\r\n            let params = null;\r\n            params = {\r\n                t_ids: this.$store.state.tids,\r\n                t_root: 2\r\n            }\r\n            this.$getApi.queryTreeIdAndMacId(params)\r\n                .then(res=>{\r\n                    if(res !== undefined){\r\n                        if(res.length === 0){\r\n                            this.$emit(\"child-msg\",true)\r\n                        }\r\n                        else{\r\n                            //渲染表格\r\n                            this.$emit(\"child-msg\",false);\r\n                            if(res.length>0){\r\n                                res.forEach(folder =>{\r\n                                    this.initList.pumpFolderData[folder.t_id + '_' + folder.t_root]=folder.t_name;\r\n                                    this.initList.pump_macNameList[folder.t_name]=[];\r\n                                    this.initList.pump_macIdList[folder.t_id + '_' + folder.t_root]=[];\r\n                                    if(folder.machine_list !== undefined && folder.machine_list.length>0){\r\n                                        folder.machine_list.forEach(machine =>{\r\n                                            this.initList.pumpMacData[machine.mac_id + '_' + machine.m_type]=machine.mac_me;\r\n                                            this.initList.pump_macNameList[folder.t_name].push(machine.mac_me);\r\n                                            this.initList.pump_macIdList[folder.t_id + '_' + folder.t_root].push(machine.mac_id);\r\n                                        })\r\n                                    }\r\n                                });\r\n                                this.getListData(this.type);\r\n                            }\r\n                        }\r\n                    }\r\n                    else{\r\n                        console.log('组织数据错误')\r\n                    }\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求错误' ,err)\r\n                })\r\n        },\r\n        getListData(type){\r\n            type = Number(type);\r\n            if(MacTypeBtn.includes(type)){\r\n                let that = this;\r\n                this.$getApi.queryAllMachine(this.$store.state.tids)\r\n                    .then(res=>{\r\n                        if(res && res.line_list && res.line_list.length){//走行部的线路选择下拉列表\r\n                            res.line_list.forEach(line=>{\r\n                                that.initList.lineList[line.line_id] = line.line_name;\r\n                            })\r\n                        }\r\n                        let result = getMacInfo(res,this.initList.folderData,type,that.initList.lineList);   //获取机组信息\r\n                        let linkObj = [\"\",\"\",type];\r\n                        if(res && res.machine_list && res.machine_list.length){\r\n                            for(let i=0;i<res.machine_list.length;i++){\r\n                                if(type==1 && res.machine_list[i].t_root==0){\r\n                                    linkObj = [res.machine_list[i].t_id,res.machine_list[i].t_name,type];\r\n                                    break;\r\n                                }\r\n                                if(type==5 && res.machine_list[i].t_root==1){\r\n                                    linkObj = [res.machine_list[i].t_id,res.machine_list[i].t_name,type];\r\n                                    break;\r\n                                }\r\n                                if(type==6 && res.machine_list[i].t_root==2){\r\n                                    linkObj = [res.machine_list[i].t_id,res.machine_list[i].t_name,type];\r\n                                    break;\r\n                                }\r\n                                if(type==7 && res.machine_list[i].t_root==3){\r\n                                    linkObj = [res.machine_list[i].t_id,res.machine_list[i].t_name,type];\r\n                                    break;\r\n                                }\r\n                            }\r\n                        }\r\n                        this.$store.commit('setTreeMac', linkObj);\r\n                        //组织position结构\r\n                        [this.data] = result;\r\n                        this.renderTable()\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            }\r\n            else if(type==2){\r\n                this.$getApi.queryAllCondition(this.$store.state.tids)\r\n                    .then(res=>{\r\n                        let result = getConInfo(res);\r\n                        if(!result){\r\n                            this.$emit(\"child-msg\",true)\r\n                        }else{\r\n                            //组织position结构\r\n                            [this.data] = result;\r\n                            //渲染表格\r\n                            this.$emit(\"child-msg\",false);\r\n                            this.renderTable()\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            }\r\n            else if(PumpTypeBtn.includes(type)){\r\n                this.$getApi.queryPumps(this.$store.state.tids)\r\n                    .then(res=>{\r\n                        if($.isEmptyObject(this.initList.pump_macNameList)){\r\n                            this.$emit(\"child-msg\",true)\r\n                        }else{\r\n                            let result = getPumpInfo(res,this.initList.pumpFolderData,this.initList.pump_macNameList,this.initList.pump_macIdList,type);   //有点问题\r\n                            let linkObj = [\"\",\"\",type];\r\n                            if(res.length>0){\r\n                                for(let i=0;i<res.length;i++){\r\n                                    if(type==3 && res[i].t_root==1){\r\n                                        linkObj = [res[i].folder_id,res[i].folder_name,type];\r\n                                        break;\r\n                                    }\r\n                                    if(type==8 && res[i].t_root==2){\r\n                                        linkObj = [res[i].folder_id,res[i].folder_name,type];\r\n                                        break;\r\n                                    }\r\n                                }\r\n                            }\r\n                            this.$store.commit('setTreeMac', linkObj);\r\n                            [this.data] = result;\r\n                            this.renderTable()\r\n                            //渲染表格\r\n                            this.$emit(\"child-msg\",false);\r\n                        }\r\n\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            }\r\n            else if(type==4){\r\n                this.$getApi.queryLines()\r\n                    .then(res=>{\r\n                        let result = getTmsInfo(res);\r\n                        if(res.length>0){\r\n                            let linkObj = [res[0].line_id,res[0].line_name,4];\r\n                            this.$store.commit('setTreeMac', linkObj);\r\n                        }\r\n                        else{\r\n                            let linkObj = [\"\",\"\",4];\r\n                            this.$store.commit('setTreeMac', linkObj);\r\n                        }\r\n                        [this.data] = result;\r\n                        this.renderTable()\r\n                        //渲染表格\r\n                        this.$emit(\"child-msg\",false);\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            }\r\n        },\r\n        /* 渲染表格 */\r\n        renderTable(){\r\n            let HotTable = document.getElementById('HotTable');\r\n            let property = {\r\n                type:this.type,\r\n                tids:this.$store.state.tids,\r\n                getData:this.getData,\r\n                getHot:this.getHot,\r\n                initList:this.initList,\r\n                changeList:this.changeList,\r\n                router: this.$router,\r\n                store : this.$store\r\n            }\r\n            //创建table\r\n            this.hot = new machdtable(HotTable,property);\r\n            this.reRenderSelectOption(this.hot,this.data);\r\n            /* 监听滚动事件 */\r\n            let wtHolder = document.getElementsByClassName('wtHolder')[0]\r\n            wtHolder.addEventListener('scroll',(e)=>{\r\n                this.hot.destroyEditor()//处理当选择下拉框后拖动滚动条框框会跟着跑的问题\r\n            });\r\n            this.filterRows(this.hot,this.data);//保留过滤结果\r\n        },\r\n        reRenderSelectOption(hot,data){ //重新渲染selectOption,让每个option\r\n            let cellMeta  = hot.getCellMetaAtRow(0);\r\n            let rows = data.length\r\n            cellMeta.forEach(item=>{\r\n                if(item.prop == \"pump.t_name\"){ //组织\r\n                    for(let i=0;i<=rows;i++){\r\n                        hot.setCellMeta(i,item.col,\"selectOptions\",row=>{ return  data[hot.toPhysicalRow(row)].pump.t_name_sources})\r\n                    }\r\n                }\r\n                if(item.prop == \"pump.mac_me\"){ //机组\r\n                    for(let i=0;i<rows;i++){\r\n                        hot.setCellMeta(i,item.col,\"selectOptions\",row=>{ return  data[hot.toPhysicalRow(row)].pump.macName_sources})\r\n                    }\r\n                }\r\n                if(item.prop == \"pump.pump_type_val\"){ //子级类型\r\n                    for(let i=0;i<rows;i++){\r\n                        hot.setCellMeta(i,item.col,\"selectOptions\",row=>{ return  data[hot.toPhysicalRow(row)].pump.pump_type_sources})\r\n                    }\r\n                }\r\n                if(item.prop == \"condition.pos_name1\"){ //工况参考测点1\r\n                    for(let i=0;i<rows;i++){\r\n                        hot.setCellMeta(i,item.col,\"selectOptions\",row=>{ return  data[hot.toPhysicalRow(row)].condition.pos_sources1})\r\n                    }\r\n                }\r\n                if(item.prop == \"condition.pos_name2\"){ //工况参考测点2\r\n                    for(let i=0;i<rows;i++){\r\n                        hot.setCellMeta(i,item.col,\"selectOptions\",row=>{ return  data[hot.toPhysicalRow(row)].condition.pos_sources2})\r\n                    }\r\n                }\r\n            })\r\n        },\r\n        //保留过滤后的结果\r\n        filterRows(hot,data){\r\n            const filtersPlugin = hot.getPlugin('filters');\r\n            /*const macTypeList = [1,5,6,7];//机组类型\r\n            const pumpTypeList = [3,8];//机泵、车厢*/\r\n            if(MacTypeBtn.includes(Number(this.type)) && sessionStorage.getItem('filter_type1')){\r\n                let filterArr =  JSON.parse(sessionStorage.getItem('filter_type1'));\r\n                filterArr.forEach(condition =>{\r\n                    filtersPlugin.addCondition(condition.column,condition.conditions[0].name,condition.conditions[0].args,condition.operations);\r\n                });\r\n                filtersPlugin.filter();\r\n            }\r\n            else if(this.type == 2 && sessionStorage.getItem('filter_type2')){\r\n                let filterArr =  JSON.parse(sessionStorage.getItem('filter_type2'));\r\n                filterArr.forEach(condition =>{\r\n                    filtersPlugin.addCondition(condition.column,condition.conditions[0].name,condition.conditions[0].args,condition.operations);\r\n                });\r\n                filtersPlugin.filter();\r\n            }\r\n            else if(PumpTypeBtn.includes(Number(this.type)) && sessionStorage.getItem('filter_type3')){\r\n                let filterArr =  JSON.parse(sessionStorage.getItem('filter_type3'));\r\n                filterArr.forEach(condition =>{\r\n                    filtersPlugin.addCondition(condition.column,condition.conditions[0].name,condition.conditions[0].args,condition.operations);\r\n                });\r\n                filtersPlugin.filter();\r\n            }\r\n        },\r\n        //获取data\r\n        getData(){\r\n            return this.data\r\n        },\r\n        /* 获取hot实例 */\r\n        getHot(){\r\n            return this.hot\r\n        }\r\n    }\r\n}\r\n"],"sourceRoot":""}