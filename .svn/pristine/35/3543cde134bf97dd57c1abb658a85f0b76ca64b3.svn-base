() {\r\n            return this.searchFolderList2.sort(compareName('t_name'));\r\n        },\r\n        sortFolderList3: function () {\r\n            return this.searchFolderList3.sort(compareName('t_name'));\r\n        },\r\n        sortFolderList4: function () {\r\n            return this.searchFolderList4.sort(compareName('t_name'));\r\n        },\r\n        sortPumpList: function () {\r\n            return this.searchPumpList.sort(compareName('pump_name'));\r\n        },\r\n\r\n    },\r\n    methods: {\r\n        //保存总貌图\r\n        /**\r\n         * type: 1组织总貌；3：机泵总貌\r\n         * **/\r\n        keepOverview(type){\r\n            let overViewObj = {},w,ppx,ppy;\r\n            if(type===1){//组织总貌图\r\n                overViewObj = {\r\n                    o_id:this.chooseFolder.t_id,//总貌图uid（=组织/机组/机泵uid）\r\n                    mac_id:\"A\",\r\n                    overview_info:{},\r\n                    itemList:[],//节点列表\r\n                };//总貌图\r\n            }\r\n            else if(type===3){//机泵总貌图\r\n                overViewObj = {\r\n                    o_id:this.chooseMac.pump_id,//总貌图uid（=组织/机组/机泵uid）\r\n                    mac_id:this.chooseMac.mac_id,\r\n                    overview_info:{},\r\n                    itemList:[],//节点列表\r\n                };//总貌图\r\n            }\r\n            overViewObj.overview_info={\r\n                version:1,\r\n                o_name:this.o_name,//总貌图名称\r\n                overviewFont:this.overviewFont,//总貌图名称字号大小\r\n                bg_width:this.origBlock.oriW,//背景宽\r\n                bg_height:this.origBlock.oriH,//背景高\r\n            }\r\n            let obj={},finalX,finalY,finalCircleX,finalCircleY;\r\n\r\n            for(let i=0;i<this.MoveMsg.length;i++){\r\n                let item = this.MoveMsg[i];\r\n                finalX = item.x + item.moveX;\r\n                finalY = item.y + item.moveY;\r\n                finalCircleX = item.oriCircleX + item.moveCircleX;\r\n                finalCircleY = item.oriCircleY + item.moveCircleY;\r\n                obj={\r\n                    id:i,//计数id\r\n                    type:type,//类型 1=组织 2=机组 3=机泵 4=测点\r\n                    itemType:this.MoveMsg[i].itemType,//设备类型 如1=破碎机\r\n                    uid:this.MoveMsg[i].uid,//节点uid，如\"3_05\"，表示id=05的振动测点\r\n                    px:(finalX/this.origBlock.oriW).toFixed(2),//相对于背景图的x位置\r\n                    py:(finalY/this.origBlock.oriH).toFixed(2),//相对于背景图的y位置\r\n                    width:this.MoveMsg[i].width,\r\n                    height:this.MoveMsg[i].height,\r\n                    itemName:this.MoveMsg[i].text,//编辑的部件名\r\n                    circleD:this.MoveMsg[i].circleD,//圆点的直径\r\n                    circleX: (finalCircleX/this.origBlock.oriW).toFixed(2),//小圆百分比的x位置\r\n                    circleY: (finalCircleY/this.origBlock.oriH).toFixed(2),//小圆百分比的y\r\n                };\r\n                overViewObj.itemList.push(obj);\r\n            }\r\n            this.$axios.post('updateTreeOverview',overViewObj)\r\n                .then(res=>{\r\n                    if(res.msg==0){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '保存成功',\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                    else if(res.msg==1){\r\n                        console.log('参数为空')\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '保存失败',\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                    else if(res.msg==2){\r\n                        console.log('后台运行错误')\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '保存失败',\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求失败',err)\r\n                })\r\n        },\r\n        //上传总貌图背景图\r\n        /**\r\n         * type: 1组织总貌；2：机泵总貌\r\n         * **/\r\n        selectImg(type){\r\n            let that=this;\r\n            let imgFile = $('#imgLocal')[0].files[0];//取到上传的图片\r\n\r\n            let param = new FormData(); // 创建form对象\r\n            param.append('files', imgFile);//对应后台接收图片名\r\n            if(type===1){\r\n                param.append('id', this.chooseFolder.t_id);\r\n                param.append('mac_id', \"A\");\r\n            }\r\n            else if(type===2){\r\n                param.append('id', this.chooseMac.pump_id);\r\n                param.append('mac_id', this.chooseMac.mac_id);\r\n            }\r\n            let overview_json = {\r\n                version: 1,\r\n                o_name: that.o_name,\r\n                overviewFont: that.overviewFont\r\n            };\r\n            overview_json = JSON.stringify(overview_json);\r\n            param.append('overview_json', overview_json);\r\n            this.$axios.post('createTreeOverview',param,{\r\n                headers: {\r\n                    'Content-Type': 'multipart/form-data',\r\n                }\r\n            }).then(res => {\r\n                // console.log(res);//\r\n                $('#imgLocal')[0].value = \"\";\r\n                that.overviewUrl = '';\r\n                if(res.msg==0){\r\n                    that.overviewUrl = res.bgurl;\r\n                    // 图片地址\r\n                    let img_url = res.bgurl;\r\n                    // let img_url = require(\"assets/img/test/timg.jpg\");\r\n                    // 创建对象\r\n                    let img = new Image();\r\n                    // 改变图片的src\r\n                    img.src = img_url;\r\n                    // 加载完成执行\r\n                    img.onload = function() {\r\n                        /* 获取图片的真实宽高，计算背景图区域 */\r\n                        that.computePicWidth(img);\r\n                        that.MoveMsg.forEach(item=>{\r\n                            item.style.left = parseInt(item.px * that.origBlock.oriW) + 'px';\r\n                            item.style.top = parseInt(item.py * that.origBlock.oriH) + 'px';\r\n                            item.x = parseInt(item.px * that.origBlock.oriW);\r\n                            item.y = parseInt(item.py * that.origBlock.oriH) ;\r\n                        });\r\n                        that.$forceUpdate();\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '上传成功',\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                }\r\n                else if(res.msg==1){\r\n                    console.log(\"参数为空\");\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '上传失败',\r\n                        button: \"确认\",\r\n                    });\r\n                }\r\n                else if(res.msg==2){\r\n                    console.log(\"后台运行错误\")\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '上传失败',\r\n                        button: \"确认\",\r\n                    });\r\n                }\r\n                else if(res.msg==3){\r\n                    console.log(\"获取json数据时为空\")\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '上传失败',\r\n                        button: \"确认\",\r\n                    });\r\n                }\r\n            }).catch(function(error){\r\n                console.log(error);\r\n                $('#imgLocal')[0].value = \"\";\r\n            })\r\n                .catch(err=>{\r\n                    console.log('请求错误',err)\r\n                })\r\n        },\r\n        //获取组织信息\r\n        getTidData(){\r\n            let that=this;\r\n            let paramObj = null;\r\n            paramObj = {\r\n                t_ids: this.$store.state.tids\r\n            };\r\n            this.$axios.post('queryAllT_OverviewInfo',paramObj)\r\n                .then(res=>{\r\n                    that.folderList1=[];\r\n                    that.folderList2=[];\r\n                    that.folderList3=[];\r\n                    that.folderList4=[];\r\n                    for(let i=0;i<res.length;i++){\r\n                        if(res[i].t_root==1){\r\n                            that.folderList1.push(res[i]);\r\n                        }\r\n                    }\r\n                    that.searchFolderList1 = that.folderList1.concat();\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求错误', err)\r\n                })\r\n        },\r\n        //确定选择组织\r\n        /**\r\n         * chooseInfo:所选中的组织信息；\r\n         * e:点击元素；\r\n         * type:1一级组织，2二级组织，3三级组织，4四级组织；\r\n         * pageType：1组织设置，2机泵设置\r\n         * **/\r\n        selectFolder: function(chooseInfo,e,type,pageType){\r\n            $('pump_li').siblings().removeClass('active');\r\n            let num = Number(type + 1);\r\n            if(chooseInfo.child && chooseInfo.child.length){\r\n                this[`folderList${num}`] = chooseInfo.child.concat();\r\n                this[`searchFolderList${num}`] = chooseInfo.child;\r\n                this.$refs[`searchInput${num}`].value = '';\r\n            }\r\n            else{\r\n                this[`searchFolderList${num}`] = [];\r\n                this.$refs[`searchInput${num}`].value = '';\r\n            }\r\n            if(e!==null){\r\n                $(e.currentTarget).addClass('active').siblings().removeClass('active');\r\n            }\r\n            this.chooseFolder=chooseInfo;\r\n            this.choosePumpList.length = 0;//切换组织时机泵列表清空\r\n            if(type) this.$refs[`searchInput${type}`].value =this.chooseFolder.t_name;\r\n            this[`isShow${type}`]=false;\r\n            let that=this;\r\n            let tidObj={\r\n                t_id:this.chooseFolder.t_id,\r\n                type:pageType\r\n            };\r\n            this.$axios.post('queryPumpOverviewInfoByTId',tidObj)\r\n                .then(res=>{\r\n                    if(pageType===1){\r\n                        that.overviewUrl='';\r\n                        if(res.msg==0){\r\n                            that.MoveMsg=[];\r\n                            let result  = res.folder.overview;\r\n                            if(!$.isEmptyObject(result)){\r\n                                that.overviewUrl= result.bgurl;\r\n                                if(result.o_name){\r\n                                    that.o_name = result.o_name;\r\n                                }\r\n                                else{\r\n                                    that.o_name = that.chooseFolder.t_name;\r\n                                }\r\n                                if(result.overviewFont){\r\n                                    that.overviewFont = result.overviewFont;\r\n                                }\r\n                                else{\r\n                                    that.overviewFont = 20;\r\n                                }\r\n                                // that.overviewUrl = require(\"assets/img/overview_pump.jpg\");\r\n                                let img_url = result.bgurl;\r\n                                // let img_url = require(\"assets/img/overview_pump.jpg\");\r\n                                // 创建对象\r\n                                let img = new Image();\r\n                                // 改变图片的src\r\n                                img.src = img_url;\r\n                                // 加载完成执行\r\n                                img.onload = function() {\r\n                                    /* 获取图片的真实宽高，计算背景图区域 */\r\n                                    that.computePicWidth(img);\r\n\r\n                                    let oriItemList = result.itemList;\r\n                                    for (let i = 0; i < oriItemList.length; i++) {\r\n                                        if (typeof oriItemList[i].circleD === \"undefined\") {\r\n                                            oriItemList[i].circleD = 14;//设置小圆点的默认值为14\r\n                                        }\r\n                                        let obj = {\r\n                                            index: i,\r\n                                            text: oriItemList[i].itemName,//输入框中文字\r\n                                            px: oriItemList[i].px,//框框百分比的x\r\n                                            py: oriItemList[i].py,//框框百分比的y\r\n                                            x: parseInt(oriItemList[i].px * that.origBlock.oriW),//页面的x\r\n                                            y: parseInt(oriItemList[i].py * that.origBlock.oriH),//页面的y\r\n                                            uid: oriItemList[i].uid,\r\n                                            itemType: oriItemList[i].pump_type,//机泵类型\r\n                                            width: oriItemList[i].width,//旧：机泵图标宽；新：框框的宽\r\n                                            height: oriItemList[i].height,//旧：机泵图标高；新：框框的高\r\n                                            circleD: oriItemList[i].circleD,//小圆的直径\r\n                                            circleX: oriItemList[i].circleX,//小圆百分比的x\r\n                                            circleY: oriItemList[i].circleY,//小圆百分比的y\r\n                                            moveX: 0,//相对原来的框的移动位置x\r\n                                            moveY: 0,//相对原来的框的移动位置y\r\n                                            moveCircleX: 0,//圆点相对原来的框的移动位置x\r\n                                            moveCircleY: 0,//圆点相对原来的框的移动位置y\r\n                                            oriCircleX: parseInt(oriItemList[i].circleX * that.origBlock.oriW),//圆点初始x\r\n                                            oriCircleY: parseInt(oriItemList[i].circleY * that.origBlock.oriH),//圆点初始y\r\n                                            style: {\r\n                                                left: parseInt(oriItemList[i].px * that.origBlock.oriW) + 'px',\r\n                                                top: parseInt(oriItemList[i].py * that.origBlock.oriH) + 'px',\r\n                                                zIndex: 2,\r\n                                            },\r\n                                            circleStyle: {\r\n                                                left: parseInt(oriItemList[i].circleX * that.origBlock.oriW) + 'px',\r\n                                                top: parseInt(oriItemList[i].circleY * that.origBlock.oriH) + 'px',\r\n                                                zIndex: 2,\r\n                                            },\r\n                                        };\r\n                                        for (let j = 0; j < res.pump_list.length; j++) {\r\n                                            if (oriItemList[i].uid == res.pump_list[j].pump_id) {\r\n                                                res.pump_list[j].dragFlag = false;\r\n                                                obj.dragFlag = false;\r\n                                                obj.mac_id = res.pump_list[j].mac_id;\r\n                                                obj.ch_class = res.pump_list[j].ch_class;\r\n                                                obj.pump_name = res.pump_list[j].pump_name;\r\n\r\n                                                obj.url = require('assets/img/cement_machines/' + that.getPumpImg(res.pump_list[j].pump_type).machine + '.png');\r\n                                            }\r\n                                        }\r\n                                        that.MoveMsg.push(obj);\r\n                                    }\r\n                                }\r\n                            }\r\n                            else{\r\n                                that.overviewUrl=\"\";\r\n                                that.o_name = that.chooseFolder.t_name;\r\n                            }\r\n                            that.choosePumpList=res.pump_list;\r\n                            if(that.choosePumpList.length>0){\r\n                                for(let i=0;i<that.choosePumpList.length;i++){\r\n                                    that.choosePumpList[i].url=require('assets/img/cement_machines/'+that.getPumpImg(that.choosePumpList[i].pump_type).machine+'.png');\r\n                                    that.choosePumpList[i].width=that.getPumpImg(that.choosePumpList[i].pump_type).width;\r\n                                    that.choosePumpList[i].height=that.getPumpImg(that.choosePumpList[i].pump_type).height;\r\n                                    that.choosePumpList[i].itemType=that.getPumpImg(that.choosePumpList[i].pump_type).itemType;\r\n                                    if(that.choosePumpList[i].dragFlag === false){\r\n                                        return\r\n                                    }\r\n                                    else{\r\n                                        that.choosePumpList[i].dragFlag=true;\r\n                                    }\r\n\r\n                                }\r\n\r\n                            }\r\n                            // that.o_name +=that.chooseMac.pump_name;\r\n                        }\r\n                    }\r\n                    else if(pageType===2){\r\n                        if(res.msg==0){\r\n                            that.choosePumpList=res.pump_list;\r\n                            that.searchPumpList=that.choosePumpList;\r\n                            if(that.choosePumpList.length>0){\r\n                                that.$refs.searchMacInput.value =that.sortPumpList[0].pump_name;\r\n                                if(that.chooseMac.pump_id!=\"\" && e===null){//从总貌图带过来的pump信息\r\n                                    that.selectMac(that.chooseMac);\r\n                                }\r\n                                else{\r\n                                    that.chooseMac=that.sortPumpList[0];\r\n                                    that.selectMac(that.chooseMac,0);\r\n                                }\r\n                            }\r\n                            else{\r\n                                that.$refs.searchMacInput.value ='';\r\n                                that.choosePosList=[];\r\n                                that.MoveMsg=[];\r\n                                that.overviewUrl='';\r\n                                that.chooseMac={\r\n                                    ch_class: \"\",\r\n                                    mac_id: \"\",\r\n                                    pump_id: \"\",\r\n                                    pump_name: \"\"\r\n                                };\r\n                            }\r\n                        }\r\n                    }\r\n\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求错误',err)\r\n                })\r\n        },\r\n        //获取图片的真实宽高，计算背景图区域\r\n        computePicWidth(img){\r\n            let imgW = img.width;\r\n            let imgH = img.height;\r\n            let p_w=$('#overview-pic').width();\r\n            let p_h=$('#overview-pic').height();\r\n            if(imgW<=p_w && imgH<=p_h){\r\n                this.origBlock.oriW=imgW;\r\n                this.origBlock.oriH=imgH;\r\n            }\r\n            else{\r\n                if(p_w/p_h>imgW/imgH){\r\n                    this.origBlock.oriW=parseInt(imgW*p_h/imgH);\r\n                    this.origBlock.oriH=p_h;\r\n                }\r\n                else{\r\n                    this.origBlock.oriW=p_w;\r\n                    this.origBlock.oriH=parseInt(imgH*p_w/imgW);\r\n                }\r\n            }\r\n            $('#move-block').css({'width':this.origBlock.oriW + 'px','height':this.origBlock.oriH + 'px'});\r\n        },\r\n        // 选择标题字号\r\n        pickFont(item){\r\n            this.overviewFont = item;\r\n            this.isShowFont = false;\r\n        },\r\n        //选择机泵\r\n        clickPump: function(pump,index){\r\n            $('.pump-li').eq(index).addClass('active').siblings().removeClass('active');\r\n            this.choosePump=pump;\r\n        },\r\n        //点击选择组织输入框，选中状态改变\r\n        addClass(e){\r\n            $('.search').removeClass('active');\r\n            e.currentTarget.classList.add('active')\r\n        },\r\n        openList(type,num){\r\n            for(let i=1;i<=4;i++){\r\n                if(i!==type){//将其他选框下拉选项关闭\r\n                    this[`isShow${i}`]= false;\r\n                }\r\n            }\r\n            this[`isShow${type}`]=!this[`isShow${type}`];\r\n            if(this[`isShow${type}`]){//如果组织下拉展开了，那么关闭机泵下拉选项\r\n                this.isShow.macIsShow=false;\r\n            }\r\n            if(num===2){//机泵总貌图\r\n                this.isShow.macIsShow=!this.isShow.macIsShow;\r\n            }\r\n        },\r\n        // 左侧机组拖到总貌图位置\r\n        /**\r\n         * type: 1组织总貌；2：机泵总貌\r\n         * **/\r\n        drop(e,type) {\r\n            console.log(e)\r\n            let msg = this.dragMsg,\r\n                x,\r\n                y;\r\n            if(msg){\r\n                x = e.offsetX - msg.left;\r\n                y = e.offsetY - msg.top;\r\n                if(msg.url){\r\n                    let obj = {};\r\n                    if(type===1){\r\n                        msg.pump.dragFlag = false;\r\n                        obj = {\r\n                            x : x,\r\n                            y : y,\r\n                            uid : msg.uid,\r\n                            mac_id: msg.mac_id,\r\n                            ch_class: msg.ch_class,\r\n                            pump_name: msg.pump_name,\r\n                            itemType: msg.itemType,\r\n                            width : msg.width,\r\n                            height : msg.height,\r\n                            text: msg.text,\r\n                            url: msg.url,\r\n                            oriCircleX:x + 15,\r\n                            oriCircleY:y + 55,\r\n                            circleD: 14,//小圆的直径\r\n                            circleX: 0,//小圆百分比的x\r\n                            circleY: 0,//小圆百分比的y\r\n                            moveCircleX:0,\r\n                            moveCircleY:0,\r\n                            moveX:0,\r\n                            moveY:0,\r\n                            style:{\r\n                                left: x + 'px',\r\n                                top: y + 'px',\r\n                                zIndex: 2,\r\n                            },\r\n                            circleStyle: {\r\n                                left: ( x + 15 ) + 'px',\r\n                                top: ( y + 55 ) + 'px',\r\n                                zIndex: 2,\r\n                            },\r\n                        };\r\n                    }\r\n                    else if(type===2){\r\n                        msg.pos.dragFlag = false;\r\n                        obj = {\r\n                            x : e.offsetX - msg.left,\r\n                            y : e.offsetY - msg.top,\r\n                            uid : msg.uid,\r\n                            itemType: msg.itemType,\r\n                            width : msg.width,\r\n                            height : msg.height,\r\n                            text: msg.text,\r\n                            url: msg.url,\r\n                            oriCircleX:x + 15,\r\n                            oriCircleY:y + 55,\r\n                            circleD: 14,//小圆的直径\r\n                            circleX: 0,//小圆百分比的x\r\n                            circleY: 0,//小圆百分比的y\r\n                            moveCircleX:0,\r\n                            moveCircleY:0,\r\n                            moveX:0,\r\n                            moveY:0,\r\n                            style:{\r\n                                left: x + 'px',\r\n                                top: y + 'px',\r\n                                zIndex: 2,\r\n                            },\r\n                            circleStyle: {\r\n                                left: ( x + 15 ) + 'px',\r\n                                top: ( y + 55 ) + 'px',\r\n                                zIndex: 2,\r\n                            },\r\n                        };\r\n                    }\r\n                    this.MoveMsg.push(obj);\r\n                }\r\n                else{\r\n                    let item = this.MoveMsg[msg.index];\r\n                    if(!(\"width\" in item)){\r\n                        item.width=$('.move-div').eq(msg.index).width();\r\n                        item.height=$('.move-div').eq(msg.index).height();\r\n                    }\r\n                    for (let i = 0, l = this.MoveMsg.length; i < l; i++) {\r\n                        if (i !== msg.index)\r\n                            this.MoveMsg[i].style.zIndex = 2;\r\n                    }\r\n                    let tag = e.target;\r\n\r\n                    if (x <= item.width && y <= item.height) {\r\n                        if ($(tag).hasClass('move-div')) {\r\n                            if (tag.style.left === item.style.left && tag.style.top === item.style.top) {\r\n                                x += item.x;\r\n                                y += item.y;\r\n                            } else {\r\n                                x += Number(tag.style.left.slice(0, -2)); y += Number(tag.style.top.slice(0, -2));\r\n                            }\r\n                        }\r\n                        if (tag.tagName === 'IMG' || tag.tagName === 'P') {\r\n                            let p = tag.parentNode;\r\n                            if (p.style.left === item.style.left && p.style.top === item.style.top) {\r\n                                x += (item.x + tag.offsetLeft);\r\n                                y += (item.y + tag.offsetTop);\r\n                            } else {\r\n                                x += (Number(p.style.left.slice(0, -2)) + tag.offsetLeft);\r\n                                y += (Number(p.style.top.slice(0, -2)) + tag.offsetTop);\r\n                            }\r\n                        }\r\n                    }\r\n                    item.x = x;\r\n                    item.y = y;\r\n                    item.px = (x/this.origBlock.oriW).toFixed(2);//相对于背景图的x位置\r\n                    item.py = (y/this.origBlock.oriH).toFixed(2);//相对于背景图的y位置\r\n                    item.style = {\r\n                        left: x + 'px',\r\n                        top: y + 'px',\r\n                        zIndex: 3,\r\n                    }\r\n                }\r\n            }\r\n        },\r\n        /**\r\n         * 按住ctrl键多选用于位置操作\r\n         * **/\r\n        addChooseItem(item,index){\r\n            if (window.event.ctrlKey) {//按住ctrl键\r\n                item.index = index;\r\n                this.chooseItemList.push(item);\r\n            }\r\n            else{\r\n                this.chooseItemList = [];\r\n            }\r\n            console.log(this.chooseItemList)\r\n        },\r\n        /**\r\n         * type: 1左对齐；2：水平居中，3：右对齐，4：顶部对齐，5：垂直居中，6：底部对齐\r\n         * **/\r\n        calcPos(type){\r\n            let referL,referM,referR,referT,referMH,referB,width,height,moveX0,moveY0;\r\n            let choosePos = this.chooseItemList;\r\n            let divList = document.getElementsByClassName('move-div');\r\n\r\n            if(!choosePos.length){\r\n                return\r\n            }\r\n            let referItem = choosePos[0];//第一个为基准\r\n            width = referItem.width;//第一个的框宽\r\n            height = referItem.height;//第一个的框高\r\n            referL = referItem.x;//第一个左边\r\n            referT = referItem.y;//第一个上边\r\n            if(typeof referItem.moveX !== \"undefined\"){//如果有移动，需要加上移动后的坐标\r\n                referL = referItem.x + referItem.moveX;\r\n                referT = referItem.y + referItem.moveY;\r\n            }\r\n            referMH = referT + height/2;//垂直的中间距离\r\n            referR = referL + width;//右侧的坐标\r\n            referM = referL + width / 2;//中间的坐标\r\n            referB = referT + height;//底部的坐标\r\n\r\n            let l = 0;\r\n            switch (type) {\r\n                case 0://左对齐\r\n                    for(let i=0;i<choosePos.length;i++){\r\n                        let item = choosePos[i];\r\n                        if(item.index != referItem.index){\r\n                            l = referL - choosePos[i].x;\r\n                            choosePos[i].moveX = l;\r\n                            if(typeof choosePos[i].moveY === \"undefined\"){\r\n                                choosePos[i].moveY  = 0;\r\n                            }\r\n                            for(let j=0;j<divList.length;j++){\r\n                                let index = divList[j].getAttribute('custom-id');\r\n                                if(index == item.index){\r\n                                    divList[j].style.transform = `translate(${l}px,${choosePos[i].moveY}px)`;\r\n                                }\r\n                            }\r\n                        }\r\n                    }\r\n                    break;\r\n                case 1://中对齐\r\n                    for(let i=0;i<choosePos.length;i++){\r\n                        let item = choosePos[i];\r\n                        if(item.index != referItem.index){\r\n                            l = referM - choosePos[i].width / 2 - choosePos[i].x;\r\n                            choosePos[i].moveX = l;\r\n                            if(typeof choosePos[i].moveY === \"undefined\"){\r\n                                choosePos[i].moveY  = 0;\r\n                            }\r\n                            divList[i].style.transform = `translate(${l}px,${choosePos[i].moveY}px)`\r\n                        }\r\n                    }\r\n                    break;\r\n                case 2://右对齐\r\n                    for(let i=0;i<choosePos.length;i++){\r\n                        let item = choosePos[i];\r\n                        if(item.index != referItem.index){\r\n                            l = referR - choosePos[i].width - choosePos[i].x;\r\n                            choosePos[i].moveX = l;\r\n                            if(typeof choosePos[i].moveY === \"undefined\"){\r\n                                choosePos[i].moveY  = 0;\r\n                            }\r\n                            divList[i].style.transform = `translate(${l}px,${choosePos[i].moveY}px)`\r\n                        }\r\n                    }\r\n                    break;\r\n                case 3://上对齐\r\n                    for(let i=0;i<choosePos.length;i++){\r\n                        let item = choosePos[i];\r\n                        if(item.index != referItem.index){\r\n                            l = referT - choosePos[i].y;\r\n                            choosePos[i].moveY = l;\r\n                            if(typeof choosePos[i].moveX === \"undefined\"){\r\n                                choosePos[i].moveX  = 0;\r\n                            }\r\n                            divList[i].style.transform = `translate(${choosePos[i].moveX}px,${l}px)`\r\n                        }\r\n                    }\r\n                    break;\r\n                case 4://垂直居中对齐\r\n                    for(let i=0;i<choosePos.length;i++){\r\n                        let item = choosePos[i];\r\n                        if(item.index != referItem.index){\r\n                            l = referMH - choosePos[i].height / 2 - choosePos[i].y;\r\n                            choosePos[i].moveY = l;\r\n                            if(typeof choosePos[i].moveX === \"undefined\"){\r\n                                choosePos[i].moveX  = 0;\r\n                            }\r\n                            divList[i].style.transform = `translate(${choosePos[i].moveX}px,${l}px)`\r\n                        }\r\n                    }\r\n                    break;\r\n                case 5://下对齐\r\n                    for(let i=0;i<choosePos.length;i++){\r\n                        let item = choosePos[i];\r\n                        if(item.index != referItem.index){\r\n                            l = referB - choosePos[i].height - choosePos[i].y;\r\n                            choosePos[i].moveY = l;\r\n                            if(typeof choosePos[i].moveX === \"undefined\"){\r\n                                choosePos[i].moveX  = 0;\r\n                            }\r\n                            divList[i].style.transform = `translate(${choosePos[i].moveX}px,${l}px)`\r\n                        }\r\n                    }\r\n                    break;\r\n            }\r\n        },\r\n        /*\r\n        * 拖拽按钮type:1向上，2向右上，3右，4，右下，5下，6左下，7，左，8左上\r\n        * */\r\n        onResize: function(x, y, width, height) {\r\n            this.moveDivMsg.width = width;\r\n            this.moveDivMsg.height = height;\r\n            this.moveDivMsg.x = x;\r\n            this.moveDivMsg.y = y;\r\n        },\r\n        onDrag: function(x, y) {\r\n            this.moveDivMsg.x = x;\r\n            this.moveDivMsg.y = y;\r\n        },\r\n        // 拖动左侧组件\r\n        /**\r\n         * type: 1组织总貌；2：机泵总貌\r\n         * **/\r\n        dragstart(item, index, e,type) {\r\n            if(item.dragFlag){\r\n                this.dragMsg = null;\r\n                let tag = e.target;\r\n                this.dragMsg = {\r\n                    url: item.url,\r\n                    uid: item.pos_type+\"_\"+item.pos_id,\r\n                    itemType: item.pos_type,\r\n                    left: e.offsetX,\r\n                    top: e.offsetY,\r\n                    width: 80,\r\n                    height: 60,\r\n                    text: item.pos_name,\r\n                    type: index + 1,\r\n                    inputStyle:{\r\n                        height: '30px'\r\n                    }\r\n                };\r\n                if(type===1){\r\n                    this.dragMsg.itemType=item.pump_type;\r\n                    this.dragMsg.uid = item.pump_id;\r\n                    this.dragMsg.pump = item;\r\n                    this.dragMsg.text = item.pump_name;\r\n                    this.dragMsg.mac_id = item.mac_id;\r\n                    this.dragMsg.ch_class = item.ch_class;\r\n                    this.dragMsg.pump_name = item.pump_name;\r\n                }\r\n                else if(type===2){\r\n                    this.dragMsg.pos = item;\r\n                }\r\n            }\r\n            else{\r\n                let name = '';\r\n                if(type===1){\r\n                    name = '机泵';\r\n                }\r\n                else if(type===2){\r\n                    name = '测点';\r\n                }\r\n                swal({\r\n                    title: \"提示\",\r\n                    text: `当前${name}已存在！`,\r\n                    button: \"确认\",\r\n                });\r\n            }\r\n        },\r\n        dragover() {},\r\n        onDragstop(item,index){\r\n            console.log(item)\r\n            item.moveX = this.moveDivMsg.x;\r\n            item.moveY = this.moveDivMsg.y;\r\n            console.log(this.MoveMsg[index])\r\n        },\r\n        onResizstop(item,index){\r\n            item.moveX = this.moveDivMsg.x;\r\n            item.moveY = this.moveDivMsg.y;\r\n            item.width=this.moveDivMsg.width;\r\n            item.height=this.moveDivMsg.height;\r\n            item.inputStyle = {//更新input的高\r\n                height:this.moveDivMsg.height/2 + 'px'\r\n            };\r\n            this.$forceUpdate();\r\n        },\r\n        onDragcircle(x,y){\r\n            this.moveDivMsg.circleX = x;\r\n            this.moveDivMsg.circleY = y;\r\n        },\r\n        onResizecircle(x, y, width, height){\r\n            this.moveDivMsg.circleD = width;\r\n            this.moveDivMsg.circleX = x;\r\n            this.moveDivMsg.circleY = y;\r\n        },\r\n        onResizstopcircle(item){\r\n            item.moveCircleX = this.moveDivMsg.circleX;\r\n            item.moveCircleY = this.moveDivMsg.circleY;\r\n            item.circleD=this.moveDivMsg.circleD;\r\n        },\r\n        onDragstopcircle(item){\r\n            item.moveCircleX = this.moveDivMsg.circleX;\r\n            item.moveCircleY = this.moveDivMsg.circleY;\r\n        },\r\n        // 总貌图内部拖动\r\n        /**\r\n         * type: 1组织总貌；2：机泵总貌\r\n         * **/\r\n        dragScreen(index, e, item, type) {\r\n            this.dragMsg = null;\r\n            let tag = e.currentTarget;\r\n            if(type===1){\r\n                this.dragMsg = {\r\n                    mac_id: item.mac_id,\r\n                    pump_id: item.pump_id,\r\n                    pump_name: item.pump_name,\r\n                    ch_class: item.ch_class,\r\n                    index: index,\r\n                    left: e.layerX,\r\n                    top: e.layerY,\r\n                    w:item.w,\r\n                    h:item.h,\r\n                    pump:item,\r\n                    inputStyle:item.inputStyle\r\n                }\r\n            }\r\n            else if(type===2){\r\n                this.dragMsg = {\r\n                    index: index,\r\n                    left: e.layerX,\r\n                    top: e.layerY,\r\n                    w:item.w,\r\n                    h:item.h,\r\n                    inputStyle:item.inputStyle\r\n                }\r\n            }\r\n            this.MoveMsg[index].px=(e.layerX/this.origBlock.oriW).toFixed(2);//相对于背景图的x位置\r\n            this.MoveMsg[index].py=(e.layerY/this.origBlock.oriH).toFixed(2);//相对于背景图的y位置\r\n        },\r\n        //禁止拖动背景图\r\n        stopDrag(){\r\n            this.dragMsg = null;\r\n            return false;\r\n        },\r\n        // 右键点击部件\r\n        rightClick(item, index, e) {\r\n            // console.log(item)\r\n            this.isRightClick = true;\r\n            this.rightClickMsg = {\r\n                index,\r\n                data: item,\r\n            };\r\n            this.$refs.rightMsg.style.left = item.x + e.layerX + 'px';\r\n            this.$refs.rightMsg.style.top = item.y + e.layerY + 'px';\r\n        },\r\n        // 删除组件\r\n        delModelPos() {\r\n            this.isTip = true;\r\n        },\r\n        // 确定删除组件\r\n        confirmTip(type) {\r\n            let rcm = this.rightClickMsg,\r\n                model = this.MoveMsg,\r\n                id,\r\n                list = [];\r\n\r\n            switch(type){\r\n                case 1:\r\n                    list = this.choosePumpList;\r\n                    break;\r\n                case 2:\r\n                    list = this.choosePosList;\r\n                    break;\r\n            }\r\n            if (rcm.index >= 0 && rcm.data) {\r\n                model.splice(rcm.index, 1);\r\n                list.forEach(item =>{\r\n                    if(type===1){\r\n                        id=item.pump_id;\r\n                    }\r\n                    else if(type===2){\r\n                        id=item.pos_type + '_' + item.pos_id;\r\n                    }\r\n                    if(id == rcm.data.uid){\r\n                        item.dragFlag = true;//删除了该机泵，该机泵需要变更为可拖动状态\r\n                    }\r\n                })\r\n            }\r\n            this.cancelClick();\r\n            this.isTip = false;\r\n        },\r\n        // 取消点击机组组态效果\r\n        cancelClick() {\r\n            this.isShow1 = false;\r\n            this.isShow2 = false;\r\n            this.isShow3 = false;\r\n            this.isShow4 = false;\r\n            this.isShow.macIsShow=false;//机组下拉框显示隐藏= false;\r\n            if(this.isRightClick) this.isRightClick = false;\r\n            if(this.isShowFont) this.isShowFont = false;\r\n        },\r\n        //  机组组态保存警告框关闭\r\n        closeAlarm() {\r\n            this.isTip = false;\r\n        },\r\n        // 关闭搜索\r\n        closeSearch(){\r\n            this.isShow1 = false;\r\n            this.isShow2 = false;\r\n            this.isShow3 = false;\r\n            this.isShow4 = false;\r\n            this.isShow.macIsShow=false;//机组下拉框显示隐藏= false;\r\n        },\r\n        //模拟点击\r\n        monidianji: function () {\r\n            document.getElementById('imgLocal').click()\r\n        },\r\n        /**测点排序方法**/\r\n        sortPositionsName:function(arr) {\r\n            var that = this;\r\n            var sortArr = [];\r\n            var pos_speed = [];//转速测点\r\n            var pos_vibrate= [];//振动测点\r\n            var pos_envelope = [];//包络测点\r\n            var pos_vibrateOrder = [];//振动阶次测点\r\n            var pos_envelopeOrder = [];//包络阶次测点\r\n            var pos_samp = [];//长采样测点\r\n            var pos_slosh = [];//晃度测点\r\n            var pos_overturn = [];//倾覆测点\r\n            var pos_inclinate = [];//倾角测点\r\n            var pos_synthetic = [];//合成倾角测点\r\n            var pos_temp = [];//合成倾角测点\r\n            var pos_process = [];//过程量测点\r\n            var pos_digital = [];//数字量测点\r\n            var pos_crafts = [];//工艺量测点\r\n            var pos_bolt = [];//螺栓测点\r\n            for(var m = 0,posLen = arr.length;m < posLen;m ++){\r\n                switch(Number(arr[m].pos_type)){\r\n                    case 1:\r\n                        pos_speed.push(arr[m]);\r\n                        break;\r\n                    case 3:\r\n                        pos_vibrate.push(arr[m]);\r\n                        break;\r\n                    case 4:\r\n                        pos_envelope.push(arr[m]);\r\n                        break;\r\n                    case 8:\r\n                        pos_vibrateOrder.push(arr[m]);\r\n                        break;\r\n                    case 9:\r\n                        pos_envelopeOrder.push(arr[m]);\r\n                        break;\r\n                    case 200:\r\n                        pos_samp.push(arr[m]);\r\n                        break;\r\n                    case 6:\r\n                        pos_slosh.push(arr[m]);\r\n                        break;\r\n                    case 11:\r\n                        pos_overturn.push(arr[m]);\r\n                        break;\r\n                    case 12:\r\n                        pos_inclinate.push(arr[m]);\r\n                        break;\r\n                    case 13:\r\n                        pos_synthetic.push(arr[m]);\r\n                        break;\r\n                    case 7:\r\n                        pos_temp.push(arr[m]);\r\n                        break;\r\n                    case 2:\r\n                        pos_process.push(arr[m]);\r\n                        break;\r\n                    case 5:\r\n                        pos_digital.push(arr[m]);\r\n                        break;\r\n                    case 10:\r\n                        pos_crafts.push(arr[m]);\r\n                        break;\r\n                    case 14:\r\n                        pos_bolt.push(arr[m]);\r\n                        break;\r\n                    default:\r\n                        pos_speed.push(arr[m]);\r\n                        break;\r\n                }\r\n            }\r\n\r\n            sortArr = pos_speed.concat(pos_vibrate,pos_envelope,pos_vibrateOrder,pos_envelopeOrder,pos_slosh,pos_samp,pos_overturn,pos_inclinate,pos_synthetic,pos_temp,pos_process,pos_digital,pos_crafts,pos_bolt);\r\n\r\n            return sortArr;\r\n        },\r\n    }\r\n};\r\nexport default overviewCoal;\r\n"],"sourceRoot":""}