rMsg > input {\n    height: 40px;\n    width: 100%;\n    border: none;\n    outline: none;\n    background: none;\n}\n.user-content .userType {\n    width: 100px;\n}\n.user-content .setUserType {\n    width: 100%;\n    height: 40px;\n    border: none;\n    background: none;\n    outline: none;\n    text-align: center;\n    cursor: pointer;\n}\n.user-content .alertBox {\n    position: absolute;\n    z-index: 110;\n    color: #000;\n    width: 280px;\n    max-height: 340px;\n    border-radius: 4px;\n    top: 100px;\n    left: 200px;\n    background: #e4e8f4;\n    box-shadow: 0 2px 10px 0 #617ad3;\n}\n.user-content .organizationTitle {\n    height: 40px;\n    line-height: 40px;\n    padding-left: 8px;\n    color: #fff;\n    background: #343C45;\n}\n.user-content .organizationList {\n    max-height: 260px;\n    font-size: 13px;\n    overflow-y: auto;\n    overflow-x: hidden;\n}\n.user-content .organizationList::-webkit-scrollbar {\n    display: none;\n}\n.user-content .itemList0, .user-content .itemList1 {\n    padding: 0 20px;\n    height: 28px;\n    line-height: 28px;\n    cursor: pointer;\n    user-select: none;\n    display: flex;\n    justify-content: space-around;\n    align-items: center;\n}\n.user-content .organizationName {\n    width: 50%;\n    overflow: hidden;\n    white-space: nowrap;\n    text-overflow: ellipsis;\n}\n.user-content .itemList0 > img, .user-content .itemList1 > img {\n    height: 16px;\n    width: 16px;\n}\n.user-content .itemList1 {\n    background: #E4E8F4;\n}\n.user-content .yesOrNo {\n    height: 40px;\n    line-height: 40px;\n    background: #E4E8F4;\n    text-align: center;\n    position: relative;\n    border-bottom-left-radius: 4px;\n    border-bottom-right-radius: 4px;\n    border-top: 1px solid #ccc;\n}\n.user-content .yesOrNo > div {\n    position: absolute;\n    right: 10px;\n}\n.user-content .yesOrNo > div > button, .user-content .yes, .user-content .no {\n    height: 24px;\n    line-height: 24px;\n    width: 50px;\n    border: none;\n    outline: none;\n    color: #fff;\n    font-size: 12px;\n    border-radius: 4px;\n}\n.user-content .yes {\n    background: #597DD2;\n    margin-right: 20px;\n}\n.user-content .no {\n    background: #798CA1;\n}\n.user-content .alert {\n    width: 100%;\n    height: 100%;\n    position: fixed;\n    top: 0;\n    left: 0;\n    z-index: 5;\n    background: rgba(0, 0, 0, 0.4);\n}\n.user-content .inputAlert {\n    width: 310px;\n    background: #E4E8F4;\n    border-radius: 4px;\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    margin-left: -155px;\n    margin-top: -125px;\n}\n.user-content .inputAlertTitle {\n    height: 40px;\n    line-height: 40px;\n    background: #343C45;\n    padding-left: 20px;\n    color: #fff;\n    border-top-left-radius: 4px;\n    border-top-right-radius: 4px;\n    font-size: 16px;\n}\n.user-content .inputAlertText {\n    padding: 30px 0;\n    font-size: 18px;\n    text-align: center;\n    border-bottom: 1px solid #ccc;\n}\n.user-content .inputAlertBtn {\n    height: 40px;\n    line-height: 40px;\n    text-align: center;\n}\n.user-content .cofirmInputAlert {\n    height: 24px;\n    line-height: 24px;\n    width: 50px;\n    border: none;\n    outline: none;\n    color: #fff;\n    font-size: 12px;\n    border-radius: 4px;\n    background: #597DD2;\n}\n.user-content .changeTip {\n    width: 310px;\n    height: 200px;\n    margin-left: -155px;\n    margin-top: -100px;\n    position: absolute;\n    left: 50%;\n    top: 50%;\n}\n.user-content .changeTipText {\n    padding: 30px 0;\n    font-size: 18px;\n    text-align: center;\n    background: #E4E8F4;\n}\n.user-content .group-check {\n    margin: 10px 0 10px 50px;\n}\n.user-content .group-check .group-child {\n      margin: 5px 0 5px 15px;\n      vertical-align: middle;\n}\n.user-content .alert-content-tree {\n    max-height: 220px;\n    overflow-y: auto;\n}\n.user-content .alert-content-tree::-webkit-scrollbar {\n      display: none;\n}\n.user-content .search-div {\n    margin: 5px;\n}\n.user-content .search-div .search-input {\n      width: 180px;\n      height: 32px;\n      line-height: 32px;\n      border: 1px solid #a9a9a9;\n      border-radius: 4px;\n      padding: 0 10px;\n      position: relative;\n      margin: 0 auto;\n}\n.user-content .search-div .search-input:focus {\n        border-color: #617ad3;\n        box-shadow: 0 0 4px 0 #617ad3;\n}\n"]}