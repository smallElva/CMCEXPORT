rch\">\r\n                  <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList2.length>0\">\r\n                      <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput2\" @input=\"searchFolder($event,2)\">\r\n                      <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(2)\"></i>\r\n                  </div>\r\n                  <div class=\"search-list-div\" v-show=\"searchFolderList2.length>0 && isShow2\" @click.stop>\r\n                      <ul class=\"search-result\">\r\n                          <li class=\"search-li\" v-for=\"(item,index) in sortFolderList2\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,2,1)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                      </ul>\r\n                  </div>\r\n              </div>\r\n              <!--三级组织-->\r\n              <div class=\"tree-search\">\r\n                  <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList3.length>0\">\r\n                      <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput3\" @input=\"searchFolder($event,3)\">\r\n                      <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(3)\"></i>\r\n                  </div>\r\n                  <div class=\"search-list-div\" v-show=\"searchFolderList3.length>0 && isShow3\" @click.stop>\r\n                      <ul class=\"search-result\">\r\n                          <li class=\"search-li\" v-for=\"(item,index) in sortFolderList3\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,3,1)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                      </ul>\r\n                  </div>\r\n              </div>\r\n              <!--四级组织-->\r\n              <div class=\"tree-search\">\r\n                  <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList4.length>0\">\r\n                      <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput4\" @input=\"searchFolder($event,4)\">\r\n                      <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(4)\"></i>\r\n                  </div>\r\n                  <div class=\"search-list-div\" v-show=\"searchFolderList4.length>0 && isShow4\" @click.stop>\r\n                      <ul class=\"search-result\">\r\n                          <li class=\"search-li\" v-for=\"(item,index) in sortFolderList4\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,4,1)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                      </ul>\r\n                  </div>\r\n              </div>\r\n              <div class=\"search-pump-div\" v-show=\"choosePumpList.length>0\">\r\n                  <ul class=\"pump-result\">\r\n                      <li class=\"pump-li\" v-for=\"(pump,index) in sortPumpList\" @click=\"clickPump(pump,index)\" draggable=\"true\" @dragstart=\"dragstart(pump, index, $event,1)\">\r\n                          <i class=\"iconfont icon-zhongzi-shebeimoxing\"></i>\r\n                          {{pump.pump_name}}\r\n                      </li>\r\n                  </ul>\r\n              </div>\r\n              <div id=\"imgForm\" class=\"upload\" v-show=\"chooseFolder.t_id!=''\">\r\n                  <button class=\"btn\" @click=\"monidianji\">上传图片</button>\r\n                  <input class=\"input-loc-img\"  name=\"imgLocal\" id=\"imgLocal\" type='file' accept=\"image/*\" @change=\"selectImg(1)\"/>\r\n              </div>\r\n              <div class=\"upload2\" v-show=\"overviewUrl!=''&& choosePumpList.length>0\">\r\n                  <button class=\"btn\" @click=\"keepOverview(1)\">保存</button>\r\n              </div>\r\n          </div>\r\n          <div class=\"overview-container\">\r\n              <div class=\"overview-pic\" id=\"overview-pic\">\r\n                  <div id=\"move-block\" class=\"move-block\" @drop.stop.prevent='drop($event,1)' @dragover.prevent='dragover' @click.stop='cancelClick' @contextmenu.prevent=''>\r\n                      <div class=\"move-div\" v-for=\"(item,index) in MoveMsg\" :style=\"item.style\" draggable=\"true\" @dragstart.stop=\"dragScreen(index, $event,item,1)\" @contextmenu.prevent='rightClick(item, index, $event)' @dblclick=\"getPumpInfo(item)\">\r\n                          <img :src=\"item.url\" :width=\"item.width\" :height=\"item.height\">\r\n                          <p>{{item.text}}</p>\r\n                      </div>\r\n                      <ul class='rightBtn' v-show='isRightClick' ref='rightMsg'>\r\n                          <li @click='delModelPos'>删除</li>\r\n                      </ul>\r\n                      <img :src=\"overviewUrl\" class=\"overview-bg\" ref='overviewBg' @dragstart.stop=\"stopDrag()\">\r\n                  </div>\r\n              </div>\r\n              <div class='alertBox alertTip' v-show='isTip'>\r\n                    <div class='alertTitle'>提示  <i class=\"iconfont icon-zhongzi-guanbi icon-close\"  @click='closeAlarm'></i></div>\r\n                    <div class='alertText'>\r\n                      请确认是否删除该部件？\r\n                    </div>\r\n                    <div class='yesOrNo'>\r\n                        <button class='yes' @click='confirmTip(1)'>确定</button>\r\n                        <button class='no' @click='closeAlarm'>取消</button>\r\n                    </div>\r\n              </div>\r\n\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {compareName} from \"@/utils/utils\";\r\nimport MyContentTitle from 'components/common/MyContentTitle'\r\nimport overview from 'components/common/overviewCommon.js'\r\nimport { ConvertPinyin } from 'assets/lib/pinyin.js'\r\nexport default {\r\n  name: \"Overview\",\r\n  mixins: [overview],\r\n  props:['parentToChild'],\r\n  data(){\r\n  