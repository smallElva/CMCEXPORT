eight: 58px;\n    line-height: 62px;\n    text-align: center;\n    border-right: 1px solid #597DD2;\n    background: #fff;\n}\n.monitor-content .btn-content {\n    position: absolute;\n    top: 0;\n    left: 120px;\n    height: 58px;\n    display: inline-block;\n    z-index: -1;\n    cursor: move;\n}\n.monitor-content .choose-btn {\n    height: 58px;\n    padding-left: 0px;\n    border-radius: 7px;\n    position: relative;\n}\n.monitor-content .choose-btn .choose-content {\n      padding-left: 10px;\n      position: absolute;\n      height: 420px;\n      width: 150px;\n      top: -477px;\n      left: 3px;\n      overflow-x: hidden;\n      overflow-y: auto;\n      border: 2px solid #6699FF;\n      color: #6699FF;\n      border-radius: 10px;\n      background: #fff;\n      z-index: 999;\n}\n.monitor-content .choose-btn .choose-content .choose-item {\n        height: 30px;\n        font-size: 16px;\n        text-align: left;\n}\n.monitor-content .choose-btn .choose-content .choose-item:hover label {\n          font-weight: bold;\n          cursor: pointer;\n}\n.monitor-content .choose-btn .choose-content .choose-item .choose-name {\n          display: inline-block;\n          line-height: 30px;\n          display: inline-block;\n          height: 30px;\n          margin-right: 5px;\n}\n.monitor-content .choose-btn .choose-content .choose-item .check-box {\n          position: relative;\n          display: inline-block;\n          height: 14px;\n          width: 14px;\n}\n.monitor-content .choose-btn .choose-content .choose-item .check-box input[type=\"checkbox\"] {\n            position: absolute;\n            left: 0;\n            top: 0;\n            visibility: hidden;\n}\n.monitor-content .choose-btn .choose-content .choose-item .check-box input[type=\"checkbox\"]:checked + label {\n            border-color: green;\n            border: 2px solid #6699FF;\n}\n.monitor-content .choose-btn .choose-content .choose-item .check-box input[type=\"checkbox\"]:checked + labe