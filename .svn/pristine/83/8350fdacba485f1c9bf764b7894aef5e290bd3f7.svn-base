#data-content .content .table_block .index_table > thead > tr > th {\n        font-size: 16px;\n}\n#data-content .content .table_block .index_table > tbody > tr {\n      height: 40px;\n      padding: 0;\n}\n#data-content .content .table_block .index_table > thead {\n      display: block;\n}\n#data-content .content .table_block .index_table > tbody {\n      display: block;\n      height: calc(100vh - 180px);\n      font-size: 14px;\n      /*height: 800px;*/\n      -ms-scroll-chaining: chained;\n      -ms-overflow-style: none;\n      -ms-content-zooming: zoom;\n      -ms-scroll-rails: none;\n      -ms-content-zoom-limit-min: 100%;\n      -ms-content-zoom-limit-max: 500%;\n      -ms-scroll-snap-type: proximity;\n      -ms-scroll-snap-points-x: snapList(100%, 200%, 300%, 400%, 500%);\n      -ms-overflow-style: none;\n      overflow-y: overlay;\n}\n#data-content .content .table_block .index_table > tbody > tr:nth-child(odd) {\n      background-color: #EFF1F6;\n}\n#data-content .content .table_block .index_table > tbody > tr:nth-child(even) {\n      background-color: #fff;\n}\n#data-content .content .table_block .index_table > tbody > tr:hover {\n      background-color: #abb7c7;\n}\n#data-content .content .table_block .index_table > thead > tr {\n      background-color: #C9DBEF;\n}\n#data-content .content .noData {\n    width: 100%;\n    height: 50vh;\n    position: absolute;\n    top: 50%;\n    left: 0;\n    text-align: center;\n    font-size: 20px;\n}\n"]}