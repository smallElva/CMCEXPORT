content .myHeader .dashDivSelectd {\n    position: absolute;\n    right: 150px;\n    top: 25px;\n    border-radius: 20px;\n    border: 1px solid #597DD2;\n    width: 75px;\n    height: 30px;\n    line-height: 30px;\n    box-sizing: border-box;\n    background: #597DD2;\n    cursor: pointer;\n    display: flex;\n    justify-content: space-between;\n    transition: .5s;\n    flex-flow: row-reverse;\n}\n#data-content .myHeader .dashDivSelectd .dashSpan {\n    border-radius: 50%;\n    border: 1px solid #fff;\n    background-color: #FFFFFF;\n    display: inline-block;\n    width: 28px;\n    height: 28px;\n    box-sizing: border-box;\n}\n#data-content .myHeader .dashDivSelectd .dashText {\n    display: inline-block;\n    padding-left: 6px;\n    height: 30px;\n    line-height: 28px;\n    color: #fff;\n}\n#data-content .myHeader .headerBtn {\n    position: absolute;\n    right: 20px;\n    top: 25px;\n    width: 100px;\n    height: 30px;\n    background: #597dd2;\n    border-radius: 4px;\n    outline: none;\n    border: none;\n    color: #fff;\n}\n#data-content .myHeader .headerBtn:hover {\n      background: #355fc3;\n}\n#data-content .myHeader .headerBtn:active {\n      background: #5177d0;\n}\n#data-content .myHeader .valBlock {\n    position: absolute;\n    top: 55px;\n    right: 20px;\n    width: 210px;\n    background: #fff;\n    border: 1px solid #597dd2;\n    box-shadow: 0px 13px 12px 1px rgba(215, 231, 254, 0.5);\n    border-radius: 8px;\n    z-index: 99;\n}\n#data-content .myHeader .valBlock .listBlock {\n      max-height: 480px;\n      overflow: auto;\n      border-top-right-radius: 8px;\n      border-top-left-radius: 8px;\n}\n#data-content .myHeader .valBlock .listBlock .valList {\n        display: flex;\n        padding: 0 10px;\n        height: 40px;\n        line-height: 40px;\n        justify-content: space-between;\n        cursor: pointer;\n}\n#data-content .myHeader .valBlock .listBlock .valList:nth-of-type(odd) {\n          background: #EFF1F6;\n}\n#data-c