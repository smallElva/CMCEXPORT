le.display = \"none\";\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 20px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 40px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.$bus.$emit(\"reloadoverview\"); //主动渲染总貌图\r\n        this.flag = !this.flag;\r\n      } else {\r\n        //缩小\r\n        homeHeader.style.display = \"block\";\r\n        aside.style.display = \"block\";\r\n        content.style.width = \"100px\"; //改变宽让宽自动渲染\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 90px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 120px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.$bus.$emit(\"reloadoverview\"); //主动渲染总貌图\r\n        this.flag = !this.flag;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-content-header {\r\n  /* width: 100%-15px;\r\n   */\r\n  box-sizing: border-box;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  background: #343c45;\r\n  font-size: 20px;\r\n  color: #fff;\r\n  padding-left: 15px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n","<template>\r\n  <div class=\"machine-content\">\r\n    <my-content-title :title='title'></my-content-title>\r\n    <button class=\"my-btn btn-active btn3\" @click=\"onShowListFile\"  v-show=\"treeBtn\">{{btnName}}</button>\r\n    <router-view id='myContentCenter' v-on:listenToChildEvent =\"childEvent\"></router-view>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyContentTitle from 'components/common/MyContentTitle';\r\nimport MachineTable from './MachineTable.vue'\r\nexport default {\r\n  name: \"machine\",\r\n  props:['parentToChild'],\r\n  data(){\r\n    return{\r\n      isAdd:false,\r\n      treeBtn:true,//切换树状图按钮是否显示\r\n      title:'机组设置',\r\n      btnName:'',\r\n      activeNum: \"01\",\r\n      parentMachineData:[\"根目录\"],\r\n      tNameTable:\"\",\r\n      chooseType:\"\",\r\n      chooseTid:\"\",\r\n      chooseTname:\"\",\r\n      toggleBtn: true,\r\n      obj:{\r\n        name:\"\",\r\n        chName: \"\",\r\n        flag:false,\r\n        type:null,//当前是哪个按钮\r\n      }\r\n    }\r\n  },\r\n  created(){\r\n    let btnText = '切换树状图';\r\n    if(this.obj.type==4){\r\n      btnText = '切换树状图';\r\n    }\r\n    else{\r\n      btnText = '站点设置';\r\n    }\r\n    this.btnName = this.$route.name == 'machinetree' ?'切换至表格':`${btnText}`\r\n    this.$emit('ievent',this.activeNum);\r\n    this.title = this.obj.chName;\r\n    if(!$.isEmptyObject(this.$route.params)){\r\n      let linkInfo = this.$route.params;\r\n      // this.flag = linkInfo.flag;\r\n      this.activeNum = linkInfo.activeNum;\r\n      this.chooseType = linkInfo.folderType;\r\n      this.chooseTid = linkInfo.folderId;\r\n      this.chooseTname = linkInfo.folderName;\r\n    }\r\n    else{\r\n      this.flag = 0;\r\n      this.chooseType = 1;\r\n      this.chooseTid =\"\";\r\n      this.chooseTname = \"\";\r\n    }\r\n  },\r\n  watch: {\r\n    obj: {\r\n      handler(val) {\r\n        this.title = val.chName;\r\n        if(val.chName=='机组设置'){\r\n          this.treeBtn = true;\r\n          this.btnName = '切换树状图';\r\n        }\r\n        if(val.type==4){\r\n          this.btnName = this.$route.name == 'machinetree' ?'切换至表格':'站点设置'\r\n        }\r\n        else{\r\n          this.btnName = this.$route.name == 'machinetree' ?'切换至表格':'切换树状图'\r\n        }\r\n      },\r\n      deep: true\r\n    },\r\n  },\r\n  methods: {\r\n    onShowListFile: function() {\r\n        if(this.$route.name == 'machinetree'){\r\n            this.$router.push({name: this.obj.name})\r\n            this.btnName = \"切换树状图\";\r\n        }else{\r\n            this.$router.push({name: \"machinetree\"})\r\n            this.btnName = \"切换至表格\";\r\n        }\r\n    },\r\n    childEvent(obj){\r\n      this.obj=obj;\r\n      this.isAdd = obj.flag;\r\n      this.title = obj.chName;\r\n      this.treeBtn = obj.treeBtn;\r\n    }\r\n\r\n  },\r\n  components:{\r\n    MachineTable,\r\n    MyContentTitle\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n  @import \"~assets/css/components/machine.scss\";\r\n  .machine-content{\r\n    position: relative;\r\n  }\r\n   .add{\r\n        display:none\r\n    }\r\n</style>\r\n","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\n    \"div\",\n    { staticClass: \"my-content-header\", on: { dblclick: _vm.dblclick } },\n    [_c(\"span\", { staticClass: \"page-title\" }, [_vm._v(_vm._s(_vm.title))])]\n  )\n}\nvar staticRenderFns = []\nrender._withStripped = true\n\nexport { render, staticRenderFns }","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\n    \"div\",\n    { staticClass: \"machine-content\" },\n    [\n      _c(\"my-content-title\", { attrs: { title: _vm.title } }),\n      _vm._v(\" \"),\n      _c(\n        \"button\",\n        {\n          directives: [\n            {\n              name: \"show\",\n              rawName: \"v-show\",\n              value: _vm.treeBtn,\n              expression: \"treeBtn\"\n            }\n          ],\n          staticClass: \"my-btn btn-active btn3\",\n          on: { click: _vm.onShowListFile }\n        },\n        [_vm._v(_vm._s(_vm.btnName))]\n      ),\n      _vm._v(\" \"),\n      _c(\"router-view\", {\n        attrs: { id: \"myContentCenter\" },\n        on: { listenToChildEvent: _vm.childEvent }\n      })\n    ],\n    1\n  )\n}\nvar staticRenderFns = []\nrender._withStripped = true\n\nexport { render, staticRenderFns }","import { render, staticRenderFns } from \"./MyContentTitle.vue?vue&type=template&id=e68802e2&scoped=true&\"\nimport script from \"./MyContentTitle.vue?vue&type=script&lang=js&\"\nexport * f