eft: 0;\n    font-size: 18px;\n    text-align: center;\n}\n.machine-content .create-btn {\n    position: absolute;\n    top: 30px;\n    right: 300px;\n    z-index: 999;\n    font-size: 16px;\n    color: #000;\n    width: 112px;\n    height: 32px;\n    line-height: 32px;\n    text-align: center;\n    border: 1px solid #fff;\n    border-radius: 3px;\n    background: #fff;\n}\n.machine-content .toggle-btn, .machine-content .keep-btn {\n    position: absolute;\n    top: 30px;\n    right: 60px;\n    font-size: 16px;\n    color: #fff;\n    width: 112px;\n    height: 32px;\n    line-height: 32px;\n    text-align: center;\n    border: 1px solid #fff;\n    border-radius: 3px;\n    background: transparent;\n}\n.machine-content .toggle-btn:hover, .machine-content .keep-btn:hover {\n      background: #fff;\n      color: #000;\n}\n.machine-content .keep-btn {\n    right: 180px;\n}\n.machine-content #tableContent {\n    height: calc(100% - 40px);\n}\n.machine-content .handsontable tr {\n    background: #888;\n}\n.machine-content .handsontable th {\n    background-color: #fff;\n    color: #000;\n    line-height: 40px;\n    font-size: 16px;\n}\n.machine-content .htCore thead th {\n    line-height: 40px;\n}\n.machine-content .handsontable thead th .relative {\n    padding: 0 4px;\n}\n.machine-content .htCore tbody th {\n    line-height: 31px;\n}\n.machine-content .handsontable td {\n    border-color: #888;\n    line-height: 30px;\n    text-align: center;\n    font-size: 14px;\n}\n.machine-content .handsontable .changeType {\n    background: transparent;\n    border-radius: 0;\n    border: none;\n    color: #bbb;\n    font-size: 9px;\n    line-height: 9px;\n    padding: 2px;\n    margin: 12px 1px 0 5px;\n    float: right;\n}\n.machine-content .handsontable .changeType:hover {\n    border: none;\n}\n.machine-content .handsontable .columnSorting.sortAction:hover {\n    text-decoration: none;\n    cursor: pointer;\n}\n.machine-content .handsontable thead th {\n    border-color: #888;\n}