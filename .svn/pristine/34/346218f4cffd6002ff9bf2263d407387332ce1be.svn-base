derData[folder.folderId]=folder.folderName;\r\n                                            this.initList.macData[folder.folderId]={};\r\n                                            this.initList.macData[folder.folderId][machine.machine_id] = machine.machine_me;\r\n                                            this.initList.posData[machine.machine_id]={};\r\n                                            machine.position_list.forEach(position =>{\r\n                                                this.initList.posData[machine.machine_id][position.position_id] = position.position_name;\r\n                                                this.initList.codeData[position.position_id + '_' + position.position_name]=getSpecVal(position.position_type,position.tt_pos_loc);\r\n                                            })\r\n                                        }\r\n                                    })\r\n                                }\r\n                            });\r\n                        }\r\n                        let result = getModBusClientInfo(res);\r\n                        if(!result){\r\n                            this.$emit(\"child-msg\",true)\r\n                        }else{\r\n                            //组织position结构\r\n                            [this.data] = result;\r\n                            //渲染表格\r\n                            this.$emit(\"child-msg\",false)\r\n                            this.renderTable()\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            }\r\n            else if(type==3){\r\n                this.$getApi.queryOPCClient()\r\n                    .then(res=>{\r\n                        console.log(res);\r\n                        if(res.tree_list.length>0){\r\n                            res.tree_list.forEach(folder =>{\r\n                                if(folder.machine_list.length>0){\r\n