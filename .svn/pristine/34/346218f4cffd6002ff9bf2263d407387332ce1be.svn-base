        '端口号',\r\n                '数据类型',\r\n                '起始地址',\r\n                '设备号',\r\n                '输入范围高',\r\n                '输入范围低',\r\n                '输出范围高',\r\n                '输出范围低',\r\n                '输入输出设置',\r\n                '是否有效'\r\n        ]\r\n    }\r\n    else if(type == 3){\r\n        header = [\r\n            '组织',\r\n            '机组',\r\n            '测点',\r\n            '有效值',\r\n            'OPC地址',\r\n            '输入输出设置',\r\n            '是否有效',\r\n        ]\r\n    }\r\n    return header\r\n}\r\n\r\n\r\n/* 表数据部分\r\n */\r\n\r\n//设置columns（公共）（组织名称、机组名称、测点名称、采集器名称、测点类型、通道名称、是否有效、是否报警）\r\nexport function setColumns(type,data){\r\n    let columns;\r\n    if(type == 1){ //modbusserver\r\n        columns=[\r\n            {\r\n                data: 'device_id',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: 'coil_start',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: 'coil_len',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: 'input_start',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: 'input_len',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: 'inputregister_start',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: 'inputregister_len',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: 'holdingregister_start',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: 'holdingregister_len',\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data:\"state_val\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].state_sources\r\n            },\r\n        ]\r\n    }\r\n    else if(type == 2){//modebus client\r\n        columns = [\r\n            {\r\n                data: \"tree_name\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].t_name_sources,\r\n                readOnly:true\r\n            },\r\n            {\r\n                data: \"machine_name\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].mac_me_sources,\r\n                readOnly: true\r\n            },\r\n            {\r\n                data: \"position_name\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].pos_name_sources,\r\n                readOnly: true\r\n            },\r\n            {\r\n                data: 'code_val',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].code_name_sources,\r\n                readOnly: true\r\n            },\r\n            {\r\n                data: \"ip\",\r\n            },\r\n            {data: \"port\"},\r\n            {\r\n                data: \"data_type_val\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].data_type_sources,\r\n            },\r\n            {data: \"address\",},\r\n            {data: \"number\"},\r\n            {   data: \"input_h\",\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: \"input_l\",\r\n                type: 'numeric'\r\n            },\r\n            {   data: \"output_h\",\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: \"output_l\",\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data:\"IODevices_val\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].IODevices_sources,\r\n                readOnly: true\r\n            },\r\n            {\r\n                data:\"state_val\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].state_sources\r\n            },\r\n        ]\r\n    }\r\n    else if(type==3){\r\n        columns = [\r\n            {\r\n                data: \"tree_name\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].t_name_sources,\r\n                readOnly:true\r\n            },\r\n            {\r\n                data: \"machine_name\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].mac_me_sources,\r\n                readOnly: true\r\n            },\r\n            {\r\n                data: \"position_name\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].pos_name_sources,\r\n                readOnly: true\r\n            },\r\n            {\r\n                data: 'code_val',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].code_name_sources,\r\n                readOnly: true\r\n            },\r\n            {\r\n                data: \"address\",\r\n            },\r\n            {\r\n                data:\"IODevices_val\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].IODevices_sources,\r\n                readOnly: true\r\n            },\r\n            {\r\n                data:\"state_val\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].state_sources\r\n            },\r\n        ]\r\n    }\r\n    return columns\r\n}\r\n\r\n\r\n","import getApi from 'network/getApi'\r\nimport hdtable from 'common/hdtable.js'\r\nimport {createServer,changeServer,changeModBusClient,createModClient,createOpc,changeOpcClient} from './data_info.js'\r\nimport {setHeaders,setColumns} from './data_columns.js'\r\nimport {setChangeCellResult} from 'utils/utils'\r\nimport {getCodeName} from \"@/components/content/datas/data/table/js/data_methods\";\r\n\r\n\r\n\r\nexport default function machdtable(el,property){\r\n    let data = property.getData()\r\n    //创建表格属性的实例\r\n    let tableDataIns= new tableData(property)\r\n    // 组装振动表格数据\r\n    let vibTable ={\r\n\r\n        // 表格右键菜单\r\n        contextMenu:tableDataIns.contextMenu(),\r\n        // 表格显示的数据\r\n        data:data,\r\n        //表格的列\r\n        colHeaders:tableDataIns.colHeaders(),\r\n        //表格的列头自定义\r\n        // nestedHeaders:tableDataIns.nestedHeaders(),\r\n        //数据渲染renderData\r\n        columns:tableDataIns.columns(),\r\n\r\n        //单元格属性设置\r\n        cells:tableDataIns.cells(),\r\n        //新建行\r\n        afterCreateRow:tableDataIns.afterCreateRow(),\r\n\r\n        beforeChange: tableDataIns.beforeChange(),\r\n\r\n        afterChange: tableDataIns.afterChange(),\r\n        //移除行事件\r\n        beforeRemoveRow:tableDataIns.beforeRemoveRow(),\r\n\r\n        afterRemoveRow:tableDataIns.afterRemoveRow(),\r\n    }\r\n    let type = property.type;\r\n    if(type==1){\r\n        vibTable.nestedHeaders=tableDataIns.nestedHeaders();\r\n    }\r\n    return hdtable(el,vibTable)\r\n}\r\n\r\n\r\n//表格的方法\r\nclass tableData{\r\n    constructor(property){\r\n        this.type= property.type\r\n        this.tids= property.tids\r\n        this.getData= property.getData\r\n        this.getHot= property.getHot\r\n        this.initList= property.initList\r\n        this.changeList= property.changeList;\r\n        this.router = property.router;\r\n        this.store = property.store\r\n    }\r\n    contextMenu(){\r\n        let that = this;\r\n        let userType = parseInt(sessionStorage.getItem('userType'));\r\n        return {\r\n            items:{\r\n                \"row_above\":{\r\n                    name:\"插入新行\"\r\n                },\r\n                \"remove_row\": {\r\n                    name: '删除这行',\r\n                    callback(key, options) {\r\n                        swal({\r\n                            title: \"警告\",\r\n                            text: \"当前信息一经删除，该数据也将不保留。确定删除吗？\",\r\n                            buttons: {\r\n                                sure: \"确认\",\r\n                                cancel: \"取消\",\r\n                            },\r\n                            dangerMode: true,\r\n                        })\r\n                            .then((willDelete) => {\r\n                                if (willDelete) {\r\n                                    let index = options[0].start.row;\r\n                                    let amount = parseInt(options[0].end.row-options[0].start.row) + 1;\r\n                                    that.getHot().alter(\"remove_row\",index,amount);\r\n                                } else {\r\n                                    return false;\r\n                                }\r\n                            });\r\n                    },\r\n                },\r\n            }\r\n        }\r\n    }\r\n    /* 表格的数据渲染 */\r\n    colHeaders(){\r\n        if(this.type == 1){\r\n            return setHeaders(this.type)[1]\r\n        }\r\n        else{\r\n            return setHeaders(this.type)\r\n        }\r\n\r\n    }\r\n    nestedHeaders(){\r\n        return setHeaders(this.type)\r\n    }\r\n    columns(){\r\n        return setColumns(this.type,this.getData())\r\n    }\r\n    /* 表格的方法渲染 */\r\n    //振动的单元格渲染\r\n    cells(){\r\n        let that = this\r\n        return function (row, col, prop){\r\n            let data =that.getData()\r\n            if(data.length == 0) return\r\n            let type = that.type\r\n            let cellProperties = this;\r\n\r\n            if(type==2 && data[row].operate == 1 && (col == 0 || col == 1 || col == 2 || col == 3 || prop=='IODevices_val')){\r\n                cellProperties.readOnly  = false;\r\n            }\r\n            return cellProperties;\r\n        }\r\n    }\r\n\r\n    //创建测点之后的方法(共用)\r\n    afterCreateRow(){\r\n        let that =this\r\n        return function(index,amount,source){//添加行事件\r\n            console.log('afterCreateRow');\r\n            let data = that.getData();\r\n            let changeClientData=that.changeList.changeClientData;\r\n            let addClientData=that.changeList.addClientData;\r\n            if(that.type==1){\r\n                let server = createServer();\r\n                changeClientData[index]=server;\r\n                data[index]= server;\r\n            }\r\n            else if(that.type==2){\r\n                let modClient = createModClient(that.type,that.initList,data.length);\r\n                changeClientData[index]=modClient;\r\n                data[index] = modClient;\r\n            }\r\n        }\r\n    }\r\n\r\n    //删除测点之前的方法,将要删除的方法存到deleteVData（共用）\r\n    beforeRemoveRow(){\r\n        let that =this;\r\n        return function(index,amount){\r\n            let deleteData = that.changeList.deleteData;\r\n            if(index!=null){\r\n                for(let i=0;i<amount;i++){\r\n                    let cellInfo = that.getPositionByIndex(index+i);\r\n                    if(cellInfo.operate===0){\r\n                        deleteData.push(cellInfo.id);\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n\r\n    }\r\n    //删除测点方法 发送请求（共用）\r\n    afterRemoveRow(){\r\n        let that =this;\r\n        return function(){\r\n            let deleteData = that.changeList.deleteData\r\n            console.log('afterRemoveRow');\r\n            if(deleteData.length==0){\r\n                swal({\r\n                    title: \"提示\",\r\n                    text: \"删除信息成功\",\r\n                    button: \"确认\",\r\n                })\r\n                    .then((value) => {\r\n                        that.router.go(0)\r\n                    });\r\n            }\r\n            if(deleteData.length>0 && that.type===1) {\r\n                getApi.deleteModBusServer(deleteData)\r\n                    .then(res=>{\r\n                        deleteData=[];\r\n                        if (res.msg == 0) {\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: \"删除信息成功\",\r\n                                button: \"确认\",\r\n                            })\r\n                                .then((value) => {\r\n                                    that.router.go(0)\r\n                                });\r\n                        } else if (res.msg == 1) {\r\n                            console.log('参数为空！')\r\n                        } else if (res.msg == 2) {\r\n                            console.log('后台运行错误！')\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            }\r\n            if(deleteData.length>0 && that.type===2) {\r\n                getApi.deleteModBusClient(deleteData)\r\n                    .then(res=>{\r\n                        deleteData=[];\r\n                        if (res.msg == 0) {\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: \"删除信息成功\",\r\n                                button: \"确认\",\r\n                            })\r\n                                .then((value) => {\r\n                                    that.router.go(0)\r\n                                });\r\n                        } else if (res.msg == 1) {\r\n                            console.log('参数为空！')\r\n                        } else if (res.msg == 2) {\r\n                            console.log('后台运行错误！')\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            }\r\n        }\r\n    }\r\n\r\n    //改变数据的时候拼装数据结构（共用）\r\n    beforeChange(){\r\n        let that =this;\r\n        return function (changes,source){\r\n            let change_index;\r\n            let change_prop;\r\n            let change_oldVal;\r\n            let change_newVal;\r\n            let flag;\r\n            let hot = that.getHot();\r\n            let data = that.getData();\r\n            let IP = \"^((25[0-5]|2[0-4]\\\\d|[1]{1}\\\\d{1}\\\\d{1}|[1-9]{1}\\\\d{1}|\\\\d{1})($|(?!\\\\.$)\\\\.)){4}$\";//IP正则\r\n            let pat=new RegExp(IP);\r\n            let num = /^\\d+$/;//非负整数正则\r\n            let patNum =new RegExp(num);\r\n            let floatNum = /^[1-9]\\d*\\.\\d*|0\\.\\d*[1-9]\\d*|0?\\.0+|0$/;//非负浮点数正则\r\n            let patFloatNum = new RegExp(floatNum);\r\n            //创建更改组织、机组、采集器、通道、任务的联动实例\r\n            //地址至少是正整数（含0），设备号1-255，范围都是浮点型;地址和长度都先正整型（含0）好了;地址和长度你可以默认为0；端口是1-65535\r\n            for(let i= 0;i<changes.length;i++){\r\n                let physicalRow = hot.toPhysicalRow(changes[i][0]);\r\n                change_index = changes[i][0];\r\n                change_prop = changes[i][1];\r\n                change_oldVal = changes[i][2];\r\n                change_newVal = changes[i][3];\r\n                if(change_oldVal == change_newVal ){\r\n                    continue\r\n                }\r\n                if(change_newVal==\"\"){\r\n                    return false;\r\n                }\r\n                let cellInfo=that.getPositionByIndex(change_index,that.type);\r\n                //modbus server\r\n                if(change_prop==\"address\" || change_prop==\"coil_start\" || change_prop==\"coil_len\" || change_prop==\"input_start\" || change_prop==\"input_len\" || change_prop==\"inputregister_start\" || change_prop==\"inputregister_len\" || change_prop==\"holdingregister_start\" ||change_prop==\"holdingregister_len\"){\r\n                    if(!patNum.test(change_newVal)){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '请输入非负整数',\r\n                            button: \"确认\",\r\n                        });\r\n                        return false;\r\n                    }\r\n                }\r\n                //modbus client\r\n                if (change_prop == \"tree_name\") {  //组织\r\n                    flag = setChangeCellResult(cellInfo.t_name_sources,cellInfo.t_name_sources_key,change_newVal,change_index);\r\n                    if(flag === false) { return false}\r\n                    else{\r\n                        cellInfo.tree_id = flag;\r\n                        that.update_t_name(cellInfo, change_newVal,flag);\r\n                    }\r\n                }\r\n                if (change_prop == \"machine_name\") { //机组\r\n                    flag = setChangeCellResult(cellInfo.mac_me_sources,cellInfo.mac_me_sources_key,change_newVal,change_index);\r\n                    if(flag === false) { return false}\r\n                    else{\r\n                        cellInfo.machine_id = flag;\r\n                        that.update_macName(cellInfo, change_newVal,flag);\r\n                    }\r\n                }\r\n                if(change_prop==\"position_name\"){//测点\r\n                    flag = setChangeCellResult(cellInfo.pos_name_sources,cellInfo.pos_name_sources_key,change_newVal,change_index);\r\n                    if(flag === false) { return false}\r\n                    else{\r\n                        cellInfo.position_id = flag;\r\n                        that.update_positionName(cellInfo, change_newVal,flag);\r\n                    }\r\n                }\r\n                if(change_prop==\"code_val\"){//特征值\r\n                    flag = setChangeCellResult(cellInfo.code_name_sources,cellInfo.code_name_sources_key,change_newVal,change_index);\r\n                    if(flag === false) { return false}\r\n                    else{\r\n                        cellInfo.code = flag;\r\n                    }\r\n                }\r\n                if(change_prop==\"data_type_val\"){//数据类型\r\n                    flag = setChangeCellResult(cellInfo.data_type_sources,cellInfo.data_type_sources_key,change_newVal,change_index);\r\n                    if(flag === false) { return false}\r\n                    else{\r\n                        cellInfo.data_type = flag;\r\n                    }\r\n                }\r\n                if(change_prop==\"IODevices_val\"){//数据输入输出类型\r\n                    flag = setChangeCellResult(cellInfo.IODevices_sources,cellInfo.IODevices_sources_key,change_newVal,change_index);\r\n                    if(flag === false) { return false}\r\n                    else{\r\n                        cellInfo.IODevices = flag;\r\n                    }\r\n                }\r\n                if(change_prop==\"state_val\"){//状态\r\n                    flag = setChangeCellResult(cellInfo.state_sources,cellInfo.state_sources_key,change_newVal,change_index);\r\n                    if(flag === false) { return false}\r\n                    else{\r\n                        cellInfo.state = flag;\r\n                    }\r\n                }\r\n                //验证端口号1-65535\r\n                if(change_prop==\"port\"){\r\n                    let newNum = change_newVal;\r\n                    if(newNum<1 || newNum>65535 || !patFloatNum.test(newNum)){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '请输入1~65535的整数',\r\n                            button: \"确认\",\r\n                        });\r\n                        return false;\r\n                    }\r\n                    else{\r\n                        change_newVal = newNum;\r\n                        cellInfo.port = newNum;\r\n                    }\r\n                }\r\n                //验证设备号1-255浮点数\r\n                if(change_prop==\"number\" || change_prop==\"device_id\"){\r\n                    let newNum = change_newVal;\r\n                    if(newNum<1 || newNum>255 || !patFloatNum.test(newNum)){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '请输入1~255的整数',\r\n                            button: \"确认\",\r\n                        });\r\n                        return false;\r\n                    }\r\n                    else{\r\n                        change_newVal = newNum;\r\n                        if(cellInfo.number){\r\n                            cellInfo.number = newNum;\r\n                        }\r\n                        else if(cellInfo.device_id){\r\n                            cellInfo.device_id = newNum;\r\n                        }\r\n                    }\r\n                }\r\n                //验证IP号\r\n                if(change_prop == \"ip\"){\r\n                    if(!pat.test(change_newVal)){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: 'IP格式错误',\r\n                            button: \"确认\",\r\n                        });\r\n                        return false;\r\n                    }\r\n                }\r\n                //\r\n                if(change_prop==\"input_h\" || change_prop==\"input_l\" || change_prop==\"output_h\" || change_prop==\"output_l\"){\r\n                    if(!patNum.test(change_newVal)){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '请输入非负整数',\r\n                            button: \"确认\",\r\n                        });\r\n                        return false;\r\n                    }\r\n                    else{\r\n                    if(change_prop == \"input_l\"){\r\n                            if(Number(change_newVal)>=cellInfo.input_h){\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '输入范围高应该大于输入范围低！',\r\n                                    button: \"确认\",\r\n                                });\r\n                                return false;\r\n                            }\r\n                        }\r\n                        else if(change_prop == \"input_h\"){\r\n                            if(Number(change_newVal)<=cellInfo.input_l){\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '输入范围高应该大于输入范围低！',\r\n                                    button: \"确认\",\r\n                                });\r\n                                return false;\r\n                            }\r\n                        }\r\n                        else if(change_prop == \"output_l\"){\r\n                            if(Number(change_newVal)>=cellInfo.output_h){\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '输出范围高应该大于输出范围低！',\r\n                                    button: \"确认\",\r\n                                });\r\n                                return false;\r\n                            }\r\n                        }\r\n                        else if(change_prop == \"output_h\"){\r\n                            if(Number(change_newVal)<=cellInfo.output_l){\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '输出范围高应该大于输出范围低！',\r\n                                    button: \"确认\",\r\n                                });\r\n                                return false;\r\n                            }\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n\r\n        }\r\n    }\r\n\r\n    //改变数据后的方法 （共用）\r\n    afterChange(){\r\n        let that =this\r\n        return function (changes,source){//修改数据事件\r\n            console.log('afterChange');\r\n            let changeClientData=that.changeList.changeClientData;\r\n            if(changes){\r\n                changes.forEach(change=>{\r\n                    let hot = that.getHot();\r\n                    let physicalRow = hot.toPhysicalRow(change[0]);\r\n                    if(that.type==1) {\r\n                        let server=that.getPositionByIndex(change[0],1);\r\n                        let sendServer = changeServer(that.type,server,change[0]);\r\n                        changeClientData[physicalRow]=sendServer;\r\n                    }\r\n                    if(that.type==2) {\r\n                        if(change[2]!=change[3]){\r\n                            let modBusClient=that.getPositionByIndex(change[0],2);\r\n                            let sendModBusClient = changeModBusClient(that.type,modBusClient,change[0]);\r\n                            changeClientData[physicalRow]=sendModBusClient;\r\n                        }\r\n                    }\r\n                    if(that.type==3){\r\n                        let modBusClient=that.getPositionByIndex(change[0],3);\r\n                        let sendModBusClient = changeOpcClient(that.type,modBusClient,change[0]);\r\n                        changeClientData[physicalRow]=sendModBusClient;\r\n                    }\r\n                })\r\n\r\n            }\r\n\r\n        }\r\n    }\r\n\r\n    /* 根据索引行获取真实数据和物理行 (共用)*/\r\n    getPositionByIndex(index,type){\r\n        let hot = this.getHot()\r\n        let data = this.getData()\r\n        let physicalRow = hot.toPhysicalRow(index);\r\n        let info = data[physicalRow];\r\n\r\n        return info\r\n    }\r\n    /* 改变组织 */\r\n    update_t_name(cellInfo, t_name,t_id) {\r\n        cellInfo.tree_name = t_name;\r\n        this.update_macName_source(cellInfo,t_name, t_id)\r\n    }\r\n    /* 更改机组源 */\r\n    update_macName_source(cellInfo,t_name, t_id) {\r\n        let m_mac_source =this.initList.macData[t_id];\r\n        let mac_source_key = Object.keys(m_mac_source);\r\n        let mac_source = Object.values(m_mac_source);\r\n        let mac_me = mac_source[0];\r\n        let mac_id = mac_source_key[0];\r\n        cellInfo.mac_me_sources = mac_source;\r\n        cellInfo.mac_me_sources_key = mac_source_key;\r\n\r\n        this.update_macName(cellInfo, mac_me,mac_id);\r\n    }\r\n    /* 更改机组 */\r\n    update_macName(cellInfo, mac_me, mac_id) {\r\n        cellInfo.machine_id = mac_id;\r\n        cellInfo.machine_name = mac_me;\r\n        //更改测点源\r\n        this.update_position_source(cellInfo, mac_me,mac_id)\r\n    }\r\n    /*更改测点源*/\r\n    update_position_source(cellInfo,mac_me,mac_id){\r\n        let p_pos_source = this.initList.posData[mac_id];\r\n        let pos_source_key = Object.keys(p_pos_source);\r\n        let pos_source = Object.values(p_pos_source);\r\n        let pos_me = pos_source[0];\r\n        let pos_id = pos_source_key[0];\r\n        cellInfo.pos_name_sources = pos_source;\r\n        cellInfo.pos_name_sources_key = pos_source_key;\r\n        //更改测点名\r\n        this.update_positionName(cellInfo, pos_me,pos_id);\r\n    }\r\n    /*更改测点*/\r\n    update_positionName(cellInfo,pos_me,pos_id){\r\n        cellInfo.position_id = pos_id;\r\n        cellInfo.position_name = pos_me;\r\n        //更改特征值源\r\n        this.update_code_source(cellInfo, pos_me,pos_id)\r\n    }\r\n    update_code_source(cellInfo, pos_me,pos_id){\r\n        let c_code_source ={};\r\n        let code_list = this.initList.codeData[pos_id +'_'+ pos_me];\r\n        code_list.forEach(code =>{\r\n            c_code_source[code] = getCodeName(code);\r\n        });\r\n        let code_source_key = Object.keys(c_code_source);\r\n        let code_source = Object.values(c_code_source);\r\n        cellInfo.code_name_sources = code_source;\r\n        cellInfo.code_name_sources_key = code_source_key;\r\n        cellInfo.code_val = code_source[0];\r\n        cellInfo.code = code_source_key[0];\r\n    }\r\n    deepClone(obj){\r\n        return JSON.parse(JSON.stringify(obj))\r\n    }\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n","import {getCodeName} from './data_methods.js'\r\n\r\n/* 固定参数 */\r\n\r\nlet state_source={\"0\":\"禁用\", \"1\":\"启用\"};//是否有效\r\nlet IODevices_source={'1':'输入','2':'输出'};//输入输出设置\r\nlet data_type_source={\"0\":\"float型\",\"1\":\"short型\"};//数据类型\r\n\r\nexport function getServerInfo(res){\r\n    let data = []; //机组集合\r\n    res.ModBusServer.forEach(server =>{\r\n        let ModBusServer = {\r\n            operate:0,\r\n            id:server.id,\r\n            device_id:server.device_id,\r\n            coil_start:server.coil_start,\r\n            coil_len:server.coil_len,\r\n            input_start:server.input_start,\r\n            input_len:server.input_len,\r\n            inputregister_start:server.inputregister_start,\r\n            inputregister_len:server.inputregister_len,\r\n            holdingregister_start:server.holdingregister_start,\r\n            holdingregister_len:server.holdingregister_len,\r\n            state_val: state_source[server.state],\r\n            state:server.state,\r\n            state_sources:Object.values(state_source),\r\n            state_sources_key:Object.keys(state_source),\r\n        }\r\n        data.push(ModBusServer);\r\n    })\r\n    console.log(data)\r\n    return[data]\r\n}\r\n\r\nexport function createServer(){\r\n\r\n    let ModBusServer = {\r\n        operate:1,\r\n        id:0,\r\n        device_id:1,\r\n        coil_start:0,\r\n        coil_len:0,\r\n        input_start:0,\r\n        input_len:0,\r\n        inputregister_start:0,\r\n        inputregister_len:0,\r\n        holdingregister_start:0,\r\n        holdingregister_len:0,\r\n        state_val: state_source[\"1\"],\r\n        state:\"1\",\r\n        state_sources:Object.values(state_source),\r\n        state_sources_key:Object.keys(state_source),\r\n    }\r\n    return ModBusServer\r\n}\r\n\r\nexport function changeServer(type,oldData,index){\r\n    let ModBusServer = {\r\n        id:oldData.id,\r\n        device_id:oldData.device_id,\r\n        coil_start:oldData.coil_start,\r\n        coil_len:oldData.coil_len,\r\n        input_start:oldData.input_start,\r\n        input_len:oldData.input_len,\r\n        inputregister_start:oldData.inputregister_start,\r\n        inputregister_len:oldData.inputregister_len,\r\n        holdingregister_start:oldData.holdingregister_start,\r\n        holdingregister_len:oldData.holdingregister_len,\r\n        state:oldData.state,\r\n    }\r\n    return ModBusServer\r\n}\r\n\r\nexport function getModBusClientInfo(res) {\r\n    let data = [];\r\n    let modeBusClient={};\r\n\r\n    res.modBusClient.forEach(modItem =>{\r\n        modeBusClient={\r\n            operate:0,\r\n            address: modItem.address,\r\n            code_val: getCodeName(modItem.code),\r\n            code: modItem.code,\r\n            code_name_sources:[],\r\n            code_name_sources_key:[],\r\n            data_type_val: data_type_source[modItem.data_type],\r\n            data_type:modItem.data_type,\r\n            data_type_sources:Object.values(data_type_source),\r\n            data_type_sources_key:Object.keys(data_type_source),\r\n            IODevices_val: IODevices_source[modItem.iODevices],\r\n            IODevices: modItem.iODevices,\r\n            IODevices_sources:Object.values(IODevices_source),\r\n            IODevices_sources_key:Object.keys(IODevices_source),\r\n            id: modItem.id,\r\n            input_h: modItem.input_h,\r\n            input_l: modItem.input_l,\r\n            ip: modItem.ip,\r\n            machine_id: modItem.machine_id,\r\n            machine_name:  modItem.machine_name,\r\n            mac_me_sources: [],\r\n            mac_me_sources_key:[],\r\n            number: modItem.number,\r\n            output_h: modItem.output_h,\r\n            output_l: modItem.output_l,\r\n            port: modItem.port,\r\n            position_id: modItem.position_id,\r\n            position_name: modItem.position_name,\r\n            pos_name_sources:[],\r\n            pos_name_sources_key:[],\r\n            state_val: state_source[modItem.state],\r\n            state:modItem.state,\r\n            state_sources:Object.values(state_source),\r\n            state_sources_key:Object.keys(state_source),\r\n            tree_id: modItem.tree_id,\r\n            tree_name: modItem.tree_name,\r\n            t_name_sources:[],\r\n            t_name_sources_key:[]\r\n        }\r\n        data.push(modeBusClient);\r\n    })\r\n    console.log(data)\r\n    return[data]\r\n}\r\nexport function createModClient(type,initList,index) {\r\n    let t_tree_source = initList.folderData;\r\n    let macData = initList.macData;\r\n    let posData = initList.posData;\r\n    let codeData = initList.codeData;\r\n\r\n    let tree_source_key = Object.keys(t_tree_source);\r\n    let tree_source = Object.values(t_tree_source);\r\n\r\n    let m_mac_source = macData[tree_source_key[0]];\r\n    let mac_source_key = Object.keys(m_mac_source);\r\n    let mac_source = Object.values(m_mac_source);\r\n\r\n    let p_pos_source = posData[mac_source_key[0]];\r\n    let pos_source_key = Object.keys(p_pos_source);\r\n    let pos_source = Object.values(p_pos_source);\r\n\r\n    let c_code_source ={};\r\n    let code_list = codeData[pos_source_key[0] +'_'+ pos_source[0]];\r\n    code_list.forEach(code =>{\r\n        c_code_source[code] = getCodeName(code);\r\n    });\r\n    let code_source_key = Object.keys(c_code_source);\r\n    let code_source = Object.values(c_code_source);\r\n    let modeBusClient={\r\n        operate:1,\r\n        id:0,\r\n        address: \"0\",\r\n        code_val: code_source[0],\r\n        code:code_source_key[0],\r\n        code_name_sources:code_source,\r\n        code_name_sources_key:code_source_key,\r\n        data_type_val: data_type_source[\"1\"],\r\n        data_type: \"1\",\r\n        data_type_sources: Object.values(data_type_source),\r\n        data_type_sources_key: Object.keys(data_type_source),\r\n        IODevices_val: \"\",\r\n        IODevices:\"\",\r\n        IODevices_sources:Object.values(IODevices_source),\r\n        IODevices_sources_key:Object.keys(IODevices_source),\r\n        input_h: \"1\",\r\n        input_l: \"0\",\r\n        ip: \"\",\r\n        machine_id: mac_source_key[0],\r\n        machine_name:  mac_source[0],\r\n        mac_me_sources: mac_source,\r\n        mac_me_sources_key: mac_source_key,\r\n        number: \"0\",\r\n        output_h:\"1\",\r\n        output_l: \"0\",\r\n        port: \"1\",\r\n        position_id: pos_source_key[0],\r\n        position_name: pos_source[0],\r\n        pos_name_sources:pos_source,\r\n        pos_name_sources_key:pos_source_key,\r\n        state_val: state_source[\"1\"],\r\n        state: \"1\",\r\n        state_sources:Object.values(state_source),\r\n        state_sources_key:Object.keys(state_source),\r\n        tree_id: tree_source_key[0],\r\n        tree_name: tree_source[0],\r\n        t_name_sources:tree_source,\r\n        t_name_sources_key:tree_source_key,\r\n    };\r\n    return modeBusClient\r\n}\r\nexport function changeModBusClient(type,oldData,index) {\r\n    let modeBusClient={\r\n        id:oldData.id,\r\n        address: oldData.address,\r\n        code:oldData.code,\r\n        data_type: oldData.data_type,\r\n        IODevices:oldData.IODevices,\r\n        input_h: oldData.input_h,\r\n        input_l: oldData.input_l,\r\n        ip: oldData.ip,\r\n        machine_id: oldData.machine_id,\r\n        machine_name:  oldData.machine_name,\r\n        number: oldData.number,\r\n        output_h:oldData.output_h,\r\n        output_l: oldData.output_l,\r\n        port: oldData.port,\r\n        position_id: oldData.position_id,\r\n        position_name: oldData.position_name,\r\n        state: oldData.state,\r\n        tree_id: oldData.tree_id,\r\n        tree_name: oldData.tree_name,\r\n    };\r\n\r\n    return modeBusClient\r\n}\r\n\r\nexport function getOpcInfo(res) {\r\n    let data = [];\r\n    let opcClient={};\r\n    res.OPCBusClient.forEach(opcItem =>{\r\n        opcClient={\r\n            operate:0,\r\n            address: opcItem.address,\r\n            code_val: getCodeName(opcItem.code),\r\n            code: opcItem.code,\r\n            code_name_sources:[],\r\n            code_name_sources_key:[],\r\n            IODevices_val: IODevices_source[opcItem.iODevices],\r\n            IODevices: opcItem.iODevices,\r\n            IODevices_sources:Object.values(IODevices_source),\r\n            IODevices_sources_key:Object.keys(IODevices_source),\r\n            id: opcItem.id,\r\n            machine_id: opcItem.machine_id,\r\n            machine_name:  opcItem.machine_name,\r\n            mac_me_sources: [],\r\n            mac_me_sources_key:[],\r\n            position_id: opcItem.position_id,\r\n            position_name: opcItem.position_name,\r\n            pos_name_sources:[],\r\n            pos_name_sources_key:[],\r\n            state_val: state_source[opcItem.state],\r\n            state:opcItem.state,\r\n            state_sources:Object.values(state_source),\r\n            state_sources_key:Object.keys(state_source),\r\n            tree_id: opcItem.tree_id,\r\n            tree_name: opcItem.tree_name,\r\n            t_name_sources:[],\r\n            t_name_sources_key:[]\r\n        };\r\n        data.push(opcClient);\r\n    });\r\n    console.log(data)\r\n    return[data]\r\n}\r\n\r\nexport function createOpc(type,initList,index) {\r\n    let t_tree_source = initList.folderData;\r\n    let macData = initList.macData;\r\n    let posData = initList.posData;\r\n    let codeData = initList.codeData;\r\n\r\n    let tree_source_key = Object.keys(t_tree_source);\r\n    let tree_source = Object.values(t_tree_source);\r\n\r\n    let m_mac_source = macData[tree_source_key[0]];\r\n    let mac_source_key = Object.keys(m_mac_source);\r\n    let mac_source = Object.values(m_mac_source);\r\n\r\n    let p_pos_source = posData[mac_source_key[0]];\r\n    let pos_source_key = Object.keys(p_pos_source);\r\n    let pos_source = Object.values(p_pos_source);\r\n\r\n    let c_code_source ={};\r\n    let code_list = codeData[pos_source_key[0] +'_'+ pos_source[0]];\r\n    code_list.forEach(code =>{\r\n        c_code_source[code] = getCodeName(code);\r\n    });\r\n    let code_source_key = Object.keys(c_code_source);\r\n    let code_source = Object.values(c_code_source);\r\n    let opcClient={\r\n        operate:1,\r\n        id:0,\r\n        address:\"\",\r\n        code_val: code_source[0],\r\n        code:code_source_key[0],\r\n        code_name_sources:code_source,\r\n        code_name_sources_key:code_source_key,\r\n        IODevices_val: \"\",\r\n        IODevices:\"\",\r\n        IODevices_sources:Object.values(IODevices_source),\r\n        IODevices_sources_key:Object.keys(IODevices_source),\r\n        machine_id: mac_source_key[0],\r\n        machine_name:  mac_source[0],\r\n        mac_me_sources: mac_source,\r\n        mac_me_sources_key: mac_source_key,\r\n        position_id: pos_source_key[0],\r\n        position_name: pos_source[0],\r\n        pos_name_sources:pos_source,\r\n        pos_name_sources_key:pos_source_key,\r\n        state_val: state_source[\"1\"],\r\n        state: \"1\",\r\n        state_sources:Object.values(state_source),\r\n        state_sources_key:Object.keys(state_source),\r\n        tree_id: tree_source_key[0],\r\n        tree_name: tree_source[0],\r\n        t_name_sources:tree_source,\r\n        t_name_sources_key:tree_source_key,\r\n    };\r\n    return opcClient\r\n}\r\n\r\nexport function changeOpcClient(type,oldData,index) {\r\n    let modeBusClient={\r\n        id:oldData.id,\r\n        address: oldData.address,\r\n        code:oldData.code,\r\n        IODevices:oldData.IODevices,\r\n        machine_id: oldData.machine_id,\r\n        machine_name:  oldData.machine_name,\r\n        position_id: oldData.position_id,\r\n        position_name: oldData.position_name,\r\n        state: oldData.state,\r\n        tree_id: oldData.tree_id,\r\n        tree_name: oldData.tree_name,\r\n    };\r\n\r\n    return modeBusClient\r\n}\r\n\r\n","\r\n//采集器类型名称转化\r\nexport function dgmType_name(type){\r\n    let dgmType_name = '';\r\n    switch(parseInt(type)){\r\n        case 1:\r\n       