ment.getElementById('HotTable');\r\n            let property = {\r\n                type:this.type,\r\n                tids:this.$store.state.tids,\r\n                getData:this.getData,\r\n                getHot:this.getHot,\r\n                initList:this.initList,\r\n                changeList:this.changeList,\r\n                router: this.$router,\r\n                store : this.$store\r\n            }\r\n            //创建table\r\n            this.hot = new machdtable(HotTable,property);\r\n            this.reRenderSelectOption(this.hot,this.data);\r\n            /* 监听滚动事件 */\r\n            let wtHolder = document.getElementsByClassName('wtHolder')[0]\r\n            wtHolder.addEventListener('scroll',(e)=>{\r\n                this.hot.destroyEditor()//处理当选择下拉框后拖动滚动条框框会跟着跑的问题\r\n            });\r\n        },\r\n        reRenderSelectOption(hot,data){ //重新渲染selectOption,让每个option\r\n            let cellMeta  = hot.getCellMetaAtRow(0);\r\n            let rows = data.length\r\n            cellMeta.forEach(item=>{\r\n                if(item.prop == \"tree_name\"){ //组织\r\n                    for(let i=0;i<=rows;i++){\r\n                        hot.setCellMeta(i,item.col,\"selectOptions\",row=>{ return  data[hot.toPhysicalRow(row)].t_name_sources})\r\n                    }\r\n                }\r\n                if(item.prop == \"pump.mac_me\"){ //机组\r\n                    for(let i=0;i<rows;i++){\r\n                        hot.setCellMeta(i,item.col,\"selectOptions\",row=>{ return  data[hot.toPhysicalRow(row)].pump.macName_sources})\r\n                    }\r\n                }\r\n                if(item.prop == \"condition.pos_name1\"){ //工况参考测点1\r\n                    for(let i=0;i<rows;i++){\r\n                        hot.setCellMeta(i,item.col,\"selectOptions\",row=>{ return  data[hot.toPhysicalRow(row)].condition.pos_sources1})\r\n                    }\r\n                }\r\n                if(item.prop == \"condition.pos_name2\"){ //工况参考测点2\r\n            