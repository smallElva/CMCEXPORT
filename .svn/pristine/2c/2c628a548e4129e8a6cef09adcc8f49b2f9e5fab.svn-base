gmUp-content .dgmUpTitle {\n    height: 50px;\n    line-height: 50px;\n    background: #343C45;\n    color: #fff;\n    font-size: 20px;\n    padding-left: 10px;\n}\n.dgmUp-content .uploadPackageBtn, .dgmUp-content .upgradeDgmBtn {\n    border-radius: 4px;\n    height: 34px;\n    width: 120px;\n    font-size: 16px;\n    background: #fff;\n    color: #597DD2;\n    position: absolute;\n    top: 8px;\n}\n.dgmUp-content .uploadPackageBtn {\n    right: 160px;\n}\n.dgmUp-content .dgmUpMsg {\n    display: flex;\n    justify-content: space-between;\n    height: calc(100% - 80px);\n    padding-top: 25px;\n}\n.dgmUp-content .dgmPackage, .dgmUp-content .upgradeDgm {\n    background: #fff;\n    border: 1px solid #888;\n    border-radius: 4px;\n    height: calc(100% + 50px);\n    overflow: hidden;\n}\n.dgmUp-content .dgmPackage {\n    width: 62%;\n}\n.dgmUp-content .packageTitle, .dgmUp-content .upgradeTitle {\n    background: #798CA1;\n    color: #fff;\n    letter-spacing: 1px;\n    height: 48px;\n    line-height: 48px;\n    font-size: 20px;\n    padding-left: 15px;\n    border-top-left-radius: 4px;\n    border-top-right-radius: 4px;\n    position: relative;\n}\n.dgmUp-content .packageTableTitle > th:first-child, .dgmUp-content .packageTableTitle > th:nth-child(2), .dgmUp-content .packageTableTitle > th:nth-child(3), .dgmUp-content .packageTableBody > td:first-child, .dgmUp-content .packageTableBody > td:nth-child(2), .dgmUp-content .packageTableBody > td:nth-child(3), .dgmUp-content .packageTableBody > td:last-child {\n    width: 120px;\n}\n.dgmUp-content .packageTableBody > td:nth-child(5) {\n    overflow: hidden;\n    white-space: nowrap;\n    text-overflow: ellipsis;\n}\n.dgmUp-content .packageTableTitle > th:last-child {\n    color: #597DD2;\n    width: 120px;\n}\n.dgmUp-content .list1 {\n    background: #EFF1F6;\n}\n.dgmUp-content .packageOperat {\n    background: transparent;\n    border: 1px solid #597DD2;\n    color: #597DD2;\n    border-radius: 4px;\n    width: 80px;\n    height: 30px;\n}\n.dgmUp-content .packageOperat:hover {\n    background: #597DD2;\n    color: #fff;\n}\n.dgmUp-content .alertBox {\n    position: fixed;\n    top: 0;\n    left: 0;\n    background: rgba(0, 0, 0, 0.4);\n    z-index: 5;\n    height: 100%;\n    width: 100%;\n}\n.dgmUp-content .alertTip {\n    height: 180px;\n    width: 300px;\n    background: #fff;\n    top: 50%;\n    left: 50%;\n    margin-top: -90px;\n    margin-left: -150px;\n    position: absolute;\n    border-radius: 4px;\n}\n.dgmUp-content .alertTipTitle, .dgmUp-content .checkDetailTitle {\n    height: 40px;\n    line-height: 40px;\n    background: #343C45;\n    padding-left: 10px;\n    border-top-left-radius: 5px;\n    border-top-right-radius: 5px;\n    font-size: 20px;\n    color: #fff;\n    position: relative;\n    user-select: none;\n}\n.dgmUp-content .alerTipText {\n    height: 100px;\n    line-height: 100px;\n    font-size: 20px;\n    text-align: center;\n}\n.dgmUp-content .alertTipBtn {\n    height: 40px;\n    line-height: 40px;\n    text-align: center;\n}\n.dgmUp-content .yes, .dgmUp-content .no {\n    color: #fff;\n    outline: none;\n    border: none;\n    border-radius: 4px;\n    width: 80px;\n    height: 28px;\n    line-height: 28px;\n    background: #798CA1;\n}\n.dgmUp-content .yes {\n    margin-right: 20px;\n    background: #597DD2;\n}\n.dgmUp-content .upgradeDgm {\n    width: 35%;\n}\n.dgmUp-content .searchDgm {\n    background: #798CA1;\n    color: #fff;\n    border: 1px solid #fff;\n    outline: none;\n    border-radius: 4px;\n    height: 30px;\n    position: absolute;\n    top: 10px;\n    right: 20px;\n    width: 25%;\n    padding: 0 35px 0 10px;\n}\n.dgmUp-content .searchImg {\n    font-size: 18px;\n    position: absolute;\n    right: 30px;\n    top: 2px;\n}\n.dgmUp-content .upgradeDgmBtn {\n    right: 20px;\n}\n.dgmUp-content .upgradeDgmTr > th:first-child, .dgmUp-content .upgradeDgmTr > td:first-child {\n    width: 50px;\n}\n.dgmUp-content .checkImg {\n    height: 24px;\n    width: 24px;\n    vertical-align: middle;\n    cursor: pointer;\n}\n.dgmUp-content .checkDetailBtn {\n    height: 30px;\n    width: 80px;\n    outline: none;\n    border-radius: 4px;\n    border: none;\n    color: #fff;\n    background: #597DD2;\n}\n.dgmUp-content .detailTr > th:first-child, .dgmUp-content .detailTr > td:first-child {\n    width: 130px;\n}\n.dgmUp-content .detailTr > th:nth-child(even), .dgmUp-content .detailTr > td:nth-child(even) {\n    width: 100px;\n}\n.dgmUp-content .alertDetail {\n    background: transparent;\n}\n.dgmUp-content .checkDetail {\n    width: 550px;\n    max-height: 400px;\n    position: absolute;\n    right: 60px;\n    top: 40%;\n    background: #fff;\n    border-radius: 4px;\n    border: 1px solid #888;\n    box-shadow: 0 3px 3px 1px rgba(33, 35, 40, 0.2);\n}\n.dgmUp-content .closeAlert {\n    height: 24px;\n    width: 24px;\n    position: absolute;\n    top: 8px;\n    right: 20px;\n    cursor: pointer;\n}\n.dgmUp-content .checkDetail tbody {\n    max-height: 320px;\n    height: auto;\n}\n.dgmUp-content .checkDetail tr {\n    height: 40px;\n    line-height: 40px;\n}\n.dgmUp-content .checkDetail th, .dgmUp-content .checkDetail td {\n    border-bottom: none;\n}\n.dgmUp-content .item0 {\n    background: #DEE2ED;\n}\n@media (max-width: 1366px) {\n.dgmUp-content .dgmPackage {\n      width: 60%;\n}\n.dgmUp-content .upgradeDgm {\n      width: 37%;\n}\n.dgmUp-content td, .dgmUp-content th {\n      font-size: 14px;\n}\n.dgmUp-content tbody {\n      height: calc(100vh - 300px);\n}\n}\n"]}