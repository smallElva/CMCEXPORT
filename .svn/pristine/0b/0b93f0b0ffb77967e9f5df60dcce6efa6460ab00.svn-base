wC;AAC5D,uBAAuB,uBAAuB;AAC9C;AACA;AACA;AACA;AACA;AACA;AACA;AACA,iBAAiB;AACjB;AACA;AACA;AACA,WAAW;AACX;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,WAAW;AACX;AACA;AACA;AACA;AACA;AACA,eAAe,yBAAyB;AACxC,iBAAiB,6BAA6B;AAC9C,qBAAqB,6BAA6B;AAClD;AACA;AACA;AACA;AACA,iCAAiC,aAAa;AAC9C,eAAe;AACf;AACA;AACA;AACA;AACA;AACA;AACA;AACA,+BAA+B,aAAa;AAC5C;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,qBAAqB;AACrB;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,qBAAqB;AACrB;AACA;AACA;AACA;AACA,aAAa;AACb;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,SAAS;AACT;AACA;AACA;AACA;AACA;AACA;AACA;;;;;;;;;;;;;;ACrIA;AAAA;AAAA;AAAA;AAAA;AAAmF;AAC3B;AACL;AACc;;;AAGjE;AACsH;AACtH,gBAAgB,6HAAU;AAC1B,EAAE,0EAAM;AACR,EAAE,+EAAM;AACR,EAAE,wFAAe;AACjB;AACA;AACA;AACA;;AAEA;;AAEA;AACA,IAAI,KAAU,EAAE,YAiBf;AACD;AACe,gF;;;;;;;;;;;;ACvCf;AAAA;AAAA,wCAAuO,CAAgB,qRAAG,EAAC,C;;;;;;;;;;;;ACA3P;AAAA;AAAA;AAAA;;;;;;;;;;;;;ACAA;AAAA;AAAA;AAAA;AAAA;AAAA","file":"js/58-1f5fb494.js","sourcesContent":["<template>\r\n    <div class=\"wisdom rule-content\">\r\n        <div class=\"header\">\r\n            <div class=\"title\">智子规则</div>\r\n            <div class=\"controls\">\r\n                <div class=\"search\">\r\n                    <label for=\"searchInput\">搜索规则名称：</label>\r\n                    <input class=\"input\" id=\"searchInput\" placeholder=\"请输入\"  v-model=\"searchVal\" @input=\"searchRule\">\r\n                </div>\r\n                <button class=\"create-btn\" @click=\"createFilterRule()\">新建</button>\r\n            </div>\r\n        </div>\r\n        <div class=\"content\">\r\n            <div class=\"table_block\">\r\n                <table class=\"index_table\">\r\n                    <thead>\r\n                    <tr>\r\n                        <th v-for=\"(item,index) in head\" :key=\"index\">{{item}}</th>\r\n                    </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                    <tr v-for=\"(item,index) in data\" :key=\"index\">\r\n                        <td>{{Number(index + 1)}}</td>\r\n                        <td>{{item.name}}</td>\r\n                        <td>{{item.remark}}</td>\r\n                        <td>{{scopeList[item.scope]}}</td>\r\n                        <td>\r\n                            <!-- <button class=\"btn btn-blue\" @click=\"checkPage(item)\">查看</button> -->\r\n                            <button class=\"btn btn-blue\" @click=\"editPage(item)\">编辑</button>\r\n                            <button class=\"btn btn-red\" @click=\"deleteItem(item,index)\">删除</button>\r\n                        </td>\r\n                    </tr>\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n            <div class=\"noData\" v-show=\"!hasData\">无数据</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\n    export default {\r\n        name: \"rule\",\r\n        data(){\r\n            return{\r\n               head:['序号','规则名称','备注','作用域','操作'],//表头\r\n               hasData:true,//是否有数据\r\n               searchVal:'',//搜索关键词\r\n               data:[],//智子规则数据\r\n               originData: [],//智子规则原始数据\r\n               scopeList:[\r\n                   '全局',\r\n                   '机组动力部件',\r\n                   '生产输出部件',\r\n                   '传动机构',\r\n                   '动静结合部',\r\n                   '非动静结合部',\r\n                   '工况工艺'\r\n               ]\r\n            }\r\n        },\r\n        created(){\r\n            \r\n        },\r\n        mounted(){\r\n           this.getData();\r\n        },\r\n        watch:{\r\n           \r\n        },\r\n        \r\n        methods: {\r\n            /**\r\n             * 获取过滤规则\r\n             */\r\n            getData(){\r\n                let params = {\r\n                    type: 1,//0过滤,,1智子\r\n                }\r\n                this.$getApi.getRuleConfig(params)\r\n                    .then(res => {\r\n                        if(res.msg===0){\r\n                            this.data = res.rules;\r\n                            this.originData = res.rules;\r\n                            if(res.rules.length){\r\n                                this.hasData = true;\r\n                            }\r\n                            else{\r\n                                this.hasData = false;\r\n                            }\r\n                        }\r\n                    })\r\n                        .catch(err=>{\r\n                            console.log('请求错误' ,err)\r\n                        })\r\n            },\r\n             /**\r\n             * 新建过滤规则\r\n             */\r\n            createFilterRule(){\r\n                let obj = {\r\n                    id:null,\r\n                }\r\n                this.$store.commit(\"setRuleInfo\", obj); //赋值给vuex\r\n                this.$router.push({\r\n                    name: `createRule`,\r\n                })\r\n            },\r\n            /**\r\n             * 查看过滤规则\r\n             */\r\n            checkPage(item){\r\n                this.$router.push({\r\n                    name: `createRule`,\r\n                })\r\n            },\r\n            /**\r\n             * 编辑过滤规则\r\n             * \r\n             */\r\n            editPage(item){\r\n                let obj = {\r\n                    id:item.id,\r\n                }\r\n                this.$store.commit(\"setRuleInfo\", obj); //赋值给vuex\r\n                this.$router.push({\r\n                    name: `createRule`,\r\n                })\r\n            },\r\n            deleteItem(item,index){\r\n                swal({\r\n                        title: \"警告\",\r\n                        text: \"当前数据一经删除，该数据也将不保留。确定删除吗？\",\r\n                        buttons: {\r\n                            sure: \"确认\",\r\n                            cancel: \"取消\",\r\n                        },\r\n                            dangerMode: true,\r\n                        })\r\n                            .then((willDelete) => {\r\n                                   if (willDelete) {\r\n                                    let params = [\r\n                                            item.id\r\n                                        ]\r\n                                    this.$getApi.deleteRule(params)\r\n                                            .then(res => {\r\n                                                if(res.msg===0){\r\n                                                    swal({\r\n                                                        title: \"提示\",\r\n                                                        text: \"删除成功\",\r\n                                                        button: \"确认\",\r\n                                                    });\r\n                                                    this.getData();\r\n                                                }\r\n                                            })\r\n                                                .catch(err=>{\r\n                                                    console.log('请求错误' ,err)\r\n                                                })\r\n                                } else {\r\n                                return false;\r\n                            }\r\n                });    \r\n                \r\n            },\r\n            // 关键词搜索\r\n            searchRule(){\r\n                var reg = RegExp(this.searchVal);\r\n                var that = this;\r\n                if (that.searchVal.length>0) {\r\n                    that.data = [];\r\n                    // 过滤需要的数据\r\n                    for(let i=0;i<that.originData.length;i++){\r\n                        if (reg.test(that.originData[i].name) || reg.test(that.originData[i].remark) || reg.test(that.scopeList[that.originData[i].scope])) {\r\n                            that.data.push(that.originData[i]);\r\n                        }\r\n                    }\r\n                }\r\n                else{\r\n                    that.data = that.originData.concat();\r\n                }\r\n            },\r\n        }\r\n    }\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n    @import \"~assets/css/components/wisdom.scss\";\r\n</style>\r\n","// extracted by mini-css-extract-plugin","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\"div\", { staticClass: \"wisdom rule-content\" }, [\n    _c(\"div\", { staticClass: \"header\" }, [\n      _c(\"div\", { staticClass: \"title\" }, [_vm._v(\"智子规则\")]),\n      _vm._v(\" \"),\n      _c(\"div\", { staticClass: \"controls\" }, [\n        _c(\"div\", { staticClass: \"search\" }, [\n          _c(\"label\", { attrs: { for: \"searchInput\" } }, [\n            _vm._v(\"搜索规则名称：\")\n          ]),\n          _vm._v(\" \"),\n          _c(\"input\", {\n            directives: [\n              {\n                name: \"model\",\n                rawName: \"v-model\",\n                value: _vm.searchVal,\n                expression: \"searchVal\"\n              }\n            ],\n            staticClass: \"input\",\n            attrs: { id: \"searchInput\", placeholder: \"请输入\" },\n            domProps: { value: _vm.searchVal },\n            on: {\n              input: [\n                function($event) {\n                  if ($event.target.composing) {\n                    return\n                  }\n                  _vm.searchVal = $event.target.value\n                },\n                _vm.searchRule\n              ]\n            }\n          })\n        ]),\n        _vm._v(\" \"),\n        _c(\n          \"button\",\n          {\n            staticClass: \"create-btn\",\n            on: {\n              click: function($event) {\n                return _vm.createFilterRule()\n              }\n            }\n          },\n          [_vm._v(\"新建\")]\n        )\n      ])\n    ]),\n    _vm._v(\