       //             this.$forceUpdate();\r\n        //         },\r\n        //         deep:true\r\n        //    }\r\n        },\r\n        computed:{\r\n            reSetMacList() {\r\n                let list = this.macList;\r\n                if(list && list.length){\r\n                    list.forEach(item=>{\r\n                        item.id = item.mac_id;\r\n                        item.label = item.mac_me;\r\n                    })\r\n                }\r\n                else{\r\n                    list = [];\r\n                }\r\n                return list;\r\n            },\r\n            reSetPosList() {\r\n                let list = this.posList;\r\n                if(list && list.length){\r\n                    list.forEach(item=>{\r\n                        item.id = item.pos_type+'_'+item.pos_id +'_'+ item.mac_id;\r\n                        item.label = item.pos_name;\r\n                    })\r\n                }\r\n                else{\r\n                    list = [];\r\n                }\r\n                return list;\r\n            },\r\n            reSetRuleList(){\r\n                let list = this.ruleList;\r\n                if(list && list.length){\r\n                    list.forEach(item=>{\r\n                        item.id = item.id;\r\n                        item.label = item.name;\r\n                    })\r\n                }\r\n                else{\r\n                    list = [];\r\n                }\r\n                return list;\r\n            },\r\n            reSetFilterList(){\r\n                let list = this.filterList;\r\n                if(list && list.length){\r\n                    list.forEach(item=>{\r\n                        item.id = item.id;\r\n                        item.label = item.name;\r\n                    })\r\n                }\r\n                else{\r\n                    list = [];\r\n                }\r\n                return list;\r\n            }\r\n        },\r\n        methods: {\r\n             /**\r\n             * 点击确定\r\n             */\r\n            keepData(){\r\n\r\n                let params ={\r\n                    pos_name: this.pos_name,\r\n                    pos_type: 201,\r\n                    pos_id: this.setPosId(),//\r\n                    t_id:this.chooseTid,\r\n                    t_name:this.chooseTname,\r\n                    mac_id: this.chooseMacId,\r\n                    mac_me: this.chooseMacMe,\r\n                    rule_id: this.chooseRuleId,\r\n                    rule_name: this.chooseRuleName,\r\n                    filterRule_id: this.chooseFilterId,\r\n                    filter_name: this.chooseFilterName,\r\n                    ruleDataSource: this.ruleDataSource,\r\n                    filterDataSource: this.filterDataSource,\r\n                    calcAmount: this.calcAmount,\r\n                    computationTime_num:this.computationTime_num,//按数据数量计算,数量（几组数据计算一次）\r\n                    computationType_num:this.computationType_num,//按按时间计算,数量（几天/小时/分钟）\r\n                    computationType_unit:this.computationType_unit,//按按时间计算,时间单位\r\n                    pushWay:this.pushWay,//推送方式，0关闭，1开启\r\n                    trigMode_a:this.trigMode_a,//触发方式分子\r\n                    trigMode_b:this.trigMode_b,//触发方式分母\r\n                    trigMode_way:this.trigMode_way,//触发方式：判断符号类型\r\n                    // trigMode_num:this.trigMode_num,//触发方式数量\r\n                    trigMode_refer:this.trigMode_refer,//触发方式参考值\r\n                    // trigMode_action:this.trigMode_action,//触发方式：%\r\n                    push_timeNum:this.push_timeNum,//推送方式：几天\r\n                    push_timeUnit:this.push_timeUnit,//推送方式：时间单位\r\n                    push_timeAcount:this.push_timeAcount,//推送方式：几次\r\n                }\r\n                this.$getApi.createInPart(params)\r\n                    .then(res => {\r\n                        if(res.msg===0){\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: \"创建成功\",\r\n                                button: \"确认\",\r\n                            });\r\n                        }\r\n                    })\r\n                        .catch(err=>{\r\n                            console.log('请求错误' ,err)\r\n                        })\r\n            },\r\n            /**\r\n             * 获取智子规则信息\r\n             */\r\n            getRule(){\r\n                let params = {\r\n                    type: 1,//0过滤，1智子\r\n                }\r\n                this.$getApi.getRuleConfig(params)\r\n                    .then(res => {\r\n                        if(res.msg===0){\r\n                            this.ruleList = res.rules;\r\n                        }\r\n                    })\r\n                        .catch(err=>{\r\n                            console.log('请求错误' ,err)\r\n                        })\r\n            },\r\n            /**\r\n             * 获取过滤规则信息\r\n             */\r\n            getFilter(){\r\n                let params = {\r\n                    type: 0,//0过滤，1智子\r\n                }\r\n                this.$getApi.getRuleConfig(params)\r\n                    .then(res => {\r\n                        if(res.msg===0){\r\n                            this.filterList = res.rules;\r\n                        }\r\n                    })\r\n                        .catch(err=>{\r\n                            console.log('请求错误' ,err)\r\n                        })\r\n            },\r\n             /**\r\n             * 获取组织信息\r\n             */\r\n            getTree(){\r\n                let params = null;\r\n                params={\r\n                    type: 2,\r\n                    t_ids:JSON.parse(sessionStorage.getItem('getTidList')),\r\n                };\r\n                this.$axios.post('getPositionDataConfig',params)\r\n                        .then(res => {\r\n                            if(res.msg===0 && res.list && res.list.length){\r\n                                this.treeList = this.getFolderList(res.list);\r\n                            }\r\n                            else{\r\n                                console.log('请求失败')\r\n                            }\r\n                        }).catch(err => {\r\n                            console.log(err)\r\n                        })\r\n            },\r\n            //递归组织\r\n            getFolderList(list){\r\n                if(list && list.length){\r\n                    list.forEach(item=>{\r\n                        item.id = item.t_id;\r\n                        item.label = item.t_name;\r\n                        item.status = false;\r\n                        if(item.children){\r\n                            this.getFolderList(item.children);\r\n                        }\r\n                    })\r\n                }\r\n                return list;\r\n            },\r\n            //获取机组信息\r\n            setMacData(){\r\n                let params = null;\r\n                this.macList = [];\r\n                this.chooseMacId = null;\r\n                this.choosePosId = null;\r\n                \r\n                params={\r\n                    t_id:this.chooseTid\r\n                };\r\n                this.$axios.post('getMachine',params)\r\n                        .then(res => {\r\n                            if(res.msg===0 && res.list && res.list.length){\r\n                                this.macList = res.list;\r\n                            }\r\n                            else{\r\n                                console.log('请求失败')\r\n                            }\r\n                        }).catch(err => {\r\n                            console.log(err)\r\n                        })\r\n            },\r\n            //获取测点信息\r\n            setPosData(){\r\n                let params = null;\r\n                this.posList = [];\r\n                params={\r\n                    mac_id:this.chooseMacId\r\n                };\r\n                this.$axios.post('getPosition',params)\r\n                        .then(res => {\r\n                            if(res.msg===0 && res.list && res.list.length){\r\n                                this.posList = res.list;\r\n                            }\r\n                            else{\r\n                                console.log('请求失败')\r\n                            }\r\n                        }).catch(err => {\r\n                            console.log(err)\r\n                        })\r\n            },\r\n            // 选择组织\r\n            selectTree(node){\r\n                this.chooseTid = node.id;\r\n                this.chooseTname = node.label;\r\n                this.setMacData();\r\n            },\r\n            // 选择机组\r\n            selectMac(node){\r\n                this.chooseMacId = node.id;\r\n                this.chooseMacMe = node.label;\r\n                this.setPosData();\r\n            },\r\n            // 选择测点\r\n            selectPos(node){\r\n                this.choosePosId = node.pos_type + '_' + node.pos_id  + '_' + node.mac_id;\r\n            },\r\n            // 选择智子规则\r\n            selectRule(node){\r\n                this.chooseRuleName = node.label;\r\n                let arr = node.dataSource;\r\n                let tips = '请选择'\r\n                arr.forEach(item=>{\r\n                    item.choosePosId = null;\r\n                    item.posList = [];\r\n                    tips += posType_name(item.posType) + ' '\r\n                    this.posList.forEach((pos=>{\r\n                        if(pos.pos_type == item.posType){\r\n                            pos.id = pos.pos_type+'_'+pos.pos_id +'_'+ pos.mac_id;\r\n                            pos.label = pos.pos_name;\r\n                            item.posList.push(pos);\r\n                        }\r\n                    }))\r\n                })\r\n                this.ruleTips = tips;\r\n                this.ruleDataSource = arr;\r\n            },\r\n            // 选择过滤规则\r\n            selectFilter(node){\r\n                this.chooseFilterName = node.label;\r\n                let arr = node.dataSource;\r\n                let tips = '请选择'\r\n                arr.forEach(item=>{\r\n                    item.choosePosId = null;\r\n                    item.posList = [];\r\n                    tips += posType_name(item.posType) + ' '\r\n                    this.posList.forEach((pos=>{\r\n                        if(pos.pos_type == item.posType){\r\n                            pos.id = pos.pos_type+'_'+pos.pos_id +'_'+ pos.mac_id;\r\n                            pos.label = pos.pos_name;\r\n                            item.posList.push(pos);\r\n                        }\r\n                    }))\r\n                })\r\n                this.filterDataSource = arr;\r\n                this.filterTips = tips;\r\n            },\r\n            /**\r\n             * 点击重置\r\n             */\r\n            reset(){\r\n\r\n            },\r\n             /**\r\n             * 跳转至过滤规则列表页\r\n             */\r\n            routeToBefore(){\r\n                this.$router.push({\r\n                    name: `intelligent`,\r\n                })\r\n            },\r\n             /**\r\n             * 选择时间单位\r\n             */\r\n            selectTime(node){\r\n\r\n            },\r\n             /**\r\n             * 选择时间单位\r\n             */\r\n            selectTime(node){\r\n\r\n            },\r\n             /**\r\n             * 选择判断符号\r\n             */\r\n            selectCalcWay(node){\r\n\r\n            },\r\n             /**\r\n             * 选择%\r\n             */\r\n            selectAction(node){\r\n\r\n            },\r\n            /* 设置测点ID */\r\n            setPosId() {\r\n                let arr = [];\r\n                if(this.posList.length){\r\n                    this.posList.forEach(item=>{\r\n                        if(item.pos_type == 201){\r\n                            arr[item.pos_id] = item.pos_id;\r\n 