able\r\n        this.flag = !this.flag;\r\n      } else {\r\n        //缩小\r\n        homeHeader.style.display = \"block\";\r\n        aside.style.display = \"block\";\r\n        content.style.width = \"100px\"; //改变宽让宽自动渲染\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 90px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 120px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.flag = !this.flag;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-content-header {\r\n  /* width: 100%-15px;\r\n   */\r\n  box-sizing: border-box;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  background: #343c45;\r\n  font-size: 20px;\r\n  color: #fff;\r\n  padding-left: 15px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n","<template>\r\n  <div class=\"position-content\">\r\n    <button class=\"my-btn btn-active btn3\" @click=\"onShowListFile\">{{btnName}}</button>\r\n    <my-content-title :title='title'></my-content-title>\r\n    <router-view id='myContentCenter'></router-view>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyContentTitle from 'components/common/MyContentTitle';\r\nexport default {\r\n  name: \"Position\",\r\n  props:['parentToChild'],\r\n  data(){\r\n    return{\r\n      title:'测点设置',\r\n      btnName: \"\",\r\n      activeNum: \"02\",\r\n      parentFolderData:[\"根目录\"],\r\n      chooseMacId:\"\",\r\n      chooseMacName:\"\",\r\n      chooseType:\"\",\r\n    }\r\n  },\r\n  created: function() {\r\n    this.btnName = this.$route.name == 'positiontree' ?'切换至表格':'切换树状图'\r\n    this.$emit('ievent',this.activeNum);\r\n  },\r\n  mounted: function() {\r\n    if(!$.isEmptyObject(this.$route.params)){\r\n      let linkInfo = this.$route.params;\r\n      // this.flag = linkInfo.flag;\r\n      this.activeNum = linkInfo.activeNum;\r\n      this.chooseMacId = linkInfo.macId;\r\n      this.chooseMacName = linkInfo.macName;\r\n      this.onShowListFile();\r\n    }\r\n    else{\r\n      this.getDataFromTable();\r\n    }\r\n  },\r\n  watch: {\r\n    \"$route\":\"fetchData\"\r\n  },\r\n  methods: {\r\n    onShowListFile: function() {\r\n      console.log(this.$route.name);\r\n      if(this.$route.name == 'positiontree'){\r\n        this.$router.push({name: \"positiontable\"})\r\n        this.btnName = \"切换树状图\";\r\n        return\r\n      }else {\r\n        this.$router.push({name: \"positiontree\"})\r\n        this.btnName = \"切换至表格\";\r\n      }\r\n\r\n\r\n    },\r\n    fetchData(){\r\n      if(this.$route.name == 'vib'){\r\n        this.btnName = '切换树状图';\r\n      }\r\n    },\r\n    getDataFromTable: function (id) {\r\n      this.chooseMacId=id;\r\n    },\r\n    getTypeFromTable: function (type) {\r\n      this.chooseType=type;\r\n    }\r\n  },\r\n  components:{\r\n    MyContentTitle\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n  @import \"~assets/css/components/position.scss\";\r\n  .position-content{\r\n    position: relative;\r\n  }\r\n</style>\r\n","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\n    \"div\",\n    { staticClass: \"my-content-header\", on: { dblclick: _vm.dblclick } },\n    [_c(\"span\", { staticClass: \"page-title\" }, [_vm._v(_vm._s(_vm.title))])]\n  )\n}\nvar staticRenderFns = []\nrender._withStripped = true\n\nexport { render, staticRenderFns }","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\n    \"div\",\n    { staticClass: \"position-content\" },\n    [\n      _c(\n        \"button\",\n        {\n          staticClass: \"my-btn btn-active btn3\",\n          on: { click: _vm.onShowListFile }\n        },\n        [_vm._v(_vm._s(_vm.btnName))]\n      ),\n      _vm._v(\" \"),\n      _c(\"my-content-title\", { attrs: { title: 