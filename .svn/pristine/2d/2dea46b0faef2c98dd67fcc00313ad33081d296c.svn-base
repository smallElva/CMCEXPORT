gin_box\" v-if=\"hasLicence\">\r\n            <div class=\"login_div\">\r\n                <div class=\"icon_btn\"><i class=\"iconfont icon-zhanghaoshezhi\"></i></div>\r\n                <input type=\"text\" class=\"tpl-form-input\" id=\"user-name\" placeholder=\"账号\" v-model=\"username\">\r\n            </div>\r\n            <div class=\"login_div\">\r\n                <div class=\"icon_btn\"><i class=\"iconfont icon-zhongzi-mima\"></i></div>\r\n                <input type=\"password\" class=\"tpl-form-input\" id=\"pass_word\" placeholder=\"密码\" v-model=\"password\">\r\n            </div>\r\n            <div class=\"login_btns\">\r\n                <button type=\"button\" class=\"login_btn\" id=\"login_btn\" @click=\"loginFunc();\" v-bind:disabled=\"username==''||password==''\">登录</button>\r\n            </div>\r\n        </div>\r\n        <div class=\"login_box\" v-else>\r\n            <div class=\"licence_div\">\r\n                <p class=\"licence_p\">机器码</p>\r\n                <p class=\"licence_text\">{{licenceNum}}</p>\r\n            </div>\r\n            <div class=\"login_btns\">\r\n                <button type=\"button\" class=\"login_btn\" id=\"licence_btn\" @click=\"monidianji();\">注册</button>\r\n                <input class=\"input-loc-img\" name=\"fileLocal\" id=\"fileLocal\" type='file' @change=\"setLicenceInfo\"/>\r\n            </div>\r\n        </div>\r\n    </div>\r\n      <div class=\"version_cont\">\r\n          <p class=\"version_text\">版权所有：浙江中自庆安新能源技术有限公司 V{{versionNum}}</p>\r\n      </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport hex_md5 from 'md5-hex';\r\n\r\nexport default {\r\n    name:'Login',\r\n    data(){\r\n      return{\r\n        versionNum:'',\r\n        username:'',\r\n        password: '',\r\n        hasLicence: true,//是否有licence\r\n        licenceNum:'',//机器码信息\r\n      }\r\n    },\r\n    created: function(){\r\n        //获取版本信息\r\n        this.$axios.post('getCMCVersion')\r\n            .then(res => {\r\n                if(res.version){\r\n                    this.versionNum = res.version;\r\n                    sessionStorage.setItem('version', this.versionNum);\r\n                }\r\n                else{\r\n                    console.log('请求失败')\r\n                }\r\n            }).catch(err => {\r\n            console.log(err)\r\n        });\r\n        //获取licence信息\r\n        this.$getApi.getLicense()\r\n            .then(res=>{\r\n                this.licenceNum = res.uuid;\r\n                let licenceState = res.error;\r\n                if(licenceState===0){\r\n                    this.hasLicence = true;\r\n                }\r\n                else{\r\n                    this.hasLicence = false;\r\n                }\r\n            })\r\n            .catch(function(err) {\r\n                console.log('failed', err);\r\n            });\r\n    },\r\n    mounted(){\r\n        document.onkeydown = function(e) {\r\n            var ee = e ? e : window.event;\r\n            if (ee.keyCode == 13) {\r\n                $(\"#login_btn\").click();\r\n            }\r\n        };\r\n    },\r\n    methods: {\r\n        loginFunc: function () {\r\n            var psw_crypt= hex_md5(\"cmc\"+this.password);//对密码加密\r\n            let data ='username='+encodeURIComponent(this.username)+'&password='+psw_crypt+''\r\n            this.$axios.post('userLogin',data,{headers:{\"Content-Type\":'application/x-www-form-urlencoded;charsetset=UTF-8'}}).then(res=>{\r\n               switch(res.msg){\r\n                    case 0:\r\n                        sessionStorage.setItem('username', this.username);\r\n                        sessionStorage.setItem('token', res.token);\r\n                        sessionStorage.setItem('userType', res.type);\r\n                        this.$router.push({name:'home'});\r\n                        break;\r\n                    case 1:\r\n                        swal({\r\n                            text: '用户名或密码不能为空',\r\n                            button: \"确认\",\r\n                        });\r\n                        break;\r\n                    case 2:\r\n                        swal({\r\n                            text: '账户或密码错误',\r\n                            button: \"确认\",\r\n                        });\r\n                        break;\r\n                    case 3:\r\n                        swal({\r\n                            text: '账户或密码错误',\r\n                            button: \"确认\",\r\n                        });\r\n                        break;\r\n                   case 4:\r\n                       swal({\r\n                           text: '数据库配置错误',\r\n                           button: \"确认\",\r\n                       });\r\n                       break;\r\n                    default:\r\n                        swal({\r\n                            text: '返回错误',\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                },err =>{\r\n                    swal({\r\n                        text: '数据连接失败',\r\n                        button: \"确认\",\r\n                    });\r\n                })\r\n\r\n        },\r\n        monidianji(){\r\n            document.getElementById('fileLocal').click()\r\n        },\r\n        setLicenceInfo(){\r\n            let that=this;\r\n            let selectedFile = document.getElementById('fileLocal').files[0];\r\n            let reader = new FileReader();//这是核心,读取操作就是由它完成.\r\n            reader.readAsText(selectedFile);//读取文件的内容,也可以读取文件的URL\r\n            document.getElementById('fileLocal').value = null;\r\n            reader.onloadend = function () {\r\n                //读取完成，无论成功失败,然后此时文件的内容存储到了result中,直接操作即可\r\n                let license = this.result;\r\n                let params = null;\r\n                params = {\r\n                   \"License\":license\r\n                };\r\n                //注册License\r\n                that.$getApi.setLicense(this.result)\r\n                    .then(result=>{\r\n                        if(result!=null){\r\n                            var days = Math.ceil(result.validTime/1000/3600/24);\r\n                            that.licenceNum = result.uuid;\r\n                            let licenceState = result.error;\r\n                            if(licenceState===0){\r\n                                that.hasLicence = true;\r\n                            }\r\n                            else{\r\n                                that.hasLicence = false;\r\n                            }\r\n                            switch (licenceState) {\r\n                                case 0:\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: `注册成功`,\r\n                                        button: \"确认\",\r\n                                    });\r\n                                    break;\r\n                                case -1:\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: `Licence错误`,\r\n                                        button: \"确认\",\r\n                                    });\r\n                                    break;\r\n                                case -2:\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: `license过期`,\r\n                                        button: \"确认\",\r\n                                    });\r\n                                    break;\r\n                                case -3:\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: `license系统时间被更改`,\r\n                                        button: \"确认\",\r\n                                    });\r\n                                    break;\r\n                                case -4:\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: `license已被注册`,\r\n                                        button: \"确认\",\r\n                                    });\r\n                                    break;\r\n                                case -5:\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: `license未激活`,\r\n                                        button: \"确认\",\r\n                                    });\r\n                                    break;\r\n                            };\r\n                        }\r\n                    })\r\n                    .catch(function(err) {\r\n                        console.log('failed', err);\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: `请求失败`,\r\n                            button: \"确认\",\r\n                        });\r\n                    });\r\n            }\r\n        }\r\n    },\r\n     beforeCreate: function() {\r\n      document.getElementsByTagName(\"body\")[0].className=\"add_bg\";\r\n    },\r\n    beforeDestroy: function() {\r\n        document.body.removeAttribute(\"class\",\"add_bg\");\r\n    }\r\n}\r\n</script>\r\n<style lang='scss'>\r\n\r\n.add_bg{\r\n  background: url(\"~assets/img/login_bg.png\") no-repeat fixed;\r\n  background-size: 100% 100%;\r\n  width: 100%;\r\n  height: 100%;\r\n  font-size: 12px;\r\n  overflow: hidden;\r\n}\r\n\r\n.login-edit{\r\n  position: absolute;\r\n  top: 50%;\r\n  right: 10%;\r\n  margin-top: -326px;\r\n  width:546px;\r\n  height:652px;\r\n  background:rgba(255,255,255,1);\r\n  border-radius:8px;\r\n  box-shadow:0px 1px 43px 0px rgba(75,121,255,0.17);\r\n  text-align: center;\r\n  .login_title{\r\n    margin-to