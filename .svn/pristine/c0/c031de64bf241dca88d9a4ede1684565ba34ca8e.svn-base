 @dblclick=\"dblclick\">\r\n    <span class=\"page-title\">{{title}}</span>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"MyContentTitle\",\r\n  props: [\"title\"],\r\n  data() {\r\n    return {\r\n      flag: true\r\n    };\r\n  },\r\n  methods: {\r\n    dblclick() {\r\n      //放大\r\n      let homeHeader = document.getElementById(\"homeHeader\");\r\n      let aside = document.getElementById(\"aside\");\r\n      let content = document.getElementById(\"content\");\r\n      let wHeight = document.documentElement.clientHeight;\r\n      if (this.flag) {\r\n        homeHeader.style.display = \"none\";\r\n        aside.style.display = \"none\";\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 20px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 40px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.$bus.$emit(\"reloadoverview\"); //主动渲染总貌图\r\n        this.flag = !this.flag;\r\n      } else {\r\n        //缩小\r\n        homeHeader.style.display = \"block\";\r\n        aside.style.display = \"block\";\r\n        content.style.width = \"100px\"; //改变宽让宽自动渲染\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 90px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 120px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.$bus.$emit(\"reloadoverview\"); //主动渲染总貌图\r\n        this.flag = !this.flag;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-content-header {\r\n  /* width: 100%-15px;\r\n   */\r\n  box-sizing: border-box;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  background: #343c45;\r\n  font-size: 20px;\r\n  color: #fff;\r\n  padding-left: 15px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n","<template>\r\n    <div class=\"hmiSet\">\r\n        <my-content-title :title='title'></my-content-title>\r\n        <div class=\"table-btn\">\r\n            <button class=\"my-btn btn-active btn3\" @click=\"save\">保存设置</button>\r\n        </div>\r\n        <div class=\"hmiSet-content\">\r\n            <div class=\"hmiSet-block\">\r\n                <div>HMI图谱选择：</div>\r\n                <div class=\"group-block\">\r\n                    <div class=\"group-check\" v-for=\"(value, key,index) in atlasJson\">\r\n                        <input type=\"checkbox\" class=\"checkbox\" v-model=\"value.check\" :id=\"key\">\r\n                        <label :for=\"key\">{{value.text}}</label>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import MyContentTitle from 'components/common/MyContentTitle';\r\n    import getApi from 'network/getApi'\r\n    export default {\r\n        name: \"hmiSet\",\r\n        components:{\r\n            MyContentTitle\r\n        },\r\n        data(){\r\n            return{\r\n                title:'HMI图谱设置',\r\n                activeNum:'09',//当前界面的index\r\n                atlasJson:{\r\n                    \"wave\":{text:\"波形频谱图\",value:\"wave\",check:1},\r\n                    \"trend\":{text:\"趋势图\",value:\"trend\",check:1},\r\n                    \"wave3d\":{text:\"三维频谱图\",value:\"wave3d\",check:1},\r\n                    \"tmstrend\":{text:\"轨道波磨图\",value:\"tmstrend\",check:1},\r\n                    \"punchCard\":{text:\"电流卡片\",value:\"punchCard\",check:1},\r\n                    \"dip\":{text:\"倾角分布图\",value:\"dip\",check:1},\r\n                    \"overturn\":{text:\"机舱轨迹图\",value:\"overturn\",check:1},\r\n                    \"scatter\":{text:\"散点图\",value:\"scatter\",check:1},\r\n                    \"dashboard\":{text:\"危险转速区间监测\",value:\"dashboard\",check:1},\r\n                    \"railCorrugation\":{text:\"轨道波磨地图\",value:\"railCorrugation\",check:1},\r\n                    \"realSummary\":{text:\"实时数据汇总列表\",value:\"realSummary\",check:1},\r\n                    \"real\":{text:\"实时数据列表\",value:\"real\",check:1},\r\n                    \"history\":{text:\"历史数据列表\",value:\"history\",check:1},\r\n                    \"monitor\": {text:\"监测报表\",value:\"monitor\",check:1},\r\n                    \"maintain\": {text:\"设备维护记录\",value:\"maintain\",check:1},\r\n                    \"census\":{text:\"统计列表\",value:\"census\",check:1},\r\n                    \"alarm\":{text:\"报警日志\",value:\"alarm\",check:1},\r\n                    \"server\":{text:\"服务器信息\",value:\"server\",check:1},\r\n                    \"system\":{text:\"系统日志\",value:\"system\",check:1},\r\n                    \"tmswave3d\":{text:\"轨道波磨频谱瀑布\",value:\"tmswave3d\",check:1},\r\n                }\r\n            }\r\n        },\r\n        created: function(){\r\n            this.$emit('ievent', this.activeNum);\r\n            this.getHMIData();\r\n        },\r\n        methods: {\r\n            getHMIData(){\r\n                getApi.getAtlasConfig()\r\n                    .then(res=>{\r\n                        if(res.msg===0){\r\n                            let data = res.atlasConfig;\r\n                            if(!$.isEmptyObject(data)){\r\n                                for(let key in data){\r\n                                    if(data[key]===0){\r\n                                        this.$set(this.atlasJson[key],'check',0);\r\n                                    }\r\n                                }\r\n                            }\r\n                        }\r\n                        else{\r\n                            console.log('请求失败')\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            },\r\n            save(){\r\n                let obj = {};\r\n                for(let key in this.atlasJson){\r\n                    let item = this.atlasJson[key];\r\n                    obj[key] = item.check?1:0;\r\n                }\r\n                getApi.setAtlasConfig(obj)\r\n                    .then(res=>{\r\n                        if(res){\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: \"设置成功\",\r\n                                button: \"确认\",\r\n                            });\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            },\r\n\r\n        },\r\n    }\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n    @import \"~assets/css/components/hmiSet.scss\";\r\n</style>\r\n","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\n    \"div\",\n    { staticClass: \"my-content-header\", on: { dblclick: _vm.dblclick } },\n    [_c(\"span\", { staticClass: \"page-title\" }, [_vm._v(_vm._s(_vm.title))])]\n  )\n}\nvar staticRenderFns = []\nrender._withStripped = true\n\nexport { render, staticRenderFns }","var render = function() {\n  var _vm = this\n  var _h = _vm.$createElement\n  var _c = _vm._self._c || _h\n  return _c(\n    \"div\",\n    { staticClass: \"hmiSet\" },\n    [\n      _c(\"my-content-title\", { attrs: { title: _vm.title } }),\n      _vm._v(\" \"),\n      _c(\"div\", { staticClass: \"table-btn\" }, [\n        _c(\n          \"button\",\n          { staticClass: \"my-btn btn-active btn3\", on: { click: _vm.save } },\n          [_vm._v(\"保存设置\")]\n        )\n      ]),\n      _vm._v(\" \"),\n      _c(\"div\", { staticClass: \"hmiSet-content\" }, [\n        _c(\"div\", { staticClass: \"hmiSet-block\" }, [\n          _c(\"div\", [_vm._v(\"HMI图谱选择：\")]),\n          _vm._v(\" \"),\n          _c(\n            \"div\",\n            { staticClass: \"group-block\" },\n            _