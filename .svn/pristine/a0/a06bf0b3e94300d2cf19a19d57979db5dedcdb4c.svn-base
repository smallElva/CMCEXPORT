IC,GAAG,GAAG,EAAV;;AAH6B,6CAKfH,GALe;AAAA;;AAAA;AAK7B,wDAAmB;AAAA,UAAVI,CAAU;;AACjB,UAAI,CAACD,GAAG,CAACC,CAAD,CAAR,EAAa;AACXF,cAAM,CAAC5E,IAAP,CAAY8E,CAAZ;AACAD,WAAG,CAACC,CAAD,CAAH,GAAS,CAAT;AACD;AACF;AAV4B;AAAA;AAAA;AAAA;AAAA;;AAW7B,SAAOF,MAAP;AACD,C;;;;;;;;;;;;;AClND;AAAA;AAAA;AAAA;AAAA;AAAqF;AAC3B;AACL;AACc;;;AAGnE;AACmH;AACnH,gBAAgB,6HAAU;AAC1B,EAAE,4EAAM;AACR,EAAE,iFAAM;AACR,EAAE,0FAAe;AACjB;AACA;AACA;AACA;;AAEA;;AAEA;AACA,IAAI,IAAU;AACd,YAAY,mBAAO,CAAC,4JAAgG;AACpH,cAAc,mBAAO,CAAC,2DAAK;AAC3B;AACA;AACA;AACA;AACA,KAAK;AACL;AACA;AACA,sBAAsB,oIAA6C,EAAE;AAAA;AACrE;AACA,gBAAgB,iFAAM;AACtB,yBAAyB,0FAAe;AACxC,OAAO;AACP,KAAK;AACL;AACA;AACA;AACe,gF;;;;;;;;;;;;ACvCf;AAAA;AAAA,wCAAmO,CAAgB,uRAAG,EAAC,C;;;;;;;;;;;;ACAvP;AAAA;AAAA;AAAA;AAAge,CAAgB,4eAAG,EAAC,C;;;;;;;;;;;;ACApf;AAAA;AAAA;AAAA;AAAA;AAAA","file":"js/42-8aed2fe063bd16fc6d9b.js","sourcesContent":["<template>\r\n    <div id=\"record-content\">\r\n        <header class=\"myHeader\">\r\n            <div class=\"form-group\">\r\n                <div class=\"select-group\">\r\n                    <span class=\"lableName\">组织：</span>\r\n                    <select id=\"select-folder\" class=\"select-header\" v-model=\"chooseTid\" @change=\"selectFolder\">\r\n                        <option v-for=\"item in sortFolderList\" :value=\"item.t_id\" :title=\"item.t_name\">{{item.t_name}}</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"select-group\">\r\n                    <span class=\"lableName\">机组：</span>\r\n                    <select id=\"select-mac\" class=\"select-header\" v-model=\"chooseMacId\" @change=\"selectMac\">\r\n                        <option v-for=\"item in sortMacList\" :value=\"item.mac_id\" :title=\"item.mac_me\">{{item.mac_me}}</option>\r\n                    </select>\r\n                </div>\r\n                <div class=\"select-group\">\r\n                    <span class=\"lableName\">采集器：</span>\r\n                    <select id=\"select-position\" class=\"select-header\" v-model=\"chooseDgmId\" @change=\"selectPosTypeId\">\r\n                        <option v-for=\"item in chooseDgmList\" :value=\"item.dgm_id\" :title=\"item.dgm_name\">{{item.dgm_name}}</option>\r\n                    </select>\r\n                </div>\r\n            </div>\r\n            <div class=\"search-div\">\r\n                <input type=\"text\" class=\"record-input\" placeholder=\"关键字\" v-model=\"recordVal\" @input=\"searchRecord\">\r\n                <i class=\"iconfont icon-zhongzi-sousuo search-icon\" @click=\"searchRecord\"></i>\r\n            </div>\r\n            <button class=\"headerBtn\" @click=\"tableToExcel\">导出数据</button>\r\n        </header>\r\n        <div class=\"content\">\r\n            <div class=\"table_block\">\r\n                <table class=\"index_table\">\r\n                    <thead>\r\n                    <tr>\r\n                        <th>序号</th>\r\n                        <th>时间</th>\r\n                        <th>事件描述</th>\r\n                    </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                    <tr v-for=\"item in lastData\">\r\n                        <td>{{item.index}}</td>\r\n                        <td>{{item.time}}</td>\r\n                        <td>{{item.detail}}</td>\r\n                    </tr>\r\n                    </tbody>\r\n                </table>\r\n                <div class=\"noData\" v-show=\"noLogData\">当前机组无日志</div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import {compareName} from 'utils/utils'\r\n    export default {\r\n        name: \"Record\",\r\n        components: {},\r\n        data() {\r\n            return {\r\n                dataResult:[],//获取组织、机组原始信息\r\n                folderList:[],//初始组织列表\r\n                macList:{},//初始机组列表\r\n                dgmList:{},//初始采集器列表\r\n                chooseMacList:[],//选择的机组列表\r\n                chooseDgmList:[],//选择的采集器列表\r\n                chooseTid:'',//所选的组织id\r\n                chooseMacId:'',//所选的机组id\r\n                chooseDgmId: \"\",//所选的采集器id\r\n                lastData:[],//最后显示的数据列表\r\n                activeNum:3,//当前界面的index\r\n                recordVal:'',//搜索的内容\r\n                origRecordList:[],//获取到的所有的日志列表信息\r\n                params: {\r\n                    m: '',\r\n                    n: '',\r\n                },\r\n                noLogData:false,//是否有日志数据\r\n            }\r\n        },\r\n        created: function(){\r\n            this.$emit('ievent', this.activeNum);\r\n            // window.document.title = '查看数据';\r\n        },\r\n        mounted(){\r\n            this.getHeadData();\r\n\r\n        },\r\n        destroyed:function () {\r\n            if(this.chooseTid!=''){\r\n                this.paramsConfig={\r\n                    chooseTid:this.chooseTid,\r\n                    chooseMacId:this.chooseMacId,\r\n                };\r\n            }\r\n            sessionStorage.setItem('paramsConfig',JSON.stringify(this.paramsConfig));\r\n        },\r\n        computed: {\r\n            sortFolderList: function () {\r\n                return this.folderList.sort(compareName('t_name'));\r\n            },\r\n            sortMacList: function () {\r\n                return this.chooseMacList.sort(compareName('mac_me'));\r\n            }\r\n        },\r\n        methods: {\r\n            //获取组织、机组信息\r\n            getHeadData() {\r\n                let that = this;\r\n                let params=null;\r\n                params={\r\n                    type:3,\r\n                    t_ids:JSON.parse(sessionStorage.getItem('getTidList')),\r\n                };\r\n                this.$axios.post('getPositionDataConfig',params)\r\n                    .then(res => {\r\n                        if(res.msg===0){\r\n                            that.dataResult = res.list;\r\n                            let result = res.list;\r\n                            if(result.length>0){\r\n                                for(let i=0;i<result.length; i++){\r\n\r\n                                    that.folderList.push({'t_id':result[i].t_id,'t_name':result[i].t_name});\r\n                                    that.macList[result[i].t_id]=[];\r\n                                    if('mac_list' in result[i] && result[i].mac_list.length>0){\r\n                                        for(let j=0;j<result[i].mac_list.length;j++){\r\n                                            that.macList[result[i].t_id].push({'mac_id':result[i].mac_list[j].mac_id,'mac_me':result[i].mac_list[j].mac_me});\r\n                                            that.dgmList[result[i].mac_list[j].mac_id]=[];\r\n                                            if('dgms_list' in result[i].mac_list[j] ){\r\n                                                if(res.model != undefined){\r\n                                                    result[i].mac_list[j].dgms_list.push(res.model);\r\n                                                }\r\n                                                if(result[i].mac_list[j].dgms_list.length>0){\r\n                                                    for(let m=0;m<result[i].mac_list[j].dgms_list.length;m++){\r\n                                                        that.dgmList[result[i].mac_list[j].mac_id].push({\"dgm_id\":result[i].mac_list[j].dgms_list[m].dgm_id,\"dgm_name\":result[i].mac_list[j].dgms_list[m].dgm_name});\r\n                                                    }\r\n                                                }\r\n                                            }\r\n                                        }\r\n                                    }\r\n\r\n                                }\r\n                                that.chooseTid = that.sortFolderList[0].t_id;\r\n                            }\r\n                            else{\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '无数据',\r\n                                    button: \"确认\",\r\n                                });\r\n                            }\r\n                            if(!$.isEmptyObject(that.macList)){\r\n                                that.chooseMacList = that.macList[that.sortFolderList[0].t_id].concat();\r\n                                if(that.chooseMacList.length>0){\r\n                                    that.chooseMacId = that.sortMacList[0].mac_id;\r\n                                    if(!$.isEmptyObject(that.dgmList)){\r\n                                        that.chooseDgmList = that.dgmList[that.sortMacList[0].mac_id].concat();\r\n                                        if(that.chooseDgmList.length>0){\r\n                                            that.chooseDgmId = that.chooseDgmList[0].dgm_id\r\n                                        }\r\n                                    }\r\n                                }\r\n                            }\r\n                            let linkInfo = JSON.parse(sessionStorage.getItem('paramsConfig'));\r\n                            if(!$.isEmptyObject(linkInfo)){\r\n                                that.chooseTid = linkInfo.chooseTid;//所选的组织id\r\n                                that.chooseMacId = linkInfo.chooseMacId;//所选的机组id\r\n                                that.chooseMacList = that.macList[that.chooseTid].concat();\r\n                                that.chooseDgmList = that.dgmList[that.chooseMacId].concat();\r\n                                that.chooseDgmId = that.chooseDgmList[0].dgm_id;\r\n                            }\r\n                            that.getPosData();\r\n                        }\r\n                    }).catch(err => {\r\n                    console.log(err)\r\n                })\r\n            },\r\n            //改变所选组织\r\n            selectFolder(){\r\n                this.chooseMacList.length = 0;\r\n                this.chooseDgmList.length = 0;\r\n                if(!$.isEmptyObject(this.macList)){\r\n                    this.chooseMacList = this.macList[this.chooseTid].concat();\r\n                    if(this.chooseMacList.length>0){\r\n                        this.chooseMacId = this.sortMacList[0].mac_id;\r\n                        if(!$.isEmptyObject(this.dgmList)){\r\n                            this.chooseDgmList = this.dgmList[this.sortMacList[0].mac_id].concat();\r\n                            if(this.chooseDgmList.length>0){\r\n                                this.chooseDgmId = this.chooseDgmList[0].dgm_id\r\n                            }\r\n                        }\r\n                    }\r\n                }\r\n                this.getPosData();\r\n            },\r\n            //改变机组选择\r\n            selectMac(){\r\n                this.chooseDgmList.length = 0;\r\n                if(!$.isEmptyObject(this.dgmList)){\r\n                    this.chooseDgmList = this.dgmList[this.chooseMacId].concat();\r\n                    if(this.chooseDgmList.length>0){\r\n                        this.chooseDgmId =  this.chooseDgmList[0].dgm_id\r\n                    }\r\n                }\r\n                this.getPosData();\r\n            },\r\n            //改变测点选择\r\n            selectPosTypeId(){\r\n                this.getPosData();\r\n            },\r\n            //获取日志信息\r\n            getPosData(){\r\n                let that = this;\r\n                if(that.chooseMacId !=='' && that.chooseDgmId !==''){\r\n                    let params=null;\r\n                    params = {\r\n                        machine_id:that.chooseMacId,\r\n                        dgm_id:that.chooseDgmId\r\n                    };\r\n                    // params={\"machine_id\":\"19092417431478431\",\"dgm_id\":\"1210861\"};\r\n                    this.$axios.post('getLog',params)\r\n                        .then(res => {\r\n                            that.lastData = [];\r\n                            if(res.msg===0 && res.list.length>0){\r\n                                that.noLogData = false;\r\n                                for(let i=0;i<res.list.length; i++){\r\n                                    let obj = {};\r\n                                    obj.index = Number(i+1);\r\n                                    obj.time = that.translateDate(res.list[i].savetime_com);\r\n                                    obj.detail = that.descLabel(res.list[i].code);\r\n                                    that.lastData.push(obj);\r\n                                }\r\n                                that.origRecordList = that.lastData.concat();\r\n                            }\r\n                            else{\r\n                                that.noLogData = true;\r\n                            }\r\n                        }).catch(err => {\r\n                        console.log(err)\r\n                    })\r\n                }\r\n            },\r\n            //搜索日志\r\n            searchRecord(){\r\n                var reg = RegExp(this.recordVal);\r\n                var that = this;\r\n                if (that.recordVal.length>0) {\r\n                    that.lastData = [];\r\n                    // 过滤需要的数据\r\n                    for(let i=0;i<that.origRecordList.length;i++){\r\n                        if (reg.test(that.origRecordList[i].detail)) {\r\n                            that.lastData.push(that.origRecordList[i]);\r\n                        }\r\n                    }\r\n                }\r\n                else{\r\n                    that.lastData=that.origRecordList.concat();\r\n                }\r\n\r\n            },\r\n            //转换日期方法\r\n            translateDate(value){\r\n                value = new Date(value);\r\n                value = value.getFullYear() + '-'\r\n                    + (value.getMonth() + 1) + '-'\r\n                    + this.fixTime(value.getDate()) + ' '\r\n                    + this.fixTime(value.getHours()) + ':'\r\n                    + this.fixTime(value.getMinutes()) + ':'\r\n                    + this.fixTime(value.getSeconds());\r\n                return value;\r\n            },\r\n            //补0\r\n            fixTime (str) {\r\n                if (Number(str) < 10) {\r\n                    str = \"0\" + str;\r\n                }\r\n                return str;\r\n            },\r\n            //切换按钮\r\n            toggleBtn(){\r\n                this.isReal = !this.isReal;\r\n                if(this.isReal){\r\n                    this.toggleText = '实时';\r\n                }\r\n                else{\r\n                    this.toggleText = '历史';\r\n                }\r\n            },\r\n            //导出数据\r\n            tableToExcel(){\r\n                //要导出的json数据\r\n                var jsonData = this.lastData;\r\n                //列标题，逗号隔开，每一个逗号就是隔开一个单元格\r\n                let str = `序号,时间,事件描述\\n`;\r\n                //增加\\t为了不让表格显示科学计数法或者其他格式\r\n                for(let i = 0 ; i < jsonData.length ; i++ ){\r\n                    for(let item in jsonData[i]){\r\n                        str+=`${jsonData[i][item] + '\\t'},`;\r\n                    }\r\n                    str+='\\n';\r\n                }\r\n                //encodeURIComponent解决中文乱码\r\n                let uri = 'data:text/csv;charset=utf-8,\\ufeff' + encodeURIComponent(str);\r\n                //通过创建a标签实现\r\n                var link = document.createElement(\"a\");\r\n                link.href = uri;\r\n                //对下载的文件命名\r\n                link.download =  \"设备日志表.csv\";\r\n                document.body.appendChild(link);\r\n                link.click();\r\n                document.body.removeChild(link);\r\n            },\r\n            //code对应的事件描述\r\n            getCode(m){\r\n                let codeObj = {\r\n                    \"500\":\"采集器要设置等待超时\",\r\n                    \"501\":\"采集器发送空闲等待超时\",\r\n                    \"502\":\"采集器倾角要数据超时\",\r\n                    \"1000\":`采集器振动通道${t