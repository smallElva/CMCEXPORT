ios.post('updateSyncDriveChain',data)\r\n                            .then(res => {\r\n                                if (res.msg === 0) {\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: \"同步成功\",\r\n                                        button: \"确认\",\r\n                                    });\r\n                                    for (let i = 0, l = machine.length; i < l; i++) {\r\n                                        machine[i].isCheck && (machine[i].isCheck = false)\r\n                                    }\r\n                                    that.getFolder();\r\n                                }\r\n                                else if(res.msg === 1){\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: \"同步失败\",\r\n                                        button: \"确认\",\r\n                                    });\r\n                                }\r\n                                else if(res.msg === 4){\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: `${res.macName}机组同步后传动链数量将超过64`,\r\n                                        button: \"确认\",\r\n                                    });\r\n                                    for (let i = 0, l = machine.length; i < l; i++) {\r\n                                        machine[i].isCheck && (machine[i].isCheck = false)\r\n                                    }\r\n                                    that.getFolder();\r\n                                }\r\n                                else if(res.msg === 5){\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: \"绑定转速测点的机泵传动链不能作为同步基准\",\r\n          