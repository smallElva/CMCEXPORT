ty + item2.y,\r\n                                bx2 = item2.line.bx + item2.x,\r\n                                by2 = item2.line.by + item2.y;\r\n                            if (Math.abs(bx1 - tx2) >= 0 && Math.abs(bx1 - tx2) <= 30 && Math.abs(by1 - ty2) >= 0 && Math.abs(by1 - ty2) <= 30) {\r\n                                x += (tx2 - bx1);\r\n                                y += (ty2 - 10 - by1);\r\n                                break;\r\n                            }\r\n                            if (Math.abs(bx2 - tx1) >= 0 && Math.abs(bx2 - tx1) <= 30 && Math.abs(by2 - ty1) >= 0 && Math.abs(by2 - ty1) <= 30) {\r\n                                x += (bx2 - tx1);\r\n                                y += (by2 + 10 - ty1);\r\n                                break;\r\n                            }\r\n                        }\r\n                    }\r\n                }\r\n                let pos = {\r\n                    x: x - item.x,\r\n                    y: y - item.y,\r\n                }\r\n                item.x = x;\r\n                item.y = y;\r\n                item.style = {\r\n                    left: x + 'px',\r\n                    top: y + 'px',\r\n                    zIndex: 3,\r\n                }\r\n                if (this.frameSelect.length > 0) {\r\n                    return pos\r\n                }\r\n            },\r\n            // 传动链内部拖动\r\n            dragScreen(index, e) {\r\n                if (this.isShow.isEdit) {\r\n                    this.dragMsg = null;\r\n                    this.dragMsg = {\r\n                        index: index,\r\n                        left: e.offsetX,\r\n                        top: e.offsetY,\r\n                    }\r\n                }\r\n            },\r\n\r\n            // 右键点击部件\r\n            rightClick(item, index, e) {\r\n                if (this.isShow.isEdit && this.chainModelSystem!==0) {\r\n                    this.frameSelect.length = 0;\r\n                    this.isShow.isRight