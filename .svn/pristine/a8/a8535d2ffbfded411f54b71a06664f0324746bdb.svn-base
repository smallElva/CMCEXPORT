\n  <div class='myHeader'>\r\n    <div class=\"header\">\r\n        <div class=\"header-user\">\r\n            <p @click='goback'>\r\n                <img src=\"~assets/img/logo.png\">\r\n            </p>\r\n            <p @click='showUserMsg' id=\"userName\" class=\"userName\">{{userMsg[0].val}}</p>\r\n        </div>\r\n        <div class=\"header-content\">\r\n            <div class=\"search-div clearfloat\">\r\n                <div class=\"search-block\">\r\n                    <a @click='getLicenceInfo' title=\"Licence信息\" class=\"licence-a\">\r\n                        <i class=\"warning-icon iconfont icon-jihuo\"></i>\r\n                    </a>\r\n                    <a @click='goback' title=\"帮助页面\">\r\n                        <i class=\"warning-icon iconfont icon-zhongzi_yindaotubiao\"></i>\r\n                    </a>\r\n                    <a class=\"search-btn checkData-btn refresh-data-btn\" @click=\"findAll\">重新获取数据</a>\r\n                    <a class=\"search-btn checkData-btn\" @click=\"goAnother\" v-show=\"navShow\">查看数据</a>\r\n                    <a class=\"log_out\" @click=\"loginOut()\">退出账号</a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <!-- 用户信息弹窗-->\r\n    <div v-show='isAlertUser' class='alertBoxHeader'>\r\n      <div class='alertUser'>\r\n        <div class='alertUserTitle'>\r\n          当前用户\r\n          <i @click='closeAlertUser' class='iconfont icon-zhongzi-guanbi closeAlertUser'></i>\r\n        </div>\r\n        <ul>\r\n          <li v-for='item in userMsg' class='userItem'>\r\n            <div class='userMsgTitle'>{{item.title}}</div>\r\n            <div class='userMsgText'>{{item.val}}</div>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n    <!-- license信息弹窗-->\r\n    <div v-show='isAlertLicense' class='alertBoxHeader'>\r\n          <div class='alertUser alertLicense'>\r\n              <div class='alertUserTitle'>\r\n                  License信息\r\n                  <i @click='closeAlertLicense' class='iconfont icon-zhongzi-guanbi closeAlertUser'></i>\r\n              </div>\r\n              <ul>\r\n                  <li v-for='item in licenseMsg' class='licenseItem'>\r\n                      <div class=\"licenseTitleMsg\">{{item.title}}:</div>\r\n                      <div class=\"licenseTitleText\">{{item.val}}</div>\r\n                  </li>\r\n              </ul>\r\n              <div class=\"btn-block\">\r\n                  <button class=\"sure-btn\" type=\"button\" @click='closeAlertLicense'>确定</button>\r\n              </div>\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"myHeader\",\r\n  props: ['userMsg'],\r\n  data() {\r\n      return {\r\n          isAlertUser: false,\r\n          isAlertLicense: false,//license弹窗\r\n          type:Number(sessionStorage.getItem('userType')),\r\n          username:sessionStorage.getItem('username'),\r\n          licenseMsg:[\r\n              {\r\n                  title:'机器码',\r\n                  val:'',\r\n              },\r\n              {\r\n                  title:'有效期',\r\n                  val:'',\r\n              },\r\n              {\r\n                  title:'采集器数目限制',\r\n                  val:'',\r\n              },\r\n          ]\r\n      }\r\n  },\r\n  created() {\r\n  },\r\n  mounted() {\r\n      $('#adv-search').hover(function () {\r\n\r\n      }, function () {\r\n          $(this).fadeOut(500);\r\n      })\r\n      this.token = sessionStorage.getItem('token');\r\n  },\r\n    computed: {\r\n        navShow() {\r\n            if(this.type===0 && this.username==='manager'){\r\n                return false;\r\n            }\r\n            else{\r\n                return true\r\n            }\r\n        },\r\n    },\r\n  methods: {\r\n    findAll(){\r\n        // sessionStorage.clear();\r\n      sessionStorage.removeItem('filter_alarm1');\r\n      sessionStorage.removeItem('filter_alarm2');\r\n      sessionStorage.removeItem('filter_alarm3');\r\n      // sessionStorage.removeItem('dgm_id');\r\n      // sessionStorage.removeItem('dgm_names');\r\n      sessionStorage.removeItem('filter_type1');\r\n      sessionStorage.removeItem('filter_type2');\r\n      sessionStorage.removeItem('filter_type3');\r\n      sessionStorage.removeItem('filter_dgm_name');\r\n      sessionStorage.removeItem('filter_mac_name');\r\n      sessionStorage.removeItem('filter_tree_name');\r\n      sessionStorage.removeItem('LinkConfig');\r\n      this.$router.go(0)\r\n    },\r\n    goAnother(){\r\n        let routeUrl = this.$router.resolve({\r\n            path: \"/viewInterface\",\r\n        });\r\n        window.open(routeUrl.href, '_blank');\r\n    },\r\n    goback:function(){\r\n        this.$router.push({name: \"home\"})\r\n    },\r\n    showAdvSearch: function () {\r\n        $('#adv-search').fadeIn(500);\r\n    },\r\n    getLicenceInfo(){//获取licence信息\r\n        //获取licence信息\r\n        this.isAlertLicense = true;\r\n        this.$getApi.getLicense()\r\n            .then(res=>{\r\n                let days = Math.ceil(res.validTime/1000/3600/24);\r\n                this.licenseMsg[0].val = res.uuid;\r\n                this.licenseMsg[1].val = days + '天';\r\n                this.licenseMsg[2].val = res.num;\r\n            })\r\n            .catch(function(err) {\r\n                console.log('failed', err);\r\n                swal({\r\n                    title: \"提示\",\r\n                    text: `请求错误`,\r\n                    button: \"确认\",\r\n                });\r\n            });\r\n    },\r\n    loginOut: function () {\r\n        this.$router.push({name:'login'});\r\n        this.$axios.post('loginOut',this.token)\r\n          .then(res=>{\r\n              sessionStorage.clear();\r\n            switch (parseInt(res.msg)) {\r\n                    case 1:\r\n                        console.log(\"token为null\");\r\n                        break;\r\n            }\r\n          })\r\n            .catch(err=>{\r\n                console.log('请求错误',err)\r\n            })\r\n\r\n    },\r\n    // 显示查询当前用户信息\r\n    showUserMsg() {\r\n      this.isAlertUser = true;\r\n    },\r\n    //  关闭查询当前用户信息\r\n    closeAlertUser() {\r\n      this.isAlertUser = false;\r\n    },\r\n    //关闭licence信息\r\n    closeAlertLicense() {\r\n        this.isAlertLicense = false;\r\n    }\r\n  },\r\n}\r\n\r\n</script>\r\n\r\n<style scoped lang='scss'>\r\n    /*头部*/\r\n    .myHeader{\r\n        .header{\r\n            display: -webkit-flex; /* Safari */\r\n            display: flex;\r\n            width: 100%;\r\n            height: 80px;\r\n            .header-user{\r\n                flex:0 0 160px;\r\n                padding-top: 15px;\r\n                background: #343C45;\r\n                text-align: center;\r\n                .userName{\r\n                    display: inline-block;\r\n                    text-align: left;\r\n                    vertical-align: middle;\r\n                    /*margin: 10px 0;*/\r\n                    height: 27px;\r\n                    line-height: 27px;\r\n                    font-size:14px;\r\n                    font-family:MicrosoftYaHei;\r\n                    color: #fff;\r\n                    cursor: pointer;\r\n                    max-width: 120px;\r\n                    overflow: hidden;\r\n                    white-space: nowrap;\r\n                    text-overflow: ellipsis;\r\n                    &:hover{\r\n                        color: #597DD2;\r\n                    }\r\n                }\r\n\r\n            }\r\n            .header-content{\r\n                flex:1 1 auto;\r\n                padding: 20px 30px 0 30px;\r\n                position: relative;\r\n                .search-div{\r\n                    .search-block{\r\n                        float: right;\r\n                        .warning-icon{\r\n                            vertical-align: middle;\r\n                            font-size: 34px;\r\n                            color:#597DD2;\r\n                            &:hover{\r\n                                color:darken(#597DD2,15%);\r\n                            }\r\n                        }\r\n                        .search-input{\r\n                            width: 300px;\r\n                            height: 36px;\r\n                            border:1px solid rgba(125,125,125,1);\r\n                            border-radius:18px;\r\n                            padding: 0 20px;\r\n                            vertical-align: middle;\r\n                            margin: 0 0 0 15px;\r\n                        }\r\n                        .icon-header-search{\r\n                            font-size: 20px;\r\n                            position: relative;\r\n                            left: -50px;\r\n                            top: 4px;\r\n                            color: #888;\r\n                            cursor: pointer;\r\n                        }\r\n                        .search-btn{\r\n                            vertical-align: middle;\r\n                            font-size:16px;\r\n                            font-family:MicrosoftYaHei;\r\n                            color: #fff;\r\n                            background: #597DD2;\r\n                            border: none;\r\n                            width:100px;\r\n                            height:30px;\r\n                            line-height: 30px;\r\n                            border-radius: 4px;\r\n                            &:hover{\r\n                                color: #fff;\r\n                                background: darken(#597DD2, 10%);\r\n                            }\r\n                        }\r\n                        .checkData-btn{\r\n                            display: inline-block;\r\n                            text-align: center;\r\n                            margin: 0 20px;\r\n                        }\r\n                        .refresh-data-btn{\r\n                            width:120px;\r\n                            margin-right: 0;\r\n                        }\r\n                        .log_out{\r\n                            display: inline-block;\r\n                            vertical-align: middle;\r\n                            padding: 0 5px;\r\n                            height: 28px;\r\n                            line-height: 28px;\r\n                            border: 1px solid #050001;\r\n                            color: #050001;\r\n                            border-radius: 4px;\r\n                            &:hover{\r\n                                border-color: #597DD2;\r\n                                color: #597DD2;\r\n                            }\r\n                        }\r\n                    }\r\n\r\n                }\r\n                .header-info{\r\n                    display: none;\r\n                    .info-name{\r\n                        display: inline-block;\r\n                        padding-right: 50px;\r\n                        font-size: 16px;\r\n                    }\r\n                }\r\n                .adv-search{\r\n                    display: none;\r\n                    position: absolute;\r\n                    top: 80px;\r\n                    left: 30px;\r\n                    z-index: 9991;\r\n                    width: calc(100% - 140px);\r\n                    padding: 20px 40px;\r\n                    height: 90px;\r\n                    background:rgba(52,60,69,0.9);\r\n                    border-radius:4px;\r\n                    .search-line1{\r\n                        position: relative;\r\n                        .choose-item{\r\n                            display: inline-block;\r\n                            vertical-align: middle;\r\n                            margin-right: 40px;\r\n                            margin-bottom: 10px;\r\n                            .choose-label{\r\n                                color: #fff;\r\n                            }\r\n                            .choose-select{\r\n                                display: inline-block;\r\n                                vertical-align: middle;\r\n                                background: transparent;\r\n                                width: 180px;\r\n                                height: 36px;\r\n                                line-height: 36px;\r\n                                border-color: #fff;\r\n                                border-radius: 3px;\r\n                                color: #fff;\r\n                            }\r\n                        }\r\n                        .choose-btns{\r\n                            position: absolute;\r\n                            top: 7px;\r\n                            right: 0;\r\n                            .choose-btn{\r\n                                width: 100px;\r\n                                height: 32px;\r\n                                line-height: 32px;\r\n                                background: #597DD2;\r\n                                border:none;\r\n                                border-radius:4px;\r\n                                color: #fff;\r\n                                text-align: center;\r\n                                &:hover{\r\n                                    background: darken(#597DD2,10%);\r\n                                }\r\n                            }\r\n                            .add-btn{\r\n                                background: #E0E0E0;\r\n                                color: #000;\r\n                                margin-top: 20px;\r\n                                &:hover{\r\n                                    background: darken(#E0E0E0,10%);\r\n                                }\r\n                            }\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        .licence-a{\r\n            margin-right: 15px;\r\n        }\r\n        #userName {\r\n            cursor: pointer;\r\n        }\r\n        .alertBoxHeader {\r\n            width: 100%;\r\n            height: 100%;\r\n            position: fixed;\r\n            z-index: 999;\r\n            left: 0;\r\n            top: 0;\r\n        }\r\n        .alertUser {\r\n            width: 300px;\r\n            height: 200px;\r\n            position: absolute;\r\n            top: 50%;\r\n            left: 50%;\r\n            margin-top: -100px;\r\n            margin-left: -150px;\r\n            border-radius: 4px;\r\n            background: #E4E8F4;\r\n            box-shadow:0px 2px 10px 0px rgba(97,122,211,1);\r\n        }\r\n        .alertLicense{\r\n            width: 500px;\r\n            height: 250px;\r\n            margin-top: -175px;\r\n            margin-left: -250px;\r\n            box-shadow:0px 2px 10px 0px rgba(97,122,211,1);\r\n        }\r\n        .alertUserTitle {\r\n            height: 40px;\r\n            line-height: 40px;\r\n            position: relative;\r\n            padding-left: 15px;\r\n            background: #343C45;\r\n            color: #fff;\r\n            border-top-left-radius: 4px;\r\n            border-top-right-radius: 4px;\r\n            font-size: 16px;\r\n        }\r\n        .alertContent{\r\n            padding: 20px 0;\r\n            border: 1px solid #dcdcdc;\r\n        }\r\n        .closeAlertUser {\r\n            position: absolute;\r\n            right: 10px;\r\n            top: 0;\r\n            font-size: 20px!important;\r\n            color: #F53500;\r\n            cursor: pointer;\r\n            &:hover{\r\n                color: darken(#F53500,15%);\r\n            }\r\n        }\r\n        .userItem {\r\n            height: 40px;\r\n            line-height: 40px;\r\n            display: flex;\r\n            align-items: center;\r\n            margin: 20px 0;\r\n        }\r\n        .userMsgTitle {\r\n            width: 90px;\r\n            text-align: right;\r\n            margin-right: 10px;\r\n            font-size: 18px;\r\n        }\r\n        .userMsgText {\r\n            padding: 0 10px;\r\n            height: 36px;\r\n            line-height: 36px;\r\n            font-size: 18px;\r\n            border: 1px solid #9D9D9D;\r\n            width: 60%;\r\n            box-sizing: border-box;\r\n        }\r\n        .licenseItem{\r\n            height: 40px;\r\n            line-height: 40px;\r\n            margin: 10px 0;\r\n            padding: 0 15px;\r\n            display: flex;\r\n            align-items: center;\r\n        }\r\n        .licenseTitleMsg{\r\n            font-size: 18px;\r\n        }\r\n        .licenseTitleText{\r\n            font-size: 18px;\r\n            padding-left: 10px;\r\n        }\r\n        .btn-block{\r\n            text-align: center;\r\n            padding-top: 10px;\r\n            border-top: 1px solid #798CA1;\r\n        }\r\n        .sure-btn{\r\n            font-size: 14px;\r\n            width: 80px;\r\n            padding: 5px;\r\n            border: none;\r\n            border-radius: 4px;\r\n            background: #6e8dd7;\r\n            color: #fff;\r\n            &:hover,&:focus,&:active{\r\n                background: darken(#6e8dd7,5%);\r\n            }\r\n        }\r\n        @media screen and (max-height: 800px) {\r\n            .myHeader{\r\n                .header {\r\n                    height: 70px;\r\n                    .header-user{\r\n                        padding-top: 10px;\r\n                    }\r\n                    .header-content{\r\n                        padding: 12px 30px 0 30px;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n</style>\r\n","<template>\r\n  <div id=\"home_aside\">\r\n    <div class=\"aside-bar\">\r\n      <ul class=\"aside-nav\">\r\n          <li class=\"aside-item\" data-nav=\"0\" v-show=\"navShow\">\r\n              <router-link :to=\"router1\" active-class=\"active\"><i class=\"nav-icon iconfont icon-xitongguanli\">