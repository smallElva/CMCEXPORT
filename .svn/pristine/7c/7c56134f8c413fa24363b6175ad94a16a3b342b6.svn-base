                       list.state = true;\r\n                            } else {\r\n                                list.state = false;\r\n                            }\r\n                        } else {\r\n                            list.state = true;\r\n                        }\r\n                        break;\r\n                    case 2: //叶节点\r\n                        nextState = !item.state;\r\n                        if (nextState == false) {\r\n                            flag_one_check = false;\r\n                            for (j = 0; j < child.children.length; j++) {\r\n                                t_item = child.children[j];\r\n                                if (t_item.id == item.id) {\r\n                                    continue;\r\n                                }\r\n                                if (t_item.state == true) {\r\n                                    flag_one_check = true;\r\n                                    break;\r\n                                }\r\n                            }\r\n                            if (flag_one_check) {\r\n                                child.state = true;\r\n                            } else {\r\n                                child.state = false;\r\n                            }\r\n                        } else {\r\n                            child.state = true;\r\n                        }\r\n                        if (child.state == false) {\r\n                            flag_one_check = false;\r\n                            for (i = 0; i < list.children.length; i++) {\r\n                                t_child = list.children[i];\r\n                                if (t_child.id == child.id) {\r\n                                    continue;\r\n                                }\r\n                                if (t_child.state == true) {\r\n                                    flag_one_check = true;\r\n                                    break;\r\n                          