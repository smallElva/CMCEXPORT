                   title:'左对齐'\r\n                },\r\n                {\r\n                    icon:'icon-zuoyoujuzhong_huaban11',\r\n                    title:'水平居中'\r\n                },\r\n                {\r\n                    icon:'icon-youduiqi_huaban1',\r\n                    title:'右对齐'\r\n                },\r\n                {\r\n                    icon:'icon-dingduiqi_huaban1',\r\n                    title:'顶部对齐'\r\n                },\r\n                {\r\n                    icon:'icon-shangxiajuzhong_huaban11',\r\n                    title:'垂直居中'\r\n                },\r\n                {\r\n                    icon:'icon-diduiqi_huaban1',\r\n                    title:'底部对齐'\r\n                },\r\n            ]\r\n        };\r\n    },\r\n    mounted() {\r\n        if(this.$store.state.tids.length>0){\r\n            this.getTidData();//获取所有的机组设置数据\r\n        };\r\n    },\r\n    watch:{\r\n        overviewFont(val, oldVal){//标题字号改变\r\n            this.$refs.fontInput.style.fontSize = this.overviewFont + 'px';\r\n        },\r\n    },\r\n    computed: {\r\n        sortFolderList1: function () {\r\n            return this.searchFolderList1.sort(compareName('t_name'));\r\n        },\r\n        sortFolderList2: function () {\r\n            return this.searchFolderList2.sort(compareName('t_name'));\r\n        },\r\n        sortFolderList3: function () {\r\n            return this.searchFolderList3.sort(compareName('t_name'));\r\n        },\r\n        sortFolderList4: function () {\r\n            return this.searchFolderList4.sort(compareName('t_name'));\r\n        },\r\n        sortPumpList: function () {\r\n            return this.searchPumpList.sort(compareName('pump_name'));\r\n        },\r\n\r\n    },\r\n    methods: {\r\n        //保存总貌图\r\n        /**\r\n         * type: 1组织总貌；3：机泵总貌\r\n         * **/\r\n        keepOverview(type){\r\n            let overViewObj = {},w,ppx,ppy;\r\n            if(type===1){//组织总貌图\r\n                overViewObj = {\r\n                    o_id:this.chooseFolder.t_id,//总貌图uid（=组织/机组/机泵uid）\r\n                    mac_id:\"A\",\r\n                    overview_info:{},\r\n                    itemList:[],//节点列表\r\n                };//总貌图\r\n            }\r\n            else if(type===3){//机泵总貌图\r\n                overViewObj = {\r\n                    o_id:this.chooseMac.pump_id,//总貌图uid（=组织/机组/机泵uid）\r\n                    mac_id:this.chooseMac.mac_id,\r\n                    overview_info:{},\r\n                    itemList:[],//节点列表\r\n                };//总貌图\r\n            }\r\n            overViewObj.overview_info={\r\n                version:1,\r\n                o_name:this.o_name,//总貌图名称\r\n                overviewFont:this.overviewFont,//总貌图名称字号大小\r\n                bg_width:this.origBlock.oriW,//背景宽\r\n                bg_height:this.origBlock.oriH,//背景高\r\n            }\r\n            let obj={},finalX,finalY,finalCircleX,finalCircleY;\r\n\r\n            for(let i=0;i<this.MoveMsg.length;i++){\r\n                let item = this.MoveMsg[i];\r\n                finalX = item.x + item.moveX;\r\n                finalY = item.y + item.moveY;\r\n                finalCircleX = item.oriCircleX + item.moveCircleX;\r\n                finalCircleY = item.oriCircleY + item.moveCircleY;\r\n                item.px = (finalX/this.origBlock.oriW).toFixed(2);\r\n                item.py = (finalY/this.origBlock.oriH).toFixed(2);\r\n                item.circleX = (finalCircleX/this.origBlock.oriW).toFixed(2);\r\n                item.circleY = (finalCircleY/this.origBlock.oriH).toFixed(2);\r\n                obj={\r\n                    id:i,//计数id\r\n                    type:type,//类型 1=组织 2=机组 3=机泵 4=测点\r\n                    itemType:this.MoveMsg[i].itemType,//设备类型 如1=破碎机\r\n                    uid:this.MoveMsg[i].uid,//节点uid，如\"3_05\"，表示id=05的振动测点\r\n                    px:(finalX/this.origBlock.oriW).toFixed(2),//相对于背景图的x位置\r\n                    py:(finalY/this.origBlock.oriH).toFixed(2),//相对于背景图的y位置\r\n                    width:this.MoveMsg[i].width,\r\n                    height:this.MoveMsg[i].height,\r\n                    itemName:this.MoveMsg[i].text,//编辑的部件名\r\n                    circleD:this.MoveMsg[i].circleD,//圆点的直径\r\n                    circleX: (finalCircleX/this.origBlock.oriW).toFixed(2),//小圆百分比的x位置\r\n                    circleY: (finalCircleY/this.origBlock.oriH).toFixed(2),//小圆百分比的y\r\n                };\r\n                overViewObj.itemList.push(obj);\r\n            }\r\n            this.$axios.post('updateTreeOverview',overViewObj)\r\n                .then(res=>{\r\n                    if(res.msg==0){\r\n                        // swal({\r\n                        //     title: \"提示\",\r\n                        //     text: '保存成功',\r\n                        //     button: \"确认\",\r\n                        // });\r\n                        this.queryOverview();//更新视图\r\n                    }\r\n                    else if(res.msg==1){\r\n                        console.log('参数为空')\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '保存失败',\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                    else if(res.msg==2){\r\n                        console.log('后台运行错误')\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: '保存失败',\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求失败',err)\r\n                })\r\n        },\r\n        //上传总貌图背景图\r\n        /**\r\n         * type: 1组织总貌；2：机泵总貌\r\n         * **/\r\n        selectImg(type){\r\n            let that=this;\r\n            if(that.MoveMsg.length){\r\n                let finalX,finalY,finalCircleX,finalCircleY;\r\n                that.MoveMsg.forEach(item=>{\r\n                    finalX = item.x + item.moveX;\r\n                    finalY = item.y + item.moveY;\r\n                    finalCircleX = item.oriCircleX + item.moveCircleX;\r\n                    finalCircleY = item.oriCircleY + item.moveCircleY;\r\n                    item.px = (finalX/this.origBlock.oriW).toFixed(2);\r\n                    item.py = (finalY/this.origBlock.oriH).toFixed(2);\r\n                    item.circleX = (finalCircleX/this.origBlock.oriW).toFixed(2);\r\n                    item.circleY = (finalCircleY/this.origBlock.oriH).toFixed(2);\r\n                });\r\n            }\r\n            let imgFile = $('#imgLocal')[0].files[0];//取到上传的图片\r\n\r\n            let param = new FormData(); // 创建form对象\r\n            param.append('files', imgFile);//对应后台接收图片名\r\n            if(type===1){\r\n                param.append('id', this.chooseFolder.t_id);\r\n                param.append('mac_id', \"A\");\r\n            }\r\n            else if(type===2){\r\n                param.append('id', this.chooseMac.pump_id);\r\n                param.append('mac_id', this.chooseMac.mac_id);\r\n            }\r\n            let overview_json = {\r\n                version: 1,\r\n                o_name: that.o_name,\r\n                overviewFont: that.overviewFont\r\n            };\r\n            overview_json = JSON.stringify(overview_json);\r\n            param.append('overview_json', overview_json);\r\n            this.$axios.post('createTreeOverview',param,{\r\n                headers: {\r\n                    'Content-Type': 'multipart/form-data',\r\n                }\r\n            }).then(res => {\r\n                // console.log(res);//\r\n                $('#imgLocal')[0].value = \"\";\r\n                that.overviewUrl = '';\r\n                if(res.msg==0){\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '上传成功',\r\n                        button: \"确认\",\r\n                    });\r\n                    that.queryOverview();//更新视图\r\n                }\r\n                else if(res.msg==1){\r\n                    console.log(\"参数为空\");\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '上传失败',\r\n                        button: \"确认\",\r\n                    });\r\n                }\r\n                else if(res.msg==2){\r\n                    console.log(\"后台运行错误\")\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '上传失败',\r\n                        button: \"确认\",\r\n                    });\r\n                }\r\n                else if(res.msg==3){\r\n                    console.log(\"获取json数据时为空\")\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '上传失败',\r\n                        button: \"确认\",\r\n                    });\r\n                }\r\n            }).catch(function(error){\r\n                console.log(error);\r\n                $('#imgLocal')[0].value = \"\";\r\n            })\r\n                .catch(err=>{\r\n                    console.log('请求错误',err)\r\n                })\r\n        },\r\n        //获取总貌图数据\r\n        queryOverview(){\r\n            let that=this;\r\n            let tidObj={\r\n                t_id:this.chooseFolder.t_id,\r\n                type:type\r\n            };\r\n            this.$axios.post('queryPumpOverviewInfoByTId',tidObj)\r\n                .then(res=>{\r\n                    if(type===1){\r\n                        that.overviewUrl='';\r\n                        if(res.msg==0){\r\n                            that.MoveMsg=[];\r\n                            let result  = res.folder.overview;\r\n                            if(!$.isEmptyObject(result)){\r\n                                that.overviewUrl= result.bgurl;\r\n                                if(result.o_name){\r\n                                    tha