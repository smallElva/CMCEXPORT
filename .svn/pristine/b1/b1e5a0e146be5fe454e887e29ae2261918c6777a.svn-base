r: #fff;\n    width: 112px;\n    height: 32px;\n    line-height: 32px;\n    text-align: center;\n    border: 1px solid #fff;\n    border-radius: 3px;\n    background: transparent;\n}\n.overviewPump-content .toggle-btn:hover, .overviewPump-content .keep-btn:hover {\n      background: #fff;\n      color: #000;\n}\n.overviewPump-content .keep-btn {\n    right: 180px;\n}\n.overviewPump-content .over-search-input {\n    width: 200px;\n    height: 34px;\n    padding: 2px 12px 2px 16px;\n    border: 1px solid #888;\n    border-radius: 4px;\n    font-size: 16px;\n}\n.overviewPump-content .search-list-div {\n    position: absolute;\n    top: 50px;\n    left: 0;\n    z-index: 2;\n    width: 228px;\n    margin-top: 10px;\n    background: #fff;\n    border: 1px solid #888;\n}\n.overviewPump-content .search-list-div2 {\n    top: 100px;\n}\n.overviewPump-content .search-pump-div {\n    width: 226px;\n    margin-top: 10px;\n    border: none;\n    max-height: calc(100vh - 320px);\n    overflow: auto;\n}\n.overviewPump-content .search-result {\n    width: 226px;\n    max-height: 300px;\n    overflow-y: auto;\n}\n.overviewPump-content .search-result .search-li {\n      padding: 5px 10px 5px 0;\n      font-size: 16px;\n      color: #000;\n}\n.overviewPump-content .search-result .search-li:hover, .overviewPump-content .search-result .search-li.active {\n        background: #E4E8F4;\n        cursor: pointer;\n}\n.overviewPump-content .search-result .search-li:hover .gou-icon, .overviewPump-content .search-result .search-li.active .gou-icon {\n          opacity: 1;\n}\n.overviewPump-content .gou-icon {\n    font-size: 18px;\n    opacity: 0;\n}\n.overviewPump-content .controls {\n    padding: 10px 0;\n    text-align: center;\n}\n.overviewPump-content .controls .btn {\n      width: 60px;\n      height: 30px;\n      line-height: 30px;\n      text-align: center;\n      font-size: 16px;\n      border: none;\n      border-radius: 4px;\n      margin: 0 5px;\n      color: #fff;\n}\n.overviewPump-content .controls .ok-btn {\n      background: #597DD2;\n}\n.overviewPump-content .controls .ok-btn:hover {\n        background: #2f55af;\n}\n.overviewPump-content .controls .close-btn {\n      background: #B6C0CF;\n}\n.overviewPump-content .controls .close-btn:hover {\n        background: #8898b1;\n}\n.overviewPump-content .pump-result {\n    max-height: calc(100% - 80px);\n    overflow-y: auto;\n}\n.overviewPump-content .pump-li {\n    padding: 5px 10px;\n    font-size: 16px;\n}\n.overviewPump-content .pump-li:hover, .overviewPump-content .pump-li.active {\n      background: #E4E8F4;\n      cursor: pointer;\n}\n.overviewPump-content .upload, .overviewPump-content .upload2 {\n    position: absolute;\n    bottom: 12px;\n    left: 10px;\n}\n.overviewPump-content .upload .btn, .overviewPump-content .upload2 .btn {\n      font-size: 16px;\n      background: #597DD2;\n      color: #fff;\n      border: none;\n      text-align: center;\n      padding: 5px 10px;\n      border-radius: 4px;\n}\n.overviewPump-content .upload .btn:hover, .overviewPump-content .upload2 .btn:hover {\n        background: #2f55af;\n}\n.overviewPump-content .upload2 {\n    left: 100px;\n}\n.overviewPump-content .input-loc-img {\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    visibility: hidden;\n}\n.overviewPump-content .overview-pic {\n    width: 100%;\n    height: 100%;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n}\n.overviewPump-content .move-block {\n    width: 100%;\n    height: 100%;\n    position: relative;\n}\n.overviewPump-content .move-div {\n    position: absolute;\n    text-align: center;\n}\n.overviewPump-content .move-div img:not([src]) {\n      opacity: 0;\n}\n.overviewPump-content .overview-bg {\n    width: 100%;\n    height: 100%;\n}\n.overviewPump-content .overview-bg:not([src]) {\n      opacity: 0;\n}\n.overviewPump-content .rightBtn {\n    position: absolute;\n    z-index: 10;\n    height: 30px;\n    width: 150px;\n    background: #fff;\n    border: 1px solid #aaa;\n    cursor: default;\n    user-select: none;\n}\n.overviewPump-content .rightBtn > li {\n    height: 30px;\n    line-height: 30px;\n    padding: 0 5px;\n    box-sizing: border-box;\n}\n.overviewPump-content .rightBtn > li:hover {\n    background: #E4E8F4;\n}\n.overviewPump-content .alertBox, .overviewPump-content .deviceBase, .overviewPump-content .addRevise {\n    position: absolute;\n    z-index: 100;\n    left: 50%;\n    top: 50%;\n    background: #fff;\n    border-radius: 4px;\n}\n.overviewPump-content .alertBox {\n    width: 300px;\n    margin-top: -100px;\n    margin-left: -150px;\n}\n.overviewPump-content .alertTitle, .overviewPump-content .addReviseTitle {\n    background: #343C45;\n    height: 40px;\n    line-height: 40px;\n    color: #fff;\n    padding-left: 20px;\n    font-size: 16px;\n    border-radius: 4px;\n    position: relative;\n}\n.overviewPump-content .alertTitle > .icon-close, .overviewPump-content .addReviseTitle > .icon-close {\n    position: absolute;\n    top: 0;\n    right: 10px;\n    font-size: 24px;\n    cursor: pointer;\n    color: #F53500;\n}\n.overviewPump-content .alertTitle > .icon-close:hover, .overviewPump-content .addReviseTitle > .icon-close:hover {\n      color: #a92400;\n}\n.overviewPump-content .operatName {\n    padding: 0 20px;\n    height: 120px;\n    line-height: 120px;\n    font-size: 20px;\n}\n.overviewPump-content .editName {\n    margin-left: 20px;\n    height: 36px;\n    width: 300px;\n    font-size: 20px;\n    padding: 0 10px;\n}\n.overviewPump-content .yesOrNo {\n    height: 40px;\n    line-height: 40px;\n    text-align: center;\n    border-top: 1px solid rgba(121, 140, 161, 0.18);\n    background: #E4E8F4;\n}\n.overviewPump-content .yes, .overviewPump-content .no {\n    height: 24px;\n    line-height: 24px;\n    margin: 4px 2%;\n    border: none;\n    outline: none;\n    width: 50px;\n    font-size: 12px;\n    color: #fff;\n    border-radius: 4px;\n}\n.overviewPump-content .yes {\n    background: #597DD2;\n}\n.overviewPump-content .no {\n    background: #798CA1;\n}\n.overviewPump-content .alertText {\n    text-align: center;\n    font-size: 18px;\n    padding: 30px 0;\n    box-sizing: border-box;\n    color: #343C45;\n    background: #E4E8F4;\n}\n"]}