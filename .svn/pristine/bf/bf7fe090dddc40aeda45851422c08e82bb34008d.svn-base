e\",\r\n  props: [\"title\"],\r\n  data() {\r\n    return {\r\n      flag: true\r\n    };\r\n  },\r\n  methods: {\r\n    dblclick() {\r\n      //放大\r\n      let homeHeader = document.getElementById(\"homeHeader\");\r\n      let aside = document.getElementById(\"aside\");\r\n      let content = document.getElementById(\"content\");\r\n      let wHeight = document.documentElement.clientHeight;\r\n      if (this.flag) {\r\n        homeHeader.style.display = \"none\";\r\n        aside.style.display = \"none\";\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 20px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 40px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.flag = !this.flag;\r\n      } else {\r\n        //缩小\r\n        homeHeader.style.display = \"block\";\r\n        aside.style.display = \"block\";\r\n        content.style.width = \"100px\"; //改变宽让宽自动渲染\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 90px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 120px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.flag = !this.flag;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-content-header {\r\n  /* width: 100%-15px;\r\n   */\r\n  box-sizing: border-box;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  background: #343c45;\r\n  font-size: 20px;\r\n  color: #fff;\r\n  padding-left: 15px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n","<template>\r\n  <div class=\"dgms-content\">\r\n    <my-content-title :title='title'></my-content-title>\r\n    <div id='myContentCenter'>\r\n      <div id=\"myTable\" class=\"myTable\">\r\n          <input class=\"input-loc-file\" name=\"fileLocal\" id=\"fileLocal\" type='file' multiple=\"multiple\" @change=\"selectFile($event)\"/>\r\n          <router-link  :to=\"{path:fitBtn.name}\" @click=\"getTable(fitBtn.name,fitBtn.isDgm)\" class=\"my-btn-a btn-active btn6\" v-show=\"fitBtnShow\">{{fitBtn.textBtn}}</router-link>\r\n          <router-link  :to=\"{path:btnData[14].name}\"  @click.native=\"getTable(btnData[14].name,btnData[14].isDgm)\" class=\"my-btn-a btn-active btn7\" v-show=\"fitShow\">{{btnData[14].textBtn}}</router-link>\r\n          <select class=\"my-btn btn-active btn8\" v-show=\"handIsShow && !isShow\" v-model=\"selectTmsId\">\r\n              <option v-for=\"(val,key) in tmsList\" :value=\"key\">{{val}}</option>\r\n          </select>\r\n          <button class=\"my-btn btn-active btn7\" v-show=\"nsShow\" @click=\"getNSnum()\">获取识别码</button>\r\n          <button class=\"my-btn btn-active btn6\" id='getFile' v-show=\"fitShow\" @click=\"chooseFile()\">选择文件</button>\r\n          <button class=\"my-btn btn-active btn7\" @click=\"getTmsAll\" v-show=\"handIsShow && !isShow\" :disabled = \"ifGetTmsAll\">{{preText}}</button>\r\n          <button class=\"my-btn btn-active btn6\" @click=\"add\" v-show=\"isAdd && !isShow\">新建设备</button>\r\n          <button class=\"my-btn btn-active btn6\" @click=\"getDgmVersion\" v-show=\"selectDgmInfo.verShow\">获取版本信息</button>\r\n          <button class=\"my-btn btn-active btn3\" @click=\"save\" :disabled=\"ifBtnDisabled\">保存设置</button>\r\n          <div id=\"tableContent\">\r\n<!--             <keep-alive>-->\r\n                <router-view ref='child' @child-msg='updateIsShow' @click.native=\"chooseBtn($event)\"></router-view>\r\n<!--             </keep-alive>-->\r\n              <div class=\"noData\" id=\"noData\" v-if=\"isShow\">系统中没有该通道的设备!</div>\r\n          </div>\r\n          <div class=\"controls-btn\">\r\n              <div class=\"push-controls\">\r\n<!--                  <a class=\"top-btn\" @click=\"chooseBtn\"> ↑ </a>-->\r\n                  <a :class=\"leftClass\" @click=\"toLeft()\"> < </a>\r\n                  <a :class=\"rightClass\" @click=\"toRight()\"> > </a>\r\n              </div>\r\n              <div class=\"btn-content\" @mousedown=\"mouseMove($event)\">\r\n                  <router-link v-for=\"(btn,index) in btnData\" :key=\"btn.type\" :to=\"{path:btn.name}\"  @click.native=\"getTable(btn.name,btn.isDgm)\" class=\"con-dgm-btn\" v-show=\"btn.none\">{{btn.textBtn}}</router-link>\r\n              </div>\r\n          </div>\r\n      </div>\r\n  </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import MyContentTitle from 'components/common/MyContentTitle';\r\n  import getApi from 'network/getApi';\r\n  import {addRows,distinct} from 'utils/utils'\r\n  export default {\r\n    name: \"Dgm\",\r\n    inject: ['reload'],\r\n    data(){\r\n      return{\r\n        title:'采集器设置',\r\n        ins:0,//切换按钮active\r\n        name:'vib',\r\n        isAdd:false,\r\n        isShow:false,\r\n        chooseShow:false,//采集器tab\r\n        handIsShow:false,//手动获取按钮是否显示\r\n        fitBtn:{//拟合系数按钮\r\n            type:999,textBtn:\"拟合系数\",name:'fitinfo',isDgm: true, none:true\r\n        },\r\n        fitBtnShow: false,//拟合系数按钮是否显示\r\n        fittingInfo:[],//拟合系数选中的采集器信息\r\n        fitShow:false,//拟合系数界面的选择文件和采集器信息按钮是否显示\r\n        nsShow:false,//e821获取识别码按钮是否显现\r\n        selectDgmInfo:{\r\n           verShow:false,//获取版本信息，叶轮采集器dgu2200和dgu2000v2有\r\n           dgm_type:\"\",\r\n           dgm_id:\"\",\r\n        },\r\n        btnData: [//按钮初始化\r\n              {type:1,textBtn:\"采集器信息\",name:'dgminfo',isDgm: true, none:true},\r\n              {type:10,textBtn:\"前置处理器\",name:'prech',isDgm: true, none:true},\r\n              {type:2,textBtn:\"振动设置\",name:'vibch',isDgm: true, none:true},\r\n              {type:3,textBtn:\"温度设置\",name:'tempch',isDgm: true, none:true},\r\n              {type:4,textBtn:\"过程量设置\",name:'stach',isDgm: true, none:true},\r\n              {type:5,textBtn:\"转速设置\",name:'speedch',isDgm: true, none:true},\r\n              {type:6,textBtn:\"数字量设置\",name:'digch',isDgm: true, none:true},\r\n              {type:7,textBtn:\"串口设置\",name:'modch',isDgm: true, none:true},\r\n              // {type:8,textBtn:\"工艺量设置\"},\r\n              {type:9,textBtn:\"长采样设置\",name:'capturech',isDgm: true, none:true},\r\n              {type:11,textBtn:\"MHD从设备\",name:'prinsub',isDgm: true, none:true},\r\n              {type:12,textBtn:\"连续采集\",name:'collectch',isDgm: true, none:true},\r\n              {type:13,textBtn:\"采集设置\",name:'gatherch',isDgm: true, none:true},\r\n              {type:14,textBtn:\"法兰设置\",name:'flangech',isDgm: true, none:true},\r\n              {type:15,textBtn:\"预处理器设置\",name:'prepsch',isDgm: true, none:true},\r\n              {type:16,textBtn:\"螺栓设置\",name:'boltch',isDgm: true, none:true},\r\n              {type:17,textBtn:\"E821从设备\",name:'prinsub821',isDgm: true, none:true},\r\n              {type:18,textBtn:\"外部集成设置\",name:'ExteInter',isDgm: true, none:true},\r\n          ],\r\n          leftClass: {\r\n              \"left-btn\": true,\r\n              \"active\": false,\r\n              // \"disable\" : true,\r\n          },\r\n          rightClass: {\r\n              \"right-btn\": true,\r\n              \"active\": false,\r\n          },\r\n          fileContent:[],//拟合系数上传的文件\r\n          tmsList:{},//tms的采集器列表\r\n          selectTmsId:'',//选中的前置处理器的id\r\n          select821Info:{\r\n            master_id: '',\r\n            slaver_id: '',\r\n            num:'',\r\n          },//e821从设备界面选中的采集器信息\r\n          ifBtnDisabled: false,//按钮是否可点击\r\n          ifGetTmsAll: false,//获取前置按钮是否可点击\r\n          preText:'获取前置',//获取前置的点击按钮的文字\r\n      }\r\n    },\r\n    mounted(){\r\n        // 在点击标题的时候放大并重新渲染表格\r\n      this.$bus.$on('reloadtable',()=>{\r\n        if(this.$refs.child){\r\n          this.$refs.child.hot.render()\r\n        }\r\n      });\r\n        this.name = this.$route.name //刷新的时候让button 的active 正确\r\n        this.statePage(this.name);\r\n        let chTypeBtn = sessionStorage.getItem('chTypeBtn');\r\n        if(chTypeBtn!==null){\r\n            this.btnData.forEach(btnInfo =>{\r\n                if(chTypeBtn.indexOf(btnInfo.type)===-1){\r\n                    btnInfo.none=false;\r\n                }\r\n                else{\r\n                    btnInfo.none=true;\r\n                }\r\n            })\r\n        }\r\n        else{\r\n            this.setBtnContentPosition();\r\n        }\r\n    },\r\n    watch: {\r\n        \"$route\":\"fetchData\"\r\n    },\r\n    methods:{\r\n        getTmsAll(){//手动获取前置处理器信息\r\n            this.ifGetTmsAll = true;\r\n            this.preText = \"获取中...\";\r\n            let that = this;\r\n            this.$getApi.updateTMSPre(this.selectTmsId)\r\n                .then(res=>{\r\n                    that.ifGetTmsAll = false;\r\n                    that.preText = \"获取前置\";\r\n                    if(res.msg===0){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: `获取前置成功`,\r\n                            button: \"确认\",\r\n                        })\r\n                            .then((value) => {\r\n                                that.reload()\r\n                        });\r\n                    }\r\n                    else if(res.msg===4){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: `请求超时，请重新获取`,\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                })\r\n                .catch(function(err) {\r\n                    that.ifGetTmsAll = false;\r\n                    that.preText = \"获取前置\";\r\n                    console.log('failed', err);\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: `请求超时，请重新获取`,\r\n                        button: \"确认\",\r\n                    });\r\n                });\r\n        },\r\n        getNSnum(){//获取识别码\r\n            let params = null;\r\n            if(this.select821Info.master_id==='' || this.select821Info.slaver_id===''){\r\n                swal({\r\n                    title: \"提示\",\r\n                    text: `请先选择设备`,\r\n                    button: \"确认\",\r\n                });\r\n                return\r\n            }\r\n            params = {\r\n                master_id: this.select821Info.master_id,\r\n                slaver_id: this.select821Info.slaver_id,\r\n            };\r\n            let index = this.select821Info.num;\r\n            this.$getApi.getNSDgmInfo(params)\r\n                .then(res=>{\r\n                    if(res.msg==0){\r\n                        if(res.SNA!=undefined && res.SNA!=''){\r\n                            this.$set(this.$refs.child.data[index].dgm,'SNA',res.SNA);\r\n                            this.$set(this.$refs.child.changeList.changeMhd,index,this.$refs.child.data[index].dgm);\r\n                            this.$refs.child.hot.render();\r\n                        }\r\n                        else{\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: `获取的识别码为空`,\r\n                                button: \"确认\",\r\n                            });\r\n                        }\r\n                    }\r\n                    else{\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: `获取的识别码失败`,\r\n                            button: \"确认\",\r\n                        });\r\n                    }\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求错误' ,err)\r\n                })\r\n        },\r\n        getDgmVersion(){//获取版本信息\r\n            let params = {\r\n                 dgm_type:this.selectDgmInfo.dgm_type,//只有叶轮采集器和2000V2采集器 能获取版本\r\n                 dgm_id:this.selectDgmInfo.dgm_id //采集器id\r\n            };\r\n            let that = this;\r\n            getApi.getDgmVersionInfo(params)\r\n                .then(res => {\r\n                    if (res.msg == 0) {\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: `获取版本信息成功`,\r\n                            button: \"确认\"\r\n                        })\r\n                            .then((value) => {\r\n                                that.reload()\r\n                            });\r\n                    } else if (res.msg == 1) {\r\n                        console.log('参数为空！')\r\n                    } else if (res.msg == 2) {\r\n                        console.log('后台运行错误！')\r\n                    }\r\n                })\r\n                .catch(err=>{\r\n                    console.log('请求错误' ,err)\r\n                })\r\n        },\r\n        selectFile(event){//拟合系数上传文件\r\n            let that = this;\r\n            let selectedFile = {};//当前选中的文件\r\n            let target = event.target;\r\n            this.fileContent = [];\r\n            if(sessionStorage.getItem('fittingInfo')){\r\n                that.fittingInfo = JSON.parse(sessionStorage.getItem('fittingInfo'));\r\n            }\r\n            if(that.p_isIE() && !target.files) {\r\n                let filePath = target.value;\r\n                let fileSystem = new ActiveXObject(\"Scripting.FileSystemObject\");\r\n                selectedFile = fileSystem.GetFile(filePath);\r\n            } else {\r\n                selectedFile = target.files;\r\n            }\r\n            // target.value = null;\r\n\r\n            that.$refs.child.hot.destroy();//清除实例\r\n            for(let i=0;i<selectedFile.length;i++){\r\n                let size = selectedFile[i].size/1024;\r\n                if(size>2000){\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '附件不能大于2M',\r\n                        button: \"确认\",\r\n                    })\r\n                    target.value=\"\";\r\n                    return;\r\n                }\r\n                let fileName=selectedFile[i].name;\r\n                let suffixName = fileName.substring(fileName.lastIndexOf(\".\")+1).toLowerCase();\r\n                if(suffixName !=\"csv\"){\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '请选择csv格式文件上传！',\r\n                        button: \"确认\",\r\n                    })\r\n                    target.value=\"\";\r\n                    return;\r\n                }\r\n                let reader = new FileReader();//这是核心,读取操作就是由它完成.\r\n                reader.readAsText(selectedFile[i]);//读取文件的内容,也可以读取文件的URL\r\n                reader.onload = function (event) {\r\n                    //当读取完成后回调这个函数,然后此时文件的内容存储到了result中,直接操作即可\r\n                    // showFile();\r\n                    let datas = event.target.result;\r\n                    let rows = datas.split(\"\\n\");//所有行\r\n                    let len = rows.length;\r\n                    let row=[];//每一行\r\n                    let dgm= {};\r\n                    let obj = {};\r\n                    that.fittingInfo.forEach(fit=>{\r\n                        obj.dgm_id = fit.dgm_id;\r\n                        dgm= {\r\n                            dgm_id: fit.dgm_id,//采集器uid\r\n                            dgm_name: fit.dgm_name,//采集站名称\r\n                        };\r\n                        for(let i = 0; i<len; i++){\r\n                            row = [];\r\n                            row = rows[i].split(\",\");\r\n                            if(that.p_atob(row[0])=='standardLen') {\r\n                                dgm['fBoltLength'] = that.p_atob(row[1]);\r\n                            }\r\n                            else if(that.p_atob(row[0])=='diameter') {\r\n                                dgm['fDiameter'] = that.p_atob(row[1]);\r\n                            }\r\n                            else if(that.p_atob(row[0])=='fitting'){\r\n                                dgm[that.p_atob(row[0])] = '算法' + that.p_atob(row[1]);\r\n                            }\r\n                            else{\r\n                                dgm[that.p_atob(row[0])] = that.p_atob(row[1]);\r\n                            }\r\n                            obj[that.p_atob(row[0])] = that.p_atob(row[1]);\r\n                        }\r\n                    });\r\n\r\n                    that.fileContent.push(obj);\r\n                    let oriData = that.$refs.child.data;\r\n\r\n                    for(let j=0;j<oriData.length;j++){\r\n                        if(oriData[j].dgm.fBoltLength == dgm.fBoltLength && oriData[j].dgm.fDiameter == dgm.fDiameter){\r\n                            that.$refs.child.data.splice(j, 1)\r\n                        }\r\n                    }\r\n                    that.$refs.child.data.push({dgm});\r\n                    that.$refs.child.filter_by_dgmName();\r\n                    that.$refs.child.renderTable();\r\n                }\r\n            }\r\n        },\r\n        chooseFile(){\r\n            document.getElementById('fileLocal').click();//模拟点击\r\n        },\r\n        getTable(name,isDgm){ //点击下标签的时候改变active，改变路由，刷新之后路由不变，active不变\r\n            this.isShow  = !isDgm;\r\n            this.isAdd = false //工况设置不可以添加\r\n            this.statePage(name);\r\n        },\r\n        add(){\r\n            return addRows(this.$refs.child.hot,this.$refs.child.data,-1);\r\n        },\r\n        save(){\r\n            /* 保存按钮 */\r\n            let changeData = this.$refs.child.changeList.changeData;\r\n            let changeMhdData = this.$refs.child.changeList.changeMhd;\r\n            let changeTmsData = this.$refs.child.changeList.changeTms;\r\n            let addData = this.$refs.child.changeList.addMhd;\r\n            let addTmsData = this.$refs.child.changeList.addTms;\r\n            let addDefaultDgm = this.$refs.child.changeList.addDefaultDgm;\r\n            let type = this.$refs.child.type;\r\n            let flag = this.checkPosLoc();\r\n            let that = this;\r\n            if(flag && type!=11 && type!=17 && type!=10 && type!=13  && type!=14  && type!=15 && type!=999 && type!=18){\r\n                //修改采集器信息\r\n                if(!$.isEmptyObject(changeData)){\r\n                    that.ifBtnDisabled = true;//请求前按钮不可点击\r\n                    getApi.updateDgm(changeData)\r\n                        .then(res=>{\r\n                            that.ifBtnDisabled = false;//请求后按钮可点击\r\n                            if(res.msg==0){\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '保存成功',\r\n                                    button: \"确认\",\r\n                                })\r\n                                    .then((value) => {\r\n                                        this.$refs.child.changeList.changeData = {};\r\n                                        this.reload()\r\n                                    });\r\n\r\n                            }\r\n                            else if(res.msg==1){\r\n                                console.log('参数为空！');\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '保存失败',\r\n                                    button: \"确认\",\r\n                                })\r\n                            }\r\n                            else if(res.msg==2){\r\n                                console.log('后台运行错误！');\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '保存失败',\r\n                                    button: \"确认\",\r\n                                })\r\n                            }\r\n                            else if(res.msg==3){\r\n                                console.log('获取json数据时为空！');\r\n                                swal({\r\n                                    title: \"提示\",\r\n                                    text: '保存失败',\r\n                                    button: \"确认\",\r\n                                })\r\n                            }\r\n                        })\r\n                        .catch(err=>{\r\n                            that.ifBtnDisabled = false;//请求后按钮可点击\r\n                            console.log('请求错误' ,err)\r\n                        })\r\n                }\r\n                else{\r\n                    swal({\r\n                        title: \"提示\",\r\n                        text: '保存成功',\r\n                        button: \"确认\",\r\n                    })\r\n                }\r\n            }\r\n            if(type==11){\r\n                //修改mhd从设备采集器信息\r\n                if(!$.isEmptyObject(changeMhdData)){\r\n                    let changeMhdList = [];\r\n                    for(let i in changeMhdData){\r\n                        if(changeMhdData[i].byName==\"\"){\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: `设备名称不能为空`,\r\n                                button: \"确认\",\r\n                            });\r\n                            break;\r\n                        }\r\n                        else if(changeMhdData[i].mhd_id==\"\"){\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: `设备id不能为空`,\r\n                                button: \"确认\",\r\n                            });\r\n                            break;\r\n                        }\r\n                        else if(changeMhdData[i].mhd_id.length !==8){\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: `设备id需为8位数字`,\r\n                                button: \"确认\",\r\n                            });\r\n                            break;\r\n                        }\r\n                        else if(changeMhdData[i].mainMhd==\"\"){\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: `主设备不能为空`,\r\n                                button: \"确认\",\r\n                            });\r\n                            break;\r\n                        }\r\n                        else{\r\n                            let obj = {};\r\n                            obj[changeMhdData[i].mainMhd] = {};\r\n                            obj[changeMhdData[i].mainMhd][changeMhdData[i].mhd_id] = {\r\n                                if_output:changeMhdData[i].if_output,\r\n                                mhd_id:changeMhdData[i].mhd_id,\r\n                                byName:changeMhdData[i].byName,\r\n                                byId:changeMhdData[i].byId,\r\n                            };\r\n                            changeMhdList.push(obj);\r\n                        }\r\n\r\n                    }\r\n                    if(changeMhdList.length>0){\r\n                        that.ifBtnDisabled = true;//请求前按钮不可点击\r\n                        getApi.updateMHDList(changeMhdList)\r\n                            .then(res=>{\r\n                                that.ifBtnDisabled = false;//请求后按钮可点击\r\n                                if(res.msg==0){\r\n                                    swal({\r\n                                        title: \"提示\",\r\n                                        text: '保存成功',\r\n                                        button: \"确认\",\r\n                                    })\r\n                                        .then((value) => {\r\n                                            this.$refs.child.changeList.changeMhd={};\r\n                                            this.$refs.child.changeList.addMhd = [];\r\n                                            this.reload()\r\n                                        });\r\n\r\n                                }\r\n                            