                 <div class=\"search\" @click.stop=\"addClass($event)\">\r\n                      <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput1\" @input=\"searchFolder($event,1)\">\r\n                      <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(1,1)\"></i>\r\n                  </div>\r\n                  <div class=\"search-list-div\" v-show=\"searchFolderList1.length>0 && isShow1\" @click.stop>\r\n                      <ul class=\"search-result\">\r\n                          <li class=\"search-li\" v-for=\"(item,index) in sortFolderList1\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,1,2)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                      </ul>\r\n                  </div>\r\n              </div>\r\n              <!--二级组织-->\r\n              <div class=\"tree-search\">\r\n                  <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList2.length>0\">\r\n                      <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput2\" @input=\"searchFolder($event,2)\">\r\n                      <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(2,1)\"></i>\r\n                  </div>\r\n                  <div class=\"search-list-div\" v-show=\"searchFolderList2.length>0 && isShow2\" @click.stop>\r\n                      <ul class=\"search-result\">\r\n                          <li class=\"search-li\" v-for=\"(item,index) in sortFolderList2\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,2,2)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                      </ul>\r\n                  </div>\r\n              </div>\r\n              <!--三级组织-->\r\n              <div class=\"tree-search\">\r\n                  <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList3.length>0\">\r\n                      <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput3\" @input=\"searchFolder($event,3)\">\r\n                      <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(3,1)\"></i>\r\n                  </div>\r\n                  <div class=\"search-list-div\" v-show=\"searchFolderList3.length>0 && isShow3\" @click.stop>\r\n                      <ul class=\"search-result\">\r\n                          <li class=\"search-li\" v-for=\"(item,index) in sortFolderList3\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,3,2)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                      </ul>\r\n                  </div>\r\n              </div>\r\n              <!--四级组织-->\r\n              <div class=\"tree-search\">\r\n                  <div class=\"search\" @click.stop=\"addClass($event)\" v-show=\"searchFolderList4.length>0\">\r\n                      <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput4\" @input=\"searchFolder($event,4)\">\r\n                      <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList(4,1)\"></i>\r\n                  </div>\r\n                  <div class=\"search-list-div\" v-show=\"searchFolderList4.length>0 && isShow4\" @click.stop>\r\n                      <ul class=\"search-result\">\r\n                          <li class=\"search-li\" v-for=\"(item,index) in sortFolderList4\" :folderId=\"item.t_id\" @click=\"selectFolder(item,$event,4,2)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                      </ul>\r\n                  </div>\r\n              </div>\r\n              <div class=\"tree-search\">\r\n                  <div class=\"search\">\r\n                      <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索机泵\" ref=\"searchMacInput\" @input=\"searchMac\">\r\n                      <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click.stop=\"openList(null,2)\"></i>\r\n                  </div>\r\n                  <div class=\"search-list-div search-list-div2\" v-show=\"searchPumpList.length>0 && isShow.macIsShow\">\r\n                      <ul class=\"search-result\">\r\n                          <li class=\"search-li\" v-for=\"(item,index) in sortPumpList\"  @click=\"selectMac(item,index)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.pump_name}}</li>\r\n                      </ul>\r\n                  </div>\r\n              </div>\r\n              <div class=\"search-pump-div\" v-show=\"choosePosList.length>0 && !isShow.macIsShow\">\r\n                  <ul class=\"pump-result pump-result2\">\r\n                      <li class=\"pump-li\" v-for=\"(pos,index) in choosePosList\" @click=\"clickPump(pos,index)\" draggable=\"true\" @dragstart=\"dragstart(pos, index, $event,2)\">\r\n                          <i class=\"iconfont icon-zhongzi-shebeimoxing\"></i>\r\n                          <span class=\"pos_name\">{{pos.pos_name}}</span>\r\n                      </li>\r\n                  </ul>\r\n              </div>\r\n              <div id=\"imgForm\" class=\"upload\" v-show=\"chooseMac.pump_id!=''\">\r\n                  <button class=\"btn\" @click=\"monidianji\">上传图片</button>\r\n                  <input class=\"input-loc-img\"  name=\"imgLocal\" id=\"imgLocal\" type='file' accept=\"image/*\" @change=\"selectImg(2)\"/>\r\n              </div>\r\n              <div class=\"upload2\" v-show=\"overviewUrl!=''&& choosePosList.length>0\">\r\n                  <button class=\"btn\" @click=\"keepOverview(3)\">保存</button>\r\n              </div>\r\n          </div>\r\n          <div class=\"overview-container\">\r\n              <div class=\"overview-pic\" id=\"overview-pic\">\r\n                  <div id=\"move-block\" class=\"move-block\" @drop.stop.prevent='drop($event,2)' @dragover.prevent='dragover' @click.stop='cancelClick' @contextmenu.prevent=''>\r\n                      <div class=\"move-div\" v-for=\"(item,index) in MoveMsg\" :style=\"item.style\" draggable=\"true\" @dragstart.stop=\"dragScreen(index, $event,item,2)\" @contextmenu.prevent='rightClick(item, index, $event)'>\r\n                          <img :src=\"item.url\" :width=\"item.width\" :height=\"item.height\" />\r\n                          <p>{{item.text}}</p>\r\n                      </div>\r\n                      <ul class='rightBtn' v-show='isRightClick' ref='rightMsg'>\r\n                          <li @click='delModelPos'>删除</li>\r\n                      </ul>\r\n                      <img :src=\"overviewUrl\" class=\"overview-bg\" @dragstart.stop=\"stopDrag()\">\r\n                  </div>\r\n              </div>\r\n              <div class='alertBox alertTip' v-show='isTip'>\r\n                    <div class='alertTitle'>提示  <i class=\"iconfont icon-zhongzi-guanbi icon-close\" @click='closeAlarm'></i></div>\r\n                    <div class='alertText'>\r\n                      请确认是否删除该部件？\r\n                    </div>\r\n                    <div class='yesOrNo'>\r\n                        <button class='yes' @click='confirmTip(2)'>确定</button>\r\n                        <button class='no' @click='closeAlarm'>取消</button>\r\n                    </div>\r\n              </div>\r\n\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyContentTitle from 'components/common/MyContentTitle'\r\nimport overview from 'components/common/overviewCommon.js'\r\nimport { ConvertPinyin } from 'assets/lib/pinyin.js'\r\nexport default {\r\n  name: \"overviewPump\",\r\n  mixins: [overview],\r\n  props:['parentToChild'],\r\n  data(){\r\n      return{\r\n          title:\"机泵总貌图设置\",\r\n          activeNum: \"21\",\r\n          chooseFolder:{\r\n              t_id:\"\",\r\n              t_name:\"\",\r\n              overview:null\r\n          },\r\n          chooseMac:{\r\n              ch_class: \"\",\r\n              mac_id: \"\",\r\n              pump_id: \"\",\r\n              pump_name: \"\"\r\n          },\r\n          choosePump:{\r\n              pump_name:\"\",\r\n              pump_id:\"\",\r\n              pump_type:\"\"\r\n          },\r\n          choosePumpList:[],//选择的机组列表\r\n          searchPumpList:[],//搜索到的机组列表\r\n      }\r\n    },\r\n  created: function() {\r\n      this.$emit('ievent',this.activeNum);\r\n  },\r\n  beforeRouteLeave (to, from, next) {\r\n      if(to.name == 'overview'){\r\n          let linkObj = {\r\n              chooseFolder:this.chooseFolder,\r\n              choosePump: this.chooseMac\r\n          };\r\n          sessionStorage.setItem('LinkConfig',JSON.stringify(linkObj));\r\n      }\r\n      else{\r\n          if(sessionStorage.getItem('LinkConfig')){\r\n              sessionStorage.removeItem('LinkConfig');\r\n          }\r\n      }\r\n      next();\r\n  },\r\n  mounted: function() {\r\n      let linkInfo = {};\r\n      if(!$.isEmptyObject(JSON.parse(sessionStorage.getItem('LinkConfig')))){//带入带出选项\r\n          linkInfo = JSON.parse(sessionStorage.getItem('LinkConfig'));\r\n      }\r\n      if(!$.isEmptyObject(this.$route.params)){//双击图中的机泵跳转\r\n          linkInfo = this.$route.params;\r\n          this.activeNum = \"21\";\r\n          this.$emit('ievent',this.activeNum);\r\n      }\r\n      if(!$.isEmptyObject(linkInfo)){\r\n          this.chooseFolder=linkInfo.chooseFolder;\r\n          this.chooseMac=linkInfo.choosePump;\r\n          this.selectFolder(this.chooseFolder,null,1,2);\r\n      }\r\n      else{\r\n          this.chooseFolder={\r\n              t_id:\"\",\r\n              t_name:\"\",\r\n          }\r\n      }\r\n  },\r\n  methods: {\r\n      //搜索下拉列表\r\n      searchFolder(e,type){\r\n          let search_text = e.target.value;\r\n          if(search_text.length>0){\r\n              let test_atr = new RegExp(search_text);\r\n              this[`searchFolderList${type}`]=[];\r\n              this[`isShow${type}`]=true;\r\n              let originList = this[`folderList${type}`];\r\n              for(let i=0;i<originList.length;i++){\r\n                  let item = originList[i];\r\n                  if(test_atr.test(item.t_name) || ConvertPinyin(item.t_name).indexOf(search_text)>=0){\r\n                      this[`searchFolderList${type}`].push(item);\r\n                  }\r\n              }\r\n              this.isShow.macIsShow=false;\r\n              if(this[`searchFolderList${type}`].length===0){\r\n                  this.searchPumpList=[];\r\n                  this.choosePosList=[];\r\n                  this.$refs.searchMacInput.value ='';\r\n              }\r\n          }\r\n          else{\r\n              this[`searchFolderList${type}`]=this[`folderList${type}`].concat();\r\n              this.isShow1=false;\r\n              this.isShow2=false;\r\n              this.isShow3=false;\r\n              this.isShow4=false;\r\n              this.chooseFolder={\r\n                  t_id:\"\",\r\n                  t_name:\"\",\r\n                  overview:null\r\n              };\r\n              this.chooseMac={\r\n                  ch_class: \"\",\r\n                  mac_id: \"\",\r\n                  pump_id: \"\",\r\n                  pump_name: \"\"\r\n              };\r\n              this.MoveMsg=[];\r\n              this.overviewUrl='';\r\n              this.choosePumpList=[];\r\n              this.searchPumpList=[];\r\n              this.choosePosList=[];\r\n              this.$refs.searchMacInput.value ='';\r\n          }\r\n      },\r\n      //搜索机组下拉列表\r\n      searchMac(e){\r\n          let search_text = e.target.value;\r\n          if(search_text.length>0){\r\n              let test_atr = new RegExp(search_text);\r\n              this.searchPumpList=[];\r\n              this.isShow.macIsShow=true;\r\n              this.isShow1=false;\r\n              this.isShow2=false;\r\n              this.isShow3=false;\r\n              this.isShow4=false;\r\n              for(let i=0;i<this.choosePumpList.length;i++){\r\n                  if(test_atr.test(this.choosePumpList[i].pump_name) || ConvertPinyin(this.choosePumpList[i].pump_name).indexOf(search_text)>=0){\r\n                      this.searchPumpList.push(this.choosePumpList[i]);\r\n                  }\r\n              }\r\n          }\r\n          else{\r\n              this.chooseMac={\r\n                  ch_class: \"\",\r\n                  mac_id: \"\",\r\n                  pump_id: \"\",\r\n                  pump_name: \"\"\r\n              };\r\n              this.MoveMsg=[];\r\n              this.overviewUrl='';\r\n              this.choosePosList=[];\r\n              this.$refs.searchMacInput.value ='';\r\n          }\r\n      },\r\n      //确定选择机组\r\n      selectMac(chooseInfo,index){\r\n          $('.search-list-div2 .search-li').eq(index).addClass('active').siblings().removeClass('active');\r\n          this.chooseMac=chooseInfo;\r\n          this.$refs.searchMacInput.value =this.chooseMac.pump_name;\r\n          this.choosePosList=[];\r\n          this.isShow.macIsShow=false;\r\n          let that=this;\r\n          let macObj={\r\n              mac_id:this.chooseMac.mac_id,\r\n              pump_id:this.chooseMac.pump_id,\r\n              ch_class:this.chooseMac.ch_class\r\n          };\r\n      this.$axios.post('queryPumpGoToPosition',macObj)\r\n      .then(res=>{\r\n           if(res.msg==0){\r\n                      that.MoveMsg=[];\r\n                      if(res.overview!=null){\r\n                          that.overviewUrl= res.overview.bgurl;\r\n                          // that.overviewUrl = require(\"assets/img/test/timg.jpg\");\r\n                          // 图片地址\r\n                          let img_url = res.overview.bgurl;\r\n                          // let img_url = require(\"assets/img/test/timg.jpg\");\r\n                          // 创建对象\r\n                          let img = new Image();\r\n                          // 改变图片的src\r\n                          img.src = img_url;\r\n                          // 加载完成执行\r\n                          img.onload = function() {\r\n                              /* 获取图片的真实宽高，计算背景图区域 */\r\n                              that.computePicWidth(img);\r\n                              let oriItemList = res.overview.itemList;\r\n                              if (oriItemList != null) {\r\n                                  for (let i = 0; i < oriItemList.length; i++) {\r\n                                      let obj = {\r\n                                          px:oriItemList[i].px,\r\n                                          py:oriItemList[i].py,\r\n                                          x: parseInt(oriItemList[i].px * that.origBlock.oriW),\r\n                                          y: parseInt(oriItemList[i].py * that.origBlock.oriH),\r\n                                          uid: oriItemList[i].uid,\r\n                                          itemType: oriItemList[i].itemType,\r\n                                          width: oriItemList[i].pwidth,\r\n                                          height: oriItemList[i].pheight,\r\n                                          style: {\r\n                                              left: parseInt(oriItemList[i].px * that.origBlock.oriW) + 'px',\r\n                                              top: parseInt(oriItemList[i].py * that.origBlock.oriH) + 'px',\r\n                                              zIndex: 2,\r\n                                          }\r\n                                      };\r\n                                      for (let j = 0; j < res.position_list.length; j++) {\r\n                                          let typeId = res.position_list[j].pos_type + '_' + res.position_list[j].pos_id\r\n                                          if (oriItemList[i].uid == typeId) {\r\n                                              res.position_list[j].dragFlag = false;\r\n                                              obj.dragFlag = false;\r\n                                              obj.text = res.position_list[j].pos_name;\r\n                                              obj.url = require('assets/img/posIcon.svg');\r\n                                          }\r\n                                      }\r\n                                      that.MoveMsg.push(obj);\r\n                                  }\r\n                              }\r\n                          }\r\n                      }\r\n                      else{\r\n                          that.overviewUrl='';\r\n                      }\r\n                      that.choosePosList = that.sortPositionsName(res.position_list);\r\n                      for(let i=0;i<that.choosePosList.length;i++){\r\n                          that.choosePosList[i].url=require('assets/img/posIcon.svg');\r\n                          that.choosePosList[i].width=30;\r\n                          that.choosePosList[i].height=30;\r\n                          that.choosePosList[i].itemType=that.choosePosList[i].pos_type;\r\n                          if(that.choosePosList[i].dragFlag === false){\r\n\r\n                          }\r\n                          else{\r\n                              that.choosePosList[i].dragFlag=true;\r\n                          }\r\n                      }\r\n                  }\r\n                  else if(res.msg==1){\r\n                      console.log('参数为空!');\r\n                  }\r\n                  else if(res.msg==2){\r\n                      console.log('后台运行错误！');\r\n                  }\r\n                  else if(res.msg==3){\r\n                      that.overviewUrl='';\r\n                      that.MoveMsg=[];\r\n                      that.choosePosList=[];\r\n                  }\r\n      })\r\n          .catch(err=>{\r\n              console.log('请求错误',err)\r\n          })\r\n      },\r\n  },\r\n  components:{\r\n    MyContentTitle\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<styl