s/29-343640beb9caa595efa5.js","sourcesContent":["<template>\r\n  <div class=\"overview-content\" ref='overviewContent' @click=\"closeSearch\">\r\n      <my-content-title :title='title'></my-content-title>\r\n      <div class=\"overview-block\">\r\n          <div class=\"overview-search\">\r\n              <div class=\"search\" @click.stop>\r\n                  <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索组织\" ref=\"searchInput\" @input=\"searchFolder\">\r\n                  <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click=\"openList\"></i>\r\n              </div>\r\n              <div class=\"search-list-div\" v-show=\"searchFolderList.length>0 && isShow\" @click.stop>\r\n                  <ul class=\"search-result\">\r\n                      <li class=\"search-li\" v-for=\"(item,index) in sortFolderList\" :folderId=\"item.t_id\" @click=\"selectFolder(item,index)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.t_name}}</li>\r\n                  </ul>\r\n              </div>\r\n              <div class=\"search-pump-div\" v-show=\"choosePumpList.length>0 && !isShow\">\r\n                  <ul class=\"pump-result\">\r\n                      <li class=\"pump-li\" v-for=\"(pump,index) in sortPumpList\" @click=\"clickPump(pump,index)\" draggable=\"true\" @dragstart=\"dragstart(pump, index, $event)\">\r\n                          <i class=\"iconfont icon-zhongzi-shebeimoxing\"></i>\r\n                          {{pump.pump_name}}\r\n                      </li>\r\n                  </ul>\r\n              </div>\r\n              <div id=\"imgForm\" class=\"upload\" v-show=\"chooseFolder.t_id!=''\">\r\n                  <button class=\"btn\" @click=\"monidianji\">上传图片</button>\r\n                  <input class=\"input-loc-img\"  name=\"imgLocal\" id=\"imgLocal\" type='file' accept=\"image/*\" @change=\"selectImg\"/>\r\n              </div>\r\n              <div class=\"upload2\" v-show=\"overviewUrl!=''&& choosePumpList.length>0\">\r\n                  <button class=\"btn\" @click=\"keepOverview\">保存</button>\r\n              </div>\r\n          </div>\r\n          <div class=\"overview-container\">\r\n              <div class=\"overview-pic\" id=\"overview-pic\">\r\n                  <div id=\"move-block\" class=\"move-block\" @drop.stop.prevent='drop' @dragover.prevent='dragover' @click.stop='cancelClick' @contextmenu.prevent=''>\r\n                      <div class=\"move-div\" v-for=\"(item,index) in MoveMsg\" :style=\"item.style\" draggable=\"true\" @dragstart.stop=\"dragScreen(index, $event,item)\" @contextmenu.prevent='rightClick(item, index, $event)' @dblclick=\"getPumpInfo(item)\">\r\n                          <img :src=\"item.url\" :width=\"item.width\" :height=\"item.height\">\r\n                          <p>{{item.text}}</p>\r\n                      </div>\r\n                      <ul class='rightBtn' v-show='isRightClick' ref='rightMsg'>\r\n                          <li @click='delModelPos'>删除</li>\r\n                      </ul>\r\n                      <img :src=\"overviewUrl\" class=\"overview-bg\">\r\n                  </div>\r\n              </div>\r\n              <div class='alertBox alertTip' v-show='isTip'>\r\n                    <div class='alertTitle'>提示  <i class=\"iconfont icon-zhongzi-guanbi icon-close\"  @click='closeAlarm'></i></div>\r\n                    <div class='alertText'>\r\n                      请确认是否删除该部件？\r\n                    </div>\r\n                    <div class='yesOrNo'>\r\n                        <button class='yes' @click='confirmTip'>确定</button>\r\n                        <button class='no' @click='closeAlarm'>取消</button>\r\n                    </div>\r\n              </div>\r\n\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {compareName, cloneObj} from \"@/utils/utils\";\r\nimport MyContentTitle from 'components/common/MyContentTitle'\r\nimport { ConvertPinyin } from 'assets/lib/pinyin.js'\r\nexport default {\r\n  name: \"Overview\",\r\n  props:['parentToChild'],\r\n  data(){\r\n      return{\r\n          title:'组织总貌图设置',\r\n          activeNum: \"20\",\r\n          folderList:[],//组织列表\r\n          searchFolderList:[],//搜索匹配到的组织列表\r\n          chooseFolder:{\r\n              t_id:\"\",\r\n              t_name:\"\",\r\n          },\r\n          choosePump:{\r\n              pump_name:\"\",\r\n              pump_id:\"\",\r\n              pump_type:\"\"\r\n          },\r\n          isShow:false,//搜索下拉是否显示\r\n          choosePumpList:[],\r\n          overviewUrl:\"\",//总貌图背景路径\r\n          dragMsg: null, // 拖动的机泵的信息\r\n          MoveMsg:[],//移动的机泵的信息\r\n          isRightClick: false, // 是否显示右键信息\r\n          rightClickMsg: { // 右键选中机泵组件\r\n              index: -1, // 右键选中组件下标\r\n              data: null, // 右键选中组件信息\r\n          },\r\n          isTip:false,//是否显示提示框\r\n          origBlock:{//总貌图背景区域\r\n              origW:\"\",\r\n              origH:\"\",\r\n          }\r\n      }\r\n  },\r\n  created: function() {\r\n      this.getTidData()\r\n      this.$emit('ievent',this.activeNum);\r\n  },\r\n  beforeRouteLeave (to, from, next) {\r\n      if(to.name == 'overviewPump'){\r\n          let linkObj = {\r\n              chooseFolder:this.chooseFolder,\r\n              choosePump: this.choosePump\r\n          };\r\n          sessionStorage.setItem('LinkConfig',JSON.stringify(linkObj));\r\n      }\r\n      else{\r\n          if(sessionStorage.getItem('LinkConfig')){\r\n              sessionStorage.removeItem('LinkConfig');\r\n          }\r\n      }\r\n      next();\r\n  },\r\n  mounted: function() {\r\n      if(this.$store.state.tids.length>0){\r\n          this.getTidData();//获取所有的机组设置数据\r\n      };\r\n      let p_w=$('#overview-pic').width();\r\n      let p_h=$('#overview-pic').height();\r\n\r\n      if(p_w/p_h>16/9){\r\n          this.origBlock.oriW=parseInt(16*p_h/9);\r\n          this.origBlock.oriH=p_h;\r\n          $('#move-block').css({'width':this.origBlock.oriW + 'px','height':this.origBlock.oriH + 'px'});\r\n      }\r\n      else{\r\n          this.origBlock.oriW=p_w;\r\n          this.origBlock.oriH=parseInt(9*p_w/16);\r\n          $('#move-block').css({'width':this.origBlock.oriW + 'px','height':this.origBlock.oriH + 'px'});\r\n      }\r\n      let linkInfo = JSON.parse(sessionStorage.getItem('LinkConfig'));\r\n\r\n      if(!$.isEmptyObject(linkInfo)){\r\n          this.chooseFolder=linkInfo.chooseFolder;\r\n          this.selectFolder(this.chooseFolder);\r\n      }\r\n      else{\r\n          this.chooseFolder={\r\n              t_id:\"\",\r\n              t_name:\"\",\r\n          }\r\n      }\r\n  },\r\n  computed: {\r\n      sortFolderList: function () {\r\n          return this.searchFolderList.sort(compareName('t_name'));\r\n      },\r\n      sortPumpList: function () {\r\n          return this.choosePumpList.sort(compareName('pump_name'));\r\n      },\r\n  },\r\n  methods: {\r\n      closeSearch(){\r\n          this.isShow = false;\r\n      },\r\n      //获取组织信息\r\n      getTidData: function(){\r\n          var that=this;\r\n          var token = sessionStorage.getItem('token');\r\n          let paramObj = null;\r\n              paramObj = {\r\n                  t_ids: this.$store.state.tids\r\n              };\r\n          this.$axios.post('queryAllT_OverviewInfo',paramObj)\r\n          .then(res=>{\r\n              that.folderList=[];\r\n              for(let i=0;i<res.length;i++){\r\n                  if(res[i].t_root==1){\r\n                      that.folderList.push(res[i]);\r\n                  }\r\n              }\r\n              that.searchFolderList = that.folderList.concat();\r\n          })\r\n          .catch(err=>{\r\n              console.log('请求错误', err)\r\n          })\r\n      },\r\n      openList(){\r\n          this.isShow=!this.isShow;\r\n      },\r\n      // 右键点击部件\r\n      rightClick(item, index, e) {\r\n          // console.log(item)\r\n          this.isRightClick = true;\r\n          this.rightClickMsg = {\r\n              index,\r\n              data: item,\r\n          };\r\n          this.$refs.rightMsg.style.left = item.x + e.layerX + 'px';\r\n          this.$refs.rightMsg.style.top = item.y + e.layerY + 'px';\r\n      },\r\n      // 删除组件\r\n      delModelPos() {\r\n          this.isTip = true;\r\n      },\r\n      // 确定删除组件\r\n      confirmTip() {\r\n          let rcm = this.rightClickMsg,\r\n              model = this.MoveMsg;\r\n          if (rcm.index >= 0 && rcm.data) {\r\n              model.splice(rcm.index, 1);\r\n              this.choosePumpList.forEach(pump =>{\r\n                  if(pump.pump_id == rcm.data.uid){\r\n                      pump.dragFlag = true;//删除了该机泵，该机泵需要变更为可拖动状态\r\n                  }\r\n              })\r\n          }\r\n          this.cancelClick();\r\n          this.isTip = false;\r\n      },\r\n      // 取消点击机组组态效果\r\n      cancelClick() {\r\n          this.isShow = false;\r\n          if(this.isRightClick)\r\n          this.isRightClick = false;\r\n      },\r\n      //  机组组态保存警告框关闭\r\n      closeAlarm() {\r\n          this.isAlarm = false;\r\n          this.isTip = false;\r\n      },\r\n      //搜索下拉列表\r\n      searchFolder: function(e){\r\n          let search_text = e.target.value;\r\n          if(search_text.length>0){\r\n              let test_atr = new RegExp(search_text);\r\n              this.searchFolderList=[];\r\n              this.isShow=true;\r\n              for(let i=0;i<this.folderList.length;i++){\r\n                  if(test_atr.test(this.folderList[i].t_name) || ConvertPinyin(this.folderList[i].t_name).indexOf(search_text)>=0){\r\n                      this.searchFolderList.push(this.folderList[i]);\r\n                  }\r\n              }\r\n          }\r\n          else{\r\n              this.searchFolderList=this.folderList.concat();\r\n              this.isShow=false;\r\n              this.chooseFolder={\r\n                  t_id:\"\",\r\n                  t_name:\"\",\r\n              };\r\n              this.choosePumpList=[];\r\n          }\r\n      },\r\n      //点击组织列表\r\n      clickFolder:function(chooseInfo,index){\r\n          $('.search-li').eq(index).addClass('active').siblings().removeClass('active');\r\n          this.chooseFolder=chooseInfo;\r\n          this.$refs.searchInput.value =this.chooseFolder.t_name;\r\n      },\r\n      //选择机泵\r\n      clickPump: function(pump,index){\r\n          $('.pump-li').eq(index).addClass('active').siblings().removeClass('active');\r\n          // console.log(pump)\r\n          this.choosePump=pump;\r\n      },\r\n      //双击机泵跳转至机组总貌图设置页面\r\n      getPumpInfo(item){\r\n          let linkObj = {\r\n              chooseFolder:this.chooseFolder,\r\n              choosePump: {\r\n                  pump_id: item.uid,\r\n                  mac_id: item.mac_id,\r\n                  ch_class: item.ch_class,\r\n                  pump_name: item.pump_name,\r\n              }\r\n          };\r\n\r\n          this.$router.push({\r\n              name: `overviewPump`,\r\n              params: linkObj\r\n          })\r\n      },\r\n      //保存总貌图\r\n      keepOverview: function(){\r\n          var that=this;\r\n          var token = sessionStorage.getItem('token');\r\n          // console.log(this.MoveMsg);\r\n          // console.log(this.chooseFolder);\r\n          let overViewObj = {\r\n              o_id:this.chooseFolder.t_id,//总貌图uid（=组织/机组/机泵uid）\r\n              mac_id:\"A\",\r\n              itemList:[],//节点列表\r\n          };//总貌图\r\n          let obj={};\r\n          for(let i=0;i<this.MoveMsg.length;i++){\r\n              obj={\r\n                  id:i,//计数id\r\n                  type:1,//类型 1=组织 2=机组 3=机泵 4=测点\r\n                  itemType:this.MoveMsg[i].itemType,//设备类型 如1=破碎机\r\n                  uid:this.MoveMsg[i].uid,//节点uid，如\"3_05\"，表示id=05的振动测点\r\n                  px:(this.MoveMsg[i].x/this.origBlock.oriW).toFixed(2),//相对于背景图的x位置\r\n                  py:(this.MoveMsg[i].y/this.origBlock.oriH).toFixed(2),//相对于背景图的y位置\r\n                  pwidth:this.MoveMsg[i].width,//相对于背景图的宽度\r\n                  pheight:this.MoveMsg[i].height//相对于背景图的高度\r\n              };\r\n              overViewObj.itemList.push(obj);\r\n          }\r\n          // console.log(overViewObj);\r\n      this.$axios.post('updateTreeOverview',overViewObj)\r\n      .then(res=>{\r\n          if(res.msg==0){\r\n              swal({\r\n                  title: \"提示\",\r\n                  text: '保存成功',\r\n                  button: \"确认\",\r\n              });\r\n          }\r\n          else if(res.msg==1){\r\n              console.log('参数为空')\r\n              swal({\r\n                  title: \"提示\",\r\n                  text: '保存失败',\r\n                  button: \"确认\",\r\n              });\r\n          }\r\n          else if(res.msg==2){\r\n              console.log('后台运行错误')\r\n              swal({\r\n                  title: \"提示\",\r\n                  text: '保存失败',\r\n                  button: \"确认\",\r\n              });\r\n          }\r\n      })\r\n          .catch(err=>{\r\n              console.log('请求失败',err)\r\n          })\r\n      },\r\n      //取消编辑\r\n      editNo: function(){\r\n          this.dragMsg=null;// 拖动的机泵的信息\r\n          this.MoveMsg=[];//移动的机泵的信息\r\n      },\r\n      //机泵类型分类\r\n      getPumpImg:function(name) {\r\n          var machineImg = {};\r\n          const machine = {\r\n              '风机': {\r\n                  machine: 'fengji',\r\n                  itemType: 1,\r\n                  width: 44,\r\n                  height: 49\r\n              },\r\n              '提升机': {\r\n                  machine: 'tishengji',\r\n                  itemType: 2,\r\n                  width: 31,\r\n                  height: 72\r\n              },\r\n              '破碎机': {\r\n                  machine: 'posuiji',\r\n                  itemType: 3,\r\n                  width: 35,\r\n                  height: 34\r\n              },\r\n              '水磨': {\r\n                  machine: 'shuinimo',\r\n                  itemType: 4,\r\n                  width: 66,\r\n                  height: 21\r\n              },\r\n              '磨机': {\r\n                  machine: 'limo',\r\n                  itemType: 5,\r\n                  width: 35,\r\n                  height: 53\r\n              },\r\n              '煤磨': {\r\n                  machine: 'limo',\r\n                  itemType: 6,\r\n                  width: 35,\r\n                  height: 53\r\n              },\r\n              '立磨': {\r\n                  machine: 'limo',\r\n                  itemType: 7,\r\n                  width: 35,\r\n                  height: 53\r\n              },\r\n              '水泥磨': {\r\n                  machine: 'shuinimo',\r\n                  itemType: 8,\r\n                  width: 66,\r\n                  height: 21\r\n              },\r\n              '辊压机': {\r\n                  machine: 'gunyaji',\r\n                  itemType: 9,\r\n                  width: 42,\r\n                  height: 25\r\n              },\r\n              '窑': {\r\n                  machine: 'huizhuanyao',\r\n                  itemType: 10,\r\n                  width: 66,\r\n                  height: 21\r\n              },\r\n              '余热发电': {\r\n                  machine: 'yurefadianji',\r\n                  itemType: 11,\r\n                  width: 67,\r\n                  height: 23\r\n              },\r\n              '链': {\r\n                  machine: 'shuliaolalianji',\r\n                  itemType: 12,\r\n                  width: 80,\r\n                  height: 40\r\n              },\r\n              '减速': {\r\n                  machine: 'jiansuji',\r\n                  itemType: 13,\r\n                  width: 42,\r\n                  height: 30\r\n              },\r\n              '汽轮机': {\r\n                  machine: 'yurefadianji',\r\n                  itemType: 14,\r\n                  width: 80,\r\n                  height: 40\r\n              },\r\n          };\r\n          for (var i in machine) {\r\n              if (name.indexOf(i) !== -1) {\r\n                  machineImg = machine[i];\r\n                  break;\r\n              }\r\n              else{\r\n                  machineImg = {\r\n                      machine: 'yurefadianji',\r\n                      itemType: 14,\r\n                      width: 80,\r\n                      height: 40\r\n                  }\r\n              }\r\n          }\r\n          // }\r\n          return machineImg;\r\n      },\r\n      //确定选择组织\r\n      selectFolder: function(chooseInfo,index){\r\n          $('.search-li').eq(index).addClass('active').siblings().removeClass('active');\r\n          this.chooseFolder=chooseInfo;\r\n          this.choosePumpList.length = 0;//切换组织时机泵列表清空\r\n          this.$refs.searchInput.value =this.chooseFolder.t_name;\r\n          this.isShow=false;\r\n          var that=this;\r\n          var token = sessionStorage.getItem('token');\r\n          let tidObj={\r\n              t_id:this.chooseFolder.t_id,\r\n              type:1\r\n          };\r\n      this.$axios.post('queryPumpOverviewInfoByTId',tidObj)\r\n      .then(res=>{\r\n          that.overviewUrl='';\r\n            if(res.msg==0){\r\n                      that.MoveMsg=[];\r\n                      if(!$.isEmptyObject(res.folder.overview)){\r\n                          that.overviewUrl= res.folder.overview.bgurl;\r\n                          /*that.overviewUrl = require(\"assets/img/cement_overview.svg\");*/\r\n                          let oriItemList = res.folder.overview.itemList;\r\n                          for(let i=0;i<oriItemList.length;i++){\r\n                              let obj = {\r\n                                  x : parseInt(oriItemList[i].px*that.origBlock.oriW),\r\n                                  y : parseInt(oriItemList[i].py*that.origBlock.oriH),\r\n                                  uid : oriItemList[i].uid,\r\n                                  itemType: oriItemList[i].itemType,\r\n                                  width : oriItemList[i].pwidth,\r\n                                  height : oriItemList[i].pheight,\r\n                                  style:{\r\n                                      left: parseInt(oriItemList[i].px*that.origBlock.oriW) + 'px',\r\n                                      top: parseInt(oriItemList[i].py*that.origBlock.oriH) + 'px',\r\n                                      zIndex: 2,\r\n                                  }\r\n                              };\r\n                              for(let j=0;j<res.pump_list.length;j++){\r\n                                  if(oriItemList[i].uid==res.pump_list[j].pump_id){\r\n                                      res.pump_list[j].dragFlag = false;\r\n                                      obj.dragFlag = false;\r\n                                      obj.mac_id = res.pump_list[j].mac_id;\r\n                                      obj.ch_class = res.pump_list[j].ch_class;\r\n                                      obj.pump_name = res.pump_list[j].pump_name;\r\n                                      obj.text = res.pump_list[j].pump_name;\r\n                                      obj.url = require('assets/img/cement_machines/'+that.getPumpImg(res.pump_list[j].pump_name).machine+'.png');\r\n                                  }\r\n                              }\r\n                              that.MoveMsg.push(obj);\r\n                          }\r\n                      }\r\n                      else{\r\n                          that.overviewUrl=\"\";\r\n                      }\r\n                      that.choosePumpList=res.pump_list;\r\n                      if(that.choosePumpList.length>0){\r\n                          for(let i=0;i<that.choosePumpList.length;i++){\r\n                              that.choosePumpList[i].url=require('assets/img/cement_machines/'+that.getPumpImg(that.choosePumpList[i].pump_name).machine+'.png');\r\n                              that.choosePumpList[i].width=that.getPumpImg(that.choosePumpList[i].pump_name).width;\r\n                              that.choosePumpList[i].height=that.getPumpImg(that.choosePumpList[i].pump_name).height;\r\n                              that.choosePumpList[i].itemType=that.getPumpImg(that.choosePumpList[i].pump_name).itemType;\r\n                              if(that.choosePumpList[i].dragFlag === false){\r\n\r\n                              }\r\n                              else{\r\n                                  that.choosePumpList[i].dragFlag=true;\r\n                              }\r\n\r\n                          }\r\n\r\n                      }\r\n                      // console.log(that.choosePumpList);\r\n                      // console.log(that.MoveMsg);\r\n                  }\r\n\r\n      })\r\n          .catch(err=>{\r\n              console.log('请求错误',err)\r\n          })\r\n      },\r\n      //取消选择\r\n      quitChoose: function(){\r\n          this.isShow=false;\r\n      },\r\n      monidianji: function () {\r\n          document.getElementById('imgLocal').click()\r\n      },\r\n      //上传总貌图背景图\r\n      selectImg:function(event){\r\n          let that=this;\r\n          let imgFile = $('#imgLocal')[0].files[0];//取到上传的图片\r\n\r\n          let param = new FormData(); // 创建form对象\r\n          param.append('files', imgFile);//对应后台接收图片名\r\n          param.append('id', this.chooseFolder.t_id);\r\n          param.append('mac_id', \"A\");\r\n          // console.log(param);\r\n          let token = sessionStorage.getItem('token');\r\n          this.$axios.post('createTreeOverview',param,{\r\n              headers: {\r\n                  'Content-Type': 'multipart/form-data',\r\n              }\r\n          }).then(res => {\r\n              // console.log(res);//\r\n              $('#imgLocal')[0].value = \"\";\r\n              that.overviewUrl = '';\r\n              if(res.msg==0){\r\n                  that.overviewUrl = res.bgurl;\