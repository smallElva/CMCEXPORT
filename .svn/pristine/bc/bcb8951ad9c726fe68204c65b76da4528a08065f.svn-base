dden;\n}\n.select-language {\n  position: absolute;\n  top: 5px;\n  right: 30px;\n  width: 100px;\n}\n.login-edit {\n  position: absolute;\n  top: 50%;\n  right: 10%;\n  margin-top: -326px;\n  width: 546px;\n  height: 652px;\n  background: white;\n  border-radius: 8px;\n  box-shadow: 0px 1px 43px 0px rgba(75, 121, 255, 0.17);\n  text-align: center;\n}\n.login-edit .login_title {\n    margin-top: 112px;\n    font-size: 30px;\n    font-family: PingFang SC;\n    font-weight: 300;\n    color: #999999;\n}\n.login-edit .login_title2 {\n    margin-top: 20px;\n    font-size: 27px;\n    font-family: PingFang SC;\n    font-weight: 300;\n    color: #999999;\n}\n.login-edit .underLine {\n    width: 70px;\n    height: 10px;\n    border-radius: 6px;\n    background: #F5F8FF;\n    margin: 18px auto 0;\n}\n.login_box {\n  width: 360px;\n  margin: 40px auto 0;\n}\n.login_box .login_div {\n    width: 100%;\n    height: 50px;\n    margin-bottom: 60px;\n    position: relative;\n}\n.login_box .login_div .icon_btn {\n      width: 60px;\n      height: 100%;\n      line-height: 50px;\n      background: #396AFF;\n      border-radius: 10px;\n      position: absolute;\n      left: 0;\n      top: 0;\n}\n.login_box .login_div .icon_btn .iconfont {\n        font-size: 24px;\n        color: #fff;\n}\n.login_box .login_div input {\n      width: calc(100% - 70px);\n      height: calc(100% - 20px);\n      background: #E5EBFF;\n      border: none;\n      border-radius: 10px;\n      padding-left: 70px;\n      padding-top: 10px;\n      padding-bottom: 10px;\n      font-size: 18px;\n      color: #444;\n}\n.login_box .login_div input:focus {\n        outline: none;\n}\n.login_box .licence_div {\n    margin-top: 80px;\n    margin-bottom: 80px;\n}\n.login_box .licence_div .licence_p {\n      font-size: 20px;\n      color: #444;\n      margin-bottom: 10px;\n}\n.login_box .licence_div .licence_text {\n      font-size: 16px;\n      color: #444;\n      margin-bottom: 15px;\n      -webkit-user-select: text;\n      -khtml-user-select: text;\n      -moz-user-select: text;\n      -ms-user-select: text;\n      user-select: text;\n}\n.login_btns .login_btn {\n  width: 100%;\n  height: 50px;\n  background: linear-gradient(-90deg, #2d72fd, #3d7eff);\n  border-radius: 25px;\n  color: #fff;\n  text-align: center;\n  border: none;\n  font-size: 22px;\n  font-family: PingFang SC;\n  font-weight: 500;\n}\n.login_btns .login_btn:focus {\n    outline: none;\n}\n.login_btns .login_btn:hover {\n    background: #2d72fd;\n}\n.version_cont {\n  position: absolute;\n  bottom: 30px;\n  right: 10%;\n  width: 546px;\n}\n.version_cont .version_text {\n    text-align: center;\n    font-size: 16px;\n    color: #245CCE;\n    opacity: 0.5;\n}\n.input-loc-img {\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  visibility: hidden;\n}\n@media screen and (max-width: 1500px) {\n  /*当屏幕尺寸高度小于800px时，应用下面的CSS样式*/\n.login-edit {\n    right: 8%;\n    margin-top: -250px;\n    width: 400px;\n    height: 500px;\n}\n.login-edit .login_title {\n      margin-top: 60px;\n}\n.login-edit .underLine {\n      width: 60px;\n      height: 8px;\n      border-radius: 4px;\n      margin: 20px auto 0;\n}\n.version_cont {\n    right: 8%;\n    width: 400px;\n}\n.login_box {\n    width: 300px;\n    margin: 40px auto 0;\n}\n.login_box .login_div {\n      margin-bottom: 50px;\n}\n.login_box .login_div .icon_btn {\n        height: 100%;\n}\n.login_box .login_div .icon_btn .iconfont {\n          font-size: 24px;\n}\n.login_box .login_div input {\n        font-size: 16px;\n}\n}\n"]}