ner('scroll',(e)=>{\r\n                this.hot.destroyEditor()//处理当选择下拉框后拖动滚动条框框会跟着跑的问题\r\n            });\r\n            this.filterRows(this.hot,this.data);//保留过滤结果\r\n        },\r\n\r\n        //保留过滤后的结果\r\n        filterRows(hot,data){\r\n            const filtersPlugin = hot.getPlugin('filters');\r\n            /*const macTypeList = [1,5,6,7];//机组类型\r\n            const pumpTypeList = [3,8];//机泵、车厢*/\r\n            if(MacTypeBtn.includes(Number(this.type)) && sessionStorage.getItem('filter_type1')){\r\n                let filterArr =  JSON.parse(sessionStorage.getItem('filter_type1'));\r\n                filterArr.forEach(condition =>{\r\n                    filtersPlugin.addCondition(condition.column,condition.conditions[0].name,condition.conditions[0].args,condition.operations);\r\n                });\r\n                filtersPlugin.filter();\r\n            }\r\n            else if(this.type == 2 && sessionStorage.getItem('filter_type2')){\r\n                let filterArr =  JSON.parse(sessionStorage.getItem('filter_type2'));\r\n                filterArr.forEach(condition =>{\r\n                    filtersPlugin.addCondition(condition.column,condition.conditions[0].name,condition.conditions[0].args,condition.operations);\r\n                });\r\n                filtersPlugin.filter();\r\n            }\r\n            else if(PumpTypeBtn.includes(Number(this.type)) && sessionStorage.getItem('filter_type3')){\r\n                let filterArr =  JSON.parse(sessionStorage.getItem('filter_type3'));\r\n                filterArr.forEach(condition =>{\r\n                    filtersPlugin.addCondition(condition.column,condition.conditions[0].name,condition.conditions[0].args,condition.operations);\r\n                });\r\n                filtersPlugin.filter();\r\n            }\r\n        },\r\n        //获取data\r\n        getData(){\r\n            return this.data\r\n        },\r\n        /* 获取hot实例 */\r\n        getHot(){\r\n            return this.hot\r\n        