GAP,qBAAa,CAACQ,MAAd;AACH;AACJ,KArRG;AAsRJ;AACAlU,WAvRI,qBAuRK;AACL,aAAO,KAAKhC,IAAZ;AACH,KAzRG;;AA0RJ;AACAqD,UA3RI,oBA2RI;AACJ,aAAO,KAAKmD,GAAZ;AACH;AA7RG;AA9DU,CAAf,C","file":"js/11-e15d16320ef35e14a03f.js","sourcesContent":["\r\n\r\n/* 表头部分\r\n */\r\n\r\n//设置 列头(公共)\r\nexport function setHeaders(type){\r\n    let header\r\n    if(type == 1){ //机组设置\r\n        header = [\r\n                '机组名称',\r\n                '所属组织',\r\n                '生产厂商',\r\n                '机组型号',\r\n                '车厢数量',\r\n                '传动链名称',\r\n                '是否有效',\r\n                '机组类型',\r\n                '塔筒高度（m）',\r\n                '齿轮箱型号',\r\n                '齿轮箱厂商',\r\n                '发电机型号',\r\n                '发电机厂商',\r\n                '叶片型号',\r\n                '叶片厂商',\r\n                '塔筒型号',\r\n                '塔筒厂商',\r\n        ];\r\n    }\r\n    else if(type == 2){\r\n        header=[\r\n            '组织名称',\r\n            '机组名称',\r\n            '采集器类型',\r\n            '工况名称',\r\n            '工况参考测点1',\r\n            '下限',\r\n            '上限',\r\n            '工况参考测点2',\r\n            '下限',\r\n            '上限',\r\n            '波形存储（个/天）',\r\n            '长采样存储（个/天）',\r\n        ]\r\n    }\r\n    else if(type == 3){\r\n        header = [\r\n            '所属组织',\r\n            '机组名称',\r\n            '子级名称',\r\n            '是否有效',\r\n            '子级类型',\r\n            '传动链名称',\r\n            '车厢号',\r\n            '品牌',\r\n            'PN号',\r\n            'SN号',\r\n            '功率',\r\n            '额定转速',\r\n            '转速比',\r\n            '生产日期',\r\n            '其它',\r\n        ]\r\n    }\r\n    else if(type == 4){\r\n        header = [\r\n            '所属组织',\r\n            '机组名称',\r\n            '行驶方向',\r\n            '站点名称',\r\n            '站点ID',\r\n        ]\r\n    }\r\n    return header\r\n}\r\n\r\n\r\n/* 表数据部分\r\n */\r\n\r\n//设置columns（公共）（组织名称、机组名称、测点名称、采集器名称、测点类型、通道名称、是否有效、是否报警）\r\nexport function setColumns(type,data){\r\n    let columns;\r\n    if(type == 1){ //基本设置\r\n        columns=[\r\n            {   data: 'machine.mac_me',\r\n                readOnly: true,\r\n            },\r\n            {\r\n                data: 'machine.t_name',\r\n                editor: 'select',\r\n                readOnly: true,\r\n                selectOptions: row => data[row].machine.t_name_sources\r\n            },\r\n            {\r\n                data: 'machine.baseInfo.manufacturer',\r\n            },\r\n            {data: 'machine.baseInfo.model_number'},\r\n            {\r\n                data: 'machine.baseInfo.car_num_val',//车厢数量\r\n                editor: 'select',\r\n                readOnly: true,\r\n                selectOptions: row => data[row].machine.baseInfo.car_num_sources\r\n            },\r\n            {data: 'machine.baseInfo.driveChain_name'},\r\n            {\r\n                data: 'machine.mac_activity_val',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].machine.mac_activity_sources\r\n            },\r\n            {\r\n                data: 'machine.baseInfo.mac_type_val',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].machine.baseInfo.mac_type_sources\r\n            },\r\n\r\n            {data: 'machine.baseInfo.tower_high'},\r\n            {data: 'machine.baseInfo.model_gear'},\r\n            {data: 'machine.baseInfo.made_gear'},\r\n            {data: 'machine.baseInfo.model_generator'},\r\n            {data: 'machine.baseInfo.made_generator'},\r\n            {data: 'machine.baseInfo.model_vane'},\r\n            {data: 'machine.baseInfo.made_vane'},\r\n            {data: 'machine.baseInfo.model_tower'},\r\n            {data: 'machine.baseInfo.made_tower'},\r\n        ]\r\n    }\r\n    else if(type == 2){//工况设置\r\n        columns = [\r\n            {data: \"condition.t_name\"},\r\n            {data: \"condition.mac_me\"},\r\n            {\r\n                data: \"condition.dgm_name\",\r\n                readOnly:true,\r\n            },\r\n            {\r\n                data: 'condition.condition_state',\r\n                type: 'checkbox',\r\n                checkedTemplate: '1',\r\n                uncheckedTemplate: '0',\r\n                label: {\r\n                    position: 'after',\r\n                    property: \"condition.condition_name\"\r\n                },\r\n            },\r\n            {\r\n                data: \"condition.pos_name1\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].condition.pos_sources1\r\n            },\r\n            {data: \"condition.conditionL_1\"},\r\n            {data: \"condition.conditionH_1\"},\r\n            {\r\n                data: \"condition.pos_name2\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].condition.pos_sources2\r\n            },\r\n            {data: \"condition.conditionL_2\"},\r\n            {data: \"condition.conditionH_2\"},\r\n            {\r\n                data: \"condition.wavenum\",\r\n                type: 'numeric'\r\n            },\r\n            {\r\n                data: \"condition.capturenum_val\",\r\n                editor: 'select',\r\n                selectOptions: row => data[row].condition.capturenumSource\r\n            },\r\n        ]\r\n    }\r\n    else if(type==3){\r\n        columns = [\r\n            {\r\n                data: 'pump.folder_name',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].pump.folder_sources\r\n            },\r\n            {\r\n                data: 'pump.machine_name',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].pump.macName_sources\r\n            },\r\n            {data:'pump.pump_name'},\r\n            {\r\n                data:'pump.pump_activity_val',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].pump.pump_activity_sources\r\n            },\r\n            {\r\n                data:'pump.pump_type_val',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].pump.pump_type_sources\r\n            },\r\n            {data:'pump.driveChain_name'},\r\n            {\r\n                data:'pump.baseInfo.car_id',\r\n                readOnly: true,\r\n            },\r\n            {data:'pump.baseInfo.brand'},\r\n            {data:'pump.baseInfo.pn'},\r\n            {data:'pump.baseInfo.sn'},\r\n            {data:'pump.baseInfo.power'},\r\n            {data:'pump.baseInfo.rated_speed'},\r\n            {data:'pump.baseInfo.speed_rate'},\r\n            {data:'pump.baseInfo.mf_date'},\r\n            {data:'pump.baseInfo.other'},\r\n        ]\r\n    }\r\n    else if(type==4){\r\n        columns = [\r\n            {\r\n                data: 'tms.folder_name',\r\n                editor: 'select',\r\n                readOnly: true,\r\n                selectOptions: row => data[row].tms.folder_sources\r\n            },\r\n            {\r\n                data: 'tms.machine_name',\r\n                editor: 'select',\r\n                readOnly: true,\r\n                selectOptions: row => data[row].tms.macName_sources\r\n            },\r\n            {   data:'tms.direction_val',\r\n                editor: 'select',\r\n                selectOptions: row => data[row].tms.directionSources\r\n            },\r\n            {\r\n                data:'tms.stationName',\r\n            },\r\n            {\r\n                data:'tms.stationId',\r\n            },\r\n        ]\r\n    }\r\n    return columns\r\n}\r\n\r\n\r\n","import getApi from 'network/getApi'\r\nimport hdtable from 'common/hdtable.js'\r\nimport {createMac,changeMac,createPump,changePump,createTms,changeTms} from './mac_info.js'\r\nimport {setHeaders,setColumns} from './mac_columns.js'\r\nimport {setChangeCellResult,addSessionStorage} from 'utils/utils'\r\n\r\nlet deleteArray = [];//删除的信息数组\r\nlet colWidth = '';\r\nexport default function machdtable(el,property){\r\n    let data = property.getData();\r\n    let type = property.type;\r\n    let fixNum;\r\n    if(type == 1) { fixNum = 2 } else { fixNum = 3 };\r\n    //创建表格属性的实例\r\n    let tableDataIns= new tableData(property);\r\n    // 组装振动表格数据\r\n    let vibTable ={\r\n        // 表格右键菜单\r\n        contextMenu:tableDataIns.contextMenu(),\r\n        // 表格显示的数据\r\n        data:data,\r\n        fixedColumnsLeft: fixNum,\r\n        //表格的列\r\n        colHeaders:tableDataIns.colHeaders(),\r\n        //数据渲染renderData\r\n        columns:tableDataIns.columns(),\r\n\r\n        //单元格属性设置\r\n        cells:tableDataIns.cells(),\r\n        //单击单元格或行/列标题后触发。如果单击行/列标题，则坐标索引为负。\r\n        afterOnCellMouseUp:tableDataIns.afterOnCellMouseUp(),\r\n        //过滤之后\r\n        afterFilter:tableDataIns.afterFilter(),\r\n        //新建行\r\n        afterCreateRow:tableDataIns.afterCreateRow(),\r\n\r\n        beforeChange: tableDataIns.beforeChange(),\r\n\r\n        afterChange: tableDataIns.afterChange(),\r\n        //移除行事件\r\n        beforeRemoveRow:tableDataIns.beforeRemoveRow(),\r\n\r\n        afterRemoveRow:tableDataIns.afterRemoveRow(),\r\n        //列宽改变\r\n        afterColumnResize:tableDataIns.afterColumnResize(),\r\n    };\r\n    /**列宽设置**/\r\n    //默认情况下自适应\r\n    if(data.length===0){//如果表格没有数据，设置默认宽度，以免表头挤在一起\r\n        vibTable.colWidths = '170';\r\n    }\r\n    else if(data.length && colWidth!==''){//如果有数据，且拖动表格列宽的情况下，保存为拖动后的列宽\r\n        vibTable.colWidths = colWidth;\r\n    }\r\n    return hdtable(el,vibTable)\r\n}\r\n\r\n\r\n//表格的方法\r\nclass tableData{\r\n    constructor(property){\r\n        this.type= property.type\r\n        this.tids= property.tids\r\n        this.getData= property.getData\r\n        this.getHot= property.getHot\r\n        this.initList= property.initList\r\n        this.changeList= property.changeList;\r\n        this.router = property.router;\r\n        this.store = property.store\r\n    }\r\n    contextMenu(){\r\n        let that = this;\r\n        let userType = parseInt(sessionStorage.getItem('userType'));\r\n        if(that.type == 2){\r\n            return {\r\n                items: {\r\n                    'freeze_column':{\r\n                        name: '固定列',\r\n                    },\r\n                    \"unfreeze_column\": {\r\n                        name: '取消列固定',\r\n                    },\r\n                }\r\n            }\r\n        }\r\n        else if(that.type==1) {\r\n            if (userType === 0) {\r\n                return {\r\n                    items: {\r\n                        \"row_above\": {\r\n                            name: \"插入新行\"\r\n                        },\r\n                        \"remove_row\": {\r\n                            name: '删除这行',\r\n                            callback(key, options) {\r\n                                swal({\r\n                                    title: \"警告\",\r\n                                    text: \"一但删除，该机组数据将不保留！！！确定删除吗？\",\r\n                                    buttons: {\r\n                                        sure: \"确认\",\r\n                                        cancel: \"取消\",\r\n                                    },\r\n                                    dangerMode: true,\r\n                                })\r\n                                    .then((willDelete) => {\r\n                                        if (willDelete) {\r\n                                            options.forEach(option =>{\r\n                                                let index = option.start.row;\r\n                                                let amount = parseInt(option.end.row-option.start.row) + 1;\r\n                                                that.getHot().alter(\"remove_row\",index,amount);\r\n                                            })\r\n                                        } else {\r\n                                            return false;\r\n                                        }\r\n                                    });\r\n                            },\r\n                        },\r\n                        'freeze_column':{\r\n                            name: '固定列',\r\n                        },\r\n                        \"unfreeze_column\": {\r\n                            name: '取消列固定',\r\n                        },\r\n                    }\r\n                }\r\n            } else {\r\n                return {\r\n                    items: {\r\n                        \"row_above\": {\r\n                            name: \"插入新行\"\r\n                        },\r\n                        'freeze_column':{\r\n                            name: '固定列',\r\n                        },\r\n                        \"unfreeze_column\": {\r\n                            name: '取消列固定',\r\n                        },\r\n                    }\r\n                }\r\n            }\r\n        }\r\n        else if(that.type==3){\r\n            return {\r\n                items:{\r\n                    \"row_above\":{\r\n                        name:\"插入新行\"\r\n                    },\r\n                    \"remove_row\": {\r\n                        name: '删除这行',\r\n                        callback(key, options) {\r\n                            swal({\r\n                                title: \"警告\",\r\n                                text: \"当前机组一经删除，该数据也将不保留。确定删除吗？\",\r\n                                buttons: {\r\n                                    sure: \"确认\",\r\n                                    cancel: \"取消\",\r\n                                },\r\n                                dangerMode: true,\r\n                            })\r\n                                .then((willDelete) => {\r\n                                    if (willDelete) {\r\n                                        options.forEach(option =>{\r\n                                            let index = option.start.row;\r\n                                            let amount = parseInt(option.end.row-option.start.row) + 1;\r\n                                            that.getHot().alter(\"remove_row\",index,amount);\r\n                                        })\r\n                                    } else {\r\n                                        return false;\r\n                                    }\r\n                                });\r\n                        },\r\n                    },\r\n                    'freeze_column':{\r\n                        name: '固定列',\r\n                    },\r\n                    \"unfreeze_column\": {\r\n                        name: '取消列固定',\r\n                    },\r\n                }\r\n            }\r\n        }\r\n        else if(that.type==4){\r\n            return {\r\n                items:{\r\n                    \"row_above\":{\r\n                        name:\"插入新行\"\r\n                    },\r\n                    \"remove_row\": {\r\n                        name: '删除这行',\r\n                        callback(key, options) {\r\n                            swal({\r\n                                title: \"警告\",\r\n                                text: \"当前机组一经删除，该数据也将不保留。确定删除吗？\",\r\n                                buttons: {\r\n                                    sure: \"确认\",\r\n                                    cancel: \"取消\",\r\n                                },\r\n                                dangerMode: true,\r\n                            })\r\n                                .then((willDelete) => {\r\n                                    if (willDelete) {\r\n                                        options.forEach(option =>{\r\n                                            let index = option.start.row;\r\n                                            let amount = parseInt(option.end.row-option.start.row) + 1;\r\n                                            that.getHot().alter(\"remove_row\",index,amount);\r\n                                        })\r\n                                    } else {\r\n                                        return false;\r\n                                    }\r\n                                });\r\n                        },\r\n                    },\r\n                    'freeze_column':{\r\n                        name: '固定列',\r\n                    },\r\n                    \"unfreeze_column\": {\r\n                        name: '取消列固定',\r\n                    },\r\n                }\r\n            }\r\n        }\r\n    }\r\n    /* 表格的数据渲染 */\r\n    colHeaders(){\r\n        return setHeaders(this.type)\r\n    }\r\n    columns(){\r\n        return setColumns(this.type,this.getData())\r\n    }\r\n    /* 表格的方法渲染 */\r\n    //振动的单元格渲染\r\n    cells(){\r\n        let that = this\r\n        return function (row, col, prop){\r\n            let data =that.getData()\r\n            if(data.length == 0) return\r\n            let type = that.type\r\n            let cellProperties = this;\r\n            if(type==1){\r\n                if(row!=-1 && data[row].machine.operate == 1 && (col == 0 || col == 1)){\r\n                    cellProperties.readOnly  = false;\r\n                }\r\n                if(row!=-1 && (prop==\"machine.baseInfo.car_num_val\") && data[row].machine.t_root == 2){\r\n                    cellProperties.readOnly  = false;\r\n                }\r\n                if(prop==\"machine.baseInfo.driveChain_name\" || col> 6){\r\n                    if(data[row].machine.t_root == 1 || data[row].machine.t_root == 2){\r\n                        cellProperties.readOnly  = true;\r\n                    }\r\n                    else if(Number(data[row].machine.t_root) === 0){\r\n                        cellProperties.readOnly  = false;\r\n                    }\r\n                }\r\n\r\n            }\r\n            if(type==2){\r\n                if(col == 0 || col == 1){\r\n                    cellProperties.readOnly  = true;\r\n                }\r\n                //其他工况的状态和上下限不可编辑\r\n                if(row!=-1 && Number(row + 1) % 5 === 0 && (prop == 'condition.condition_state' || prop == 'condition.conditionL_1' || prop == 'condition.conditionH_1' || prop == 'condition.conditionL_2' || prop == 'condition.conditionH_2')){\r\n                    cellProperties.readOnly  = true;\r\n                }\r\n                //v1采集器只有一个参考测点可编辑，另一个禁止编辑\r\n                if(row!=-1 && data[row].condition.dgm_type == 1 && (prop == 'condition.conditionL_2' || prop == 'condition.conditionH_2' || prop == 'condition.pos_name2' || prop == 'condition.capturenum_val')){\r\n                    cellProperties.readOnly  = true;\r\n                }\r\n            }\r\n            if(type==3){\r\n                if(col == 0 || col == 1 || (Number(data[row].pump.item)===0 && prop == 'pump.driveChain_name')){\r\n                    cellProperties.readOnly  = true;\r\n                }\r\n                if(prop == 'pump.baseInfo.brand'||prop == 'pump.baseInfo.pn'||prop == 'pump.baseInfo.sn'||prop == 'pump.baseInfo.power'||prop == 'pump.baseInfo.rated_speed'||prop == 'pump.baseInfo.speed_rate'||prop == 'pump.baseInfo.mf_date'||prop == 'pump.baseInfo.other'){\r\n                    if(data[row].pump.t_root == 2){//走行部参数不可设\r\n                        cellProperties.readOnly  = true;\r\n                    }\r\n                    else if(data[row].pump.t_root == 1){//水泥可设\r\n                        cellProperties.readOnly  = false;\r\n                    }\r\n                }\r\n                if(prop == 'pump.baseInfo.car_id'){\r\n                    if(data[row].pump.t_root == 2){//走行部参数不可设\r\n                        cellProperties.readOnly  = false;\r\n                    }\r\n                    else if(data[row].pump.t_root == 1){//水泥可设\r\n                        cellProperties.readOnly  = true;\r\n                    }\r\n                }\r\n                if(row!=-1 && data[row].pump.operate == 1 && (col == 0 || col == 1)){\r\n                    cellProperties.readOnly  = false;\r\n                }\r\n            }\r\n            if(type==4){\r\n                if(row!=-1 && data[row].tms.operate == 1 && (col == 0 || col == 1)){\r\n                    cellProperties.readOnly  = false;\r\n                }\r\n            }\r\n            return cellProperties;\r\n        }\r\n    }\r\n\r\n    //点击单元格之后输出该行信息（共用）\r\n    afterOnCellMouseUp(){\r\n        let that = this\r\n        return function (e, coords, td){\r\n\r\n            if(coords.row>=0){\r\n                let row_info =that.getPositionByIndex(coords.row,that.type)\r\n                let t_root = row_info.t_root\r\n                let folderType = that.type;\r\n                let linkObj = [];\r\n                if(folderType===1){\r\n                  linkObj = [row_info.t_id,row_info.t_name,folderType]\r\n                }\r\n                else if(folderType===3){\r\n                  linkObj = [row_info.folder_id,row_info.folder_name,folderType]\r\n                }\r\n\r\n                that.store.commit('setTreeMac', linkObj);\r\n            }\r\n        }\r\n    }\r\n    // 过滤的方法(有待优化)（共用）\r\n    afterFilter(){\r\n        let that = this\r\n        return function(condition){\r\n            let hot = that.getHot();\r\n            // console.log(condition)\r\n            switch(parseInt(that.type)){\r\n                case 1:\r\n                    sessionStorage.setItem('filter_type1',JSON.stringify(condition));\r\n                    break;\r\n                case 2:\r\n                    sessionStorage.setItem('filter_type2',JSON.stringify(condition));\r\n                    break;\r\n                case 3:\r\n                    sessionStorage.setItem('filter_type3',JSON.stringify(condition));\r\n                    break;\r\n            }\r\n            hot.view.wt.wtOverlays.adjustElementsSize(true);\r\n        }\r\n    }\r\n    // 列宽被改变后\r\n    afterColumnResize(){\r\n        let that = this;\r\n        return function(condition){\r\n            let hot = that.getHot();\r\n            colWidth = hot.getColWidth(condition);//修改拖动后的列宽\r\n        }\r\n    }\r\n    //创建测点之后的方法(共用)\r\n    afterCreateRow(){\r\n        let that =this;\r\n        return function(index,amount,source){//添加行事件\r\n            let data = that.getData();\r\n            let changeData=that.changeList.changeData;\r\n            let addData=that.changeList.addData;\r\n            if(that.type==1){\r\n                let machine = createMac(that.type,that.initList.folderData,data.length);\r\n                changeData[machine.mac_id]=machine;\r\n                data[index].machine = machine;\r\n            }\r\n            else if(that.type==3){\r\n                let pump = createPump(that.type,that.initList.pumpFolderData,that.initList.pump_macNameList,that.initList.pump_macIdList,data.length);\r\n                data[index].pump = pump;\r\n                let sendPump = changePump(that.type,pump,index);\r\n                addData[sendPump.pump.pump_id]=sendPump;\r\n            }\r\n            else if(that.type==4){\r\n                let tms = createTms(that.type,that.initList.pumpFolderData,that.initList.pump_macNameList,that.initList.pump_macIdList,data.length);\r\n                data[index].tms = tms;\r\n                // let sendTms = changeTms(that.type,tms,index);\r\n                // sendTms.index = creatIndex;\r\n                changeData[tms.index] = tms;\r\n            }\r\n        }\r\n    }\r\n\r\n    //删除测点之前的方法,将要删除的方法存到deleteVData（共用）\r\n    beforeRemoveRow(){\r\n        let that =this;\r\n        return function(index,amount){\r\n            let deleteData = that.changeList.deleteData;\r\n            deleteArray = [];\r\n            if(index!=null){\r\n                for(let i=0;i<amount;i++){\r\n                    if(that.type==1){\r\n                        let machine = that.getPositionByIndex(index+i,1);\r\n                        if(machine.flag===1){\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: \"该机组绑定了采集器，不可删除！\",\r\n                                button: \"确认\",\r\n                            });\r\n                            return false;\r\n                        }\r\n                        else{\r\n                            deleteData.push(machine.mac_id);\r\n                            deleteArray.push(machine);\r\n                        }\r\n                    }\r\n                    else if(that.type==3){\r\n                        let pump = that.getPositionByIndex(index+i,3);\r\n                        let obj={};\r\n                        obj.mac_id=pump.mac_id;\r\n                        obj.pump_id=pump.pump_id;\r\n                        deleteData.push(obj);\r\n                        deleteArray.push(pump);\r\n                    }\r\n                    else if(that.type==4){\r\n                        let tms = that.getPositionByIndex(index+i,4);\r\n                        let obj={};\r\n                        obj.rowId=tms.rowId;\r\n                        obj.mac_id=tms.machine_id;\r\n                        obj.stationName=tms.stationName;\r\n                        deleteData.push(obj);\r\n                        deleteArray.push(tms);\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n\r\n    }\r\n    //删除测点方法 发送请求（共用）\r\n    afterRemoveRow(){\r\n        let that =this;\r\n        return function(){\r\n            let deleteData = that.changeList.deleteData\r\n            if(deleteData.length>0 && that.type===1) {\r\n                getApi.deleteMachine(deleteData,that.tids)\r\n                    .then(res=>{\r\n                        if (res.msg == 0) {\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: \"删除机组成功\",\r\n                                button: \"确认\",\r\n                            }).then((value) => {\r\n                                // that.router.go(0)\r\n                                that.remove_remake();\r\n                            });\r\n                        } else if (res.msg == 1) {\r\n                            console.log('参数为空！')\r\n                        } else if (res.msg == 2) {\r\n                            console.log('后台运行错误！')\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            }\r\n            if(deleteData.length>0 && that.type===3) {\r\n                getApi.deletePumps(deleteData)\r\n                    .then(res=>{\r\n                        if (res.msg == 0) {\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: \"删除子集成功\",\r\n                                button: \"确认\",\r\n                            }).then((value) => {\r\n                                that.remove_remake();\r\n                            });\r\n                        } else if (res.msg == 1) {\r\n                            console.log('参数为空！')\r\n                        } else if (res.msg == 2) {\r\n                            console.log('后台运行错误！')\r\n                        }\r\n                    })\r\n            }\r\n            if(deleteData.length>0 && that.type===4) {\r\n                getApi.deleteStations(deleteData)\r\n                    .then(res=>{\r\n                        if (res.msg == 0) {\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: \"删除站点成功\",\r\n                                button: \"确认\",\r\n                            }).then((value) => {\r\n                                that.remove_remake();\r\n                            });\r\n                        } else if (res.msg == 1) {\r\n                            console.log('参数为空！')\r\n                        } else if (res.msg == 2) {\r\n                            console.log('后台运行错误！')\r\n                        }\r\n                    })\r\n            }\r\n        }\r\n    }\r\n\r\n    //改变数据的时候拼装数据结构（共用）\r\n    beforeChange(){\r\n        let that =this;\r\n        return function (changes,source){\r\n            let change_index;\r\n            let change_prop;\r\n            let change_oldVal;\r\n            let change_newVal;\r\n            let flag;\r\n            let hot = that.getHot();\r\n            let data = that.getData();\r\n            /*let folder_pumpName = {};//不同组织下的机泵名集合\r\n            let folder_macName = {};//不同组织下的机组名集合\r\n            let folder_stationName = {};//不同组织下的站点名集合\r\n            let sameName = that.changeList.sameName;//机组重名列表\r\n            if(that.type == 1){\r\n                data.forEach(item=>{\r\n                    if(!folder_macName[item.machine.t_id]){\r\n                        folder_macName[item.machine.t_id]=[];\r\n                    }\r\n                    if(item.machine.mac_me){\r\n                        folder_macName[item.machine.t_id].push(item.machine.mac_me)\r\n                    }\r\n                });\r\n            }\r\n            if(that.type == 3){\r\n                data.forEach(item=>{\r\n                    if(!folder_pumpName[item.pump.folder_id]){\r\n                        folder_pumpName[item.pump.folder_id] = [];\r\n                    }\r\n                    if(item.pump.pump_name){\r\n                        folder_pumpName[item.pump.folder_id].push(item.pump.pump_name)\r\n                    }\r\n                });\r\n            }\r\n            if(that.type == 4){\r\n                data.forEach(item=>{\r\n                    if(!folder_stationName[item.tms.folder_id]){\r\n                        folder_stationName[item.tms.folder_id] = [];\r\n                    }\r\n                    if(item.tms.stationId){\r\n                        folder_stationName[item.tms.folder_id].push(item.tms.stationId)\r\n                    }\r\n                });\r\n            }*/\r\n            //创建更改组织、机组、采集器、通道、任务的联动实例\r\n\r\n            for(let i= 0;i<changes.length;i++){\r\n                let physicalRow = hot.toPhysicalRow(changes[i][0]);\r\n                let columnVal = hot.propToCol(changes[i][1]);\r\n                change_index = changes[i][0];\r\n                change_prop = changes[i][1];\r\n                change_oldVal = changes[i][2];\r\n                change_newVal = changes[i][3];\r\n                if(change_oldVal == change_newVal ){\r\n                    continue\r\n                }\r\n                let modVals = Number(change_index + 1 ) % 5;\r\n                let cellInfo=that.getPositionByIndex(change_index,that.type);\r\n                //机组\r\n                if(change_prop==\"machine.mac_me\"){   //机组名称\r\n                    let folderId = cellInfo.t_id;\r\n                    /*if(!$.isEmptyObject(folder_macName) && folder_macName[folderId]!=undefined && folder_macName[folderId].length > 0){\r\n                        if(folder_macName[folderId].indexOf(change_newVal) != -1){\r\n                            sameName[change_index+1] = change_newVal;\r\n                        }\r\n                        else{\r\n                            if(sameName[change_index+1]){\r\n                                delete sameName[change_index+1];\r\n                            }\r\n                        }\r\n                    }*/\r\n                }\r\n                if(change_prop==\"machine.baseInfo.driveChain_name\"){ //机组传动链名称不能为空\r\n                    if(change_newVal=='' || change_newVal===null){\r\n                        swal({\r\n                            title: \"提示\",\r\n                            text: `传动链名称不能为空`,\r\n                            button: \"确认\",\r\n                        });\r\n                        return false;\r\n                    }\r\n                }\r\n                if(change_