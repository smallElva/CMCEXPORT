                       if(Number(index) == item.index){\r\n                                    divList[j].style.transform = `translate(${l}px,${item.moveY}px)`\r\n                                }\r\n                            }\r\n                        }\r\n                    }\r\n                    break;\r\n                case 3://上对齐\r\n                    for(let i=0;i<choosePos.length;i++){\r\n                        let item = choosePos[i];\r\n                        if(item.moveY===0){\r\n                            item.y = referT;\r\n                            item.moveY = 0;\r\n                            item.style.top = referT + 'px';\r\n                        }\r\n                        else{\r\n                            l = referT - choosePos[i].y;\r\n                            choosePos[i].moveY = l;\r\n                            for(let j=0;j<divList.length;j++){\r\n                                let index = divList[j].getAttribute('custom-id');\r\n                                if(Number(index) == item.index){\r\n                                    divList[j].style.transform = `translate(${item.moveX}px,${l}px)`\r\n                                }\r\n                            }\r\n                        }\r\n                    }\r\n                    break;\r\n                case 4://垂直居中对齐\r\n                    for(let i=0;i<choosePos.length;i++){\r\n                        let item = choosePos[i];\r\n                        if(item.moveY===0){\r\n                            item.y = referMH;\r\n                            item.moveY = 0;\r\n                            item.style.top = referMH + 'px';\r\n                        }\r\n                        else{\r\n                            l = referMH - choosePos[i].height / 2 - choosePos[i].y;\r\n                            choosePos[i].moveY = l;\r\n                            for(let j=0;j<divList.length;j++){\r\n                                let index = divList[j].g