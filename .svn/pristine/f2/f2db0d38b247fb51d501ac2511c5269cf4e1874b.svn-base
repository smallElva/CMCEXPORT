F;AACA,gBAAgB,8FAAM;AACtB,yBAAyB,uGAAe;AACxC,OAAO;AACP,KAAK;AACL;AACA;AACA;AACe,gF;;;;;;;;;;;;ACtCf;AAAA;AAAA,wCAA6L,CAAgB,mPAAG,EAAC,C;;;;;;;;;;;;ACAjN;AAAA;AAAA;AAAA;AAAA;AAAA;;;;;;;;;;;;;ACAA;AAAA;AAAA;AAAA;AAAA;AAAmF;AAC3B;AACL;AACc;;;AAGjE;AACgG;AAChG,gBAAgB,2GAAU;AAC1B,EAAE,0EAAM;AACR,EAAE,+EAAM;AACR,EAAE,wFAAe;AACjB;AACA;AACA;AACA;;AAEA;;AAEA;AACA,IAAI,IAAU;AACd,YAAY,mBAAO,CAAC,wGAAsE;AAC1F,cAAc,mBAAO,CAAC,+CAAK;AAC3B;AACA;AACA;AACA;AACA,KAAK;AACL;AACA;AACA,sBAAsB,wHAA2C,EAAE;AAAA;AACnE;AACA,gBAAgB,+EAAM;AACtB,yBAAyB,wFAAe;AACxC,OAAO;AACP,KAAK;AACL;AACA;AACA;AACe,gF;;;;;;;;;;;;ACvCf;AAAA;AAAA,wCAA0L,CAAgB,gPAAG,EAAC,C;;;;;;;;;;;;ACA9M;AAAA;AAAA;AAAA;AAAoX,CAAgB,qYAAG,EAAC,C;;;;;;;;;;;;ACAxY;AAAA;AAAA;AAAA;AAAA;AAAA","file":"js/24-a576d59732891aa505d7.js","sourcesContent":["<template>\r\n  <div class=\"my-content-header\" @dblclick=\"dblclick\">\r\n    <span class=\"page-title\">{{title}}</span>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"MyContentTitle\",\r\n  props: [\"title\"],\r\n  data() {\r\n    return {\r\n      flag: true\r\n    };\r\n  },\r\n  methods: {\r\n    dblclick() {\r\n      //放大\r\n      let homeHeader = document.getElementById(\"homeHeader\");\r\n      let aside = document.getElementById(\"aside\");\r\n      let content = document.getElementById(\"content\");\r\n      let wHeight = document.documentElement.clientHeight;\r\n      if (this.flag) {\r\n        homeHeader.style.display = \"none\";\r\n        aside.style.display = \"none\";\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 20px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 40px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.flag = !this.flag;\r\n      } else {\r\n        //缩小\r\n        homeHeader.style.display = \"block\";\r\n        aside.style.display = \"block\";\r\n        content.style.width = \"100px\"; //改变宽让宽自动渲染\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 90px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 120px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.flag = !this.flag;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-content-header {\r\n  /* width: 100%-15px;\r\n   */\r\n  box-sizing: border-box;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  background: #343c45;\r\n  font-size: 20px;\r\n  color: #fff;\r\n  padding-left: 15px;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n","<template>\r\n  <div class='alertBox' :style='relatedOrganization.style' v-show='isShow.isAlertBox'>\r\n    <div class='organizationTitle'>所属组织</div>\r\n    <ul class='organizationList'>\r\n      <li v-for='(item, index) in organizationMsg' :class=\"'itemList' + index%2\" @click='checkOrganization(item)'>\r\n        <div class='organizationName'>{{item.name}}</div>\r\n        <img :src=\"img.check\" v-show='!item.isRelation' alt=\"\" />\r\n        <img :src=\"img.checked\" v-show='item.isRelation' alt=\"\" />\r\n      </li>\r\n    </ul>\r\n    <div class='yesOrNo'>\r\n      <div>\r\n        <button class='yes' @click='yes'>确定</button>\r\n        <button class='no' @click='no'>取消</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"AlertBox\",\r\n  props: ['relatedOrganization', 'folders', 'clone'],\r\n  data() {\r\n    return {\r\n      img:{check:require(\"assets/img/user/check.svg\"),\r\n      checked:require(\"assets/img/user/checked.svg\")},\r\n      token: '',\r\n      organizationMsg: [],\r\n      isShow: {\r\n        isAlertBox: false,\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    checkOrganization(item) {\r\n      if (item.id !== '0') {this.organizationMsg[0].isRelation = false}\r\n\r\n      else if(item.id == '0'){\r\n        let organizationMsg = this.organizationMsg\r\n        for(let i = 1;i<organizationMsg.length;i++){\r\n          organizationMsg[i].isRelation = false\r\n        }\r\n      }\r\n      item.isRelation = !item.isRelation;\r\n    },\r\n    yes() {\r\n      let data = [],\r\n        msg = this.organizationMsg;\r\n\r\n      for (let i = 0, l = msg.length; i < l; i++) {\r\n        if (msg[i].isRelation) {\r\n          data.push(msg[i].id);\r\n        }\r\n      }\r\n      if (data.length > 0) {\r\n        this.$emit('setTrees', data);\r\n      }\r\n      else {\r\n        swal({\r\n          title: \"提示\",\r\n          text: '所属组织不能为空',\r\n          button: \"确认\",\r\n        });\r\n        return;\r\n      }\r\n      this.isShow.isAlertBox = false;\r\n    },\r\n    no() {\r\n      this.isShow.isAlertBox = false;\r\n    },\r\n  },\r\n  created() {\r\n    this.token = sessionStorage.getItem('token')\r\n  },\r\n  mounted() {},\r\n  watch: {\r\n    relatedOrganization: {\r\n      handler(val) {\r\n        this.isShow.isAlertBox = val.isShow;\r\n        let msg = [],\r\n         folders  = [],\r\n          trees = val.organizations;\r\n        folders.push({folderId:\"0\",folderName:\"顶级组织\"})\r\n        folders.push(...this.folders)\r\n        this.organizationMsg.length = 0;\r\n\r\n        for (let i = 0, l = folders.length; i < l; i++) {\r\n          let isRelation = false;\r\n          for (let j = 0, len = trees.length; j < len; j++) {\r\n            if (trees[j].t_id == folders[i].folderId ) {\r\n              isRelation = true;\r\n              msg.push({\r\n                name: folders[i].folderName,\r\n                id: folders[i].folderId,\r\n                isRelation,\r\n              })\r\n            }\r\n          }\r\n          if (!isRelation) {\r\n            msg.push({\r\n              name: folders[i].folderName,\r\n              id: folders[i].folderId,\r\n              isRelation,\r\n            })\r\n          }\r\n        }\r\n        this.organizationMsg = this.clone(msg, true)\r\n      },\r\n      deep: true,\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n","<template>\r\n  <div class=\"myTable\">\r\n        <div :class=\"{'open':isOpen}\">\r\n            <button type=\"button\" class=\"my-btn btn-active btn6\" @click='addUser'>新建用户</button>\r\n            <!--<button type=\"button\" class=\"btn btn-active my-dropdown-toggle\" @click='addUserToggle'>\r\n              <span class=\"my-caret\"></span>\r\n            </button>\r\n            <ul class=\"my-dropdown-menu\">\r\n              <li><a href=\"#\" @click='addUsers();addUserToggle()'>批量新建用户</a></li>\r\n            </ul>-->\r\n        </div>\r\n        <button class=\"my-btn btn-active btn3\" @click='saveUser()'>保存设置</button>\r\n    <div class='user'>\r\n      <table class='userTable'>\r\n        <thead>\r\n          <tr class='userTableHead'>\r\n            <th v-for='item in thead' class='userTitle'>{{item}}</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody class='userTableBody'>\r\n          <tr v-for='(item, index) in userMsg' :class=\"'item' + index%2\" :style='setStyle(item.operat)'>\r\n            <td class='userMsg' @dblclick='editUser(item.id, $event,item)'>\r\n              <span>{{item.userName}}</span>\r\n              <input type=\"text\" style='display:none' v-model='item.userName' @change='finishEdit(item.userName, \"用户名称\", $event, index)' :disabled = \"item.operat\"/>\r\n            </td>\r\n            <td class='userMsg' @click.stop='setRelatedOrganization(index, $event,!item.operat)'>\r\n              {{item.folder}}\r\n            </td>\r\n            <td class='userMsg'>\r\n              <select v-model='item.type' class='setUserType' :style='setStyle(item.operat)'  :disabled = \"item.operat\">\r\n                <option :value=\"idx\" v-for='(val, idx) in userType'>{{val}}</option>\r\n              </select>\r\n            </td>\r\n            <td class='userMsg' @dblclick='editUser(item.id, $event,item)'>\r\n              <span>{{item.name}}</span>\r\n              <input type=\"text\" style='display:none' v-model='item.name' @change='finishEdit(item.name, \"真实姓名\", $event, index)' :disabled = \"item.operat\"/>\r\n            </td>\r\n            <td :class=\"'pwd5' + index\" class='userMsg' @dblclick='changePwd(item.id, $event,item)'>\r\n              <span>{{item.viewPwd}}</span>\r\n              <input type=\"password\" style='display:none' v-model='item.password' @blur='finishEdit(item.password, \"密码\", $event, index)' dataType=\"password\" :disabled = \"item.operat\"/>\r\n            </td>\r\n            <td :class=\"'pwd6' + index\" class='userMsg' @dblclick='changePwd(item.id, $event,item)'>\r\n              <span>{{item.viewConfirmPwd}}</span>\r\n              <input type=\"password\" style='display:none' v-model='item.confirmPassword' @blur='finishEdit(item.confirmPassword, \"密码\", $event, index)' dataType=\"confirmPassword\" :disabled = \"item.operat\"/>\r\n            </td>\r\n            <td class='userMsg'>\r\n              <div>\r\n                <button v-show='item.operat' @click='setEnabled(item)' class='enabled'>启用</button>\r\n                <button v-show='!item.operat' @click='setEnabled(item)' class='prohibit'>禁用</button>\r\n                <button class='delUser' @click='delUserTip(item, index)'>删除</button>\r\n              </div>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n      <alert-box :relatedOrganization='relatedOrganization' :folders='folders' @setTrees='setTrees' :clone='clone' :userType='userType'></alert-box>\r\n    </div>\r\n    <div class='alert' v-show='isShow.isAlert'>\r\n      <div class='inputAlert' v-show='isShow.isInput'>\r\n        <div class='inputAlertTitle'>错误提示</div>\r\n        <div class='inputAlertText'>{{inputAlertText}}</div>\r\n        <div class='inputAlertBtn'>\r\n          <button class='cofirmInputAlert' @click='cofirmInputAlert'>确认</button>\r\n        </div>\r\n      </div>\r\n      <div class='changeTip' v-show='isShow.isChangeTip'>\r\n        <div class='inputAlertTitle changeTipTitle'>提示</div>\r\n        <div class='changeTipText'>\r\n          {{changeMsg.text}}\r\n        </div>\r\n        <div class='yesOrNo'>\r\n          <button class='yes' @click='cofirmChange'>确定</button>\r\n          <button class='no' @click='cancelChange'>取消</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport AlertBox from 'components/content/user/AlertBox'\r\nexport default {\r\n  name: \"MyTable\",\r\n  data() {\r\n    return {\r\n      token: '',\r\n      flag:false,//是否点击保存按钮\r\n      delFlag:false,//是否点击删除按钮\r\n      thead: ['用户名称', '所属组织', '用户类型', '真实姓名', '密码', '确认密码', '操作', ],\r\n      userMsg: [],\r\n      srcUserMsg: [],\r\n      userType: ['后台管理员', '前端管理员'],\r\n      relatedOrganization: { // 关联组织\r\n        isShow: false, // 是否显示选择关联组织弹窗\r\n        organizations: [], // 当前用户关联的组织\r\n        style: {}, // 弹窗位置信息\r\n      },\r\n      folders: [],\r\n      choiceUser: -1, // 当前选中设置关联组织用户下标\r\n      inputAlertText: '', // 错误提示信息\r\n      isShow: {\r\n        isAlert: false, // 是否显示弹窗\r\n        isInput: false, // 是否显示输入提示窗\r\n        isChangeTip: false, // 是否显示修改密码、删除提示框\r\n      },\r\n      changeMsg: { // 提示框信息\r\n        text: '', // 提示框内容\r\n        dom: null, // 如果修改密码不为null\r\n      },\r\n      currentInput: null, // 当前输入框\r\n      delUserMsg: { // 需要删除的用户信息\r\n        item: null, //  对应userMsg信息\r\n        index: -1, //  对应userMsg信息的下表\r\n      },\r\n      isOpen:false\r\n    }\r\n  },\r\n  methods: {\r\n    // 是否为函数\r\n    isFunction(value) {\r\n      return Object.prototype.toString.call(value) === '[object Function]'\r\n    },\r\n    // 是否为有效的数组长度\r\n    isLength(value) {\r\n      return typeof value == 'number' && value > -1 && value % 1 == 0 && value <= Number.MAX_SAFE_INTEGER;\r\n    },\r\n    // 是否为除 symbol 外的原始类型\r\n    isStatic(value) {\r\n      return (\r\n        typeof value === 'string' ||\r\n        typeof value === 'number' ||\r\n        typeof value === 'boolean' ||\r\n        typeof value === 'undefined' ||\r\n        value === null\r\n      )\r\n    },\r\n    // 是否为类数组\r\n    isArrayLike(value) {\r\n      return value != null && this.isLength(value.length) && !this.isFunction(value);\r\n    },\r\n    // 获取数据类型\r\n    getRawType(value) {\r\n      return Object.prototype.toString.call(value).slice(8, -1)\r\n    },\r\n    // 判断数据是不是 Object 类型\r\n    isPlainObject(obj) {\r\n      return Object.prototype.toString.call(obj) === '[object Object]'\r\n    },\r\n    // 检查是不是原始数据\r\n    isPrimitive(value) {\r\n      return this.isStatic(value) || typeof value === 'symbol'\r\n    },\r\n    // 深度克隆\r\n    clone(value, deep) {\r\n      if (this.isPrimitive(value)) {\r\n        return value\r\n      }\r\n\r\n      if (this.isArrayLike(value)) { //是类数组\r\n        value = Array.prototype.slice.call(value)\r\n        return value.map(item => deep ? this.clone(item, deep) : item)\r\n      } else if (this.isPlainObject(value)) { //是对象\r\n        let target = {},\r\n          key;\r\n        for (key in value) {\r\n          value.hasOwnProperty(key) && (target[key] = deep ? this.clone(value[key], deep) : value[key])\r\n        }\r\n      }\r\n\r\n      let type = this.getRawType(value)\r\n\r\n      switch (type) {\r\n        case 'Date':\r\n        case 'RegExp':\r\n        case 'Error':\r\n          value = new window[type](value);\r\n          break;\r\n      }\r\n      return value\r\n    },\r\n    // 获取用户信息\r\n    getUserMsg() {\r\n      let that = this;\r\n      let paramObj = null;\r\n      paramObj = {\r\n        type: this.$store.state.currentUserType,\r\n        t_ids: this.$store.state.tids\r\n      };\r\n      this.$axios.post('queryAllCmcUsersInfo',paramObj)\r\n      .then(res => {\r\n        let user = res.CmcUserInfo,\r\n          msg = [],\r\n          srcMsg = [];\r\n        that.folders = res.folders;\r\n        for (let i = 0, l = user.length; i < l; i++) {\r\n          let item = user[i],\r\n            trees = [],\r\n            folder = '',\r\n            userTrees = item.user_trees;\r\n          for (let j = 0, len = userTrees.length; j < len; j++) {\r\n            if(userTrees[j].t_id==0){\r\n              userTrees[j].t_name=\"顶级组织\";\r\n            }\r\n\r\n            folder === '' ? folder += userTrees[j].t_name : folder += (',' + userTrees[j].t_name);\r\n            trees.push({\r\n              t_id: userTrees[j].t_id,\r\n              t_name: userTrees[j].t_name,\r\n              id: userTrees[j].id,\r\n              user_id: item.id,\r\n            })\r\n          }\r\n          if(user[i].username==\"admin\"){\r\n            continue;\r\n          }\r\n          msg.push({\r\n            id: item.id,\r\n            userName: item.username,\r\n            folder,\r\n            trees,\r\n            type: item.type - 1,\r\n            name: item.name === null ? '' : item.name,\r\n            password: '',\r\n            viewPwd: '******',\r\n            confirmPassword: '',\r\n            viewConfirmPwd: '******',\r\n            changeFlag: false,\r\n            operat: item.status === 0 ? false : true,\r\n          })\r\n          srcMsg.push({\r\n            id: item.id,\r\n            userName: item.username,\r\n            folder,\r\n            trees,\r\n            type: item.type - 1,\r\n            name: item.name,\r\n            password: '',\r\n            confirmPassword: '',\r\n            changeFlag: false,\r\n            operat: item.status === 0 ? false : true\r\n          })\r\n        }\r\n        that.userMsg = that.clone(msg, true);\r\n        that.srcUserMsg = that.clone(srcMsg, true);\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 新建用户\r\n    addUser() {\r\n      this.userMsg.unshift({\r\n        userName: '',\r\n        folder: '顶级组织',\r\n        type: '1',\r\n        name: '',\r\n        password: '',\r\n        confirmPassword: '',\r\n        id: null,\r\n        trees: [{\r\n          t_id: '0',\r\n          t_name: '顶级组织',\r\n          id: null,\r\n          user_id: null,\r\n        }],\r\n        viewPwd: '',\r\n        viewConfirmPwd: '',\r\n        operat: false\r\n      })\r\n    },\r\n    addUserToggle(){\r\n      this.isOpen = !this.isOpen;\r\n    },\r\n    addUsers(){\r\n      for(let i =0;i<5;i++){\r\n        this.userMsg.unshift({\r\n          userName: '',\r\n          folder: '顶级组织',\r\n          type: '1',\r\n          name: '',\r\n          password: '',\r\n          confirmPassword: '',\r\n          id: null,\r\n          trees: [{\r\n            t_id: '0',\r\n            t_name: '顶级组织',\r\n            id: null,\r\n            user_id: null,\r\n          }],\r\n          viewPwd: '',\r\n          viewConfirmPwd: '',\r\n          operat: false\r\n        })\r\n      }\r\n\r\n    },\r\n    // 保存设置\r\n    saveUser() {\r\n      let srcMsg = this.srcUserMsg,\r\n        that = this,\r\n        msg = this.userMsg,\r\n        l = srcMsg.length,\r\n        len = msg.length,\r\n        data = [];\r\n\r\n      for (var i = 0; i < len; i++) {\r\n        let item = msg[i];\r\n        let type = parseInt(item.type)+1;\r\n        if (item.id === null) {\r\n          data.push({\r\n            id: null,\r\n            name: item.name,\r\n            password: item.password,\r\n            confirmPassword: item.confirmPassword,\r\n            status: item.operat ? 1 : 0,\r\n            username: item.userName,\r\n            user_trees: item.trees,\r\n            type: type,\r\n          });\r\n        } else {\r\n          for (let j = 0; j < l; j++) {\r\n            let val = srcMsg[j];\r\n            if (item.id === val.id) {\r\n                if(item.changeFlag){\r\n                  if (item.userName === '') {\r\n                    swal({\r\n                      title: \"提示\",\r\n                      text: `用户名称不能为空`,\r\n                      button: \"确认\",\r\n                    });\r\n                    return;\r\n                  }\r\n                  else if (item.password === '') {\r\n                    swal({\r\n                      title: \"提示\",\r\n                      text: `用户${item.userName}密码不能为空`,\r\n                      button: \"确认\",\r\n                    });\r\n                    return;\r\n                  }\r\n                  else if (item.password.length>0 && item.password != item.confirmPassword) {\r\n                    swal({\r\n                      title: \"提示\",\r\n                      text: `用户${item.userName}密码不一致，请重新输入`,\r\n                      button: \"确认\",\r\n                    });\r\n                    return;\r\n                  }\r\n                  else if (item.confirmPassword === '') {\r\n                    swal({\r\n                      title: \"提示\",\r\n                      text: `用户${item.userName}确认密码不能为空`,\r\n                      button: \"确认\",\r\n                    });\r\n                    return;\r\n                  }\r\n                  else{\r\n                    data.push({\r\n                      id: val.id,\r\n                      name: item.name,\r\n                      password: item.password,\r\n                      confirmPassword: item.confirmPassword,\r\n                      status: item.operat ? 1 : 0,\r\n                      username: item.userName,\r\n                      user_trees: item.trees,\r\n                      type: type,\r\n                    })\r\n                  }\r\n\r\n                }\r\n                else if(!item.changeFlag && (item.userName !== val.userName || item.name !== val.name|| item.folder !== val.folder || item.type !== val.type || item.operat !== val.operat)){\r\n                  data.push({\r\n                    id: val.id,\r\n                    name: item.name,\r\n                    password: item.password,\r\n                    confirmPassword: item.confirmPassword,\r\n                    status: item.operat ? 1 : 0,\r\n                    username: item.userName,\r\n                    user_trees: item.trees,\r\n                    type: type,\r\n                  })\r\n                }\r\n            }\r\n          }\r\n        }\r\n      }\r\n      if(!this.isShow.isAlert) {\r\n        if (data.length > 0) {\r\n          var itemlen = data.length;\r\n          for (var k = 0; k < itemlen; k++) {\r\n            var item_new = data[k];\r\n            if (item_new.username === '') {\r\n              swal({\r\n                title: \"提示\",\r\n                text: '用户名称不能为空',\r\n                button: \"确认\",\r\n              });\r\n              return;\r\n            }\r\n            if (item_new.user_trees.length === 0) {\r\n              swal({\r\n                title: \"提示\",\r\n                text: '所属组织不能为空',\r\n                button: \"确认\",\r\n              });\r\n              return;\r\n            }\r\n            if (item_new.id === null && item_new.password === '') {\r\n              swal({\r\n                title: \"提示\",\r\n                text: `用户${item_new.username}密码不能为空`,\r\n                button: \"确认\",\r\n              });\r\n              return;\r\n            }\r\n            if (item_new.id === null && item_new.confirmPassword === '') {\r\n              swal({\r\n                title: \"提示\",\r\n                text: `用户${item_new.username}确认密码不能为空`,\r\n                button: \"确认\",\r\n              });\r\n              return;\r\n            }\r\n            if (item_new.password.length>0 && item_new.password != item_new.confirmPassword) {\r\n              swal({\r\n                title: \"提示\",\r\n                text: `用户${item_new.username}密码不一致，请重新输入`,\r\n                button: \"确认\",\r\n              });\r\n              return;\r\n            }\r\n            if (item_new.type === '') {\r\n              swal({\r\n                title: \"提示\",\r\n                text: `用户${item_new.username}请选择用户类型`,\r\n                button: \"确认\",\r\n              });\r\n              return;\r\n            }\r\n          }\r\n\r\n          this.$axios.post('insertAndUpdateCmcUserInfo',data)\r\n                  .then(res => {\r\n                    switch (res.msg) {\r\n                      case 0:\r\n                        // that.getUserMsg();\r\n\r\n                        swal({\r\n                          title: \"提示\",\r\n                          text: '保存成功',\r\n                          button: \"确认\",\r\n                        }) .then((value) => {\r\n                          this.$router.go(0)\r\n                        });\r\n                        break;\r\n                      case 1:\r\n                        console.log('参数为空');\r\n                        swal({\r\n                          title: \"提示\",\r\n                          text: '保存失败',\r\n                          button: \"确认\",\r\n                        });\r\n                        break;\r\n                      case 2:\r\n                        console.log('后台错误');\r\n                        swal({\r\n                          title: \"提示\",\r\n                          text: '保存失败',\r\n                          button: \"确认\",\r\n                        });\r\n                        break;\r\n                    }\r\n                  }).catch(err => {\r\n            console.log(err)\r\n          })\r\n        }\r\n        else{\r\n          swal({\r\n            title: \"提示\",\r\n            text: '保存成功',\r\n            button: \"确认\",\r\n          });\r\n        }\r\n      }\r\n    },\r\n    // 点击启用/禁用按钮\r\n    setEnabled(item) {\r\n      item.operat = !item.operat;\r\n    },\r\n    // 启用/禁用状态\r\n    setStyle(bool) {\r\n      if (bool) {\r\n        return {\r\n          color: '#666'\r\n        }\r\n      } else {\r\n        return {\r\n\r\n        }\r\n      }\r\n    },\r\n    // 关联组织\r\n    setRelatedOrganization(index, e, operat) {\r\n      let x = e.currentTarget.offsetWidth + e.currentTarget.offsetLef