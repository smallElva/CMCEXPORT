C;AACL;AACsC;;;AAGjG;AACmG;AACnG,gBAAgB,2GAAU;AAC1B,EAAE,kFAAM;AACR,EAAE,mGAAM;AACR,EAAE,4GAAe;AACjB;AACA;AACA;AACA;;AAEA;;AAEA;AACA,IAAI,IAAU;AACd,YAAY,mBAAO,CAAC,wGAAsE;AAC1F,cAAc,mBAAO,CAAC,+CAAK;AAC3B;AACA;AACA;AACA;AACA,KAAK;AACL;AACA;AACA,sBAAsB,2KAA+D,EAAE;AAAA;AACvF;AACA,gBAAgB,mGAAM;AACtB,yBAAyB,4GAAe;AACxC,OAAO;AACP,KAAK;AACL;AACA;AACA;AACe,gF;;;;;;;;;;;;ACvCf;AAAA;AAAA,wCAAwM,CAAgB,wPAAG,EAAC,C;;;;;;;;;;;;ACA5N;AAAA;AAAA;AAAA;AAAma,CAAgB,qaAAG,EAAC,C;;;;;;;;;;;;ACAvb;AAAA;AAAA;AAAA;AAAA;AAAA","file":"js/42-03bf0b71925859e8dcc1.js","sourcesContent":["<template>\r\n  <div>\r\n    <div id=\"myTable\" class=\"myTable\">\r\n        <div title=\"您有未能显示在列表里的失效测点\" class=\"btn5\"  v-show=\"invalidImg\"><img @click=\"invalidPosClick\" src=\"~assets/img/waring.svg\" ></div>\r\n        <button class=\"my-btn btn-active white-btn \" :class =\"{ btn1 :!svibs.isSvibs, btn4 :svibs.isSvibs , add:isAdd}\" v-if=\"!isShow\"  @click=\"add\" >新建测点</button>\r\n         <button class=\"my-btn btn-active btn1 \" v-if=\"svibs.isSvibs\"  @click=\"getTable(svibs.name,svibs.isMachine)\" >{{svibs.textBtn}}</button>\r\n        <button class=\"my-btn btn-active btn2\" @click=\"save\" :disabled=\"ifClickSave\">保存设置</button>\r\n        <div id=\"tableContent\">\r\n<!--          <keep-alive>-->\r\n            <router-view ref='child' @child-msg='updateIsShow' @invalidPosM ='invalidPosM' @click.native=\"chooseBtn($event);invalidPosClick($event)\"></router-view>\r\n<!--          </keep-alive>-->\r\n            <div class=\"noData\" v-if=\"isShow\">缺少可建该测点类型的采集器!</div>\r\n        </div>\r\n\r\n         <div class=\"controls-btn\">\r\n            <div class=\"push-controls\">\r\n                <a class=\"top-btn\" @click=\"chooseBtn\"> ↑ </a>\r\n                <a :class=\"leftClass\" @click=\"toLeft()\"> < </a>\r\n                <a :class=\"rightClass\" @click=\"toRight()\"> > </a>\r\n            </div>\r\n            <div class=\"btn-content\" @mousedown=\"mouseMove($event)\">\r\n                <router-link class=\"con-dgm-btn\" v-for=\"(btn,index) in btnData\" :key=\"btn.type\" :to=\"{path:btn.name}\" @click.native=\"getTable(btn.name,btn.isMachine)\" v-show=\"btn.none\">{{btn.textBtn}}</router-link>\r\n            </div>\r\n         </div>\r\n         <!-- 测点Tab显示 -->\r\n        <div class = \"choose-btn\"  :class=\"{chooseShow:chooseShow}\">\r\n            <div class =\"choose-content\">\r\n              <div class = \"choose-item\"   v-for=\"(btn,index) in btnData\" >\r\n                <label class = \"choose-name\" :for=\"btn.name\">{{btn.textBtn}}</label>\r\n                <div class = \"check-box\">\r\n                  <input type=\"checkbox\" checked :id=\"btn.name\" v-model=\"btn.none\">\r\n                  <label class = \"choose-box\" :for=\"btn.name\"></label>\r\n                </div>\r\n              </div>\r\n            </div>\r\n        </div>\r\n        <!-- 失效测点弹窗 -->\r\n        <div class=\"pos-waring\" v-show=\"invalidPosClickShow\">\r\n            <div class=\"table\">\r\n              <div class=\"table-caption\">\r\n                  <div :class=\"[invalidPosKey.length == Object.keys(invalidPos).length?'check-all':'uncheck']\" @click=\"checkAll\">全选</div>\r\n                  <div :class=\"[invalidPosKey.length > 0?'del':'undel']\" @click=\"delPos\">删除({{invalidPosKey.length}})</div>\r\n              </div>\r\n              <!--此行代码用于，控制列的样式。-->\r\n                <div class=\"content\">\r\n                  <div class=\"table-column-group\">\r\n                    <div class=\"table-column-1\"></div>\r\n                    <div class=\"table-column\"></div>\r\n                    <div class=\"table-column\"></div>\r\n                    <div class=\"table-column\"></div>\r\n                    <div class=\"table-column\"></div>\r\n                </div>\r\n                <!--如果不写上一个div, 也可以把样式写在以下div里。 -->\r\n                <div class=\"table-header-group\">\r\n                    <ul class=\"table-row\">\r\n                        <li class=\"table-cell\"></li>\r\n                        <li class=\"table-cell\">组织名称</li>\r\n                        <li class=\"table-cell\">机组名称</li>\r\n                        <li class=\"table-cell\">采集器名称</li>\r\n                        <li class=\"table-cell\">测点名称</li>\r\n                    </ul>\r\n                </div>\r\n                <div class=\"table-row-group\" v-for=\"(val,key) in invalidPos\">\r\n                    <ul class=\"table-row\">\r\n                        <li class=\"table-cell\">\r\n                          <input type=\"checkbox\" v-model=\"invalidPosKey\" :value=key>\r\n                        </li>\r\n                        <li class=\"table-cell\">{{val.t_name}}</li>\r\n                        <li class=\"table-cell\">{{val.mac_name}}</li>\r\n                        <li class=\"table-cell\">{{val.dgm_name}}</li>\r\n                        <li class=\"table-cell\">{{val.pos_name}}</li>\r\n                    </ul>\r\n                </div>\r\n              </div>\r\n          </div>\r\n        </div>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport getApi from 'network/getApi'\r\nimport {addRows} from './table/js/pos_methods'\r\nexport default {\r\n  name: \"PostionTable\",\r\n  inject: ['reload'], //组件的局部加载\r\n  data(){\r\n    return{\r\n       ins:0,//切换按钮active\r\n       name:'vib',\r\n       isAdd:false, //是否显示新建\r\n       isShow:false,//是否显示没有机组提示\r\n       chooseShow:true,//测点tab\r\n       invalidImg:false,\r\n       svibs:{\r\n          textBtn: \"转速提取\" ,\r\n          name:\"svibs\",\r\n          isMachine:true,\r\n          isSvibs:false,//是否显示转速提取\r\n       },\r\n       btnData: [//按钮初始化\r\n            {type: 3, textBtn: \"振动测点\",name:'vib',isMachine:true,none:true},\r\n            {type: 4, textBtn: \"包络测点\",name:'gd',isMachine:true,none:true},\r\n            {type: 15, textBtn: \"冲击测点\" ,name:\"shock\",isMachine:true,none:true},\r\n            {type: 8, textBtn: \"振动阶次测点\",name:\"vibOrder\",isMachine:true,none:true},\r\n            {type: 9, textBtn: \"包络阶次测点\" ,name:\"gdOrder\",isMachine:true,none:true},\r\n            {type: 16, textBtn: \"冲击阶次测点\" ,name:\"shockOrder\",isMachine:true,none:true},\r\n            {type: 1, textBtn: \"转速测点\" ,name:\"speed\",isMachine:true,none:true},\r\n            {type: 200, textBtn: \"长采样测点\" ,name:\"capture\",isMachine:true,none:true},\r\n            {type: 6, textBtn: \"晃度测点\" ,name:\"slosh\",isMachine:true,none:true},\r\n            {type: 11, textBtn: \"倾覆测点\" ,name:\"overturn\",isMachine:true,none:true},\r\n            {type: 12, textBtn: \"倾角测点\" ,name:\"angle\",isMachine:true,none:true},\r\n            {type: 13, textBtn: \"合成倾角测点\" ,name:\"synAngle\",isMachine:true,none:true},\r\n            {type: 7, textBtn: \"温度测点\" ,name:\"temp\",isMachine:true,none:true},\r\n            {type: 2, textBtn: \"过程量测点\" ,name:\"sta\",isMachine:true,none:true},\r\n            {type: 5, textBtn: \"数字量测点\" ,name:\"dig\",isMachine:true,none:true},\r\n            {type: 10, textBtn: \"工艺量测点\" ,name:\"mod\",isMachine:true,none:true},\r\n            {type: 14, textBtn: \"螺栓测点\" ,name:\"bolt\",isMachine:true,none:true},\r\n        ],\r\n\r\n        leftClass: {\r\n            \"left-btn\": true,\r\n            \"active\": false,\r\n            // \"disable\" : true,\r\n        },\r\n        rightClass: {\r\n            \"right-btn\": true,\r\n            \"active\": false,\r\n        },\r\n        isCheckAll:false,\r\n        isDel:true,\r\n        invalidPos:{},//失效测点\r\n        invalidPosKey:[],//失效测点key\r\n        invalidPosClickShow:false,//失效测点弹窗\r\n        ifClickSave:false,//保存按钮是否可点击\r\n    }\r\n  },\r\n  mounted(){\r\n       // 在点击标题的时候放大并重新渲染表格\r\n      this.$bus.$on('reloadtable',()=>{\r\n        if(this.$refs.child){\r\n          this.$refs.child.hot.render();\r\n        }\r\n      })\r\n      this.name = this.$route.name //刷新的时候让button 的active 正确\r\n      this.setBtnContentPosition()\r\n      if(this.name ==\"slosh\" ||this.name ==\"angle\" || this.name ==\"svibs\"){ //刷新的时候晃度和倾角测点不可以添加\r\n        this.isAdd = true\r\n      }\r\n      if(this.name == \"speed\" || this.name ==\"svibs\"){\r\n            this.svibs.isSvibs = true\r\n            if(this.name == \"svibs\"){\r\n              this.svibs.textBtn= \"转速测点\";\r\n              this.svibs.name = \"speed\"\r\n            }else if (this.name == \"speed\"){\r\n              this.svibs.textBtn= \"转速提取\";\r\n              this.svibs.name = \"svibs\"\r\n            }\r\n          }\r\n\r\n  },\r\n  methods:{\r\n    //失效测点框显示\r\n    invalidPosClick(e){\r\n     if(e.target.tagName == 'IMG'){\r\n        this.invalidPosClickShow = !this.invalidPosClickShow\r\n      }else{\r\n         this.invalidPosClickShow = false\r\n      }\r\n    },\r\n    /* 全选测点 */\r\n    checkAll(){\r\n      let keyArr = Object.keys(this.invalidPos)\r\n      if(this.invalidPosKey.length === keyArr.length){\r\n        this.invalidPosKey=[]\r\n      }else{\r\n         this.invalidPosKey =keyArr\r\n      }\r\n    },\r\n    /* 删除测点 */\r\n    delPos(){\r\n      if(this.invalidPosKey.length == 0) return;\r\n      let deleteDataArray =[]\r\n      this.invalidPosKey.forEach(key=>{\r\n        let obj = {}\r\n        let [mac_id,pos_id,pos_type] = key.split('_')\r\n        obj.mac_id = mac_id;\r\n        obj.position_id = pos_id;\r\n        obj.position_type = pos_type;\r\n        obj.position_type_id = pos_type + \"_\" + pos_id;\r\n        deleteDataArray.push(obj)\r\n      })\r\n      this.$getApi.deletePosition(deleteDataArray)\r\n          .then(res => {\r\n            if (res.msg == 0) {\r\n              /* 成功删除之后 */\r\n              //关闭弹窗\r\n              this.invalidPosClickShow =false\r\n              //删除invalidPos里面相关项\r\n              this.invalidPosKey.forEach(key=>{\r\n                this.$delete(this.invalidPos,key)\r\n              })\r\n              //清除invalidPosKey\r\n              this.invalidPosKey = []\r\n              swal({\r\n                title: \"提示\",\r\n                text: `删除测点成功`,\r\n                button: \"确认\"\r\n              })\r\n            } else if (res.msg == 1) {\r\n              console.log('参数为空！')\r\n            } else if (res.msg == 2) {\r\n              console.log('后台运行错误！')\r\n            }\r\n          })\r\n          .catch(err=>{\r\n              console.log('请求错误' ,err)\r\n          })\r\n    },\r\n      //接受子组件传来的失效测点信息\r\n    invalidPosM(invalidArr){\r\n      if(invalidArr.length == 0){\r\n        this.invalidImg = false //没有数据时隐藏按钮\r\n      }else{\r\n        this.invalidImg = true\r\n      }\r\n      invalidArr.forEach(item=>{\r\n        let key = item.mac_id+\"_\"+item.pos_id+\"_\"+item.pos_type\r\n         this.$set(this.invalidPos,key,item)\r\n       }\r\n      )\r\n    },\r\n    itemClick(index){\r\n      this.btnData[index].none = !this.btnData[index].none\r\n    },\r\n      chooseBtn(e){\r\n        if(e.target.className == \"top-btn\"){\r\n          this.chooseShow = ! this.chooseShow\r\n        }else{\r\n          this.chooseShow = true\r\n        }\r\n      },\r\n      /* 左右拖动 */\r\n      mouseMove(e){\r\n          let odiv = e.target;        //获取目标元素\r\n          //算出鼠标相对元素的位置\r\n          let disX = e.clientX - odiv.offsetLeft\r\n          let maxWidth = document.getElementsByClassName('controls-btn')[0].offsetWidth - 120;\r\n          document.onmousemove = (e)=>{       //鼠标按下并移动的事件\r\n              //用鼠标的位置减去鼠标相对元素的位置，得到元素的位置\r\n              let left = e.clientX - disX;\r\n              if(left < 120 && left > - maxWidth){\r\n                  //移动当前元素\r\n                  odiv.style.left = left + 'px';\r\n              }\r\n          };\r\n          document.onmouseup = (e) => {\r\n              document.onmousemove = null;\r\n              document.onmouseup = null;\r\n          };\r\n      },\r\n      toLeft: function () {//向左移动\r\n          let btnContent = $('.btn-content').position();\r\n          let leftVal = btnContent.left;\r\n          if(leftVal<100){\r\n              this.leftClass={\r\n                  \"left-btn\": true,\r\n                  \"active\": true,\r\n                  // \"disable\" : false,\r\n              };\r\n              this.rightClass= {\r\n                  \"right-btn\": true,\r\n                  \"active\": false,\r\n              };\r\n              $('.btn-content').css('left',leftVal+50)\r\n          }\r\n          else{\r\n              this.leftClass={\r\n                  \"left-btn\": true,\r\n                  \"active\": false,\r\n                  // \"disable\" : true,\r\n              };\r\n              this.rightClass= {\r\n                  \"right-btn\": true,\r\n                  \"active\": false,\r\n              };\r\n          }\r\n      },\r\n      toRight: function () {//向右移动\r\n          let btnContent = $('.btn-content').position();\r\n          let btnConWidth = $('.btn-content').width();\r\n          let leftVal = btnContent.left;\r\n          $('.btn-content').css('left',leftVal-50);\r\n          if(leftVal>=100){\r\n              this.leftClass={\r\n                  \"left-btn\": true,\r\n                  \"active\": false,\r\n                  // \"disable\" : true,\r\n              };\r\n              this.rightClass= {\r\n                  \"right-btn\": true,\r\n                  \"active\": true,\r\n              };\r\n          }\r\n          else if(Math.abs(leftVal)>btnConWidth-150){\r\n              swal({\r\n                title: \"提示\",\r\n                text: '没有更多测点类型',\r\n                button: \"确认\"\r\n              })\r\n              this.leftClass={\r\n                  \"left-btn\": true,\r\n                  \"active\": false,\r\n                  // \"disable\" : false,\r\n              };\r\n              this.rightClass= {\r\n                  \"right-btn\": true,\r\n                  \"active\": true,\r\n                  // \"disable\" : true,\r\n              };\r\n          }\r\n          else{\r\n              this.leftClass={\r\n                  \"left-btn\": true,\r\n                  \"active\": false,\r\n                  // \"disable\" : false,\r\n              };\r\n              this.rightClass= {\r\n                  \"right-btn\": true,\r\n                  \"active\": true,\r\n              };\r\n          }\r\n\r\n      },\r\n      getTable(name,isMachine){ //点击下标签的时候改变active，改变路由，刷新之后路由不变，active不变，点击左边测点设置 active会有问题\r\n          this.$router.push({name:name})\r\n          this.name = name\r\n          this.isAdd = false //晃度和倾角测点不可以添加\r\n          if(name ==\"slosh\" ||name ==\"angle\" || name ==\"svibs\"){\r\n            this.isAdd = true\r\n          }\r\n          if(name == \"speed\" || name ==\"svibs\"){\r\n            this.svibs.isSvibs = true\r\n            if(name == \"svibs\"){\r\n              this.svibs.textBtn= \"转速测点\",\r\n              this.svibs.name = \"speed\"\r\n            }else if (name == \"speed\"){\r\n              this.svibs.textBtn= \"转速提取\",\r\n              this.svibs.name = \"svibs\"\r\n            }\r\n          }else{\r\n             this.svibs.isSvibs = false\r\n          }\r\n          this.isShow  = !isMachine\r\n      },\r\n      setBtnContentPosition(){\r\n        let num\r\n        let btnContent = $('.btn-content').position();\r\n        let leftVal = btnContent.left;\r\n        this.btnData.forEach((item,index)=>{\r\n          if(item.name ==this.name ){\r\n            num = index\r\n          }\r\n        })\r\n        $('.btn-content').css('left',leftVal-50*num);\r\n      },\r\n      add(){\r\n        return addRows(this.$refs.child.hot,this.$refs.child.data,-1)\r\n      },\r\n      save(){\r\n          /* 保存按钮 */\r\n        let changeData = this.$refs.child.changeList.changeData || this.$refs.child.changeData\r\n        let addData = this.$refs.child.changeList.addData\r\n        let data = this.$refs.child.data\r\n        let type = this.$refs.child.type\r\n        let t = this.check_position_num(type,data)\r\n        if(t) return\r\n        let that = this;\r\n        //修改测点信息\r\n        if($.isEmptyObject(changeData) && $.isEmptyObject(addData)){\r\n          swal({\r\n                title: \"提示\",\r\n                text: '保存成功',\r\n                button: \"确认\"\r\n              })\r\n                .then((value) => {\r\n                  // this.$router.go(0)\r\n                  this.reload()\r\n                });\r\n        }\r\n        if(!$.isEmptyObject(changeData) && this.check_position(changeData,true)){\r\n            that.ifClickSave = true;\r\n          getApi.updatePosition(changeData)\r\n            .then(res=>{\r\n                that.ifClickSave = false;\r\n                if(res.msg==0){\r\n                  swal({\r\n                    title: \"提示\",\r\n                    text: '保存成功',\r\n                    button: \"确认\"\r\n                  })\r\n                  .then((value) => {\r\n                    // this.$router.go(0)\r\n                    this.$refs.child.changeList.changeData ={}\r\n                    this.reload()\r\n                  });\r\n\r\n                }\r\n                else if(res.msg==1){\r\n                    console.log('参数为空！');\r\n                }\r\n                else if(res.msg==2){\r\n                    console.log('后台运行错误！');\r\n                }\r\n                else if(res.msg==3){\r\n                    console.log('获取json数据时为空！');\r\n                }\r\n            })\r\n              .catch(err=>{\r\n                  that.ifClickSave = false;\r\n                  console.log('请求错误' ,err)\r\n              })\r\n        }\r\n        //新增测点信息\r\n        if(!$.isEmptyObject(addData) && this.check_position(addData)){\r\n            that.ifClickSave = true;\r\n            getApi.saveNewPosition(addData)\r\n              .then(res=>{\r\n                  that.ifClickSave = false;\r\n                  if(res.msg==0){\r\n                     swal({\r\n                        title: \"提示\",\r\n                        text: '保存成功',\r\n                        button: \"确认\"\r\n                      })\r\n                        .then((value) => {\r\n                          // this.$router.go(0)\r\n                          this.$refs.child.changeList.addData={}\r\n                          this.reload()\r\n                        });\r\n                  }\r\n                  else if(res.msg==1){\r\n                      console.log('参数为空！');\r\n                  }\r\n                  else if(res.msg==2){\r\n                      console.log('后台运行错误！');\r\n                  }\r\n                  else if(res.msg==3){\r\n                      console.log('获取json数据时为空！');\r\n                  }\r\n            })\r\n                .catch(err=>{\r\n                    that.ifClickSave = false;\r\n                    console.log('请求错误' ,err)\r\n                })\r\n        }\r\n\r\n      },\r\n      updateIsShow(type,isMachine){\r\n        /* 适用于转速提取 */\r\n        if(type == -1){\r\n          this.isShow = !isMachine //是否存在该设备\r\n          this.svibs.isMachine = isMachine\r\n        }\r\n        for(let i=0 ;i<this.btnData.length;i++){\r\n          if(this.btnData[i].type ==  type){\r\n            this.isShow = !isMachine //是否存在该设备\r\n            /* 存储这个值，keep-active,组件不刷新，下次点击的时候取该值 */\r\n            this.btnData[i].isMachine = isMachine\r\n          }\r\n        }\r\n      },\r\n      /* 判断测点个数限制 */\r\n      check_position_num(type,data){\r\n        if(data.length == 0) return;\r\n        let obj = {}\r\n        let flag = false\r\n        /* 一个机组一个转速 */\r\n        if(type == 1){\r\n          for(let i = 0;i<data.length;i++){\r\n            let key = data[i].mac_id\r\n            if(!obj[key]){\r\n              obj[key] = data[i]\r\n            }else{\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${ data[i].mac_me}超过一个转速`,\r\n                button: \"确认\"\r\n              })\r\n              flag = true\r\n              break\r\n            }\r\n          }\r\n        }\r\n        /* 定时长采样的序号不能重复 */\r\n        if(type == 200){\r\n          for(let i = 0;i<data.length;i++){\r\n            if(data[i].baseInfo.capture_mode != 0) continue;\r\n            let key = data[i].mac_id+\"_\"+data[i].baseInfo.capture_byId\r\n            if(!obj[key]){\r\n              obj[key] = data[i]\r\n            }else{\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${ data[i].mac_me}定时长采样序号重复`,\r\n                button: \"确认\"\r\n              })\r\n              flag = true\r\n              break\r\n            }\r\n          }\r\n        }\r\n        return flag\r\n      },\r\n      /* 测点各个列条件的判断 */\r\n      check_position(obj,isChange){ //isChange针对rock，的增加或者是修改\r\n        let type  = this.$refs.child.type\r\n        for (let i in obj){\r\n          let position = obj[i]\r\n          if(position.pos_name === \"\"){\r\n            swal({\r\n                title: \"提示\",\r\n                text: `${position.dgm_name}下存在测点没有测点名称`,\r\n                button: \"确认\"\r\n              })\r\n            return false\r\n          }\r\n          if(position.ch_id ===\"\"){\r\n            swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}没有关联通道`,\r\n                button: \"确认\"\r\n              })\r\n            return false\r\n          }\r\n\r\n          /* 任务判断 */\r\n          if(type == 3 || type == 4 || type == 8 || type == 9 || type == 11  || type == 15  || type == 16){\r\n            if(position.task_id ===\"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}没有任务`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n          }else{\r\n            /* 所有测点task_id 默认值为1 */\r\n            position.task_id = 0\r\n          }\r\n          /* 倾覆测点 */\r\n          if(type == 11){\r\n            if(position.baseInfo.tt_sensor_name === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}未设置传感器名称`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n            if(position.baseInfo.rock1_ch_id === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}晃度X没有关联通道`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n            if(position.baseInfo.rock2_ch_id === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}晃度Y没有关联通道`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n             if(position.baseInfo.rock1_pos_name === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}晃度X没有名称`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n            if(position.baseInfo.rock2_pos_name === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}晃度Y没有名称`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n          }\r\n          /* 转速提取的判断 */\r\n\r\n          /* 合成倾角 */\r\n          if(type == 13){\r\n            if(position.baseInfo.rock1_ch_id === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}倾角X没有关联通道`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n            if(position.baseInfo.rock2_ch_id === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}倾角Y没有关联通道`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n             if(position.baseInfo.rock1_pos_name === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}倾角X没有名称`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n            if(position.baseInfo.rock2_pos_name === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}倾角Y没有名称`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n            if(position.baseInfo.dip_model === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}未设置传感器类型`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n            if(position.baseInfo.tt_sensor_name === \"\"){\r\n              swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}未设置传感器名称`,\r\n                button: \"确认\"\r\n              })\r\n              return false\r\n            }\r\n          }\r\n          /* 长采样 */\r\n          if(type == 200){\r\n            if(position.baseInfo.capture_mode == 0){ //定时采样\r\n              //判断序号是否有值\r\n              if(position.baseInfo.capture_byId == \"\"){\r\n                swal({\r\n                title: \"提示\",\r\n                text: `${position.pos_name}没有序号`,\r\n                button: \"确认\"\r\n                })\r\n                return false\r\n              }\r\n            }\r\n          }\r\n          /* 转速提取 */\r\n          if(type == -1){\r\n            //测点不重复判断\r\n            let arr = []  //'type_id'\r\n            let svibsList = position.baseInfo.svibsList\r\n            for(let i = 0; i<svibsList.length; i++){\r\n              let item = svibsList[i]\r\n              let task = i%3 +1\r\n              if(item.position_id != -1){ //选择过测点的选项\r\n                  let key = `${item.position_type}_${item.position_id}`\r\n                  if(!arr.includes(key)){\r\n                    arr.push(key)\r\n                  }else{\r\n                    let pos_type = item.position_type == 3 ? '振动':\"包络\"\r\n                    swal({\r\n                    title: \"提示\",\r\n                    text: `'${item.position_name}'测点选择重复`,\r\n                    button: \"确认\"\r\n                    })\r\n                    return false\r\n                  }\r\n              }\r\n            }\r\n          }\r\n\r\n\r\n        }\r\n        /* 如果是倾覆或者是合成倾角测点，需要把里面的倾角取出来 */\r\n        if(type == 11 || type ==13){\r\n          let obj1 ={},rock1 ={},rock2 ={}\r\n          let position\r\n          //循环，删除列表中存在的非倾覆或者合成倾角测点，防止多次点击后台运行错误\r\n          for (let j in obj ){\r\n            position = obj[j]\r\n            if(position.pos_type != 11 && position.pos_type != 13){\r\n              delete obj[j]\r\n            }\r\n          }\r\n          let length = Object.keys(obj).length\r\n          for (let i in obj){\r\n              position = obj[i]\r\n              let rock1  = {\r\n                  updateTime : position.updateTime,\r\n                  mac_id : position.mac_id,//机组uid\r\n                  m_type : position.m_type,//机组uid\r\n                  dgm_type : position.dgm_type,//采集站类型：处理测点设置的差异化、测点数目等\r\n                  dgm_id : position.dgm_id,\r\n                  dgm_name : position.dgm_name,\r\n                  ch_id : position.baseInfo.rock1_ch_id,\r\n                  ch_name:position.baseInfo.rock1_ch_name,\r\n                  ch_type : position.baseInfo.rock1_ch_type,\r\n                  pos_id : position.baseInfo.rock1_id,\r\n                  pos_type : position.pos_type == 11 ? 6 : 12,/* 如果合成倾角就是倾角如果是倾覆就是晃度 */\r\n                  pos_name : position.baseInfo.rock1_pos_name,\r\n                  task_id : position.task_id, //任务\r\n                  pos_activity : position.pos_activity, //是否有效\r\n                  isalarm : position.isalarm, //报警\r\n                  baseInfo :{\r\n                      tt_sensor_name:position.baseInfo.tt_sensor_name,\r\n                      tt_pos_loc:position.baseInfo.tt_pos_loc,\r\n                      tt_ch_id: position.baseInfo.tt_ch_id,\r\n                      rock2_id:position.baseInfo.slaveid1, // 从设备号需要在倾角中单独传，rock2_id复用了这个字段\r\n                      alarmList: [],\r\n                      resampling: '0',//重新采样\r\n                      posShow: \"0\",//是否显示在总貌图\r\n                      hanning: \"1\",//是否汉宁窗\r\n                      frequency: position.baseInfo.frequency,\r\n                      spectral: position.baseInfo.spectral,\r\n                      band: position.baseInfo.band\r\n                  }\r\n                }\r\n              let rock2  = {\r\n                    updateTime : position.updateTime,\r\n                    mac_id : position.mac_id,//机组uid\r\n                    m_type : position.m_type,//机组uid\r\n                    dgm_type : position.dgm_type,//采集站类型：处理测点设置的差异化、测点数目等\r\n                    dgm_id : position.dgm_id,\r\n                    dgm_name : position.dgm_name,\r\n                    ch_id : position.baseInfo.rock2_ch_id,\r\n                    ch_name:position.baseInfo.rock2_ch_name,\r\n                    ch_type : position.baseInfo.rock2_ch_type,\r\n                    pos_id : position.baseInfo.rock2_id,\r\n                    pos_type : position.pos_type == 11 ? 6 : 12,/* 如果合成倾角就是倾角如果是倾覆就是晃度 */\r\n                    pos_name : position.baseInfo.rock2_pos_name,\r\n                    task_id : position.task_id, //任务\r\n                    pos_activity : position.pos_activity, //是否有效\r\n                    isalarm : position.isalarm, //报警\r\n                    baseInfo :{\r\n                        tt_sensor_name:position.baseInfo.tt_sensor_name,\r\n                        tt_pos_loc:position.baseInfo.tt_pos_loc,\r\n                        tt_ch_id: position.baseInfo.tt_ch_id,\r\n                        rock2_id:position.baseInfo.slaveid2,\r\n                        alarmList: [],\r\n                        resampling: '0',//重新采样\r\n                        posShow: \"0\",//是否显示在总貌图\r\n                        hanning: \"1\",//是否汉宁窗\r\n                        frequency: position.baseInfo.frequency,\r\n                        spectral: position.baseInfo.spectral,\r\n                        band: position.baseInfo.band\r\n                    }\r\n                }\r\n              if (isChange) { //修改测点（里面有些特征值是不需要改变的）\r\n                 let rock_list = this.$refs.child.initList.rock_list\r\n\r\n                 let original_rock1 = rock_list[`${rock1.mac_id}_${rock1.pos_type}_${rock1.pos_id}`] //原来的rock1 和 rock2\r\n                 let original_rock2 = rock_list[`${rock2.mac_id}_${rock2.pos_type}_${rock2.pos_id}`]\r\n\r\n                  rock1.isalarm =  original_rock1.isalarm //报警\r\n                  rock2.isalarm =  original_rock2.isalarm //报警\r\n\r\n                  rock1.baseInfo.band = original_rock1.baseInfo.band //频段\r\n                  rock2.baseInfo.band = original_rock2.baseInfo.band\r\n\r\n                  rock1.baseInfo.alarmList = original_rock1.baseInfo.alarmList //报警门限\r\n                  rock2.baseInfo.alarmList = original_rock2.baseInfo.alarmList\r\n\r\n                  obj1[rock1.mac_id + \"_\" + rock1.pos_type + \"_\" + rock1.pos_id] = rock1\r\n                  obj1[rock2.mac_id + \"_\" + rock2.pos_type + \"_\" + rock2.pos_id] = rock2\r\n              }\r\n              /* 新加的测点 */\r\n              else{\r\n                obj1[length++] =rock1\r\n                obj1[length++] =rock2\r\n              }\r\n\r\n          }\r\n          Object.assign(obj,obj1)\r\n        }\r\n        return true\r\n      },\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.add{\r\n  display:none\r\n}\r\n.none{\r\n  display:none\r\n}\r\n.chooseShow{\r\n  display: none;\r\n}\r\n\r\n\r\n .pos-waring{\r\n   /* 隐藏滚动条 */\r\n\r\n    position: absolute;\r\n    left: 100px;\r\n 