v class=\"search\">\r\n                  <input class=\"over-search-input\" type=\"text\" placeholder=\"搜索机泵\" ref=\"searchMacInput\" @input=\"searchMac\">\r\n                  <i class=\"iconfont icon-zhongzi-zhankai down-icon\" @click.stop=\"openList(2)\"></i>\r\n              </div>\r\n              <div class=\"search-list-div search-list-div2\" v-show=\"searchPumpList.length>0 && isShow.macIsShow\">\r\n                  <ul class=\"search-result\">\r\n                      <li class=\"search-li\" v-for=\"(item,index) in sortPumpList\"  @click=\"selectMac(item,index)\"><i class=\"iconfont icon-zhongzi_duigou gou-icon\"></i>{{item.pump_name}}</li>\r\n                  </ul>\r\n              </div>\r\n              <div class=\"search-pump-div\" v-show=\"choosePosList.length>0 && !isShow.macIsShow\">\r\n                  <ul class=\"pump-result\">\r\n                      <li class=\"pump-li\" v-for=\"(pos,index) in choosePosList\" @click=\"clickPump(pos,index)\" draggable=\"true\" @dragstart=\"dragstart(pos, index, $event)\">\r\n                          <i class=\"iconfont icon-zhongzi-shebeimoxing\"></i>\r\n                          {{pos.pos_name}}\r\n                      </li>\r\n                  </ul>\r\n              </div>\r\n              <div id=\"imgForm\" class=\"upload\" v-show=\"chooseMac.pump_id!=''\">\r\n                  <button class=\"btn\" @click=\"monidianji\">上传图片</button>\r\n                  <input class=\"input-loc-img\"  name=\"imgLocal\" id=\"imgLocal\" type='file' accept=\"image/*\" @change=\"selectImg\"/>\r\n              </div>\r\n              <div class=\"upload2\" v-show=\"overviewUrl!=''&& choosePosList.length>0\">\r\n                  <button class=\"btn\" @click=\"keepOverview\">保存</button>\r\n              </div>\r\n          </div>\r\n          <div class=\"overview-container\">\r\n              <div class=\"overview-pic\" id=\"overview-pic\">\r\n                  <div id=\"move-block\" class=\"move-block\" @drop.stop.prevent='drop' @dragover.prevent='dragover' @click.stop='cancelClick' @contextmenu.prevent=''>\r\n                      <div class=\"move-div\" v-for=\"(item,index) in MoveMsg\" :style=\"item.style\" draggable=\"true\" @dragstart.stop=\"dragScreen(index, $event)\" @contextmenu.prevent='rightClick(item, index, $event)'>\r\n                          <img :src=\"item.url\" :width=\"item.width\" :height=\"item.height\" />\r\n                          <p>{{item.text}}</p>\r\n                      </div>\r\n                      <ul class='rightBtn' v-show='isRightClick' ref='rightMsg'>\r\n                          <li @click='delModelPos'>删除</li>\r\n                      </ul>\r\n                      <img :src=\"overviewUrl\" class=\"overview-bg\">\r\n                  </div>\r\n              </div>\r\n              <div class='alertBox alertTip' v-show='isTip'>\r\n                    <div class='alertTitle'>提示  <i class=\"iconfont icon-zhongzi-guanbi icon-close\" @click='closeAlarm'></i></div>\r\n                    <div class='alertText'>\r\n                      请确认是否删除该部件？\r\n                    </div>\r\n                    <div class='yesOrNo'>\r\n                        <button class='yes' @click='confirmTip'>确定</button>\r\n                        <button class='no' @click='closeAlarm'>取消</button>\r\n                    </div>\r\n              </div>\r\n\r\n          </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {compareName, cloneObj} from \"@/utils/utils\";\r\nimport MyContentTitle from 'components/common/MyContentTitle'\r\nimport { ConvertPinyin } from 'assets/lib/pinyin.js'\r\nexport default {\r\n  name: \"overviewPump\",\r\n  props:['parentToChild'],\r\n  data(){\r\n      return{\r\n          title:\"机泵总貌图设置\",\r\n          activeNum: \"21\",\r\n          folderList:[],//组织列表\r\n          searchFolderList:[],//搜索匹配到的组织列表\r\n          chooseFolder:{\r\n              t_id:\"\",\r\n              t_name:\"\",\r\n              overview:null\r\n          },\r\n          chooseMac:{\r\n              ch_class: \"\",\r\n              mac_id: \"\",\r\n              pump_id: \"\",\r\n              pump_name: \"\"\r\n          },\r\n          choosePump:{\r\n              pump_name:\"\",\r\n              pump_id:\"\",\r\n              pump_type:\"\"\r\n          },\r\n          isShow:{\r\n              folderIsShow:false,//组织下拉框显示隐藏\r\n              macIsShow:false//机组下拉框显示隐藏\r\n          },//搜索下拉是否显示\r\n          choosePumpList:[],//选择的机组列表\r\n          searchPumpList:[],//搜索到的机组列表\r\n          choosePosList:[],//选择的机组的测点列表\r\n          overviewUrl:\"\",//总貌图背景路径\r\n          dragMsg: null, // 拖动的机泵的信息\r\n          MoveMsg:[],//移动的机泵的信息\r\n          isRightClick: false, // 是否显示右键信息\r\n          rightClickMsg: { // 右键选中机泵组件\r\n              index: -1, // 右键选中组件下标\r\n              data: null, // 右键选中组件信息\r\n          },\r\n          isTip:false,//是否显示提示框\r\n          origBlock:{//总貌图背景区域\r\n              origW:\"\",\r\n              origH:\"\",\r\n          }\r\n      }\r\n    },\r\n  created: function() {\r\n      this.$emit('ievent',this.activeNum);\r\n  },\r\n  beforeRouteLeave (to, from, next) {\r\n      if(to.name == 'overview'){\r\n          let linkObj = {\r\n              chooseFolder:this.chooseFolder,\r\n              choosePump: this.chooseMac\r\n          };\r\n          sessionStorage.setItem('LinkConfig',JSON.stringify(linkObj));\r\n      }\r\n      else{\r\n          if(sessionStorage.getItem('LinkConfig')){\r\n              sessionStorage.removeItem('LinkConfig');\r\n          }\r\n      }\r\n      next();\r\n  },\r\n  mounted: function() {\r\n      if(this.$store.state.tids.length>0){\r\n          this.getTidData();//获取所有的机组设置数据\r\n      };\r\n\r\n      let p_w=$('#overview-pic').width();\r\n      let p_h=$('#overview-pic').height();\r\n\r\n      if(p_w/p_h>16/9){\r\n          this.origBlock.oriW=parseInt(16*p_h/9);\r\n          this.origBlock.oriH=p_h;\r\n          $('#move-block').css({'width':this.origBlock.oriW + 'px','height':this.origBlock.oriH + 'px'});\r\n      }\r\n      else{\r\n          this.origBlock.oriW=p_w;\r\n          this.origBlock.oriH=parseInt(9*p_w/16);\r\n          $('#move-block').css({'width':this.origBlock.oriW + 'px','height':this.origBlock.oriH + 'px'});\r\n      }\r\n\r\n\r\n      let linkInfo = {};\r\n      if(!$.isEmptyObject(JSON.parse(sessionStorage.getItem('LinkConfig')))){//带入带出选项\r\n          linkInfo = JSON.parse(sessionStorage.getItem('LinkConfig'));\r\n      }\r\n\r\n      if(!$.isEmptyObject(this.$route.params)){//双击图中的机泵跳转\r\n          linkInfo = this.$route.params;\r\n          this.activeNum = \"21\";\r\n          this.$emit('ievent',this.activeNum);\r\n      }\r\n\r\n      if(!$.isEmptyObject(linkInfo)){\r\n          this.chooseFolder=linkInfo.chooseFolder;\r\n          this.chooseMac=linkInfo.choosePump;\r\n          this.selectFolder(this.chooseFolder);\r\n      }\r\n      else{\r\n          this.chooseFolder={\r\n              t_id:\"\",\r\n              t_name:\"\",\r\n          }\r\n      }\r\n\r\n  },\r\n  computed: {\r\n      sortFolderList: function () {\r\n          return this.searchFolderList.sort(compareName('t_name'));\r\n      },\r\n      sortPumpList: function () {\r\n          return this.searchPumpList.sort(compareName('pump_name'));\r\n      },\r\n  },\r\n  methods: {\r\n      closeSearch(){\r\n          this.isShow.folderIsShow=false;//组织下拉框显示隐藏\r\n          this.isShow.macIsShow=false;//机组下拉框显示隐藏= false;\r\n      },\r\n      //获取组织信息\r\n      getTidData: function(){\r\n          var that=this;\r\n          var token = sessionStorage.getItem('token');\r\n          let paramObj = null;\r\n              paramObj = {\r\n                  t_ids: this.$store.state.tids\r\n              };\r\n          this.$axios.post('queryAllT_OverviewInfo',paramObj)\r\n          .then(res=>{\r\n              that.folderList=[];\r\n              for(let i=0;i<res.length;i++){\r\n                  if(res[i].t_root==1){\r\n                      that.folderList.push(res[i]);\r\n                  }\r\n              }\r\n              that.searchFolderList = that.folderList.concat();\r\n          })\r\n              .catch(err=>{\r\n                  console.log('请求错误',err)\r\n              })\r\n      },\r\n      openList(num){\r\n          if(num==1){\r\n              this.isShow.folderIsShow=!this.isShow.folderIsShow;\r\n              this.isShow.macIsShow=false;\r\n          }\r\n          else if(num==2){\r\n              this.isShow.macIsShow=!this.isShow.macIsShow;\r\n              this.isShow.folderIsShow=false;\r\n          }\r\n\r\n      },\r\n      // 右键点击部件\r\n      rightClick(item, index, e) {\r\n          // console.log(item)\r\n          this.isRightClick = true;\r\n          this.rightClickMsg = {\r\n              index,\r\n              data: item,\r\n          };\r\n          this.$refs.rightMsg.style.left = item.x + e.layerX + 'px';\r\n          this.$refs.rightMsg.style.top = item.y + e.layerY + 'px';\r\n      },\r\n      // 删除组件\r\n      delModelPos() {\r\n          this.isTip = true;\r\n      },\r\n      // 确定删除组件\r\n      confirmTip() {\r\n          let rcm = this.rightClickMsg,\r\n              model = this.MoveMsg;\r\n          if (rcm.index >= 0 && rcm.data) {\r\n              model.splice(rcm.index, 1);\r\n              this.choosePosList.forEach(pos =>{\r\n                  let posId = pos.pos_type + '_' + pos.pos_id;\r\n                  if(posId == rcm.data.uid){\r\n                      pos.dragFlag = true;//删除了该测点，该机泵需要变更为可拖动状态\r\n                  }\r\n              })\r\n\r\n          }\r\n          this.cancelClick();\r\n          this.isTip = false;\r\n      },\r\n      // 取消点击测点组态效果\r\n      cancelClick() {\r\n          this.isShow.folderIsShow=false;//组织下拉框显示隐藏\r\n          this.isShow.macIsShow=false;//机组下拉框显示隐藏= false;\r\n          if(this.isRightClick)\r\n              this.isRightClick = false;\r\n      },\r\n      // 测点组态保存警告框关闭\r\n      closeAlarm() {\r\n          this.isAlarm = false;\r\n          this.isTip = false;\r\n      },\r\n      //搜索下拉列表\r\n      searchFolder: function(e){\r\n          let search_text = e.target.value;\r\n          if(search_text.length>0){\r\n              let test_atr = new RegExp(search_text);\r\n              this.searchFolderList=[];\r\n              this.isShow.folderIsShow=true;\r\n              this.isShow.macIsShow=false;\r\n              for(let i=0;i<this.folderList.length;i++){\r\n                  if(test_atr.test(this.folderList[i].t_name) || ConvertPinyin(this.folderList[i].t_name).indexOf(search_text)>=0){\r\n                      this.searchFolderList.push(this.folderList[i]);\r\n                  }\r\n              }\r\n              if(this.searchFolderList.length==0){\r\n                  this.searchPumpList=[];\r\n                  this.choosePosList=[];\r\n                  this.$refs.searchMacInput.value ='';\r\n              }\r\n          }\r\n          else{\r\n              this.searchFolderList=this.folderList.concat();\r\n              this.isShow.folderIsShow=false;\r\n              this.chooseFolder={\r\n                  t_id:\"\",\r\n                  t_name:\"\",\r\n                  overview:null\r\n              };\r\n              this.chooseMac={\r\n                  ch_class: \"\",\r\n                  mac_id: \"\",\r\n                  pump_id: \"\",\r\n                  pump_name: \"\"\r\n              };\r\n              this.MoveMsg=[];\r\n              this.overviewUrl='';\r\n              this.choosePumpList=[];\r\n              this.searchPumpList=[];\r\n              this.choosePosList=[];\r\n              this.$refs.searchMacInput.value ='';\r\n          }\r\n      },\r\n      //搜索机组下拉列表\r\n      searchMac(e){\r\n          let search_text = e.target.value;\r\n          if(search_text.length>0){\r\n              let test_atr = new RegExp(search_text);\r\n              this.searchPumpList=[];\r\n              this.isShow.macIsShow=true;\r\n              this.isShow.folderIsShow=false;\r\n              for(let i=0;i<this.choosePumpList.length;i++){\r\n                  if(test_atr.test(this.choosePumpList[i].pump_name) || ConvertPinyin(this.choosePumpList[i].pump_name).indexOf(search_text)>=0){\r\n                      this.searchPumpList.push(this.choosePumpList[i]);\r\n                  }\r\n              }\r\n          }\r\n          else{\r\n              this.chooseMac={\r\n                  ch_class: \"\",\r\n                  mac_id: \"\",\r\n                  pump_id: \"\",\r\n                  pump_name: \"\"\r\n              };\r\n              this.MoveMsg=[];\r\n              this.overviewUrl='';\r\n              this.choosePosList=[];\r\n              this.$refs.searchMacInput.value ='';\r\n          }\r\n      },\r\n      //点击组织列表\r\n      clickFolder:function(chooseInfo,index){\r\n          $('.search-li').eq(index).addClass('active').siblings().removeClass('active');\r\n          this.chooseFolder=chooseInfo;\r\n          this.$refs.searchInput.value =this.chooseFolder.t_name;\r\n      },\r\n      //点击机组列表\r\n      clickMac(chooseInfo,index){\r\n          $('.search-list-div2 .search-li').eq(index).addClass('active').siblings().removeClass('active');\r\n          this.chooseMac=chooseInfo;\r\n          this.$refs.searchMacInput.value =this.chooseMac.pump_name;\r\n\r\n      },\r\n      //选择机泵\r\n      clickPump: function(pump,index){\r\n          $('.pump-li').eq(index).addClass('active').siblings().removeClass('active');\r\n          this.choosePump=pump;\r\n      },\r\n      //保存总貌图\r\n      keepOverview: function(){\r\n          var that=this;\r\n          var token = sessionStorage.getItem('token');\r\n          // console.log(this.MoveMsg);\r\n          // console.log(this.chooseFolder);\r\n          let overViewObj = {\r\n              o_id:this.chooseMac.pump_id,//总貌图uid（=组织/机组/机泵uid）\r\n              mac_id:this.chooseMac.mac_id,\r\n              itemList:[],//节点列表\r\n          };//总貌图\r\n          let obj={};\r\n          for(let i=0;i<this.MoveMsg.length;i++){\r\n              obj={\r\n                  id:i,//计数id\r\n                  type:3,//类型 1=组织 2=机组 3=机泵 4=测点\r\n                  itemType:this.MoveMsg[i].itemType,//设备类型 如1=破碎机\r\n                  uid:this.MoveMsg[i].uid,//节点uid，如\"3_05\"，表示id=05的振动测点\r\n                  px:(this.MoveMsg[i].x/this.origBlock.oriW).toFixed(2),//相对于背景图的x位置\r\n                  py:(this.MoveMsg[i].y/this.origBlock.oriH).toFixed(2),//相对于背景图的y位置\r\n                  pwidth:this.MoveMsg[i].width,//相对于背景图的宽度\r\n                  pheight:this.MoveMsg[i].height//相对于背景图的高度\r\n              };\r\n              overViewObj.itemList.push(obj);\r\n          }\r\n          // console.log(overViewObj);\r\n      this.$axios.post('updateTreeOverview',overViewObj)\r\n      .then(res=>{\r\n          if(res.msg==0){\r\n                      swal({\r\n                          title: \"提示\",\r\n                          text: '保存成功',\r\n                          button: \"确认\",\r\n                      });\r\n                  }\r\n                  else if(res.msg==1){\r\n                      console.log('参数为空')\r\n                      swal({\r\n                          title: \"提示\",\r\n                          text: '保存失败',\r\n                          button: \"确认\",\r\n                      });\r\n                  }\r\n                  else if(res.msg==2){\r\n                      console.log('后台运行错误')\r\n                      swal({\r\n                          title: \"提示\",\r\n                          text: '保存失败',\r\n                          button: \"确认\",\r\n                      });\r\n                  }\r\n                  else if(res.msg==3){\r\n                      console.log('获取json数据时为空')\r\n                      swal({\r\n                          title: \"提示\",\r\n                          text: '保存失败',\r\n                          button: \"确认\",\r\n                      });\r\n                  }\r\n      })\r\n          .catch(err=>{\r\n              console.log('请求错误',err)\r\n          })\r\n      },\r\n      //取消编辑\r\n      editNo: function(){\r\n          this.dragMsg=null;// 拖动的机泵的信息\r\n          this.MoveMsg=[];//移动的机泵的信息\r\n      },\r\n      //机泵类型分类\r\n      getPumpImg:function(name) {\r\n          var machineImg = {};\r\n          const machine = {\r\n              '风机': {\r\n                  machine: 'fengji',\r\n                  itemType: 1,\r\n                  width: 8,\r\n                  height: 8\r\n              },\r\n              '提升机': {\r\n                  machine: 'qishengji',\r\n                  itemType: 2,\r\n                  width: 2.9,\r\n                  height: 8\r\n              },\r\n              '破碎机': {\r\n                  machine: 'posuiji',\r\n                  itemType: 3,\r\n                  width: 6.7,\r\n                  height: 8\r\n              },\r\n              '水磨': {\r\n                  machine: 'shuinimo',\r\n                  itemType: 4,\r\n                  width: 8,\r\n                  height: 3.2\r\n              },\r\n              '磨机': {\r\n                  machine: 'moji',\r\n                  itemType: 5,\r\n                  width: 6.4,\r\n                  height: 8\r\n              },\r\n              '煤磨': {\r\n                  machine: 'moji',\r\n                  itemType: 6,\r\n                  width: 6.4,\r\n                  height: 8\r\n              },\r\n              '立磨': {\r\n                  machine: 'moji',\r\n                  itemType: 7,\r\n                  width: 6.4,\r\n                  height: 8\r\n              },\r\n              '水泥磨': {\r\n                  machine: 'shuinimo',\r\n                  itemType: 8,\r\n                  width: 8,\r\n                  height: 3.2\r\n              },\r\n              '辊压机': {\r\n                  machine: 'hunyaji',\r\n                  itemType: 9,\r\n                  width: 8,\r\n                  height: 4\r\n              },\r\n              '窑': {\r\n                  machine: 'huizhuanyao',\r\n                  itemType: 10,\r\n                  width: 8,\r\n                  height: 2\r\n              },\r\n              '余热发电': {\r\n                  machine: 'yurefadianji',\r\n                  itemType: 11,\r\n                  width: 8,\r\n                  height: 6.2\r\n              },\r\n              '链': {\r\n                  machine: 'shuliaolalianji',\r\n                  itemType: 12,\r\n                  width: 8,\r\n                  height: 6.5\r\n              },\r\n              '减速': {\r\n                  machine: 'jiansuji',\r\n                  itemType: 13,\r\n                  width: 7.8,\r\n                  height: 8\r\n              },\r\n              '汽轮机': {\r\n                  machine: 'yurefadianji',\r\n                  itemType: 14,\r\n                  width: 8,\r\n                  height: 6.2\r\n              }\r\n          };\r\n          for (var i in machine) {\r\n              if (name.indexOf(i) !== -1) {\r\n                  machineImg = machine[i];\r\n                  break;\r\n              }\r\n              else{\r\n                  machineImg = {\r\n                      machine: 'yurefadianji',\r\n                      itemType: 14,\r\n                      width: 30,\r\n                      height: 30\r\n                  }\r\n              }\r\n          }\r\n          // }\r\n          return machineImg;\r\n      },\r\n      //确定选择组织\r\n      selectFolder: function(chooseInfo,index){\r\n          $('.search-li').eq(index).addClass('active').siblings().removeClass('active');\r\n          this.chooseFolder=chooseInfo;\r\n          this.$refs.searchInput.value =this.chooseFolder.t_name;\r\n          this.choosePumpList=[];\r\n          this.isShow.folderIsShow=false;\r\n          var that=this;\r\n          var token = sessionStorage.getItem('token');\r\n          let tidObj={\r\n  