UAAD,CAA1B,CAAZ,CAnF4B,CAmFwB;;AACpDpB,iBAAW,GAAGA,WAAW,CAACmB,IAAZ,CAAiBC,+DAAW,CAAC,UAAD,CAA5B,CAAd;AACAnB,kBAAY,GAAGA,YAAY,CAACkB,IAAb,CAAkBC,+DAAW,CAAC,UAAD,CAA7B,CAAf;AACAlB,sBAAgB,GAAGA,gBAAgB,CAACiB,IAAjB,CAAsBC,+DAAW,CAAC,UAAD,CAAjC,CAAnB;AACAjB,uBAAiB,GAAGA,iBAAiB,CAACgB,IAAlB,CAAuBC,+DAAW,CAAC,UAAD,CAAlC,CAApB;AACAf,eAAS,GAAGA,SAAS,CAACc,IAAV,CAAeC,+DAAW,CAAC,UAAD,CAA1B,CAAZ;AACAd,kBAAY,GAAGA,YAAY,CAACa,IAAb,CAAkBC,+DAAW,CAAC,UAAD,CAA7B,CAAf;AACAb,mBAAa,GAAGA,aAAa,CAACY,IAAd,CAAmBC,+DAAW,CAAC,UAAD,CAA9B,CAAhB;AACAZ,mBAAa,GAAGA,aAAa,CAACW,IAAd,CAAmBC,+DAAW,CAAC,UAAD,CAA9B,CAAhB;AACAX,cAAQ,GAAGA,QAAQ,CAACU,IAAT,CAAcC,+DAAW,CAAC,UAAD,CAAzB,CAAX;AACAV,iBAAW,GAAGA,WAAW,CAACS,IAAZ,CAAiBC,+DAAW,CAAC,UAAD,CAA5B,CAAd;AACAT,iBAAW,GAAGA,WAAW,CAACQ,IAAZ,CAAiBC,+DAAW,CAAC,UAAD,CAA5B,CAAd;AACAR,gBAAU,GAAGA,UAAU,CAACO,IAAX,CAAgBC,+DAAW,CAAC,UAAD,CAA3B,CAAb;AACAP,cAAQ,GAAGA,QAAQ,CAACM,IAAT,CAAcC,+DAAW,CAAC,UAAD,CAAzB,CAAX;AACAN,eAAS,GAAGA,SAAS,CAACK,IAAV,CAAeC,+DAAW,CAAC,UAAD,CAA1B,CAAZ;AACAL,oBAAc,GAAGA,cAAc,CAACI,IAAf,CAAoBC,+DAAW,CAAC,UAAD,CAA/B,CAAjB;AACAJ,cAAQ,GAAGA,QAAQ,CAACG,IAAT,CAAcC,+DAAW,CAAC,UAAD,CAAzB,CAAX;AACAtB,aAAO,GAAGC,SAAS,CAACP,MAAV,CAAiBQ,WAAjB,EAA6BC,YAA7B,EAA0CC,gBAA1C,EAA2DC,iBAA3D,EAA6EE,SAA7E,EAAuFC,YAAvF,EAAoGC,aAApG,EAAkHC,aAAlH,EAAgIC,QAAhI,EAAyIC,WAAzI,EAAqJC,WAArJ,EAAiKC,UAAjK,EAA4KC,QAA5K,EAAqLC,SAArL,EAA+LC,cAA/L,EAA8MC,QAA9M,CAAV;;AACA,UAAGlB,OAAO,CAAClG,MAAR,GAAe,CAAlB,EAAoB;AAChB;AACA,YAAGoG,WAAW,CAACpG,MAAZ,IAAoB,CAAvB,EAAyB;AACrBuB,cAAI,CAACrC,QAAL,GAAc,CAAd;AACH,SAFD,MAEK;AACDqC,cAAI,CAACrC,QAAL,GAAciH,SAAS,CAACnG,MAAxB;AACH;AACJ;;AACD,aAAOkG,OAAP;AACH;AAjnBM;AA1FE,CAAb;AA8sBe7H,mEAAf,E","file":"js/14-62f15946.js","sourcesContent":[" // import the component\r\n import Treeselect from '@riophae/vue-treeselect'\r\n // import the styles\r\n import '@riophae/vue-treeselect/dist/vue-treeselect.css'\r\n import {compareName,cloneObj,round,codeObj,getTime} from 'utils/utils'\r\nconst head = {\r\n  components:{\r\n    Treeselect\r\n  },\r\n  data () {\r\n    return {\r\n      options:[],//组织数据\r\n      chooseMacList:[],//选择的机组列表\r\n      chooseDgmList:[],//选择的采集器列表\r\n      choosePosList:[],//选择的测点列表\r\n      chooseTid:null,//所选的组织id\r\n      chooseMacId:null,//所选的机组id\r\n      choosePosTypeId: null,//所选的测点id\r\n      chooseDgmId: null,//所选的采集器id\r\n      chooseDgmType: null,//所选采集器类型\r\n      selIndex:0,//默认选中的测点index\r\n      choosePosId:null,\r\n      choosePosType:null,\r\n      choosePosName:'',\r\n      chooseIsRealType:2,\r\n      toggleText:'实时',\r\n      currentPage:1,//当前滚动页面\r\n      noTips:'无选项'\r\n    //   isReal:true,\r\n    };\r\n  },\r\n  watch: {\r\n    \r\n  },\r\n  beforeDestroy () {\r\n    \r\n  },\r\n  mounted () {\r\n    \r\n  },\r\n  computed: {\r\n    reSetMacList() {\r\n        let list = this.chooseMacList;\r\n        if(list && list.length){\r\n            list.forEach(item=>{\r\n                item.id = item.mac_id;\r\n                item.label = item.mac_me;\r\n            })\r\n        }\r\n        else{\r\n            list = [];\r\n        }\r\n        return list;\r\n    },\r\n    reSetDgmList() {\r\n        let list = this.chooseDgmList;\r\n        if(list && list.length){\r\n            list.forEach(item=>{\r\n                item.id = item.dgm_type;\r\n                item.dgmTypeName = this.transDgmName(item.dgm_type,this.chooseTree.t_root);\r\n                item.label = item.dgmTypeName;\r\n            })\r\n        }\r\n        else{\r\n            list = [];\r\n        }\r\n        return list;\r\n    },\r\n    reSetPosList() {\r\n        let list = this.choosePosList;\r\n        if(list && list.length){\r\n            list.forEach(item=>{\r\n                item.id = item.pos_type+'_'+item.pos_id +'_'+ item.mac_id;\r\n                item.label = item.pos_name;\r\n            })\r\n        }\r\n        else{\r\n            list = [];\r\n        }\r\n        return list;\r\n    },\r\n    reDgmList() {\r\n        let list = this.chooseDgmList;\r\n        if(list && list.length){\r\n            list.forEach(item=>{\r\n                item.id = item.dgm_id;\r\n                item.label = item.dgm_name;\r\n            })\r\n        }\r\n        else{\r\n            list = [];\r\n        }\r\n        return list;\r\n    },\r\n  },\r\n  methods: {\r\n    //获取数据\r\n    getHeadData(type) {\r\n        let that = this;\r\n        let params = null;\r\n        params={\r\n            type,\r\n            t_ids:JSON.parse(sessionStorage.getItem('getTidList')),\r\n        };\r\n        // that.options = that.$store.state.options;\r\n        this.$axios.post('getPositionDataConfig',params)\r\n                .then(res => {\r\n                    if(res.msg===0 && res.list && res.list.length){\r\n                        that.options = that.getFolderList(res.list);\r\n                        // that.$store.commit(\"setOptions\", that.options); //赋值给vuex\r\n                        that.setOrigData();\r\n                    }\r\n                    else{\r\n                        console.log('请求失败')\r\n                    }\r\n                }).catch(err => {\r\n                    console.log(err)\r\n                })\r\n    },\r\n    //造数据\r\n    setOrigData(){\r\n        this.choosePosType = null;\r\n        this.choosePosTypeId = null;\r\n        this.chooseDgmType = null;\r\n        this.chooseDgmId = null;\r\n        this.chooseTid = null;\r\n        this.chooseFolderId = null;\r\n        this.choosePos = {};\r\n        this.selectPosition = {};\r\n\r\n        this.toggleText = \"实时\";//实时与历史按钮文字\r\n        this.isReal = true;//是否实时\r\n        this.flag_real = 1;\r\n        this.chooseIsRealType = 2;//1是历史数据，2是实时数据\r\n\r\n        if(this.options && this.options.length){\r\n            this.chooseTid = this.options[0].id;\r\n            this.chooseTroot = this.options[0].t_root;\r\n            this.chooseTname = this.options[0].t_name;\r\n            this.chooseFolderId = this.options[0].t_id;\r\n            this.chooseTree = this.options[0];\r\n            this.chooseMacList = this.options[0].mac_list;\r\n            if(this.chooseMacList && this.chooseMacList.length>0){\r\n                this.chooseMacId = this.chooseMacList[0].mac_id;\r\n                this.selectMachine = this.chooseMacList[0];\r\n                this.chooseMacName = this.chooseMacList[0].mac_me;\r\n                this.chooseDgmList = this.chooseMacList[0].dgms_list;\r\n                this.choosePosList = this.chooseMacList[0].positions_list;\r\n            }\r\n            \r\n            let linkInfo = this.$store.state.treeData;\r\n            if(!$.isEmptyObject(linkInfo)){\r\n                this.chooseTree.t_id = linkInfo.t_id;//所选的组织id\r\n                this.chooseTree.t_root = linkInfo.t_root;//所选的组织id\r\n                this.chooseTid = linkInfo.t_id;//所选的组织id\r\n                this.chooseTroot = linkInfo.t_root;//所选的组织id\r\n                \r\n            }\r\n            let isReal = this.$store.state.isReal;\r\n            if (isReal) {\r\n                this.isReal = true;//是否实时\r\n                this.flag_real = 1;\r\n                this.chooseIsRealType = 2;\r\n                this.toggleText = \"实时\"\r\n            } else {\r\n                this.isReal = false;//是否实时\r\n                this.flag_real = 0;\r\n                this.chooseIsRealType = 1;\r\n                this.toggleText = \"历史\"\r\n            } \r\n            // if(this.choosePosList && this.choosePosList.length){\r\n            //     if($.isEmptyObject(this.choosePos) || this.choosePos == undefined){\r\n            //         if(this.choosePosTypeId === null || this.choosePosTypeId == undefined){\r\n            //             this.choosePos = this.choosePosList[0];\r\n            //             this.selectPosition = this.choosePosList[0];\r\n            //             this.choosePosTypeId = this.choosePosList[0].pos_type + '_' + this.choosePosList[0].pos_id  + '_' + this.chooseMacId;\r\n            //             this.choosePosName = this.choosePosList[0].pos_name;//所选的测点名\r\n            //         }\r\n            //         else{\r\n            //             let posType = this.choosePosTypeId.split('_')[0];\r\n            //             let posId = this.choosePosTypeId.split('_')[1];\r\n            //             let macId = this.choosePosTypeId.split('_')[2];\r\n            //             this.choosePosList.forEach(item=>{\r\n            //                 if(item.pos_type==posType && item.pos_id == posId && item.mac_id == macId){\r\n            //                     this.choosePos = item;\r\n            //                 }\r\n            //             })\r\n            //         }\r\n            //     }\r\n            //    else{\r\n            //         this.selectPosition = this.choosePos;\r\n            //         if(this.choosePosTypeId === null || this.choosePosTypeId == undefined){\r\n            //             this.choosePosTypeId = this.choosePos.pos_type + '_' + this.choosePos.pos_id  + '_' + this.choosePos.mac_id;\r\n            //             this.choosePosName = this.choosePos.pos_name;//所选的测点名\r\n            //         }\r\n            //    } \r\n            // }\r\n            // if(this.chooseDgmList && this.chooseDgmList.length){\r\n            //     this.chooseDgmId = this.chooseDgmList[0].dgm_id;\r\n            //     this.chooseDgmType = this.chooseDgmList[0].dgm_type;\r\n            //     this.choosePosType = -1;\r\n            //     this.getPosTypeList(this.chooseDgmType);\r\n            // }\r\n            // if(this.choosePos){\r\n            //     this.selectPosition = this.choosePos;\r\n            // }\r\n            // else{\r\n            //     this.selectPosition = {};\r\n            // }\r\n            // this.selectMachineId = this.chooseMacId;\r\n            // this.selectPositionId = this.choosePosTypeId;\r\n            // this.selectPositionName = this.choosePosName; \r\n            this.$store.commit(\"setTreeData\", this.chooseTree); //组织信息赋值给vuex\r\n            this.setMacData(); \r\n            // this.setPosData();     \r\n        }\r\n        \r\n    },\r\n    //递归组织\r\n    getFolderList(list){\r\n        if(list && list.length){\r\n            list.forEach(item=>{\r\n                item.id = item.t_id;\r\n                item.label = item.t_name;\r\n                item.status = false;\r\n                if(item.children){\r\n                    this.getFolderList(item.children);\r\n                }\r\n            })\r\n        }\r\n        return list;\r\n    },\r\n    //获取机组信息\r\n    setMacData(){\r\n        let params = null;\r\n        this.chooseMacList = [];\r\n        this.chooseMacId = null;\r\n        this.selectMachine = {};\r\n        this.chooseMacName = '';\r\n        this.chooseDgmList = [];\r\n        this.chooseDgmId = null;\r\n        this.chooseDgmType = null;\r\n        this.choosePosType = null;\r\n        params={\r\n            t_id:this.chooseTid\r\n        };\r\n        this.$axios.post('getMachine',params)\r\n                .then(res => {\r\n                    if(res.msg===0 && res.list && res.list.length){\r\n                        this.chooseMacList = res.list;\r\n                        this.chooseMacId = this.chooseMacList[0].mac_id;\r\n                        // this.selectMachine = this.chooseMacList[0];\r\n                        // this.chooseMacName = this.chooseMacList[0].mac_me;\r\n                        // this.chooseDgmList = this.chooseMacList[0].dgms_list;\r\n                        let linkInfo = this.$store.state.macData;\r\n                        if(!$.isEmptyObject(linkInfo)){\r\n                            this.chooseMacId = linkInfo.mac_id;//所选的机组id\r\n                        }\r\n                        this.chooseMacList.forEach(mac=>{\r\n                            if(mac.mac_id == this.chooseMacId){\r\n                                this.selectMachine = mac;\r\n                                this.chooseMacName = mac.mac_me;\r\n                                this.chooseDgmList = mac.dgms_list;\r\n                            }\r\n                        })\r\n                        if(this.chooseDgmList && this.chooseDgmList.length){\r\n                            this.chooseDgmId = this.chooseDgmList[0].dgm_id;\r\n                            this.chooseDgmType = this.chooseDgmList[0].dgm_type;\r\n                            this.choosePosType = -1;\r\n                            this.getPosTypeList(this.chooseDgmType);\r\n                        }\r\n                        this.selectMachineId = this.chooseMacId;\r\n                        this.$store.commit(\"setMacData\", this.selectMachine); //机组信息赋值给vuex\r\n                        this.setPosData()\r\n                    }\r\n                    else{\r\n                        console.log('请求失败')\r\n                    }\r\n                }).catch(err => {\r\n                    console.log(err)\r\n                })\r\n    },\r\n    //获取测点信息\r\n    setPosData(){\r\n        let params = null;\r\n        this.choosePosList = [];\r\n        this.choosePos = {};\r\n        this.choosePosTypeId = null;\r\n        this.choosePosName = '';\r\n        params={\r\n            mac_id:this.chooseMacId\r\n        };\r\n        this.$axios.post('getPosition',params)\r\n                .then(res => {\r\n                    if(res.msg===0 && res.list && res.list.length){\r\n                        this.choosePosList = res.list;\r\n                        // this.choosePos = this.choosePosList[0];\r\n                        this.choosePosTypeId = this.choosePosList[0].pos_type + '_' + this.choosePosList[0].pos_id  + '_' + this.chooseMacId;\r\n                        this.choosePosName = this.choosePosList[0].pos_name;//所选的测点名\r\n                        let linkInfo = this.$store.state.posData;\r\n                        if(!$.isEmptyObject(linkInfo)){\r\n                            this.choosePosTypeId = linkInfo.pos_type + '_' + linkInfo.pos_id  + '