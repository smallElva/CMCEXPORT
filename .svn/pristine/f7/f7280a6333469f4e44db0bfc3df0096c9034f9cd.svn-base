   height: 30px;\n    line-height: 30px;\n    position: absolute;\n    top: 5px;\n    right: 10px;\n    cursor: pointer;\n}\n.level-content .serverList {\n    height: 50px;\n    line-height: 50px;\n    padding-right: 20px;\n    display: flex;\n    justify-content: space-around;\n    align-items: center;\n    position: relative;\n}\n.level-content .itemName {\n    width: 40%;\n    text-align: right;\n}\n.level-content .itemModel {\n    width: 50%;\n    outline: none;\n    margin: 0;\n    padding: 0;\n    height: 30px;\n    box-sizing: border-box;\n    border: 1px solid #ccc;\n}\n.level-content .redOuter {\n    border-color: red;\n}\n.level-content .errorTips {\n    position: absolute;\n    bottom: -8px;\n    left: 45%;\n    height: 18px;\n    line-height: 18px;\n    font-size: 12px;\n    color: red;\n    display: none;\n}\n.level-content .yesOrNo {\n    height: 40px;\n    line-height: 40px;\n    text-align: center;\n}\n.level-content .yesOrNoBtn {\n    height: 30px;\n    line-height: 30px;\n    color: #fff;\n    border-radius: 4px;\n    border: none;\n    outline: none;\n}\n.level-content .yes {\n    width: 170px;\n    background: #597DD2;\n}\n.level-content .no {\n    width: 60px;\n    background: #798CA1;\n}\n.level-content .alertAlarm {\n    background: rgba(0, 0, 0, 0.4);\n}\n.level-content .delAlarm {\n    width: 300px;\n    background: #fff;\n    border-radius: 4px;\n    position: absolute;\n    left: 50%;\n    top: 50%;\n    margin-left: -150px;\n    margin-top: -100px;\n}\n.level-content .alarmTitle {\n    background: #343C45;\n    color: #fff;\n    padding-left: 15px;\n    height: 40px;\n    line-height: 40px;\n    border-top-left-radius: 4px;\n    border-top-right-radius: 4px;\n    font-size: 16px;\n}\n.level-content .alarmText {\n    padding: 20px 0;\n    text-align: center;\n    font-size: 18px;\n    background: #E4E8F4;\n    border-bottom: 1px solid #ccc;\n}\n.level-content .delBtn {\n    height: 40px;\n    line-height: 40px;\n    text-align: center;\n    background: #e4e8f4;\n}\n.level-content .delYes, .level-content .delNo {\n    width: 50px;\n    height: 24px;\n    line-height: 26px;\n    border: none;\n    outline: none;\n    background: #798CA1;\n    color: #fff;\n    border-radius: 4px;\n}\n.level-content .delYes {\n    background: #597DD2;\n    margin-right: 20px;\n}\n.level-content .setMachineServer {\n    height: calc(100% - 80px);\n    padding-top: 30px;\n    box-sizing: border-box;\n    display: flex;\n    justify-content: space-between;\n}\n.level-content .bindingServer, .level-content .upServer {\n    background: #fff;\n    border-radius: 4px;\n    border: 1px solid #888;\n    border-top: none;\n}\n.level-content .bindingServer {\n    width: 60%;\n}\n.level-content .upServer {\n    width: 37%;\n}\n.level-content .serverTitle {\n    height: 50px;\n    line-height: 50px;\n    background: #798CA1;\n    color: #fff;\n    padding-left: 10px;\n    font-size: 20px;\n    border-top-left-radius: 4px;\n    border-top-right-radius: 4px;\n}\n.level-content .bindingServerTitle {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n}\n.level-content .serachFolder {\n    width: 60%;\n    height: 50px;\n    line-height: 50px;\n    position: relative;\n}\n.level-content .titleText {\n    width: 30%;\n    height: 50px;\n    line-height: 50px;\n}\n.level-content .searchInput {\n    border-radius: 4px;\n    background: #798CA1;\n    color: #fff;\n    outline: none;\n    border: 1px solid #fff;\n    padding: 2px 10px;\n    margin-left: 10px;\n    box-sizing: border-box;\n    height: 30px;\n    width: 200px;\n}\n.level-content .searchInput:hover {\n    cursor: auto;\n}\n.level-content .searchImg {\n    height: 16px;\n    position: absolute;\n    top: 17px;\n    cursor: pointer;\n    left: 320px;\n}\n.level-content .tableTitle, .level-content .upServerTableBody {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    text-align: center;\n    user-select: none;\n}\n.level-content .tableTitle > li, .level-content .upServerTableBody > div {\n    height: 50px;\n    line-height: 50px;\n    width: 30%;\n    border-right: 1px solid #888;\n    border-bottom: 1px solid #888;\n}\n.level-content .tableTitle > li:nth-child(4) {\n    width: 40px;\n}\n.level-content .tableTitle > li:last-child, .level-content .upServerTableBody > div:last-child {\n    width: 80px;\n    border-right: none;\n}\n.level-content .tableBody {\n    height: calc(100% - 100px);\n    overflow-y: auto;\n}\n.level-content .tableBody::-webkit-scrollbar {\n    display: none;\n}\n.level-content .tableRow, .level-content .bindServer {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    text-align: center;\n    user-select: none;\n}\n.level-content .machineName {\n    width: 30%;\n    border-right: 1px solid #888;\n    border-bottom: 1px solid #888;\n    box-sizing: border-box;\n    cursor: pointer;\n}\n.level-content .machineBindServer {\n    width: 60%;\n}\n.level-content .checkOrNo {\n    width: 41px;\n    border-right: 1px solid #888;\n    border-bottom: 1px solid #888;\n    box-sizing: border-box;\n    cursor: pointer;\n}\n.level-content .checkOrNo input {\n    height: 45px;\n    line-height: 45px;\n}\n.level-content .tableRow > div:last-child {\n    width: 80px;\n    box-sizing: border-box;\n}\n.level-content .bindServer > li, .level-content .untying {\n    width: 50%;\n    height: 50px;\n    line-height: 50px;\n    border-right: 1px solid #888;\n    border-bottom: 1px solid #888;\n    box-sizing: border-box;\n}\n.level-content .untying:last-child {\n    border-right: none;\n}\n.level-content .untying {\n    width: 100%;\n}\n.level-content .tableRow:nth-child(odd), .level-content .upServerTableBody:nth-child(odd) {\n    background: #EFF1F6;\n}\n.level-content .tableRow:hover {\n    background: #ABB7C7;\n}\n.level-content .operatBtn {\n    outline: none;\n    border: none;\n    background: #5A7ED3;\n    color: #fff;\n    border-radius: 4px;\n    width: 60px;\n    height: 30px;\n    line-height: 30px;\n}\n.level-content .operatBtn0 {\n    background: #9cb2e5;\n}\n.level-content .upServerTable > li, .level-content .upServerTableBody > div {\n    width: 45%;\n}\n.level-content .btn4 {\n    right: 180px;\n}\n"]}