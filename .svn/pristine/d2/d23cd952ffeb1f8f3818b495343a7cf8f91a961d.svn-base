ACf;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;;;;;;;;;;;;ACvTA;AAAA;AAAA;AAAA;AAAA;AAAoG;AACvC;AACL;AACsC;;;AAG9F;AACmG;AACnG,gBAAgB,2GAAU;AAC1B,EAAE,+EAAM;AACR,EAAE,gGAAM;AACR,EAAE,yGAAe;AACjB;AACA;AACA;AACA;;AAEA;;AAEA;AACA,IAAI,IAAU;AACd,YAAY,mBAAO,CAAC,wGAAsE;AAC1F,cAAc,mBAAO,CAAC,+CAAK;AAC3B;AACA;AACA;AACA;AACA,KAAK;AACL;AACA;AACA,sBAAsB,gKAA4D,EAAE;AAAA;AACpF;AACA,gBAAgB,gGAAM;AACtB,yBAAyB,yGAAe;AACxC,OAAO;AACP,KAAK;AACL;AACA;AACA;AACe,gF;;;;;;;;;;;;ACvCf;AAAA;AAAA,wCAAqM,CAAgB,qPAAG,EAAC,C;;;;;;;;;;;;ACAzN;AAAA;AAAA;AAAA;AAAga,CAAgB,kaAAG,EAAC,C;;;;;;;;;;;;ACApb;AAAA;AAAA;AAAA;AAAA;AAAA","file":"js/17-ec83c2301e4a952bc011.js","sourcesContent":["<template>\r\n    <div>\r\n        <div id=\"myTable\" class=\"myTable\">\r\n            <div class=\"table-btn\">\r\n                <button class=\"my-btn btn-active white-btn\" @click=\"add\" :class=\"{add:obj.addFlag}\">新建</button>\r\n<!--                <button class=\"my-btn btn-active white-btn\" @click=\"add\" :class=\"{add:obj.openFlag}\">开启</button>-->\r\n                <button class=\"my-btn btn-active\" :class=\"{'white-btn':obj.openFlag}\" v-show=\"!obj.stopFlag\" @click=\"openOrClose()\">{{stopText}}</button>\r\n                <button class=\"my-btn btn-active\" @click=\"save\">保存设置</button>\r\n                <button class=\"my-btn btn-active\" :class=\"{add:obj.opcFlag}\" @click=\"setOpc\">OPC全局设置</button>\r\n            </div>\r\n            <div id=\"tableContent\">\r\n                <!-- <keep-alive> -->\r\n                <router-view ref='child' @child-msg='updateIsShow' ></router-view>\r\n                <!-- </keep-alive> -->\r\n                <div class=\"noData\" v-if=\"isShow\">系统中没有可建该类型机组的设备!</div>\r\n            </div>\r\n\r\n            <div class=\"controls-btn\">\r\n                <button class=\"con-dgm-btn\" v-for=\"(btn,index) in btnData\"  @click=\"getTable(btn.name,btn.type)\" :class=\"{active:btn.name==name}\">{{btn.textBtn}}</button>\r\n            </div>\r\n            <!--  OPC全局设置弹窗 /-->\r\n            <div class=\"edit-block\" id=\"edit-block\" v-show=\"editStatus\">\r\n                <div class=\"edit-header\">\r\n                    <span class=\"edit-name\">OPC全局设置</span>\r\n                    <i class=\"iconfont icon-zhongzi-guanbi close-icon\" @click=\"closeEdit()\"></i>\r\n                </div>\r\n                <div class=\"edit-list\">\r\n                    <ul>\r\n                        <li class=\"channel-item\">\r\n                            <div class=\"channel-div\">\r\n                                <label>OPC Server ip:</label>\r\n                                <input v-model=\"opcTabList.opc_ip\" type=\"text\">\r\n                            </div>\r\n                        </li>\r\n                        <li class=\"channel-item\">\r\n                            <div class=\"channel-div\">\r\n                                <label>OPCServer 域:</label>\r\n                                <input v-model=\"opcTabList.opc_domain\" type=\"text\">\r\n                            </div>\r\n                        </li>\r\n                        <li class=\"channel-item\">\r\n                            <div class=\"channel-div\">\r\n                                <label>OPCServer 用户名:</label>\r\n                                <input v-model=\"opcTabList.opc_user\" type=\"text\">\r\n                            </div>\r\n                        </li>\r\n                        <li class=\"channel-item\">\r\n                            <div class=\"channel-div\">\r\n                                <label>OPCServer 密码:</label>\r\n                                <input v-model=\"opcTabList.opc_password\" type=\"text\">\r\n                            </div>\r\n                        </li>\r\n                        <li class=\"channel-item\">\r\n                            <div class=\"channel-div\">\r\n                                <label>OPCServer 注册码:</label>\r\n                                <input v-model=\"opcTabList.opc_clsid\" type=\"text\">\r\n                            </div>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n                <div class=\"edit-controls\">\r\n                    <button class=\"fail-btn\" @click=\"closeEdit()\">取消</button>\r\n                    <button class=\"ok-btn\" @click=\"changeOpc()\">确定</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n    import getApi from 'network/getApi'\r\n    import {addRows} from 'utils/utils'\r\n\r\n    export default {\r\n        name: \"Datatable\",\r\n        data(){\r\n            return{\r\n                title:'数据传输协议',\r\n                ins:0,//切换按钮active\r\n                name:'ModServer',\r\n                isShow:false,\r\n                btnName:\"切换树状图\",\r\n                stopText:\"停止\",\r\n                btnData: [//按钮初始化\r\n                    {type:1,textBtn:\"modbus server\",name:'ModServer'},\r\n                    {type:2,textBtn:\"modbus client\",name:'ModClient'},\r\n                ],\r\n                obj:{\r\n                    name:\"\",\r\n                    addFlag:false,\r\n                    openFlag:false,\r\n                    stopFlag:false,\r\n                    opcFlag:false\r\n                },\r\n                editStatus:false,//opc全局设置弹窗是否显示\r\n                opcTabList:{}\r\n            }\r\n        },\r\n        mounted(){\r\n            // 在点击标题的时候放大并重新渲染表格\r\n            this.$bus.$on('reloadtable',()=>{\r\n                if(this.$refs.child){\r\n                    this.$refs.child.hot.render()\r\n                }\r\n            })\r\n            this.name = this.$route.name //刷新的时候让button 的active 正确\r\n            if(this.name ==\"ModServer\"){ //刷新的时候工况设置不可以添加\r\n                this.obj={\r\n                    name:\"ModServer\",\r\n                    addFlag:false,\r\n                    openFlag:false,\r\n                    stopFlag:false,\r\n                    opcFlag:true\r\n                };\r\n                this.$emit('listenToChildEvent',this.obj);\r\n                this.title='modbus server';\r\n            }\r\n            else if(this.name ==\"ModClient\"){\r\n                this.title='modbus client';\r\n                this.obj={\r\n                    name:\"ModClient\",\r\n                    addFlag:false,\r\n                    openFlag:true,\r\n                    stopFlag:true,\r\n                    opcFlag:true\r\n                };\r\n                this.$emit('listenToChildEvent',this.obj);\r\n            }\r\n           this.queryState();//获取状态\r\n            this.turnOn();//更新按钮状态\r\n            this.getOPCServer();//获取opc全局设置数据\r\n        },\r\n        methods:{\r\n\r\n            getTable(name){ //点击下标签的时候改变active，改变路由，刷新之后路由不变，active不变，点击左边测点设置 active会有问题\r\n                this.$router.push({name:name})\r\n                this.name = name;\r\n                this.$emit('listenToChildEvent',this.obj);\r\n                if(name ==\"ModServer\"){\r\n                    this.obj={\r\n                        name:\"ModServer\",\r\n                        addFlag:false,\r\n                        openFlag:false,\r\n                        stopFlag:false,\r\n                        opcFlag:true\r\n                    };\r\n                    this.$emit('listenToChildEvent',this.obj);\r\n                    this.title='数据传输协议';\r\n                }\r\n                else if(name ==\"ModClient\"){\r\n                    this.title='modbus client';\r\n                    this.obj={\r\n                        name:\"ModClient\",\r\n                        addFlag:false,\r\n                        openFlag:true,\r\n                        stopFlag:true,\r\n                        opcFlag:true\r\n                    };\r\n                    this.$emit('listenToChildEvent',this.obj);\r\n                }\r\n\r\n            },\r\n            add(){\r\n                return addRows(this.$refs.child.hot,this.$refs.child.data,-1);\r\n            },\r\n            getOPCServer(){\r\n                getApi.queryOPCServer()\r\n                    .then(res=>{\r\n                        if(res){\r\n                            this.opcTabList={\r\n                                \"id\":res.id,\r\n                                \"opc_ip\":res.opc_ip,\r\n                                \"opc_domain\":res.opc_domain,\r\n                                \"opc_user\":res.opc_user,\r\n                                \"opc_password\":res.opc_password,\r\n                                \"opc_clsid\":res.opc_clsid,\r\n                            }\r\n                        }else{\r\n                            this.opcTabList={\r\n                                \"id\":0,\r\n                                \"opc_ip\":\"\",\r\n                                \"opc_domain\":\"\",\r\n                                \"opc_user\":\"\",\r\n                                \"opc_password\":\"\",\r\n                                \"opc_clsid\":\"\",\r\n                            }\r\n                        }\r\n\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            },\r\n            setOpc(){\r\n                this.editStatus=true;\r\n            },\r\n            closeEdit(){\r\n                this.editStatus=false;\r\n            },\r\n            changeOpc(){\r\n                console.log(this.opcTabList);\r\n                this.editStatus=false;\r\n                getApi.saveOPCServer(this.opcTabList)\r\n                    .then(res=>{\r\n                        if(res.msg==0){\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: '保存成功',\r\n                                button: \"确认\",\r\n                            })\r\n                                .then((value) => {\r\n                                    this.$router.go(0)\r\n                                });\r\n\r\n                        }\r\n                        else if(res.msg==1){\r\n                            console.log('参数为空！');\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: '保存失败',\r\n                                button: \"确认\",\r\n                            });\r\n                        }\r\n                        else if(res.msg==2){\r\n                            console.log('后台运行错误！');\r\n                            swal({\r\n                                title: \"提示\",\r\n                                text: '保存失败',\r\n                                button: \"确认\",\r\n                            });\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            },\r\n            queryState(){\r\n                getApi.queryModBusServerStatue()\r\n                    .then(res=>{\r\n                        this.obj.openFlag = res.flag;\r\n                        if(this.obj.openFlag){\r\n                            this.stopText=\"开启\";\r\n                        }\r\n                        else{\r\n                            this.stopText=\"停止\";\r\n                        }\r\n                    })\r\n                    .catch(err=>{\r\n                        console.log('请求错误' ,err)\r\n                    })\r\n            },\r\n            turnOn(){\r\n                let params={\r\n                    flag:this.obj.openFlag\r\n                }\r\n                if(this.obj.openFlag){\r\n                    this.stopText=\"开启\";\r\n                    getApi.startOrStopModBusServer(params)\r\n                        .then(res=>{\r\n                            console.log('已开启');\r\n                        }