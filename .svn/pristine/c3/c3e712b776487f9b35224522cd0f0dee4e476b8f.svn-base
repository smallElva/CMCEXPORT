.device-content .left, .device-content .right {\n    padding: 12px 10px;\n}\n.device-content .left {\n    width: 17%;\n    position: relative;\n    border-right: 1px solid #E4E8F4;\n}\n.device-content .organization {\n    /*height: 50px;\r\n    line-height: 50px;*/\n    position: relative;\n    margin-bottom: 10px;\n}\n.device-content .organization ul li {\n      list-style: none;\n}\n.device-content .organization .input {\n      height: 32px;\n      line-height: 32px;\n      border: 1px solid #a9a9a9;\n      width: calc(100% - 42px);\n      border-radius: 4px;\n      padding: 0 30px 0 10px;\n      position: relative;\n}\n.device-content .organization .input input {\n      border: none;\n      outline: none;\n      width: 90%;\n      font-size: 14px;\n}\n.device-content .organization .input i {\n      position: absolute;\n      right: 10px;\n      top: 0;\n      cursor: pointer;\n}\n.device-content .organization .list {\n      position: absolute;\n      top: 32px;\n      left: 0;\n      z-index: 999;\n      width: calc(100% - 2px);\n      border: 1px solid #cccccc;\n      overflow: hidden;\n      background: #fff;\n}\n.device-content .organization .list ul {\n        max-height: 300px;\n        overflow-y: auto;\n}\n.device-content .organization .list ul li {\n      width: calc(100% - 10px);\n      height: 30px;\n      cursor: pointer;\n      line-height: 30px;\n      padding-left: 10px;\n      font-size: 14px;\n      overflow: hidden;\n      white-space: nowrap;\n      text-overflow: ellipsis;\n}\n.device-content .organization .list ul li:hover {\n      background-color: #cccccc;\n}\n.device-content .deviceEditPart {\n    position: absolute;\n    bottom: 10px;\n    left: 0;\n    padding: 0 10px;\n    width: calc(100% - 20px);\n}\n.device-content .deviceEditPart .baseModelControl {\n      display: flex;\n      justify-content: space-between;\n      width: 100%;\n      line-height: 30px;\n}\n.device-content .deviceEditPart .baseModelControl ul li {\n        list-styl