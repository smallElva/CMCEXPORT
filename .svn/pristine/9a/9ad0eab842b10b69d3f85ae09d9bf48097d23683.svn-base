.display = \"block\";\r\n        content.style.width = \"100px\"; //改变宽让宽自动渲染\r\n        if(wHeight < 800){\r\n          content.style.height = \"calc(100vh - 90px)\"; //中间内容加大\r\n        }\r\n        else{\r\n          content.style.height = \"calc(100vh - 120px)\"; //中间内容加大\r\n        }\r\n        this.$bus.$emit(\"reloadtable\"); //主动渲染table\r\n        this.$bus.$emit(\"reloadoverview\"); //主动渲染总貌图\r\n        this.flag = !this.flag;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.my-content-header {\r\n  /* width: 100%-15px;\r\n   */\r\n  box-sizing: border-box;\r\n  height: 50px;\r\n  line-height: 50px;\r\n  background: #fff;\r\n  font-size: 20px;\r\n  color: #000;\r\n  padding-left: 15px;\r\n  cursor: pointer;\r\n  margin-bottom: 5px;\r\n}\r\n</style>\r\n","<template>\r\n  <div class=\"folder-content\">\r\n    <button class=\"my-btn btn-active btn3\" @click=\"onShowListFile\">{{btnName}}</button>\r\n    <my-content-title :title='title'></my-content-title>\r\n    <router-view id='myContentCenter'></router-view>\r\n    <!-- <my-table v-show=\"flag%2==0\"  v-on:listenToChildEvent=\"getDataFromChild\" ref=\"childTable\"></my-table> -->\r\n    <!-- <my-tree v-show=\"flag%2==1\" :parentFlag=\"flag\" :chooseTid=\"chooseTid\" ref=\"childTree\"></my-tree> -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MyContentTitle from 'components/common/MyContentTitle';\r\nexport default {\r\n  name: \"Folder\",\r\n  props:['parentToChild'],\r\n  data(){\r\n    return{\r\n      title:'组织设置',\r\n      btnName:'',\r\n      activeNum: \"00\",\r\n      parentFolderData:[\"根目录\"],\r\n      chooseTid:\"\",//所选的组织id\r\n    }\r\n  },\r\n  created: function() {\r\n    this.$emit('ievent',this.activeNum);\r\n    this.btnName = this.$route.name == 'foldertree' ?'切换至表格':'切换树状图'\r\n\r\n  },\r\n  mounted: function() {\r\n    // let contentArea = document.getElementById('contentArea');\r\n    // contentArea.style.height = window.innerHeight-200 + \"px\";\r\n  },\r\